% TRANSFORMATION TCaseAssignment1
< m1:CLAUSE{CLAUSErec1}
           [ mu1,
             predrel/VERBP{VERBPrec1}
                    [ mu2,
                      I1:rel1/T1,
                      mu3,
                      head/VERB{VERBrec1}[mu4],
                      mu5 
                    ],
             mu6 
           ]
>

< m :CLAUSE{CLAUSErec1}
           [ mu1,
             predrel/VERBP{VERBPrec1}
                    [ mu2,
                      I2:rel1/T2,
                      mu3,
                      head/VERB{VERBrec1}[mu4],
                      mu5 
                    ],
             mu6 
           ]
>


   MATCHCONDITIONS
      <
       I1: rel1 IN [predrel, objrel] AND
           (T1.CAT IN [NPVAR, CNVAR, NP] WITH T1.REC.cases = [Nominative])
       I2: rel1 IN [predrel, objrel] AND
           (T2.CAT IN [NPVAR, CNVAR, NP] WITH Accusative IN T2.REC.cases )
      >




<
   SUBRULE
      <T1:NPVAR{NPVARrec1} >
      <T2:NPVAR{NPVARrec2} >

      COMP
         <
          C1: true
	      C11: rel1 = predrel 
	      A11: NPVARrec2 := NPVARrec1;
	      C13: rel1 = objrel AND
                   (NOT QUOTE_normalergative(VERBrec1)) AND
		   (CLAUSErec1.voice <> Passive)
	      A13: NPVARrec2 := NPVARrec1;
          A1: NPVARrec2.cases := [accusative];

         >
      DECOMP
         <
          C1: true
              C11: rel1 = predrel 
              A11: NPVARrec1 := NPVARrec2;
	      C13: rel1 = objrel AND
                   (NOT QUOTE_normalergative(VERBrec1)) AND
		   (CLAUSErec1.voice <> Passive)  
	      A13: NPVARrec1 := NPVARrec2;
          A1: NPVARrec1.cases := [Nominative]
         >




   SUBRULE
      <T1:CNVAR{CNVARrec1} >
      <T2:CNVAR{CNVARrec2} >

      COMP
         <
          C1: true
	      C11: rel1 = predrel 
	      A11: CNVARrec2 := CNVARrec1;
	      C13: rel1 = objrel AND
		   (VERBrec1.Caseassigner = true) AND
		   (CLAUSErec1.voice <> Passive)
	      A13: CNVARrec2 := CNVARrec1;
          A1: CNVARrec2.cases := [Accusative];
         >
      DECOMP
         <
          C1: true
              C11: rel1 = predrel 
              A11: CNVARrec1 := CNVARrec2;
	      C13: rel1 = objrel AND
		   (VERBrec1.Caseassigner = true) AND
		   (CLAUSErec1.voice <> Passive)  
	      A13: CNVARrec1 := CNVARrec2;
          A1: CNVARrec1.cases := [Nominative]
         >


   SUBRULE
      <T1:NP{NPrec1}[mu7] >
      <T2:NP{NPrec2}[mu8 ] >


      COMP
         <
          C1: true
	      C11: rel1 = predrel 
	      A11: @
	      C13: rel1 = objrel  AND
		   (VERBrec1.Caseassigner = true) AND
		   (CLAUSErec1.voice <> Passive)
	      A13: @
          A1: QUOTE_assigncase(NPrec1, mu7, NPrec2, mu8, [Accusative]);
         >

      DECOMP
         <
          C1: true
              C11: rel1 = predrel 
              A11: @
	      C13: rel1 = objrel AND
		   (VERBrec1.Caseassigner = true) AND
		   (CLAUSErec1.voice <> Passive)  
	      A13: @
          A1: QUOTE_assigncase ( NPrec2, mu8, NPrec1, mu7, [Nominative]);
         >
>

