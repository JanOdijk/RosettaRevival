% TRANSFORMATION TOblControl4a
                           (* OPENNPPROP in prepobjrel, object antecedent *)
< m1:CLAUSE{CLAUSErec1}      
           [ mu1,
             predrel/VERBP{VERBPrec1}
                    [ mu2,
                      objrel/T1,
                      mu3,
                      prepobjrel/PREPP{PREPPrec1}
                                [ mu4, 
                                  objrel/OPENNPPROP{OPENNPPROPrec1}
                                        [ subjrel/T2,
                                          I1:rel1/NP{NPrec1}[mu5] 
                                        ],
                                  mu6
                                ],
                      mu7
                    ],
             mu8 
           ]
>

< m :CLAUSE{CLAUSErec2}      
           [ mu1,
             predrel/VERBP{VERBPrec1}
                    [ mu2,
                      objrel/T1,
                      mu3,
                      prepobjrel/PREPP{PREPPrec1}
                                [ mu4, 
                                  objrel/NP{NPrec1}[mu5],
                                  mu6
                                ],
                      mu7
                    ],
             mu8 
           ]
>

   MATCHCONDITIONS
      <
       I1: rel1 IN [predrel, idrel]
      >

<
   SUBRULE (* 1 *)
      <
       T1:NPVAR{NPVARrec1}
       T2:NPVAR{NPVARrec2}
      >
      <T1:NPVAR{NPVARrec1} >

      COMP
         <
          C1: CLAUSErec1.synvpefs * AUX_openxppvps <> [] AND
              (NPVARrec1 = NPVARrec2 EXCEPT FOR [cases] ) AND
              (NPVARrec2.cases = [Nominative] ) AND 
              ( OPENNPPROPrec1 =
		   { 
		    superdeixis    : omegadeixis
		    aktionsarts    : [stative] 
		    PROsubject     : true
		   }
              )
              C11: (rel1 = predrel) AND
                   (OPENNPPROPrec1 =
                      {
                       thetanp        : NPrec1.thetanp
		       nounpatternefs : NPrec1.nounpatternefs
                      }
                   )
              A11: @
              C12: (rel1 = idrel) AND
                   (OPENNPPROPrec1 =
                      {
                       thetanp        : Omegathetanp
		       nounpatternefs : []
                      }
                   )
              A12: @
          A1: CLAUSErec2 := CLAUSErec1;
         >
      DECOMP
         <
          C1: CLAUSErec2.synvpefs * AUX_openxppvps <> []
              C11: true
              A11: OPENNPPROPrec1 :=
                      {
                       thetanp        : NPrec1.thetanp
		       nounpatternefs : NPrec1.nounpatternefs
                      };
                   rel1 := predrel                  
              C12: true
              A12: OPENNPPROPrec1 :=
                      {
                       thetanp        : Omegathetanp
		       nounpatternefs : []
                      };
                    rel1 := idrel              
          A1: CLAUSErec1 := CLAUSErec2;
              CLAUSErec1.synvpefs := CLAUSErec2.synvpefs * AUX_openxppvps;
              NPVARrec2 := NPVARrec1;
              NPVARrec2.cases := [Nominative]; 
               OPENNPPROPrec1 :=
		   { 
		    PROsubject     : true
                    aktionsarts    : [stative]
		   }
                 (* superdeixis retains default value *)
         >

   SUBRULE (* 2 *)
      <
       T1:CNVAR{CNVARrec1}
       T2:CNVAR{CNVARrec2}
      >
      <T1:CNVAR{CNVARrec1} >

      COMP
         <
          C1: CLAUSErec1.synvpefs * AUX_openxppvps <> [] AND
              (CNVARrec1 = CNVARrec2 EXCEPT FOR [cases] ) AND
              (CNVARrec2.cases = [Nominative] ) AND 
              ( OPENNPPROPrec1 =
		   { 
		    superdeixis    : omegadeixis
		    aktionsarts    : [stative] 
		    PROsubject     : true
		   }
              )
              C11: (rel1 = predrel) AND
                   (OPENNPPROPrec1 =
                      {
                       thetanp        : NPrec1.thetanp
		       nounpatternefs : NPrec1.nounpatternefs
                      }
                   )
              A11: @
              C12: (rel1 = idrel) AND
                   (OPENNPPROPrec1 =
                      {
                       thetanp        : Omegathetanp
		       nounpatternefs : []
                      }
                   )
              A12: @
          A1: CLAUSErec2 := CLAUSErec1;
         >
      DECOMP
         <
          C1: CLAUSErec2.synvpefs * AUX_openxppvps <> []
              C11: true
              A11: OPENNPPROPrec1 :=
                      {
                       thetanp        : NPrec1.thetanp
		       nounpatternefs : NPrec1.nounpatternefs
                      };
                   rel1 := predrel                  
              C12: true
