
rm -f $RBS_MAKECOMP/target/$RBS_MAKEFILE.rbs
#
if test -z "$1"
then echo "pf  @ " >> RBS_logicals
else echo "pf  $1" >> RBS_logicals
fi
#
if test -z "$2" 
then echo "dbg  @ " >> RBS_logicals
else if test "$2" = "debug"
     then echo "dbg  dbg_" >> RBS_logicals
     else echo "dbg  @ " >> RBS_logicals
     fi
fi
#
$RBS_DIR/genmake RBS_logicals $RBS_MAKECOMP $RBS_MAKEFILE > $RBS_MAKECOMP/target/$RBS_MAKEFILE.rbs
