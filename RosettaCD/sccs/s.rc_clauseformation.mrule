< m1:I1::VERBPPROP{VERBPPROPrec1}
                  [ mu1,
                    predrel/VERBP{VERBPrec1}[mu2],
                    mu10
                  ]
>

< m :I2::CLAUSE{CLAUSErec1}
               [ mu1, 
                 predrel/VERBP{VERBPrec1}
                        [ mu2,
                          auxrel/VERB{VERBrec1}
			      [ head/SUBVERB{SUBVERBrec1}
				    [ head/BVERB(AUX_wordenbverbkey){BVERBrec1}
				    ]
			      ]
                        ],
                 mu10
               ]
>

   MATCHCONDITIONS
      <
       I1: VERBPPROPrec1.voice = passive
       I2: CLAUSErec1.voice = passive
      >
      COMP
         <
          C1: true
          A1: CLAUSErec1 := COPYT_verbpproptoclause(VERBPPROPrec1);
               (* all other attributes retain the default values *)
              SUBVERBrec1 := COPYT_bverbtosubverb(BVERBrec1);
              VERBrec1 := COPYT_subverbtoverb(SUBVERBrec1)
                (* other attributes retain their default values *)
         >
      DECOMP
         <
          C1: ( CLAUSErec1 =
	        { 
		 modus       : omegamodus
		 aspect      : omegaAspect  
		 deixis      : omegadeixis 
		 retro       : false       
		 finiteness  : omegafin
		 infsort     : omegainf        
	        }
              )
              (* the default values for these attributes *) AND
              ( VERBrec1 = COPYT_subverbtoverb(SUBVERBrec1) ) AND
              ( SUBVERBrec1 = COPYT_bverbtosubverb(BVERBrec1) ) AND
              ( VERBrec1 = 
		  { 
		   number       : omeganumber   
		   persons      : []   
		   eORenForm    : NoForm   
		   modus        : omegamodus
		   tense        : omegatense  
		   status       : bareV
		  }
               )
             (* the default values for these attributes *)
          A1: VERBPPROPrec1 := COPYT_clausetoverbpprop(CLAUSErec1)
         >

&

