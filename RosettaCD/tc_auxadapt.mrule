\begin{verbatim}
%
% TRANSFORMATION THAtoA1
                                  (*  [proberen te werken] heeft -->
                                    [heeft [proberen te werken]] *)
< m1: CLAUSE{CLAUSErec1}
            [ mu1,
              predrel/VERBP{VERBPrec1}
                     [ mu2,
                       I3:rel1/I1::VERB{VERBrec1}[mu3],
                       auxrel/I2::VERB{VERBrec2}[mu4],
                       mu5 
                     ],
              mu6 
            ]
>

< m : CLAUSE{CLAUSErec1}
            [ mu1,
              predrel/VERBP{VERBPrec1}
                     [ mu2,
                       I3:rel1/VERB{VERBrec3}
                           [ auxrel/I2::VERB{VERBrec2}[mu4],
                             head/I1::VERB{VERBrec1}[mu3]
                           ],
                       mu5 
                     ],
              mu6 
            ]
>

   MATCHCONDITIONS
      <
       I1: (VERBrec1.status IN [VV, teVV]) AND
           (VERBrec1.modus = infinitive)
       I2: VERBrec2.subc IN [ hebbenaux, zijnaux ] AND
           VERBrec2.status IN [teV, bareV]
       I3: rel1 IN [head, auxrel]
      >

      COMP
         <
          C1: true
              C11: VERBrec2.status = bareV
              A11: VERBrec3.status := AuxV
              C12: VERBrec2.status = teV
              A12: VERBrec3.status := teAuxV
          A1: VERBrec3 :=
		   { 
		   conjclasses:      VERBrec2.conjclasses
		   particle:         VERBrec1.particle   
		   number:           VERBrec2.number
		   persons:          VERBrec2.persons   
		   eORenForm:        VERBrec2.eORenForm

		   modus:            VERBrec2.modus
		   tense:            VERBrec2.tense

		   possvoices:       VERBrec1.possvoices
		   reflexivity:      VERBrec1.reflexivity
		   synvps:           VERBrec1.synvps
		   thetavp:          VERBrec1.thetavp
		   CaseAssigner:     VERBrec1.CaseAssigner
		   subc:             VERBrec1.subc  

		   perfauxs:         VERBrec2.perfauxs

		   prepkey1:         VERBrec1.prepkey1
		   prepkey2:         VERBrec1.prepkey2 

		   controller:       VERBrec1.controller
		   verbraiser:       VERBrec1.verbraiser
		   IPP:              VERBrec1.IPP
                   classes:          VERBrec1.classes
                  };

         >
      DECOMP
         <
          C1: VERBrec3 =
		   { 
		   conjclasses:      VERBrec2.conjclasses
		   particle:         VERBrec1.particle   
		   number:           VERBrec2.number
		   persons:          VERBrec2.persons   
		   eORenForm:        VERBrec2.eORenForm

		   modus:            VERBrec2.modus
		   tense:            VERBrec2.tense

		   possvoices:       VERBrec1.possvoices
		   reflexivity:      VERBrec1.reflexivity
		   synvps:           VERBrec1.synvps
		   thetavp:          VERBrec1.thetavp
		   CaseAssigner:     VERBrec1.CaseAssigner
		   subc:             VERBrec1.subc  

		   perfauxs:         VERBrec2.perfauxs

		   prepkey1:         VERBrec1.prepkey1
		   prepkey2:         VERBrec1.prepkey2 

		   controller:       VERBrec1.controller
		   verbraiser:       VERBrec1.verbraiser
		   IPP:              VERBrec1.IPP
                   classes:          VERBrec1.classes

		   } 
              C11: VERBrec2.status = bareV AND
                   VERBrec3.status = AuxV
              A11: @
              C12: VERBrec2.status = teV AND
                   VERBrec3.status = teAuxV
              A12: @
          A1: @
         >

&

\end{verbatim}

