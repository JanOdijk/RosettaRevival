              NPrec1.number = singular AND
              NPrec1.definite = indef AND
              CNrec1 = CNrecdef EXCEPT FOR [numbers, posscomas] AND
              singular IN CNrec1.numbers  AND
              mass IN CNrec1.posscomas
          A1: @

         >
      DECOMP
         <
          C1: mass IN NPrec1.actcomas AND
              NPrec1.NPhead = enokNP AND
              NPrec1.number = singular AND
              NPrec1.definite = indef
          A1: CNrec1 := {
                         numbers : [singular]
                         posscomas : [mass]
                        }
         >

&

