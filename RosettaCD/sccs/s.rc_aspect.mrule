% RULE Raspectimperfective
< m1:CLAUSE{CLAUSErec1}
           [I1: mu1,
            predrel/VERBP{VERBPrec1}
                   [mu2,
                    I2:rel1/VERB{VERBrec1}[head/T1]
                   ],
            mu6
           ] 
>

< m :CLAUSE{CLAUSErec2}
           [I1: mu1,
            predrel/VERBP{VERBPrec1}
                   [mu2,
                    I2:rel1/VERB {VERBrec1}[head/T1]
                   ],
            mu6
           ]
>
   MATCHCONDITIONS
      <
       I1: QUOTE_tempdurnotfound(mu1) AND
           QUOTE_tempfreqnotfound(mu1)
       I2: VERBRec1.subc <> latenverb

      >

<
   SUBRULE (* 1 *)
      <T1: SUBVERB{SUBVERBrec1}[I1: head/BVERB(KEY1){BVERBrec1}] >
      <T1: SUBVERB{SUBVERBrec1}[I1: head/BVERB(KEY1){BVERBrec1}] >
   
MATCHCONDITIONS
      <
        I1: KEY1 <> KEY_zullenBVERBkey 
      >

      COMP
         <
          C1: CLAUSErec1.aspect = omegaaspect 
          A1: CLAUSErec2 := CLAUSErec1;
              CLAUSErec2.aspect := imperfective;
         >

      DECOMP
         <
          C1: CLAUSErec2.aspect = imperfective
          A1: CLAUSErec1:= CLAUSErec2;
              CLAUSErec1.aspect := omegaaspect;
         >

   SUBRULE (* 2 *)
      <T1: SUBVERB{SUBVERBrec1}
                  [sig1, head/SUBVERB{SUBVERBrec2}
                             [I2: head/BVERB(KEY1){BVERBrec1}]
                  ]
      >
      <T1: SUBVERB{SUBVERBrec1}
                  [sig1, head/SUBVERB{SUBVERBrec2}
                             [I2: head/BVERB(KEY1){BVERBrec1}] 
                  ]
      >

MATCHCONDITIONS
      <
        I2: KEY1 <> KEY_zullenBVERBkey 
      >
      COMP
         <
          C1: CLAUSErec1.aspect = omegaaspect 
          A1: CLAUSErec2 := CLAUSErec1;
              CLAUSErec2.aspect := imperfective;
         >

      DECOMP
         <
          C1: CLAUSErec2.aspect = imperfective
          A1: CLAUSErec1:= CLAUSErec2;
              CLAUSErec1.aspect := omegaaspect;
         >
>
&

\end{verbatim}

