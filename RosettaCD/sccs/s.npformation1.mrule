      >
<

   SUBRULE  (* 1: enokNP: kan zonder er *)
      
      <mu1: [I1:mu4] >
      <mu1: [I1:mu4] >

   MATCHCONDITIONS
      <
      I1: mu4 <> [] 
      >

      COMP
         <
          C1: (DETPrec1.posscomas * CNrec1.posscomas <> []) AND
              (DETPrec1.possnumbers * CNrec1.numbers <> []) AND
              (DETPrec1.superdeixis = CNrec1.superdeixis) AND
              ((mass IN CNrec1.posscomas) 
                                   -> (DETPrec1.definite = def))
              (* to exclude a mass analysis for: (indef) veel mooie  *)
              C11: plural  IN CNrec1.numbers   (* twee gele *)
              A11: NPrec1.number := plural 
              C12: singular  IN CNrec1.numbers (* een gele, die mooie *) 
              A12: NPrec1.number := singular        
          A1: 
              NPrec1 := { posspred          : DETPrec1.posspred
                          definite          : DETPrec1.definite
                          possnietnp        : DETPrec1.possnietnp
                          mood              : DETPrec1.mood

