   SUBRULE (* objrel *)
      <mu1 :[ mu9, predrel/VERBP{VERBPrec1}
                     [I6:mu6, I8:objrel/T1, I7:mu7 ], mu12 ]>
      <mu11:[ I8:shiftrel/T1, mu9, 
               predrel/VERBP{VERBPrec1}[ I6:mu6, I7:mu7], mu12 ]>

   MATCHCONDITIONS
      <
       I6: ALL(mu6, I61:rel6/..) AND NOT EXIST(mu6, [objrel/..])
           I61: QUOTE_VPrelprec(rel6, objrel)
       I7: ALL(mu7, I71:rel7/..) AND NOT EXIST(mu7, [objrel/..])
           I71: QUOTE_VPrelprec(objrel, rel7)
       I8: (T1.CAT IN [NPVAR] WITH T1.REC.mood 
                      IN [wh, topicxpmood, relativexpmood])              OR
           (T1.CAT IN [CNVAR] )                                          OR
           (T1.CAT IN [NP] WITH
            ((T1.REC.mood = wh)   AND
             (QUOTE_VARinNP(T1)
             )
            )
           )
      >

      COMP
         <
          C1: (
               T1.CAT IN [NP, NPVAR, CNVAR] WITH (
                T1.REC.cases * [Accusative,Nominative] <> [] AND
                T1.REC.cases * [Rcase] = []  )
              )                                           AND
              CLAUSErec1.synvpefs * AUX_objvps <> []
          A1: CLAUSErec2 := CLAUSErec1;
         >

      DECOMP
         <
          C1: (
               T1.CAT IN [NP, NPVAR, CNVAR] WITH (
                T1.REC.cases * [Accusative, Nominative] <> [] AND
                T1.REC.cases <> [Nominative]   (*to speed up*)  AND 
                T1.REC.cases * [Rcase] = []  ) 
              )                                        AND
              CLAUSErec2.synvpefs * AUX_objvps <> []
          A1: CLAUSErec1 := CLAUSErec2;
              CLAUSErec1.synvpefs :=  CLAUSErec2.synvpefs * AUX_objvps ;
         >

   SUBRULE (* indobjrel *)
      <mu1 :[ mu9, predrel/VERBP{VERBPrec1}
                     [I6:mu6, I8:indobjrel/T1, I7:mu7 ], mu12 ]>
      <mu11:[ I8:shiftrel/T1, mu9, 
               predrel/VERBP{VERBPrec1}[ I6:mu6, I7:mu7], mu12 ]>

   MATCHCONDITIONS
      <
       I6: ALL(mu6, I61:rel6/..) AND NOT EXIST(mu6, [I62:rel2/..])
           I61: QUOTE_VPrelprec(rel6, indobjrel)
       I7: ALL(mu7, I71:rel7/..) AND NOT EXIST(mu7, [I62:rel2/..])
           I71: QUOTE_VPrelprec(indobjrel, rel7)
       I8: (T1.CAT IN [NPVAR] WITH T1.REC.mood 
                 IN [wh, topicxpmood, relativexpmood])                  OR
           (T1.CAT IN [CNVAR])                                          OR
           (T1.CAT IN [NP] WITH
            ((T1.REC.mood = wh)   AND
             (QUOTE_VARinNP(T1)
             )
            )
           )
       I62: rel2 IN [aanobjrel, voorobjrel, indobjrel]
      >

      COMP
         <
          C1: (T1.CAT IN [NP, NPVAR, CNVAR] WITH (
                T1.REC.cases * [Dative] <> [] AND
                T1.REC.cases * [Rcase] = []  )
              )                                                AND
              ((CLAUSErec1.synvpefs * AUX_indobjvps <> []) OR
               (CLAUSErec1.adjuncts * [benfactNP] <> [])
              )
          A1: CLAUSErec2 := CLAUSErec1;
         >

      DECOMP
         <
          C1: (T1.CAT IN [NP, NPVAR, CNVAR] WITH (
                T1.REC.cases * [Dative] <> [] AND
                T1.REC.cases * [Rcase] = []  )
              )                                       
              C11: CLAUSErec2.synvpefs * AUX_indobjvps <> []
              A11: CLAUSErec1 := CLAUSErec2;
                   CLAUSErec1.synvpefs :=  CLAUSErec2.synvpefs * AUX_indobjvps ;
              C12: CLAUSErec2.adjuncts * [benfactNP] <> []
              A12: CLAUSErec1 := CLAUSErec2;
         A1: @
         >


   SUBRULE (* aanobjrel *)
      <mu1 :[ mu9, predrel/VERBP{VERBPrec1}
                     [I6:mu6, I8:aanobjrel/VARPREPP{VARPREPPrec1}[mu5], I7:mu7 ], mu12 ]>
      <mu11:[ I8:shiftrel/VARPREPP{VARPREPPrec1}[mu5], mu9, 
               predrel/VERBP{VERBPrec1}[ I6:mu6, I7:mu7], mu12 ]>

   MATCHCONDITIONS
      <
       I6: ALL(mu6, I61:rel6/..) AND NOT EXIST(mu6, [I62:rel2/..])
           I61: QUOTE_VPrelprec(rel6, aanobjrel)
       I7: ALL(mu7, I71:rel7/..) AND NOT EXIST(mu7, [I62:rel2/..])
           I71: QUOTE_VPrelprec(aanobjrel, rel7)
       I8: (VARPREPPrec1.mood = wh 
            OR EXIST(mu5,[I81:../T1]) OR
            EXIST(mu5, [errel/RADVP{}[I81:../T1]])
