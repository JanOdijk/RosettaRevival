         [ argrel/NP{NPrec5}[mu5],
           conjrel/COORD{COORDrec1},
           argrel/NP{NPrec6}[mu6] 
         ]
>

      COMP
         <
          C1: npc IN COORDrec1.patterns                      AND
              simple IN COORDrec1.kinds                      AND
              NPrec1.superdeixis = NPrec2.superdeixis        AND
              NPrec1.coord = nocoord                         AND
              NPrec2.coord = nocoord                         AND
              NPrec1.cases = [nominative]                    AND
              NPrec2.cases = [nominative]                    AND
              NPrec1.generic=NPrec2.generic
          A1: QUOTE_AssignCase(NPrec1,mu1,NPrec5,mu5,NPrec1.cases);
              QUOTE_AssignCase(NPrec2,mu2,NPrec5,mu6,NPrec2.cases);
              NPrec5.superdeixis := omegadeixis; 
              NPrec6.superdeixis := omegadeixis; 
              NPrec3.superdeixis := NPrec1.superdeixis;
              NPrec5.generic := omegageneric;
              NPrec6.generic := omegageneric;
              NPrec3.generic := NPrec1.generic;
              QUOTE_CompNPattributes(COORDrec1, NPrec1,NPrec2,NPrec3);
              NPrec3.cases := [nominative];
              NPrec3.coord := simplecoord;
         >
      DECOMP
         <
          C1: npc IN COORDrec1.patterns       AND
              simple IN COORDrec1.kinds       AND
              NPrec5.superdeixis = omegadeixis               AND
              NPrec6.superdeixis = omegadeixis               AND
              NPrec5.generic = omegageneric                  AND
              NPrec6.generic = omegageneric                  AND
              NPrec5.coord = nocoord                         AND
              NPrec6.coord = nocoord                         AND
              nominative IN NPrec5.cases                     AND
              nominative IN NPrec6.cases                     AND
              NPrec3.cases = [nominative]                    AND
              NPrec3.coord = simplecoord                     AND
