% RULE RNPformation13
< m1: I1::DEMPRO{DEMPROrec1}
>

< m : NP{NPrec1}[head/I1::DEMPRO{DEMPROrec1}
 
                ]
>
   MATCHCONDITIONS
      <I1: DEMPROrec1.key = KEY_ditDEMPROkey OR 
                      DEMPROrec1.key = KEY_datDEMPROkey
      >


      COMP
         <
          C1: true 
             C11: DEMPROrec1.key = KEY_datDEMPROkey
             A11: (* NPrec1 := COPYT_DEMPROtoNP(DEMPROrec1); *)
                   NPrec1.NPhead    := datNP;
                   NPrec1.human := Nohuman;
                   NPrec1.animate := Noanimate;
             C12: DEMPROrec1.key = KEY_ditDEMPROkey
             A12: (* NPrec1 := COPYT_DEMPROtoNP(DEMPROrec1); *)
                    NPrec1.NPhead    := ditNP;
                   NPrec1.human := Nohuman;
                   NPrec1.animate := Noanimate;
          A1: 
               NPrec1.number := DEMPROrec1.number;
               NPrec1.sexes := DEMPROrec1.sexes;

              NPrec1.definite  := def;
              NPrec1.genders   := [omegagender];
              NPrec1.actcomas  := [];
              NPrec1.posspred  := false;
              NPrec1.possRform := true;
              NPrec1.generic := nogeneric;
              NPrec1.superdeixis := omegadeixis;
              NPrec1.specq      := yesspec;

         >

      DECOMP
         <
          C1: (* (NPrec1 = COPYT_DEMPROtoNP(DEMPROrec1); *)
               NPrec1.number = DEMPROrec1.number AND
               NPrec1.sexes = DEMPROrec1.sexes AND

              (NPrec1.definite  = def) AND
              (NPrec1.genders   = [omegagender]) AND
              (NPrec1.actcomas  = []) AND
              (NPrec1.posspred  = false) AND
              (NPrec1.possRform = true) AND 
              (NPrec1.generic = nogeneric) AND 
              (NPrec1.superdeixis = omegadeixis) AND
              (NPrec1.class = omegaTimeAdvClass) AND
              (NPrec1.deixis = omegadeixis ) AND
              (NPrec1.aspect = omegaAspect) AND
               (NPrec1.retro = false) AND
              NPrec1.specq      = yesspec  
(* adapted by JO  7-NOV-1988 10:13:36 *)
            C11: NPrec1.NPhead = datNP                  AND 
                 DEMPROrec1.key = KEY_datDEMPROkey      AND
                 NPrec1.human = nohuman                 AND
                 NPrec1.animate  = noanimate
            A11: @
            C12: NPrec1.NPhead = ditNP                  AND
                 DEMPROrec1.key = KEY_ditDEMPROkey      AND
                 NPrec1.human = nohuman                 AND
                 NPrec1.animate  = noanimate
            A12: @

          A1:   @
         >
&


\end{verbatim}
