% FILTER Fdeixisretroadaptation1
< m: SENTENCE{SENTENCErec1}
            [I2:mu1,
             predrel/VERBP{VERBPrec1}
                    [mu2],
             mu3
            ] 
>

   MATCHCONDITIONS
      <
       m: (SENTENCErec1.adverbial = true ->
           (SENTENCErec1.temporal = false OR SENTENCErec1.conjretro = true)
          )  AND
          (SENTENCErec1.senttype = subordinateclause ->
           (SENTENCErec1.finiteness = infinite OR
            SENTENCErec1.mood = relative OR
            (NOT QUOTE_temprefnotfound(mu1)) OR
            SENTENCErec1.conjretro = true
           )
          )
       I2: QUOTE_tempretronotfound(mu1)
      >
<
   SUBRULE
      <mu2: [I1: mu5]>

   MATCHCONDITIONS
      <
       I1:(EXIST (mu5, [auxrel/I11:: VERB{VERBrec1}[mu4]]))
       I11: (VERBrec1.status IN [bareV, TeV]) AND
            (VERBrec1.subc IN [hebbenaux, zijnaux]) AND
            (VERBrec1.tense IN [omegatense, presenttense, pasttense])
      >

         <
          C1: ((SENTENCErec1.aspect = perfective) AND
               (SENTENCErec1.retro = false)
              )
              C11: (SENTENCErec1.deixis = pastdeixis) AND
                   (SENTENCErec1.superdeixis = omegadeixis) AND
                   (SENTENCErec1.finiteness = finite) AND
                   (VERBrec1.tense = presenttense)
              A11: @

              C12: (SENTENCErec1.superdeixis = pastdeixis) AND
                   (SENTENCErec1.deixis = omegadeixis) AND
                   (SENTENCErec1.finiteness = finite) AND
                   (VERBrec1.tense = pasttense)
              A12: @

              C13: (SENTENCErec1.superdeixis = omegadeixis) AND
                   (SENTENCErec1.deixis = pastdeixis) AND
                   (SENTENCErec1.finiteness = infinite) AND
                   (VERBrec1.tense = omegatense)
              A13: @

              C14: (SENTENCErec1.superdeixis = pastdeixis) AND
                   (SENTENCErec1.deixis = omegadeixis) AND
                   (SENTENCErec1.finiteness = infinite) AND
                   (VERBrec1.tense = omegatense)
              A14: @

          A1: @
         >

   SUBRULE 
      <mu2: [I1: mu7]>

   MATCHCONDITIONS
      <
       I1: (EXIST (mu7, [I12: rel1/VERB{VERBrec2}[I13: mu5]]))
       I12: (rel1 IN [head, auxrel, progauxrel]) AND 
            (VERBrec2.status IN [AuxV, teAuxV]) AND
            (VERBrec2.tense IN [omegatense, presenttense, pasttense])
       I13: (EXIST (mu5, [auxrel/I14::VERB{VERBrec3}[mu6]]))
       I14: (VERBrec3.subc IN [hebbenaux, zijnaux])

      >

         <
          C1: ((SENTENCErec1.aspect = perfective) AND
               (SENTENCErec1.retro = false)
              )
              C11: (SENTENCErec1.deixis = pastdeixis) AND
                   (SENTENCErec1.superdeixis = omegadeixis) AND
                   (SENTENCErec1.finiteness = finite) AND
                   (VERBrec2.tense = presenttense)
              A11: @

              C12: (SENTENCErec1.superdeixis = pastdeixis) AND
                   (SENTENCErec1.deixis = omegadeixis) AND
                   (SENTENCErec1.finiteness = finite) AND
                   (VERBrec2.tense = pasttense)
              A12: @

              C13: (SENTENCErec1.superdeixis = omegadeixis) AND
                   (SENTENCErec1.deixis = pastdeixis) AND
                   (SENTENCErec1.finiteness = infinite) AND
                   (VERBrec2.tense = omegatense)
              A13: @

              C14: (SENTENCErec1.superdeixis = pastdeixis) AND
                   (SENTENCErec1.deixis = omegadeixis) AND
                   (SENTENCErec1.finiteness = infinite) AND
                   (VERBrec2.tense = omegatense)
              A14: @

          A1: @
         >
   SUBRULE
      <mu2: [I1: mu8]>

   MATCHCONDITIONS
      <
       I1:(NOT EXIST (mu8, [auxrel/I11:: VERB{VERBrec6}[mu9]])) AND
          (EXIST (mu8, [head/I12:: VERB{VERBrec7}[mu10]]))
       I11: (VERBrec6.status IN [ teV, auxV, teAuxV]) OR
            (VERBrec6.subc IN [hebbenaux, zijnaux,wordenaux]) OR
            (VERBrec6.tense IN [ presenttense, pasttense])
       I12: VERBrec7.modus = pastpart
      >

      COMP
         <
          C1: ((SENTENCErec1.aspect = perfective) AND
               (SENTENCErec1.retro = false)
              )
              C11: (SENTENCErec1.deixis = pastdeixis) AND
                   (SENTENCErec1.superdeixis = omegadeixis) AND
                   (SENTENCErec1.finiteness = infinite) 
              A11: @
              C12: (SENTENCErec1.superdeixis = pastdeixis) AND
                   (SENTENCErec1.deixis = omegadeixis) AND
                   (SENTENCErec1.finiteness = infinite) 
              A12: @

          A1: @
         >
>
&
