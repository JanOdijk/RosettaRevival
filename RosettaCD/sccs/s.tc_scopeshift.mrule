\begin{verbatim}
%
% RULE Radjunctvar1
< m1:VERBPPROP{VERBPPROPrec1}
       [ mu1,
         predrel/VERBP{VERBPrec1}
           [ I1:mu2,
             head/VERB{VERBrec1}[mu3],
             mu4 
           ],
         mu5
       ] 
m2: T1
>

< m :VERBPPROP{VERBPPROPrec2}
       [ mu1,
         predrel/VERBP{VERBPrec1}
           [ I1:mu2,
             rel1/T1,
             head/VERB{VERBrec1}[mu3],
             mu4 
           ],
         mu5
       ] 

>

   MATCHCONDITIONS
      <
       I1: NOT EXIST(mu2,[I11:rel2/..])
           I11: rel2 IN [complrel, locargrel, dirargrel]
       I2: T1.CAT IN [ADJPPROPVAR, NPPROPVAR, PREPPPROPVAR, ADVPPROPVAR]
             WITH T1.REC.PROsubject =true  
      >

   PARAMETERS
      <
       adjunctpar:adjuncttype
      >

      COMP
         <
          C1: true
              C11: resAP IN VERBrec1.adjuncts               AND
                   [resAP] * VERBPPROPrec1.adjuncts = []    AND
                   T1.CAT = ADJPPROPVAR                     AND
                   adjunctpar = resAP
              A11: VERBPPROPrec2 := VERBPPROPrec1;
                   VERBPPROPrec2.adjuncts := VERBPPROPrec1.adjuncts + [resAP];
                   rel1 := complrel;
              C12: resNP IN VERBrec1.adjuncts               AND
                   [resNP] * VERBPPROPrec1.adjuncts = []    AND
                   T1.CAT = NPPROPVAR                       AND
                   adjunctpar = resNP      
              A12: VERBPPROPrec2 := VERBPPROPrec1;
                   VERBPPROPrec2.adjuncts := VERBPPROPrec1.adjuncts + [resNP];
                   rel1 := complrel;
              C13: resPP IN VERBrec1.adjuncts               AND
                   [resPP] * VERBPPROPrec1.adjuncts = []    AND
                   (T1.CAT IN [PREPPPROPVAR] 
                       WITH other IN T1.REC.actsubcefs)     AND
                   adjunctpar = resPP      
              A13: VERBPPROPrec2 := VERBPPROPrec1;
                   VERBPPROPrec2.adjuncts := VERBPPROPrec1.adjuncts + [resPP];
                   rel1 := complrel;
              C14: diradjunct IN VERBrec1.adjuncts               AND
                   [diradjunct] * VERBPPROPrec1.adjuncts = []    AND
                   ((T1.CAT IN [PREPPPROPVAR] 
                       WITH dir IN T1.REC.actsubcefs) OR
                    (T1.CAT IN [ADVPPROPVAR]
                       WITH diradv IN T1.REC.actsubcefs)
                   )                                      AND
                   adjunctpar = diradjunct
              A14: VERBPPROPrec2 := VERBPPROPrec1;
                   VERBPPROPrec2.adjuncts := 
                         VERBPPROPrec1.adjuncts + [diradjunct];
                   rel1 := dirargrel;
              C15: locadjunct IN VERBrec1.adjuncts               AND
                   [locadjunct] * VERBPPROPrec1.adjuncts = []    AND
                   ((T1.CAT IN [PREPPPROPVAR] 
                       WITH loc IN T1.REC.actsubcefs) OR
                    (T1.CAT IN [ADVPPROPVAR]
                       WITH locadv IN T1.REC.actsubcefs)
                   )                                      AND
                   adjunctpar = locadjunct
              A15: VERBPPROPrec2 := VERBPPROPrec1;
                   VERBPPROPrec2.adjuncts := 
                       VERBPPROPrec1.adjuncts + [locadjunct];
                   rel1 := locargrel;
          A1: @
         >
      DECOMP
         <
          C1: true
              C11: resAP IN VERBrec1.adjuncts      AND
                   T1.CAT = ADJPPROPVAR            AND
                   rel1 = complrel                  AND 
                   resAP IN VERBPPROPrec2.adjuncts
              A11: adjunctpar := resAP;
                   VERBPPROPrec1 := VERBPPROPrec2;
                   VERBPPROPrec1.adjuncts := VERBPPROPrec2.adjuncts - [resAP];
              C12: resNP IN VERBrec1.adjuncts      AND
                   T1.CAT = NPPROPVAR              AND
                   rel1 = complrel                  AND 
                   resNP IN VERBPPROPrec2.adjuncts
              A12: adjunctpar := resNP;
                   VERBPPROPrec1 := VERBPPROPrec2;
                   VERBPPROPrec1.adjuncts := VERBPPROPrec2.adjuncts - [resNP];
              C13: resPP IN VERBrec1.adjuncts      AND
                   (T1.CAT IN [PREPPPROPVAR] 
                       WITH other IN T1.REC.actsubcefs)     AND
                   rel1 = complrel                  AND 
                   resPP IN VERBPPROPrec2.adjuncts
              A13: adjunctpar := resPP;      
                   VERBPPROPrec1 := VERBPPROPrec2;
                   VERBPPROPrec1.adjuncts := VERBPPROPrec2.adjuncts - [resPP];
              C14: diradjunct IN VERBrec1.adjuncts      AND
                   ((T1.CAT IN [PREPPPROPVAR] 
                       WITH dir IN T1.REC.actsubcefs) OR
                    (T1.CAT IN [ADVPPROPVAR]
                       WITH diradv IN T1.REC.actsubcefs)
                   )                                      AND
                   rel1 = dirargrel                       AND 
                   diradjunct IN VERBPPROPrec2.adjuncts
              A14: adjunctpar := diradjunct;      
                   VERBPPROPrec1 := VERBPPROPrec2;
                   VERBPPROPrec1.adjuncts := 
                         VERBPPROPrec2.adjuncts - [diradjunct];
              C15: locadjunct IN VERBrec1.adjuncts        AND
                   ((T1.CAT IN [PREPPPROPVAR] 
                       WITH loc IN T1.REC.actsubcefs) OR
                    (T1.CAT IN [ADVPPROPVAR]
                       WITH locadv IN T1.REC.actsubcefs)
                   )                                      AND
                   rel1 = locargrel                       AND 
                   locadjunct IN VERBPPROPrec2.adjuncts
              A15: adjunctpar := locadjunct;      
                   VERBPPROPrec1 := VERBPPROPrec2;
                   VERBPPROPrec1.adjuncts := 
                      VERBPPROPrec2.adjuncts - [locadjunct];
          A1: @
         >


&

\end{verbatim}
