\begin{verbatim}

%
% RULE RADVPamountmod

< m1: ADVP{ADVPrec1}[I3:mu1]
  m2: T1
>

< m : ADVP{ADVPrec1}[I2:rel1/T2,
                 I3:mu1
                ]
>

   MATCHCONDITIONS
      <
       m2: (T1.cat IN [ADVP] WITH T1.REC.actsubcefs * [degreeadv] <> [])
            OR
           ((T1.cat IN [NP] WITH T1.REC.actsubcs * [unitnoun, plurunitnoun] <> [])
                                                   (*, amountnp *) OR
            (T1.cat IN [NP] WITH T1.REC.actcomas * [mass] <> []) 
           ) 
            
       I2: ((T2.cat IN [ADVP] WITH T2.REC.actsubcefs * [degreeadv] <> []) AND
            (rel1 IN [degreemodrel])
           ) OR
           (((T2.cat IN [NP] WITH T2.REC.actsubcs * [unitnoun, plurunitnoun] <> []) 
                                                   (* amountnp *) OR
            (T2.cat IN [NP] WITH T2.REC.actcomas * [mass] <> []) 
           ) AND            
            (rel1 IN [premodrel])
           ) 
        m1: ADVPrec1.temporal = false
        m: ADVPrec1.temporal = false
       I3: NOT EXIST(mu1, [premodrel/..]) AND
           NOT EXIST(mu1, [degreemodrel/..])


      >


< 
   SUBRULE (* 1 *)    (* te zeer , bijna te *)
      <T1:ADVP{ADVPrec3}[mu9] >
      <T2:ADVP{ADVPrec4}[mu9] >

      COMP
         <
          C1: ADVPrec3.superdeixis = ADVPrec1.superdeixis 
          A1: ADVPrec4 := ADVPrec3;
              ADVPrec4.superdeixis := omegadeixis;
              rel1 := degreemodrel;

         >

      DECOMP
         <
          C1: ADVPrec4.superdeixis = omegadeixis
          A1: ADVPrec3 := ADVPrec4;
              ADVPrec3.superdeixis := ADVPrec1.superdeixis;
         >

   SUBRULE (* 2 *) (* drie meter te *)
      <T1:NP{NPrec1}[mu8] >
      <T2:NP{NPrec2}[mu9] >

      COMP
         <
          C1: NPrec1.superdeixis = ADVPrec1.superdeixis AND
              NPrec1.cases = [Nominative]
          A1: QUOTE_assigncase(NPrec1,mu8,NPrec2,mu9,[Nominative]); 
              NPrec2.superdeixis := omegadeixis;
              NPrec2.generic := omegageneric;
              rel1 := premodrel;

         >

      DECOMP
         <
          C1: NPrec2.generic = omegageneric AND
              NPrec2.superdeixis = omegadeixis
          A1: QUOTE_assigncase(NPrec2,mu9,NPrec1,mu8,[Nominative]);
              NPrec1.superdeixis := ADVPrec1.superdeixis;
              NPrec1.generic := nogeneric;
         >

>
&

\end{verbatim}
