% RULE RADVPomcomplmod

< m1: ADVP{ADVPrec1}[head/ADV{ADVrec1}[mu2]]
  m2: I5::SENTENCE{SENTENCErec1}[mu5]
>


< m : ADVP{ADVPrec1}[head/ADV{ADVrec1}[mu2],
                 omcomplrel/I6::SENTENCE{SENTENCErec2}[mu5]
                ]

>


   MATCHCONDITIONS
      <m1: ADVPrec1.thanas = omcompl
       m:  ADVPrec1.thanas = omcompl 
       I5:SENTENCErec1 = 
                          {
                           prosubject: true
                           finiteness: infinite
                           infsort: omteinf
                          } AND
           SENTENCErec1.mood IN [declarative, proobjmod]
       I6: SENTENCErec2 = 
                          {
                           prosubject: true
                           finiteness: infinite
                           infsort   : omteinf
                          }  AND
           SENTENCErec2.mood IN [declarative, proobjmod]
      >

      COMP
         <
          C1: SENTENCErec1.superdeixis = ADVPrec1.superdeixis
          A1: SENTENCErec2 := SENTENCErec1;
              SENTENCErec2.superdeixis := omegadeixis;

         >
      DECOMP
         <
          C1: SENTENCErec2.superdeixis = omegadeixis
          A1: SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.superdeixis := ADVPrec1.superdeixis;
         >


&

