% TRANSFORMATION tcaseassignment1a
< m1:CLAUSE{CLAUSErec1}
           [ mu1,
             predrel/VERBP{VERBPrec1}
                    [ mu2,
                      rel1/T1,
                      mu3,
                      head/VERB{VERBrec1}[mu4],
                      mu5 
                    ],
             mu6 
           ]
>

< m :CLAUSE{CLAUSErec1}
           [ mu1,
             predrel/VERBP{VERBPrec1}
                    [ mu2,
                      I2:rel1/T2,
                      mu3,
                      head/VERB{VERBrec1}[mu4],
                      mu5 
                    ],
             mu6 
           ]
>


   MATCHCONDITIONS
      <
       I2: rel1 IN [objrel] AND
           (T2.CAT IN [NPVAR, CNVAR, NP] WITH Nominative IN T2.REC.cases) AND
           (T2.CAT IN [NPVAR, CNVAR, NP] WITH T2.REC.cases <> [Nominative])
                                             
      >


<
   SUBRULE
      <T1: NPVAR{NPVARrec1} >
      <T2: NPVAR{NPVARrec2} >

(* no COMP part *)

      DECOMP
         <
          C1: rel1 = objrel AND
              (QUOTE_normalergative(VERBrec1) OR
               CLAUSErec1.voice = passive
              )
          A1: NPVARrec1 := NPVARrec2;
              NPVARrec1.cases := [Nominative]
         >


   SUBRULE
      <T1: CNVAR{CNVARrec1} >
      <T2: CNVAR{CNVARrec2} >

(* no COMP part *)

      DECOMP
         <
          C1: rel1 = objrel AND
              (QUOTE_normalergative(VERBrec1) OR
               CLAUSErec1.voice = passive
              )
          A1: CNVARrec1 := CNVARrec2;
              CNVARrec1.cases := [Nominative]
         >


   SUBRULE
      <T1: NP{NPrec1}[mu7] >
      <T2: NP{NPrec2}[mu8] >

(* no COMP part *)

      DECOMP
         <
          C1: rel1 = objrel AND
              (QUOTE_normalergative(VERBrec1) OR
               CLAUSErec1.voice = passive
              )
          A1: NPrec1 := NPrec2;
              QUOTE_assigncase(NPrec2, mu8, NPrec1, mu7, [Nominative]);
         >

>

&

\end{verbatim}
