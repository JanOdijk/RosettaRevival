% RULE Taspectneutralization
< m1:CLAUSE{CLAUSErec1}
           [mu1] 
>

< m :CLAUSE{CLAUSErec2}
           [mu1]
>
      COMP
         <
          C1: (CLAUSErec1.aspect = imperfective) OR
              (CLAUSErec1.aspect = perfective)
          A1: CLAUSErec2 := CLAUSErec1;
              CLAUSErec2.aspect := omegaaspect;
         >

      DECOMP
         <
          C1: CLAUSErec2.aspect = omegaaspect
             C11: true
             A11: CLAUSErec1 := CLAUSErec2;
                  CLAUSErec1.aspect := perfective;
             C12: true
             A12: CLAUSErec1 := CLAUSErec2;
                  CLAUSErec1.aspect := imperfective;
          A1: @
         >

&

\end{verbatim}

