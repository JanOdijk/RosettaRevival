                        }  AND
              NPrec1.actcomas * [count] <> [] 

              AND
              PERSPROrec1 = COPYT_BPERSPROTOPERSPRO(BPERSPROrec1) AND
              PERSPROrec1.persprocases = [nominative]
              C11: BPERSPROrec1.key <> KEY_wijBPERSPROkey
              A11: POSSADJrec1.eORenForm  := noForm 
              C12: BPERSPROrec1.key = KEY_wijBPERSPROkey AND     (* onze *)
                   ((plural IN CNrec1.numbers) OR
                    (singular IN CNrec1.numbers AND
                     CNrec1.genders * [mascgender, femgender] <> []
                    )
                   )
              A12: POSSADJrec1.eORenForm := eForm 
              C13: BPERSPROrec1.key = KEY_wijBPERSPROkey AND      (* ons *)
                   (singular IN CNrec1.numbers AND
                     CNrec1.genders * [neutgender] <> []
                   )
              A13: POSSADJrec1.eORenForm := NoForm 

              (* the POSSADJ happens to have default values, except for 
                 eorenform *)


          A1: NPrec2 := { posspred          : true
                          definite          : def 
                          possnietnp        : false
                          mood              : POSSADJrec1.mood 

                          number            : NOUNrec3.number
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                         }  
 
         >

      DECOMP
         <
          C1: POSSADJrec1.antecedent = noant AND
              POSSADJrec1.mood = declxpmood AND
              POSSADJrec1.geni = false
              (* .reduced doesn't have to be tested in analysis since
                 both forms are accepted *)
               AND 
               NPrec2 = { posspred          : true
                          definite          : def 
                          possnietnp        : false
                          mood              : POSSADJrec1.mood 

                          number            : NOUNrec3.number
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                         }  
              C11: POSSADJrec1.eORenForm  = noForm AND
                   BPERSPROrec1.key <> KEY_wijBPERSPROkey
              A11: @
              C12: POSSADJrec1.eORenForm  = eForm AND        (* onze *)
                   BPERSPROrec1.key = KEY_wijBPERSPROkey AND
                   ((plural IN CNrec1.numbers) OR
                    (singular IN CNrec1.numbers AND
                     CNrec1.genders * [mascgender, femgender] <> []
                    )
                   )
              A12:@
              C13: POSSADJrec1.eORenForm = NoForm  AND
                   BPERSPROrec1.key = KEY_wijBPERSPROkey AND (* ons *)
                   (singular IN CNrec1.numbers AND
                    CNrec1.genders * [neutgender] <> []
                   )
              A13: @ 

          A1: PERSPROrec1 := COPYT_BPERSPROTOPERSPRO(BPERSPROrec1);
              (* persprocases remains default *)
              NPrec1 := { req           : [pospol,negpol,omegapol]
                          env           : [pospol,negpol,omegapol]
                          class         : omegaTimeAdvClass
                          superdeixis   : omegadeixis
                          deixis        : omegadeixis
                          aspect        : omegaAspect
                          retro         : false
                          definite      : def
                          person        : PERSPROrec1.person
                          number        : PERSPROrec1.number
                          cases         : [nominative]
                          genders       : [PERSPROrec1.gender] 
                          sexes         : []
                          actsubcs      : [othernoun]
                          temporal      : false
                          possgeni      : false
                          animate       : PERSPROrec1.animate
                          human         : Omegahuman
                          actcomas      : [count]
                          thetanp       : omegathetanp
                          nounpatternefs: []
                          posspred      : true
                          possnietnp    : false
                          mood          : declxpmood
                          generic       : omegageneric (* 11/11/88; FJ *)
                          possRform     : PERSPROrec1.possRform
                          syntquant     : omegaquant
                          NPhead        : PERSPROrec1.NPhead
                          specQ         : yesspec
                          };
         >

