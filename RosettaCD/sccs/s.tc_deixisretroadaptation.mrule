% FILTER Fpredeixisretro
< m :SENTENCE{SENTENCErec1}[ I10:mu1, 
                         predrel/VERBP{VERBPrec1}
                                [mu2, I1:rel1/VERB{VERBrec1}[mu4], I2:mu5], 
                         mu3 
                       ]
>

   MATCHCONDITIONS
      <
       I1: (rel1=auxrel AND (VERBrec1.status IN [bareV,teV]) AND
            (VERBrec1.subc IN [hebbenaux, zijnaux])
           )                                                OR
           (rel1 IN [head, auxrel, progauxrel] AND 
            VERBrec1.status IN [AuxV, teAuxV] AND
            (EXIST (mu4, [auxrel/I11::VERB{VERBrec3}[mu6]]))
           )                                                OR
           (rel1=head AND (VERBrec1.modus = pastpart)
           )
       I11: (VERBrec3.subc IN [hebbenaux, zijnaux])
       I2: NOT EXIST(mu5,[I21:rel2/..])
           I21: rel2 IN [head, auxrel, progauxrel]
       I10: QUOTE_tempretronotfound(mu1)

     >

         <
          C1:    (SENTENCErec1.retro = false                         OR

                  (
                   (SENTENCErec1.deixis <> presentdeixis      OR
                    VERBrec1.tense <> presenttense)            AND
                   (SENTENCErec1.deixis <> pastdeixis         OR
                    VERBrec1.tense <> pasttense)               AND
                   (SENTENCErec1.superdeixis <> presentdeixis OR
                    [VERBrec1.tense] * [presenttense,
                          omegatense]=[])                      AND
                   (SENTENCErec1.superdeixis <> pastdeixis    OR
                    [VERBrec1.tense] * [pasttense,
                          omegatense]=[])         
                  )                                               
                                                                   OR

                  SENTENCErec1.aspect <> perfective
                 )                                                        AND

                 (SENTENCErec1.retro=true                            OR
