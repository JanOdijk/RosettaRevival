% RULE RCNformation1
< m1: NOUN{NOUNrec1}[mu1]
>

< m : CN{CNrec1}[head/NOUN{NOUNrec2}[mu1]
                ]
>

      COMP
         <
          C1: true
             C11: true
             A11: NOUNrec2 := NOUNrec1;
                  CNrec1 := COPYT_NOUNtoCN(NOUNrec2);
                  CNrec1.numbers :=  [NOUNrec2.number]; 

             C12: unitnoun IN NOUNrec1.subcs AND
                  NOUNrec1.number = plural
             A12: NOUNrec2 := NOUNrec1;
                  CNrec1 := COPYT_NOUNtoCN(NOUNrec2);
                  CNrec1.numbers := [plural];
                  NOUNrec2.number := singular;
          A1: CNrec1.actsubcs := NOUNrec2.subcs;
              CNrec1.nounpatternefs := NOUNrec2.nounpatterns; 
              (* .req hasn't been determined yet;
                 .superdeixis/.definite have default value *)     
         >

      DECOMP
         <
          C1: (* CNrec1 = COPYT_NOUNtoCN(NOUNrec2)  *)
               CNrec1 = 
                  {                 
                   class:         NOUNrec2.class
                   deixis:        NOUNrec2.deixis
                   aspect:        NOUNrec2.aspect
                   retro:         NOUNrec2.retro
                   genders:       NOUNrec2.genders
                   sexes:         NOUNrec2.sexes
                   temporal:      NOUNrec2.temporal
                   possgeni:      NOUNrec2.possgeni
                   animate:       NOUNrec2.animate
                   human:         NOUNrec2.human
                   posscomas:     NOUNrec2.posscomas
                   thetanp:       NOUNrec2.thetanp
                  } AND
              (CNrec1.actsubcs * NOUNrec2.subcs <> []) AND
              (CNrec1.nounpatternefs * NOUNrec2.nounpatterns <> [] OR
               NOUNrec2.nounpatterns = []) AND
              (CNrec1.superdeixis = omegadeixis) AND
              (CNrec1.definite       = omegadef) AND
              (CNrec1.cases          = [Nominative]) 

             C11: NOUNrec2.number IN CNrec1.numbers 
             A11: NOUNrec1 := NOUNrec2;

             C12: unitnoun IN NOUNrec2.subcs AND
                  NOUNrec2.number = singular AND
                  plural IN CNrec1.numbers
             A12: NOUNrec1 := NOUNrec2;
                  NOUNrec1.number  := plural;       (* efficiency; 
                                                      cf. documentation *)
          A1:  @
         >
&

\end{verbatim}
