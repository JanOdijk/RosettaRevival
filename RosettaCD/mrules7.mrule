                        I6:rel1/VERB{VERBrec1}
                            [ head/I8::VERB{VERBrec2}[mu4],
                              adjoinrel/VERB{VERBrec3}[mu5]
                            ],
                        I4:mu6 
                      ],
               mu7
             ]
>

