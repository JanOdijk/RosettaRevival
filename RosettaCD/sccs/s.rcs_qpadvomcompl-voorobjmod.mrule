\begin{verbatim}
 RULE RADVVOOROBJMOD
< m1: I3::ADVP{ADVPrec1}[I1:mu1,
                         head/ADV{ADVrec1}[mu2],
                         mu3
                         ]
  m2: PREPP{PREPPrec1}[head/PREP(AUX_voorioprepkey){PREPrec1},
                      I2:objrel/T1
                     ]
>

< m : I3::ADVP{ADVPrec1}
              [voorobjrel/PREPP{PREPPrec2}
                          [head/PREP(AUX_voorioprepkey){PREPrec1},
                           I2:objrel/T1
                          ],
               mu1,
               head/ADV{ADVrec1}[mu2],
               mu3
              ]
>

   MATCHCONDITIONS
      <I1: NOT EXIST(mu1, [voorobjrel/..])
       I2: T1.cat IN [NPVAR,CNVAR]
       I3: ADVPrec1.thanas = omcompl  (* `te' *)
      >


      COMP
         <
          C1: PREPPrec1.superdeixis = ADVPrec1.superdeixis
          A1: PREPPrec2 := PREPPrec1;
             PREPPrec2.superdeixis := omegadeixis;
         >
      DECOMP
         <
          C1: PREPPrec2.superdeixis = omegadeixis
          A1: PREPPrec1 := PREPPrec2;
              PREPPrec1.superdeixis := ADVPrec1.superdeixis;
         >

&

\end{verbatim}
