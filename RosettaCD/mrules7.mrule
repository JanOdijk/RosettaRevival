          I1:[ I2:mu1,
               conjrel/I8::VERB{VERBrec2}[mu4],
               I3:mu2,
               predrel/VERBP{VERBPrec1}
                   I9:[ mu3,
                        I6:rel1/VERB{VERBrec3}[mu5],
                        I4:mu6 
                      ],
               mu7
             ]

>

   MATCHCONDITIONS
      <
       I1: RIGHTTOLEFT
       I2: ALL(mu1, I21:rel2/..)
           I21: rel2 IN [leftdislocrel, shiftrel]
       I3: NOT EXIST(mu2, [conjrel/..])
       I4: NOT EXIST(mu6, [I41:rel4/..])
           I41: rel4 IN [auxrel, progauxrel, head]
       I6: rel1 IN [head, auxrel, progauxrel]
