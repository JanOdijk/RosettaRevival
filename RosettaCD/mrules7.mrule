\begin{verbatim}
% TRANSFORMATION TV23             (* gekocht heeft/werd/is; aan het kopen is;
                                       aan het kopen geweest  is *)
< m1:I7::SENTENCE{SENTENCErec1}
          I1:[ I2:mu1,
               I3:mu2,
               predrel/VERBP{VERBPrec1}
                   I9:[ mu3,
                        I10:rel3/I5::VERB{VERBrec1}[mu4],
                        I6:rel1/I8::VERB{VERBrec2}[mu5],
                        I4:mu6 
                      ],
               mu7
             ]
>

< m :I7::SENTENCE{SENTENCErec1}
          I1:[ I2:mu1,
               conjrel/I8::VERB{VERBrec2}[mu5],
               I3:mu2,
               predrel/VERBP{VERBPrec1}
                   I9:[ mu3,
                        I10:rel3/I5::VERB{VERBrec1}[mu4],
                        I4:mu6 
                      ],
               mu7
             ]

>

   MATCHCONDITIONS
      <
       I1: RIGHTTOLEFT
       I2: ALL(mu1, I21:rel2/..)
           I21: rel2 IN [leftdislocrel, shiftrel]
       I3: NOT EXIST(mu2, [conjrel/..])
       I4: NOT EXIST(mu6, [I41:rel4/..])
           I41: rel4 IN [auxrel, progauxrel, head]
       I5: VERBrec1.modus = pastpart OR VERBrec1.status = aanhetV
       I6: rel1 IN [auxrel, progauxrel]
       I7 : SENTENCErec1.finiteness = finite
       I8: VERBrec2.subc IN [hebbenaux, wordenaux, zijnaux] AND
           VERBrec2.status = bareV
       I9: RIGHTTOLEFT       
       I10: rel3 IN [head, progauxrel]
       >

      COMP
         <
          C1: rel1 = progauxrel            AND
              rel3 = head                  AND 
              VERBrec1.status = aanhetV    AND
              VERBrec2.subc = zijnaux
          A1: @

          C2: rel1 = auxrel                 AND
              VERBrec1.modus = pastpart
          A2: @
         >
      DECOMP
         <
          C1: rel3 = head                 AND
              VERBrec1.status = aanhetV   AND
              VERBrec2.subc = zijnaux    
          A1: rel1 := progauxrel;
              

          C2: VERBrec1.modus = pastpart
          A2: rel1 := auxrel    
         >


&

\end{verbatim}
