      <
       I6: ALL(mu6, I61:rel6/..)
           I61: QUOTE_Srelprec(rel6, sentadvrel) 
       I7: ALL(mu7, I71:rel7/..)
           I71: QUOTE_Srelprec(sentadvrel, rel7 ) AND [rel7] * 
                                     [subjvpadvrel, tempadvrel] = []
       I8: [sentadv, causadv] * ADVPrec1.actsubcefs <> []
      >

