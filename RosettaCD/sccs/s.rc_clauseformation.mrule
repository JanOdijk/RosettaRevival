                 predrel/I1::PREPP{PREPPrec1}[mu2],
                 mu10
               ] 
>

< m :CLAUSE{CLAUSErec1}
           [ mu1,
             predrel/VERBP{VERBPrec1}
                    [ predrel/I3::VARPREPP{VARPREPPrec1}[mu2],
                      head/VERB{VERBrec1}
                          [ head/SUBVERB{SUBVERBrec1}
                                [head/BVERB(AUX_zijnBverbkey){BVERBrec1} ] 
                          ]                       
                    ],
             mu10 
           ]
>
   MATCHCONDITIONS
      <
       I1: PREPPrec1.actsubcefs * [other]  <> []
       I3: VARPREPPrec1.actsubcefs * [other]  <> []
       I2: (CLAUSErec1.voice = Active ) AND
           (synzijn IN CLAUSErec1.synvpefs ) AND
           CLAUSErec1.thetavp = thetavpaux
      >

      COMP
         <
          C1: (
               PREPPPROPrec1 =
		     {
		      mood        : declxpmood
		      actsubcefs  : PREPPrec1.actsubcefs
		      PROsubject  : false
                      thetapp     : PREPPrec1.thetapp
		      synppefs    : PREPPrec1.synppefs
		      headkey     : PREPPrec1.headkey
		      class       : omegaTimeAdvClass
		      deixis      : omegadeixis
		      aspect      : omegaAspect
		      retro       : false
                      specq       : PREPPrec1.specq
		     }
              ) 
          A1: SUBVERBrec1 := COPYT_bverbtosubverb(BVERBrec1);
              VERBrec1 := COPYT_subverbtoverb(SUBVERBrec1);
              (* all other attributes of verb retain their default values *)
              CLAUSErec1 :=
		     { 
		      voice       : Active
		      synvpefs    : [synzijn]
		      aktionsarts : PREPPPROPrec1.aktionsarts
		      superdeixis : PREPPPROPrec1.superdeixis
		     };
              VARPREPPrec1 := PREPPrec1;

         >
      DECOMP
         <
          C1: (CLAUSErec1 = CLAUSErecdef EXCEPT FOR [ voice, synvpefs,
                           aktionsarts, superdeixis, thetavp] )
          A1: PREPPPROPrec1 :=
		     {
		      mood        : declxpmood
		      actsubcefs  : VARPREPPrec1.actsubcefs
		      PROsubject  : false
                      thetapp     : VARPREPPrec1.thetapp
		      synppefs    : VARPREPPrec1.synppefs
		      headkey     : VARPREPPrec1.headkey
		      class       : omegaTimeAdvClass
		      deixis      : omegadeixis
		      aspect      : omegaAspect
		      retro       : false
		      aktionsarts : CLAUSErec1.aktionsarts
		      superdeixis : CLAUSErec1.superdeixis
                      specq       : VARPREPPrec1.specq
		     };
             PREPPrec1 := VARPREPPrec1;
         >

&
