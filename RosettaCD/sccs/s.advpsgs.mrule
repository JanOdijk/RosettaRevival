      COMP
         <
          C1: true
              C11: RADVrec1.key = KEY_ergensRADVkey
              A11: SUBADVrec1 := AUX_SUBADVreconR;
                   SUBADVrec1.possnietnp := true;
              C12: RADVrec1.key <> KEY_ergensRADVkey
