% RULE Rbnounpl
< m1:BNOUN{BNOUNrec1} 
>

< m :NP{NPrec1}
       [ detrel/ART(AUX_deARTkey){ARTrec1},
         head/CN{CNrec1}
             [ head/NOUN{NOUNrec1}
                   [ head/SUBNOUN{SUBNOUNrec1}[head/BNOUN{BNOUNrec1}] 
                   ] 
             ]
       ]
>


      COMP
         <
          C1: BNOUNrec1.pluralforms <> [NOplural] 
          A1: SUBNOUNrec1 := COPYT_bnounTOsubnoun(BNOUNrec1);
              NOUNrec1    := COPYT_subnounTOnoun(SUBNOUNrec1);
              NOUNrec1.number := plural;
              CNrec1 := COPYT_nounTOcn(NOUNrec1);
              CNrec1 :=
                {
		  superdeixis   : Omegadeixis
		  definite      : omegadef
		  numbers       : [plural]
		  cases         : [Nominative]
		  actsubcs      : [othernoun]
                  detlessnp     : false
                };
               NPrec1 := COPYT_cnTOnp(CNrec1);
               NPrec1 :=
		  {
		  definite       : def
		  person         : 3
		  number         : plural
		  actcomas       : [count]
		  posspred       : true
		  possnietnp     : false
		  mood           : declxpmood
		  generic        : nogeneric
		  possRform      : false
		  syntquant      : omegaquant
		  NPhead         : otherNP
		  specQ          : yesspec
		  }
               
         >
      DECOMP
         <
          C1: BNOUNrec1.pluralforms <> [noplural]           AND
              SUBNOUNrec1 = COPYT_bnounTOsubnoun(BNOUNrec1) AND
              NOUNrec1    = COPYT_subnounTOnoun(SUBNOUNrec1) AND
              NOUNrec1.number = plural AND
              CNrec1 = COPYT_nounTOcn(NOUNrec1) AND
