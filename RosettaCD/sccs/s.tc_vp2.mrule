\begin{verbatim}
%
% TRANSFORMATION RIdVerbpattern
< m1: I1::VERBPPROP{VERBPPROPrec1}
                   [mu1,
                    predrel/VERBP{VERBPrec1}
                           [mu3,
                            head/VERB{VERBrec1}[mu2],
                            mu4
                           ],
                    mu5
                   ]
>

< m : I2::VERBPPROP{VERBPPROPrec2}
                   [mu1,
                    predrel/VERBP{VERBPrec1}
                           [mu3,
                            head/VERB{VERBrec1}[mu2],
                            mu4
                           ],
                    mu5
                   ]
>

   MATCHCONDITIONS
      <
       I1: VERBPPROPrec1.synvpefs <> []
       I2: VERBPPROPrec2.synvpefs * AUX_idvps <> []
      >

      COMP
         <
          C1: true
          A1: VERBPPROPrec2 := VERBPPROPrec1;
         >
      DECOMP
         <
          C1: true
          A1: VERBPPROPrec1 := VERBPPROPrec2;
              VERBPPROPrec1.synvpefs := 
              (VERBrec1.synvps - AUX_idvps) + VERBPPROPrec2.synvpefs;
         >

&

\end{verbatim}

