% RULE RNPapposition (* conditions probably must be sharpened  *)
< m1: NP{NPrec1}[mu1] 
  m2: NP{NPrec2}[mu2] 
>

< m : NP{NPrec3}[head/NP{NPrec1}[mu1],
                 puncrel/PUNC(KEY_kommapunckey){PUNCrec1},
                 apposrel/NP{NPrec4}[mu4],
                 I1: sig1
                ]
>

   MATCHCONDITIONS
      <
       I1: ALL(sig1, puncrel/PUNC(KEY_kommapunckey){PUNCrec2})
      >

<
   SUBRULE
      < >
      <sig1: [puncrel/PUNC(KEY_kommapunckey){PUNCrec3}] >

   (*  No COMP *)

      DECOMP
         <
          C1: NPrec1.number = NPrec4.number AND
              NPrec1.mood = declxpmood AND
              NPrec1.mood = NPrec4.mood AND
              NPrec1.NPhead IN [persproNP, othernp, allesnp] AND
              NPrec4.NPhead IN [persproNP, othernp, allesnp] AND
              NPrec4.generic = omegageneric AND
              NPrec4.cases = NPrec1.cases

          A1: NPrec1 := NPrec3;
              QUOTE_assigncase(NPrec4,mu4,NPrec2,mu2,[Nominative]); 
              NPrec2.cases := [nominative];
              NPrec2.superdeixis := NPrec1.superdeixis;
              NPrec2.generic := nogeneric;
         >

   SUBRULE
      < >
      <sig1: []>


      COMP
         <
          C1: NPrec1.number = NPrec2.number AND
              NPrec1.superdeixis = NPrec2.superdeixis AND
              NPrec1.mood = declxpmood AND
              NPrec1.mood = NPrec2.mood AND
              NPrec1.NPhead IN [persproNP, othernp, allesnp] AND
              NPrec2.NPhead IN [persproNP, othernp, allesnp] AND
              NPrec2.generic = nogeneric AND
              NPrec2.cases = [Nominative]
              
          A1: NPrec3 := NPrec1;
              NPrec4 := NPrec2;
              NPrec4.generic := omegageneric;
              NPrec4.cases := NPrec1.cases;
         >
      DECOMP
         <
          C1: NPrec1.number = NPrec4.number AND
              NPrec1.mood = declxpmood AND
              NPrec1.mood = NPrec4.mood AND
              NPrec1.NPhead IN [persproNP, othernp, allesnp] AND
              NPrec4.NPhead IN [persproNP, othernp, allesnp] AND
              NPrec4.generic = omegageneric AND
              NPrec4.cases = NPrec1.cases

          A1: NPrec1 := NPrec3;
              QUOTE_assigncase(NPrec4,mu4,NPrec2,mu2,[Nominative]); 
              NPrec2.superdeixis := NPrec1.superdeixis;
              NPrec2.generic := nogeneric;
         >

>
&

\end{verbatim}
