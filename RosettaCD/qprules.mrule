              NPrec2.superdeixis = omegadeixis 
          A1: NPVARrec1 := COPYT_nptovar(NPrec2);
              NPVARrec1.index := LEVEL;
              QUOTE_assigncase(NPrec2, mu6, NPrec1, mu5, [Nominative]) ;
              NPrec1.generic := nogeneric;
              NPrec1.superdeixis := QPrec1.superdeixis;
         >

