%
% RULE DETderivation1
< m1: BDET{BDETrec1} 
>

< m : DET{DETrec1}[head/BDET{BDETrec1}]
>

      COMP
         <
          C1: true
              C11: BDETrec1.eFormation = noformation
              A11: @
              C12: BDETrec1.eformation <> noformation
                   C121: true
                   A121: DETrec1.eorenform := noform
                   C121: true
                   A121: DETrec1.eorenform := eform
              A12: @

          A1: DETrec1 := 
                        { possnietnp  : BDETrec1.possnietnp 
                          definite    : BDETrec1.definite
                          posspred    : BDETrec1.posspred
                          possnumbers : BDETrec1.possnumbers 
                          posscomas   : BDETrec1.posscomas
                          mood        : BDETrec1.mood
                          eFormation  : BDETrec1.eFormation
                          enFormation : BDETrec1.enFormation
                          (* syntquant : BDETrec1.syntquant *)
                         }
  
         >
      DECOMP
         <
          C1: DETrec1 = 
                        { possnietnp  : BDETrec1.possnietnp 
                          definite    : BDETrec1.definite
                          posspred    : BDETrec1.posspred
                          possnumbers : BDETrec1.possnumbers 
                          posscomas   : BDETrec1.posscomas
                          mood        : BDETrec1.mood
                          eFormation  : BDETrec1.eFormation
                          enFormation : BDETrec1.enFormation
                          (* syntquant   :BDETrec1.syntquant *)
                         }
                C11: BDETrec1.eFormation = noformation
                A11: @
                C12: BDETrec1.eformation <> noformation 
                     C121: DETrec1.eorenform = noform
                     A121: @
                     C121: DETrec1.eorenform = eform
                     A121: @
                A12: @              
          A1: @
         >

&

\end{verbatim}
