               NPrec1 = COPYT_cnTOnp(CNrec1) AND
               NPrec1 =
		  {
		  definite       : def
		  person         : 3
		  number         : plural
		  actcomas       : [count]
		  posspred       : true
		  possnietnp     : false
		  mood           : declxpmood
		  generic        : nogeneric
		  possRform      : false
		  syntquant      : omegaquant
		  NPhead         : otherNP
		  specQ          : yesspec
		  }
               
          A1: @
