% RULE Rreciprocalsubst1
< m1:CLAUSE{CLAUSErec1}
            [ mu1,
              predrel/VERBP{VERBPrec1}
                     [ mu3,
                       I2:rel2/I4::NPVAR{NPVARrec2},
                       mu4 
                     ],
              mu5
            ] 
SUBST:
 m2: NP{NPrec1}[I3:sig1]
>

< m :CLAUSE{CLAUSErec1}
            [ mu1,
              predrel/VERBP{VERBPrec1}
                  I7:[ mu3,
                       I2:rel2/NP{NPrec2}[I5:sig2],
                       mu4 
                     ],
              mu5
            ] 
E 2
I 1
 
I 2
>



   MATCHCONDITIONS
      <
        I2: rel2 IN [objrel, indobjrel]      
        I4: NPVARrec2.index = LEVEL
        I3: ALL(sig1, head/RECIPRO{}) AND EXIST(sig1, [head/RECIPRO{}])
        I5: ALL(sig2, head/RECIPRO{}) AND EXIST(sig2, [head/RECIPRO{}])
        I7: RIGHTTOLEFT (* intentionally absent in generation *)
      >

   PARAMETERS
      <
       LEVEL
       antecedent     : indextype
      >

<
   SUBRULE (* ant = subjrel *)
      <mu1: [mu2, I3:subjrel/T1, mu6] >
      <mu1: [mu2, I8:subjrel/T1, mu6] >

   MATCHCONDITIONS
      <
        I3: (T1.CAT IN [NPVAR, CNVAR] WITH T1.REC.index = antecedent)  AND
            QUOTE_elkaarant(T1)
        I8: QUOTE_elkaarant(T1)
      >

      COMP
         <
          C1: NPVARrec2 = COPYT_nptovar(NPrec1) EXCEPT FOR [cases] AND
              NPrec1.generic = nogeneric AND
              NPrec1.cases = [Nominative]
              C11: CLAUSErec1.deixis = omegadeixis              AND
                   NPrec1.superdeixis = CLAUSErec1.superdeixis
              A11: QUOTE_assigncase(NPrec1,sig1,NPrec2,sig2,NPVARrec2.cases); 
                   NPrec2.superdeixis := omegadeixis
              C12: CLAUSErec1.deixis <> omegadeixis             AND
                   NPrec1.superdeixis = CLAUSErec1.deixis
              A12: QUOTE_assigncase(NPrec1,sig1,NPrec2,sig2,NPVARrec2.cases); 
                   NPrec2.superdeixis := omegadeixis  
          A1: NPrec2.generic := omegageneric;
         >
      DECOMP
         <
          C1: NPrec2.generic = omegageneric 
              C11: CLAUSErec1.deixis = omegadeixis AND
                   NPrec2.superdeixis = omegadeixis
              A11: QUOTE_assigncase(NPrec2,sig2,NPrec1,sig1,[Nominative]); 
                   NPrec1.superdeixis := CLAUSErec1.superdeixis;
              C12: CLAUSErec1.deixis <> omegadeixis AND
                   NPrec2.superdeixis = omegadeixis  
              A12: QUOTE_assigncase(NPrec2,sig2,NPrec1,sig1,[Nominative]); 
                   NPrec1.superdeixis := CLAUSErec1.deixis;
          A1: antecedent := T1.REC.index ;
              NPVARrec2 := COPYT_nptovar(NPrec2);
              NPVARrec2.index := LEVEL;
              NPrec1.generic := nogeneric;
         >

   SUBRULE (* ant =preadvrel *)
      <mu1: [mu2, I3:preadvrel/T1, mu6] >
      <mu1: [mu2, I8:preadvrel/T1, mu6] >

   MATCHCONDITIONS
      <
        I3: (T1.CAT IN [NPVAR, CNVAR] WITH T1.REC.index = antecedent)  AND
            QUOTE_elkaarant(T1)
        I8: QUOTE_elkaarant(T1)
      >

      COMP
         <
          C1: NPVARrec2 = COPYT_nptovar(NPrec1) EXCEPT FOR [cases] AND
              NPrec1.generic = nogeneric AND
              NPrec1.cases = [Nominative]
              C11: CLAUSErec1.deixis = omegadeixis              AND
                   NPrec1.superdeixis = CLAUSErec1.superdeixis
              A11: QUOTE_assigncase(NPrec1,sig1,NPrec2,sig2,NPVARrec2.cases); 
                   NPrec2.superdeixis := omegadeixis
              C12: CLAUSErec1.deixis <> omegadeixis             AND
                   NPrec1.superdeixis = CLAUSErec1.deixis
              A12: QUOTE_assigncase(NPrec1,sig1,NPrec2,sig2,NPVARrec2.cases); 
                   NPrec2.superdeixis := omegadeixis  
          A1: NPrec2.generic := omegageneric;
         >
      DECOMP
         <
          C1: NPrec2.generic = omegageneric 
              C11: CLAUSErec1.deixis = omegadeixis AND
                   NPrec2.superdeixis = omegadeixis
              A11: QUOTE_assigncase(NPrec2,sig2,NPrec1,sig1,[Nominative]); 
                   NPrec1.superdeixis := CLAUSErec1.superdeixis;
              C12: CLAUSErec1.deixis <> omegadeixis AND
                   NPrec2.superdeixis = omegadeixis  
              A12: QUOTE_assigncase(NPrec2,sig2,NPrec1,sig1,[Nominative]); 
                   NPrec1.superdeixis := CLAUSErec1.deixis;
          A1: antecedent := T1.REC.index ;
              NPVARrec2 := COPYT_nptovar(NPrec2);
              NPVARrec2.index := LEVEL;
              NPrec1.generic := nogeneric;
         >


   SUBRULE (* ant = postsubjrel *)
      <mu1: [mu2, I3:postsubjrel/T1, mu6] >
      <mu1: [mu2, I8:postsubjrel/T1, mu6] >

   MATCHCONDITIONS
      <
        I3: (T1.CAT IN [NPVAR, CNVAR] WITH T1.REC.index = antecedent)  AND
            QUOTE_elkaarant(T1)
        I8: QUOTE_elkaarant(T1)
      >

      COMP
         <
          C1: NPVARrec2 = COPYT_nptovar(NPrec1) EXCEPT FOR [cases] AND
              NPrec1.generic = nogeneric AND
              NPrec1.cases = [Nominative]
              C11: CLAUSErec1.deixis = omegadeixis              AND
                   NPrec1.superdeixis = CLAUSErec1.superdeixis
              A11: QUOTE_assigncase(NPrec1,sig1,NPrec2,sig2,NPVARrec2.cases); 
                   NPrec2.superdeixis := omegadeixis
              C12: CLAUSErec1.deixis <> omegadeixis             AND
                   NPrec1.superdeixis = CLAUSErec1.deixis
              A12: QUOTE_assigncase(NPrec1,sig1,NPrec2,sig2,NPVARrec2.cases); 
                   NPrec2.superdeixis := omegadeixis  
          A1: NPrec2.generic := omegageneric;
         >
      DECOMP
         <
          C1: NPrec2.generic = omegageneric 
              C11: CLAUSErec1.deixis = omegadeixis AND
                   NPrec2.superdeixis = omegadeixis
              A11: QUOTE_assigncase(NPrec2,sig2,NPrec1,sig1,[Nominative]); 
                   NPrec1.superdeixis := CLAUSErec1.superdeixis;
              C12: CLAUSErec1.deixis <> omegadeixis AND
                   NPrec2.superdeixis = omegadeixis  
              A12: QUOTE_assigncase(NPrec2,sig2,NPrec1,sig1,[Nominative]); 
                   NPrec1.superdeixis := CLAUSErec1.deixis;
          A1: antecedent := T1.REC.index ;
              NPVARrec2 := COPYT_nptovar(NPrec2);
              NPVARrec2.index := LEVEL;
              NPrec1.generic := nogeneric;
         >


>
