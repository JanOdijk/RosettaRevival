%RULE RSTARTVPPROP000

<m1: I3::SUBVERB{SUBVERBREC1}[mu1]
>
<m: I1::VERBPPROP{VERBPPROPrec1}
                 [ subjrel/NP{NPrec1}
                          [ head/PERSPRO{PERSPROrec1}
                                [ head/BPERSPRO(AUX_hetbpersprokey){BPERSPROrec1}]
                          ], 
                   predrel/VERBP{VERBPrec1}
                          [ head/I2::VERB{VERBrec1}
                                [ head/I3::SUBVERB{SUBVERBrec1}[mu1]]
                          ]
                 ]
>

   MATCHCONDITIONS
      <
       I1: VERBPPROPrec1 = 
		  { 
                   thetavp     : vp000
		   req         : [ pospol, negpol, omegapol]
		   env         : [ pospol, negpol, omegapol]
		   superdeixis : Omegadeixis
		   PROsubject  : false
		   voice       : omegavoice
		   synvpefs    : []
                   aktionsarts : []
                   adjuncts    : []
		  }

       I2: VERBrec1 =
		  {
		   number       : omeganumber   
		   persons      : []   
		   eORenForm    : NoForm   
		   modus        : omegamodus  
		   tense        : omegatense  
                   foundauxes   : []
		  }                               AND
            VERBrec1.status IN [bareV, partV]

       I3: SUBVERBrec1.thetavp = vp000     
 

      >


COMP
 <
  C1: true
      C11: SUBVERBrec1.lastaffix <> partaffix
      A11: VERBrec1 := COPYT_subverbtoverb(SUBVERBrec1);
      C12: SUBVERBrec1.lastaffix = partaffix
      A12: VERBrec1 := COPYT_subverbtoverb(SUBVERBrec1);
           VERBrec1.status := partV; 
  A1: PERSPROrec1 := COPYT_BPERSPROtoPERSPRO(BPERSPROrec1);
      PERSPROrec1.reduced := false;
      (* cases retains the default value *)

      NPrec1 := AUX_NPONhetBPRSPR;
      VERBPPROPrec1.thetavp := vp000
 >
DECOMP
 <
  C1: (PERSPROrec1 = COPYT_bpersproTOperspro(BPERSPROrec1)) AND
      (* no check on reduced, so both het and 't are accepted *)
      (nominative IN PERSPROrec1.persprocases) AND 
      NPrec1 = AUX_NPONhetBPRSPR 
      (* check on thetavp of VERBPPROP in the MC's *)
      C11: VERBrec1 = COPYT_subverbtoverb(SUBVERBrec1)  AND
           SUBVERBrec1.lastaffix <> partaffix           AND
           VERBrec1.status = bareV 
      A11: @
      C12: VERBrec1 = COPYT_subverbtoverb(SUBVERBrec1)  AND
           VERBrec1.status = partV                      AND
           SUBVERBrec1.lastaffix = partaffix
      A12: @
  A1: @
 >
&

\end{verbatim}

