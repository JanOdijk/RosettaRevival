\begin{verbatim}
%
% TRANSFORMATION taktactivity1
< m1:VERBPPROP{VERBPPROPrec1}
              [mu1,
               predrel/VERBP{VERBPrec1}
                      [I1:mu2,
                       head/VERB{VERBrec1}[mu3],
                       mu4 
                      ],
               mu5
              ]
>

< m :VERBPPROP{VERBPPROPrec2}
              [mu1,
               predrel/VERBP{VERBPrec1}
                      [I1:mu2,
                       head/VERB{VERBrec1}[mu3],
                       mu4
                      ],
               mu5
              ]

>
   MATCHCONDITIONS
      < I1: NOT EXIST(mu2, [I2:rel1/T1])
        I2: (rel1 IN [objrel, prepobjrel]) AND 
            (T1.CAT IN [NPVAR, VARPREPP, CNVAR])
      >
      COMP
         <
          C1: (durativeclass IN VERBrec1.classes) AND
              (VERBrec1.thetavp IN [vp000, vp100, vp120, vp123]) AND
              (VERBPPROPrec1.aktionsarts = [])
          A1: VERBPPROPrec2 := VERBPPROPrec1;
              VERBPPROPrec2.aktionsarts := [activity];
         >
      DECOMP
         <
          C1: (durativeclass IN VERBrec1.classes) AND
              (VERBrec1.thetavp IN [vp000, vp100, vp120, vp123]) AND
              (activity IN VERBPPROPrec2.aktionsarts)
          A1: VERBPPROPrec1 := VERBPPROPrec2;
              VERBPPROPrec1.aktionsarts := [];

         >

&
\end{verbatim}
