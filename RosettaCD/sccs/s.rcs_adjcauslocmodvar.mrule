                [ mu1,
                  locadvrel/I1::ADVPVAR{ADVPVARrec1},
                  predrel/ADJP{ADJPrec1}[mu2],
                  mu3 
                ]
>

   MATCHCONDITIONS
      <
       m : NOT EXIST(mu3, [I11:rel1/..])
           I11: rel1 IN [postsentadvrel, posttemprel]
       I1: locadv IN ADVPVARrec1.actsubcefs 
      >

      COMP
         <
          C1: true
          A1: @
         >
      DECOMP
         <
          C1: true
          A1: WBONUS -1
              @
         >



&

\end{verbatim}
