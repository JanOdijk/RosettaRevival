                               [ head/RECIPRO{RECIPROrec1}],
                         head/I1::VERB{VERBrec1}[mu3]
                       ],
                mu11
              ]
>
   MATCHCONDITIONS
      <
       I1:VERBrec1.reflexivity = reciprocal  
       I2:  (plural IN CNVARrec1.numbers ) 
            (* OR NP is a collective NP *)
       I3: ( NPrec1 = AUX_NPONelkaar)

      >
 
       (* compositional part intentionally absent *)
      DECOMP
         <
          C1: (RECIPROrec1.key <> KEY_elkaarkey )
          A1: @
         >

&

