% RULE Rrelmarking1
< m1:CLAUSE{CLAUSErec1}[mu1, I1:rel1/T1, mu2 ] 
>

< m :CLAUSE{CLAUSErec1}[mu1, I2:rel1/T2, mu2 ] 
>

   MATCHCONDITIONS
      <
       I1: T1.CAT IN [NPVAR,PREPPVAR, ADVPVAR] WITH T1.REC.index = indexpar
       I2: (T2.CAT IN [NPVAR,PREPPVAR, ADVPVAR] 
                 WITH T2.REC.mood IN[relativexpmood, topicxpmood]) 
       m : (T2.CAT IN [NPVAR] WITH T2.REC.mood = topicxpmood) ->
           ((NOT EXIST(mu1, [postsubjrel/..])) AND
            ((NOT EXIST(mu1, [subjrel/..])) OR
             EXIST(mu1,[I11:subjrel/T3])
            ) 
           )
           I11:QUOTE_UptoAmbiguity(T3,T2) 
      >

   PARAMETERS
      <
       indexpar   :indextype
       xpmoodpar  :xpmoodtype
      >

<

   SUBRULE
      <T1:NPVAR{NPVARrec1} 
      >
      <T2:NPVAR{NPVARrec2} 
      >
 


      COMP
         <
          C1: NPVARrec1.mood = declxpmood AND
              xpmoodpar IN [relativexpmood, topicxpmood] AND
              ((rel1 = subjrel) -> (xpmoodpar <> topicxpmood)) AND
              [NPVARrec1.NPhead] * [hetNP, watindefproNP, 
                                   zegenNP, reducedpersproNP] = []
          A1: NPVARrec2 := NPVARrec1;
              NPVARrec2.mood := xpmoodpar; 
         >
      DECOMP
         <
          C1: ((rel1 = subjrel) -> (NPVARrec2.mood <> topicxpmood)) AND
              [NPVARrec2.NPhead] * [hetNP, watindefproNP, 
                                   zegenNP, reducedpersproNP] = []
              C11: NPVARrec2.mood = relativexpmood
              A11: @
              C12: NPVARrec2.mood = topicxpmood
              A12: WBONUS -10;
                   @
          A1: indexpar := NPVARrec2.index; 
              NPVARrec1 := NPVARrec2;
              NPVARrec1.mood := declxpmood;
              xpmoodpar := NPVARrec2.mood
         >


   SUBRULE
      <T1:PREPPVAR{PREPPVARrec1} >
      <T2:PREPPVAR{PREPPVARrec2} >


      COMP
         <
          C1: PREPPVARrec1.mood = declxpmood AND
              xpmoodpar IN [topicxpmood, relativexpmood] 
          A1: PREPPVARrec2 := PREPPVARrec1;
              PREPPVARrec2.mood := xpmoodpar; 
         >
      DECOMP
         <
          C1: PREPPVARrec2.mood IN [topicxpmood, relativexpmood]
              C11: PREPPVARRec2.mood = relativexpmood
              A11: @
              C12: PREPPVARrec2.mood = topicxpmood
              A11: WBONUS -1;
                   @
          A1: indexpar := PREPPVARrec2.index; 
              PREPPVARrec1 := PREPPVARrec2;
              PREPPVARrec1.mood := declxpmood;
              xpmoodpar := PREPPVARrec2.mood
         >

   SUBRULE
      <T1:ADVPVAR{ADVPVARrec1} >
      <T2:ADVPVAR{ADVPVARrec2} >


      COMP
         <
          C1: ADVPVARrec1.mood = declxpmood AND
              xpmoodpar = topicxpmood 
          A1: ADVPVARrec2 := ADVPVARrec1;
              ADVPVARrec2.mood := xpmoodpar; 
         >
      DECOMP
         <
          C1: ADVPVARrec2.mood = topicxpmood
          A1: WBONUS -1;
              indexpar := ADVPVARrec2.index; 
              ADVPVARrec1 := ADVPVARrec2;
              ADVPVARrec1.mood := declxpmood;
              xpmoodpar := ADVPVARrec2.mood
         >

>
&

\end{verbatim}
