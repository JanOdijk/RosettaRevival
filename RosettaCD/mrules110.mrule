% RULE RNPformation11

< m1: CN{CNrec1}I2:[mu1,
                 head/EN(KEY_countenkey){ENrec1},
                 I4:mu2
                ]

>

< m:  NP{NPrec1}[detrel/DETP{DETPrec1}[head/ART(KEY_eenartkey){ARTrec1}],
                 head/CN{CNrec1}I2:[mu1,
                                 head/EN(KEY_countenkey){ENrec1},
                                 mu2
                                ]
                ]                       
>

   MATCHCONDITIONS
      <
      I2: EXIST(mu1, [I12:modrel/T1]) OR
          EXIST(mu2, [I13:rel1/T2])
      I12: T1.CAT IN [ADJP, SENTENCE]
      I13: ((T2.CAT IN [ADJP, ADVP, PREPP]) OR 
            (T2.CAT IN [SENTENCE] WITH T2.REC.mood =relative)) AND
            rel1 IN [posrel, postmodrel]

      I4: NOT EXIST(mu2, [posrel/NP{NPrec2}[mu9]])
      >


      COMP
         <
          C1: CNrec1.numbers = [singular] 
          A1: DETPrec1 := COPYT_ARTtoDETP(ARTrec1);
              DETPrec1.posspred := true;
              DETPrec1.possnietnp := true;
              DETPrec1.mood := declxpmood;              
              NPrec1 := { posspred          : DETPrec1.posspred
                          definite          : DETPrec1.definite
                          possnietnp        : DETPrec1.possnietnp
                          mood              : DETPrec1.mood

                          number            : singular
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : [count]
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : enokNP
                         }  
         >

      DECOMP
         <
          C1: CNrec1.numbers = [singular] AND
              DETPrec1  = COPYT_ARTtoDETP(ARTrec1) AND
              DETPrec1.posspred = true AND
              DETPrec1.possnietnp = true AND
              DETPrec1.mood = declxpmood AND
              CNrec1.posscomas * [count] <> [] AND
              NPrec1.actcomas * [count] <> [] AND
              NPrec1 = { posspred          : DETPrec1.posspred
                          definite          : DETPrec1.definite
                          possnietnp        : DETPrec1.possnietnp
                          mood              : DETPrec1.mood

                          number            : singular
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro

                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : enokNP
                         }  
            
              AND (NPrec1.cases = [nominative]) 
             
          A1:  @
         >

&

