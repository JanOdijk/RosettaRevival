              VARPREPPrec1.actsubcefs * PREPrec1.subcs <> [] 
          A1: VERBPPROPrec1 := VERBPPROPrec2;
              VERBPPROPrec1.adjuncts := VERBPPROPrec2.adjuncts - [benfactPP];
