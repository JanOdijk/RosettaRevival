% RULE DETPformation3  (* RDETPsuperdeixis should not apply to these DETPs.
                          Therefore this rule must be ordered after 
                          RDETPsuperdeixis! *)

< m1: QP{QPrec1}[mu1, head/I1::Q{Qrec1}, I2:mu2] 
>

< m : DETP{DETPrec1}[head/QP{QPrec2}[mu1, head/I1::Q{Qrec1}, I2:mu2] 
                    ]
>

   MATCHCONDITIONS
      <
       I1: (Qrec1.key <> KEY_meestqkey) (* wordt nooit DETP *) AND
           (Qrec1.key <> KEY_meest2qkey) (* DETPformation3a *) 
            (* Qrec1.posspred = true 
              zou uitsluiten `veel` en `weinig`, (die worden al DETP
              via DETPformation1); probleem: 'veel' als amountmod in 'veel 
              mooier' *)
       I2: ALL(mu2, thanascomplrel/..)
       >


      COMP
         <
          C1: true
              C11: singular IN QPrec1.possnumbers AND
                   mass IN QPrec1.posscomas
              A11: DETPrec1:= 
                         {
                          possnumbers :  [singular]
                          posscomas   :  [mass]
                         };
              C12: plural IN QPrec1.possnumbers AND
                   count IN QPrec1.posscomas
              A12: DETPrec1:= 
                         {
                          possnumbers :  [plural]
                          posscomas   :  [count]
                         };
                       
          A1: DETPrec1 :=
                 { possnietnp  :  true (* ?? *)
                   definite    :  QPrec1.definite
                   posspred    :  QPrec1.posspred
                   mood        :  QPrec1.mood
                   superdeixis :  QPrec1.superdeixis
                   possgenders :  [omegagender]
                 };

                QPrec2 := QPrec1;
                QPrec2.superdeixis := omegadeixis;
         >

      DECOMP
         <
          C1: DETPrec1 = 
                 { possnietnp  :  true (* ?? *)
                   definite    :  QPrec2.definite
                   posspred    :  QPrec2.posspred
                   mood        :  QPrec2.mood
                   possgenders :  [omegagender]
                 } AND
               DETPrec1.superdeixis <> omegadeixis AND
               QPrec2.superdeixis = omegadeixis
              C11: singular IN QPrec2.possnumbers AND
                   mass IN QPrec2.posscomas AND
                   DETPrec1.possnumbers * [singular] <> [] AND
                   DETPrec1.posscomas * [mass] <> [] 
              A11: @
              C12: plural IN QPrec2.possnumbers AND
                   count IN QPrec2.posscomas AND
                   DETPrec1.possnumbers * [plural] <> [] AND
                   DETPrec1.posscomas * [count] <> [] 
              A12: @
          A1:  QPrec1 := QPrec2;
               QPrec1.superdeixis := DETPrec1.superdeixis;
         >
&

\end{verbatim}
