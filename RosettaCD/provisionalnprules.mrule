\begin{verbatim}

% RULE NPformation5     

< m1: CN{CNrec1}[mu1,
                 head/NOUN{NOUNrec1}[mu2],
                 mu3]
>

< m:  NP{NPrec1}[detrel/DETP{DETPrec1}[head/T1],
                 head/CN{CNrec1}[mu1,
                                 head/NOUN{NOUNrec1}[mu2],
                                 mu3]
                ]
>

<
(* 1 *)
SUBRULE
< >
<T1:ART(AUX_deartkey){ARTrec1}>

      COMP
         <
          C1: (NOUNrec1.genders * [mascgender,femgender] <>[]) 
                                                       (* see under remarks *)
              OR (NOUNrec1.number = plural)
          A1: DETPrec1 := COPYT_ARTtoDETP(ARTrec1);
                         (* .req hasn't been determined yet;
                            the other attributes have default value *)

              NPrec1 :=  COPYT_CNtoNP(CNrec1);
              NPrec1.definite := def
                         (* .req hasn't been determined yet;
                            the other attributes have default value *)
         >

      DECOMP
         <
          C1: ((NOUNrec1.genders * [mascgender,femgender] <>[]) 
                                                       (* see under remarks *)
              OR (NOUNrec1.number = plural))

              AND ( DETPrec1  = COPYT_ARTtoDETP(ARTrec1)) 

              AND (NPrec1  =  COPYT_CNtoNP(CNrec1) EXCEPT FOR [definite])
             
              AND 
              (NPrec1.definite = def)
          A1:  @
         >


(* 2 *)
SUBRULE
< >
<T1:ART(AUX_hetartkey){ARTrec1}>

      COMP
         <
          C1: (NOUNrec1.genders = [neutgender])
              AND (NOUNrec1.number = singular)
          A1: DETPrec1 := COPYT_ARTtoDETP(ARTrec1);
                    (* .req hasn't been determined yet;
                       the other attributes have default value *)

              NPrec1 :=  COPYT_CNtoNP(CNrec1)
                         (* .req hasn't been determined yet;
                            the other attributes have default value *)
         >

      DECOMP
         <
          C1: (NOUNrec1.genders = [neutgender])
              AND (NOUNrec1.number = singular)

              AND (DETPrec1 = COPYT_ARTtoDETP(ARTrec1))

              AND (NPrec1  =  COPYT_CNtoNP(CNrec1))

          A1:  @
         >
>
&


\end{verbatim}
