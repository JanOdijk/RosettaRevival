%
% RULE RNPpartitiveformation1    (* DETP + () + NOUN  --> 
                                    DETP van de/het + [() + NOUN] *)

< m1: CN{CNrec1}[mu1,
                 head/NOUN{NOUNrec1}[mu2],
                 I1:mu3]

  m2: DETP{DETPrec1}[mu4]
>

< m:  NP{NPrec1}[detrel/DETP{DETPrec3}[modrel/DETP{DETPrec4}[mu4],
                                       partrel/PREP(KEY_vanprepkey){PREPrec1},
                                       head/DETP{DETPrec2}[head/T1]
                                      ],
                 head/CN{CNrec1}[mu1,
                                 head/NOUN{NOUNrec1}[mu2],
                                 mu3]
                ]
>
   MATCHCONDITIONS
      <
       I1: NOT EXIST(mu3, [posrel/NP{NPrec2}[mu9]])
      >

<
(* 1 *)
SUBRULE
< >
<T1:ART(KEY_deartkey){ARTrec1}>

      COMP
         <
          C1: ((NOUNrec1.genders * [mascgender,femgender] <>[]) 
                                                       (* see under remarks *)
              OR (NOUNrec1.number = plural) ) AND
              (DETPrec1.superdeixis = CNrec1.superdeixis)
          A1: DETPrec2 := COPYT_ARTtoDETP(ARTrec1);
              DETPrec2.posspred := true;
              DETPrec3 := 
                         { possnietnp  :  DETPrec1.possnietnp
                           definite    :  adef
                           posspred    :  DETPrec1.posspred
                           possnumbers :  DETPrec2.possnumbers 
                                            (* om een van de aan te kunnen 
                                               is extra atribuut nodig *)
                           posscomas   :  DETPrec1.posscomas
                           mood        :  DETPrec1.mood
                           eORenform   :  Noform
                           superdeixis :  omegadeixis
                           };
              NPrec1 := { posspred          : DETPrec3.posspred
                          definite          : DETPrec3.definite
                          possnietnp        : DETPrec3.possnietnp
                          mood              : DETPrec3.mood

                          number            : NOUNrec1.number
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                         };
             DETPrec4 := DETprec1;
             DETPrec4.superdeixis := omegadeixis;
         >

      DECOMP
         <
          C1: ((NOUNrec1.genders * [mascgender,femgender] <>[]) 
                                                       (* see under remarks *)
              OR (NOUNrec1.number = plural)) AND 

              DETPrec2  = COPYT_ARTtoDETP(ARTrec1)  AND
              DETPrec2.posspred = true AND
              DETPrec3 = 
                         { possnietnp  :  DETPrec4.possnietnp
                           definite    :  adef
                           posspred    :  DETPrec4.posspred
                           possnumbers :  DETPrec2.possnumbers 
                                            (* om een van de aan te kunnen 
                                               is extra atribuut nodig *)
                           posscomas   :  DETPrec4.posscomas
                           mood        :  DETPrec4.mood
                           eORenform   :  Noform
                           superdeixis :  omegadeixis
                           }
               AND
               NPrec1 = { posspred          : DETPrec3.posspred
                          definite          : DETPrec3.definite
                          possnietnp        : DETPrec3.possnietnp
                          mood              : DETPrec3.mood

                          number            : NOUNrec1.number
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                         }  

              AND (NPrec1.cases = [nominative]) AND
              (DETPrec4.superdeixis = omegadeixis)
             
          A1:  DETPrec1 := DETPrec4;
               DETPrec1.superdeixis := CNrec1.superdeixis;
         >

(* 2 *)
SUBRULE
< >
<T1:ART(KEY_hetartkey){ARTrec1}>

      COMP
         <
          C1: (NOUNrec1.genders = [neutgender])
              AND (NOUNrec1.number = singular)
              AND (DETPrec1.superdeixis = CNrec1.superdeixis)
          A1: DETPrec2 := COPYT_ARTtoDETP(ARTrec1);
              DETPrec3 := 
                         { possnietnp  :  DETPrec1.possnietnp
                           definite    :  adef
                           posspred    :  DETPrec1.posspred
                           possnumbers :  DETPrec2.possnumbers 
                                            (* om een van de aan te kunnen 
                                               is extra atribuut nodig *)
                           posscomas   :  DETPrec1.posscomas
                           mood        :  DETPrec1.mood
                           eORenform   :  DETPrec2.eORenform
                           };
               NPrec1 := { posspred          : DETPrec1.posspred
                          definite          : DETPrec1.definite
                          possnietnp        : DETPrec1.possnietnp
                          mood              : DETPrec1.mood

                          number            : singular
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                         };
               DETPrec4 := DETPrec1;
               DETPrec4.superdeixis := omegadeixis;
         >

      DECOMP
         <
          C1: (NOUNrec1.genders = [neutgender])
              AND (NOUNrec1.number = singular)
              AND (DETPrec2 = COPYT_ARTtoDETP(ARTrec1))
              AND
              DETPrec3 = 
                         { possnietnp  :  DETPrec4.possnietnp
                           definite    :  adef
                           posspred    :  DETPrec4.posspred
                           possnumbers :  DETPrec2.possnumbers 
                                            (* om een van de aan te kunnen 
                                               is extra atribuut nodig *)
                           posscomas   :  DETPrec4.posscomas
                           mood        :  DETPrec4.mood
                           eORenform   :  DETPrec2.eORenform
                           }
               AND
               NPrec1 = { posspred          : DETPrec4.posspred
                          definite          : DETPrec4.definite
                          possnietnp        : DETPrec4.possnietnp
                          mood              : DETPrec4.mood

                          number            : singular
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                         }  
              AND (NPrec1.cases = [nominative]) 
              AND (DETPrec4.superdeixis = omegadeixis)
          A1:  DETPrec1 := DETPrec4;
               DETPrec1.superdeixis := CNrec1.superdeixis;
         >
>
&

