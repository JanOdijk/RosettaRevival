% TRANSFORMATION tsubstspeed
< m1:CLAUSE{CLAUSErec1}[mu1, predrel/VERBP{VERBPrec1}[mu2], mu3]
>

< m :CLAUSE{CLAUSErec1}[mu1, predrel/VERBP{VERBPrec1}[mu2], mu3]
>


   MATCHCONDITIONS
      <
       m : (EXIST(mu1, [subjrel/I11::NP{NPrec1}[mu4]])            ->   
            (
             (CLAUSErec1.synvpefs * AUX_subjidvps <> []    )   OR
             (CLAUSERec1.thetavp IN [vp010, vp012] AND
              CLAUSErec1.synvpefs * AUX_Raisingvps <> []
             )                                                 OR
             (EXIST(mu1,[I13:rel3/ADVP{ADVPrec1}[mu7]]))       OR       
             ((AUX_objidvps + AUX_closedpredvps) *
                   CLAUSErec1.synvpefs <> []           AND
              (CLAUSErec1.voice = Passive           OR 
               CLAUSErec1.thetavp IN [vp010, vp012]
              )                                             AND
              NOT EXIST(mu2, [objrel/..])
             )
            )
           )                                                          AND
           (EXIST(mu2, [objrel/I11::NP{NPrec1}[mu4]])             ->
            ((AUX_objidvps+ AUX_closedpredvps) * 
                          CLAUSErec1.synvpefs  <>[])
           )                                                          AND
           (EXIST(mu2, [indobjrel/I11::NP{NPrec1}[mu4]])          ->
            CLAUSErec1.synvpefs * AUX_indobjidvps <>[]    
           )                                                          AND

           NOT EXIST(mu1, [preadvrel/NP{}[mu4]])
                                                                      AND

           (
            EXIST(mu2, [locadvrel/PREPP{}[mu4]])                  ->
            CLAUSErec1.synvpefs * AUX_locadvidvps <> []
           )                                                          AND
           (EXIST(mu2, [prepobjrel/PREPP{}[I12:mu5 ]])           ->
            (((AUX_poidvps + AUX_popredvps) 
                        * CLAUSErec1.synvpefs  <> []
             )                                          OR
             (AUX_VRvps * CLAUSErec1.synvpefs <> []
             )
            )
           )
      I12: EXIST(mu5, [objrel/I11::NP{NPrec1}[mu6 ]]) 
      I11: NOT(NPrec1.NPhead IN AUX_specialNPs)
      I13: rel3 IN [subjvpadvrel, sentadvrel] AND
           [synOPENINFSENT,synCLOSEDINFSENT] * ADVPrec1.advpatternefs <> []
      >

      COMP
         <
          C1: true
          A1: @
         >
      DECOMP
         <
          C1: true
          A1: @
         >


&

\end{verbatim}
