% TRANSFORMATION TOblControl6b
                       (* SENTENCE, subject antecedent *)
< m1:CLAUSE{CLAUSErec1}
           [ mu1,
             subjrel/T1,
             mu2,
             predrel/VERBP{VERBPrec1}
                    [ mu3,
                      I2:complrel/SENTENCE{SENTENCErec1}
                                 [ I3:sig1,
                                   subjrel/T2,
                                   I4:mu4 
                                 ],
                      rel4/VERB{VERBrec1}[mu6],
                      mu7 
                    ],
              mu8 
           ]
>

< m :CLAUSE{CLAUSErec1}
           [ mu1,
             subjrel/T1,
             mu2,
             predrel/VERBP{VERBPrec1}
                    [ mu3,
                      I2:complrel/SENTENCE{SENTENCErec1}
                                 [ I3:sig1,
                                   I4:mu4 
                                 ],
                      rel4/VERB{VERBrec1}[mu6],
                      mu7 
                    ],
              mu8 
           ]
>

   MATCHCONDITIONS
      <
       I2: SENTENCErec1.PROSubject = true
       I3: ALL(sig1, shiftrel/..)
       I4: NOT EXIST(mu4, [shiftrel/..])
      >

<
   SUBRULE
      <
       T1:NPVAR{NPVARrec1}
       T2:NPVAR{NPVARrec2}
      >
      <T1:NPVAR{NPVARrec1}>

      COMP
         <
          C1: (VERBrec1.controller = subject) AND
              (NPVARrec1 = NPVARrec2 EXCEPT FOR [cases] ) AND
              (NPVARrec2.cases = [Nominative])
          A1: @
         >
      DECOMP
         <
          C1: (VERBrec1.controller = subject) 
          A1: NPVARrec2 := NPVARrec1;
              NPVARrec2.cases := [Nominative]
         >


   SUBRULE
      <
       T1:CNVAR{CNVARrec1}
       T2:CNVAR{CNVARrec2}
      >
      <T1:CNVAR{CNVARrec1}>

      COMP
         <
          C1: (VERBrec1.controller = subject ) AND
              (CNVARrec1 = CNVARrec2 EXCEPT FOR [cases] ) AND
              (CNVARrec2.cases = [Nominative])
          A1: @
         >
      DECOMP
         <
          C1: (VERBrec1.controller = subject )
          A1: CNVARrec2 := CNVARrec1;
              CNVARrec2.cases := [Nominative]
         >
>

&

