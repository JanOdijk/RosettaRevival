%
% RULE RADJSENTENCESUBSTITUTION1   (*substitution of non-subject sentencevars, 
                                     exluding omteinf-complements *)
< m1: ADJPPROP{ADJPPROPrec1}
               [mu1,
                predrel/ADJP{ADJPrec1}
                       [mu2,
                        I2:rel1/SENTENCEVAR{SENTENCEVARrec1},
                        mu3
                       ] 
               ]
SUBST:
m2: SENTENCE{SENTENCErec1}[mu6]          (* substituent *)
>

< m : ADJPPROP{ADJPPROPrec1}
               [mu1,
                predrel/ADJP{ADJPrec1}
                       [mu2,
                        I1:rel1/SENTENCE{SENTENCErec1}[mu6],
                        mu3
                       ] 
               ]
>
   MATCHCONDITIONS
      <
       I1: rel1 IN [complrel, sentadvrel] AND
           (SENTENCErec1.infsort = teinf -> rel1 =  complrel) AND
           (SENTENCErec1.infsort = omteinf -> rel1 <>  complrel)
       I2: rel1 IN [complrel, sentadvrel] AND
           (SENTENCEVARrec1.infsort = teinf -> rel1 =  complrel) AND
           (SENTENCEVARrec1.infsort = omteinf -> rel1 <>  complrel)

      >

   PARAMETERS
      <
       LEVEL
      >

      COMP
         <
          C1: SENTENCEVARrec1.index = LEVEL AND
              SENTENCEVARrec1 = COPYT_sentencetovar(SENTENCErec1) 
              C11: rel1 = complrel AND 
                   SENTENCErec1.infsort <> teinf AND
                   ADJPrec1.adjpatternefs * [synnoadjpargs] <> [] 
                 (* sluit ook "subjectzinnen" uit die met "het" in 
                  subjrel als complement optreden *)
              A11: @
              C12: rel1 = complrel AND
                   SENTENCErec1.infsort = teinf AND
                   ADJPrec1.adjpatternefs * AUX_teinfcompladjps <> []
              A12: @
              C13: rel1 <> complrel
              A13: @
          A1: @
         >
      DECOMP
         <
          C1: true
              C11: rel1 = complrel AND 
                   SENTENCErec1.infsort <> teinf AND
                   ADJPrec1.adjpatternefs * [synnoadjpargs] <> [] 
              A11: @
              C12: rel1 = complrel AND
                   SENTENCErec1.infsort = teinf AND
                   ADJPrec1.adjpatternefs * AUX_teinfcompladjps <> []
              A12: @
              C13: rel1 <> complrel
              A13: @
          A1: SENTENCEVARrec1 := COPYT_sentencetovar(SENTENCErec1);
              SENTENCEVARrec1.index := LEVEL;
         >


&

\end{verbatim}
