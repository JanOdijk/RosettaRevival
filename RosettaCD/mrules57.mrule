\newpage
\begin{verbatim}
% TRANSFORMATION Zichspelling1   (* reflexive, subject antecedent, NPVAR *)

< m1:VERBPPROP{VERBPPROPrec1}[ mu10,
                               subjrel/NPVAR{NPVARrec1},
                               mu1,
                               predrel/VERBP{VERBPrec1}
                                        [ mu2,
                                          head/I1::VERB{VERBrec1}[mu3]
                                        ],
                               mu11
                             ]
>

< m :VERBPPROP{VERBPPROPrec1}[ mu10,
                               subjrel/NPVAR{NPVARrec1},
                               reflrel/T1,
                               mu1,
                               predrel/VERBP{VERBPrec1}
                                        [ mu2,
                                          head/I1::VERB{VERBrec1}[mu3]
                                        ],
                               mu11
                             ]
>
   MATCHCONDITIONS
      <
       I1:VERBrec1.reflexivity = reflexive
      >
<
   SUBRULE (*me*)
      < >
      <T1:REFLPRO(KEY_mereflprokey){REFLPROrec1} >
      COMP
         <
          C1: NPVARrec1=
                { number: singular
	          person: 1
		}
          A1: @
         >
      DECOMP
         <
          C1: NPVARrec1=
                { number: singular
	          person: 1
		}
          A1: @
         >

   SUBRULE  (*mij*)
      < >
      <T1: REFLPRO(KEY_mijreflprokey){REFLPROrec1}>
    
           (* no COMP part *)
 
      DECOMP
         <
          C1: NPVARrec1=
                { number: singular
	          person: 1
		}
          A1: @
         >

   SUBRULE (*je*)
      < >
      <T1: REFLPRO(KEY_jereflprokey){REFLPROrec1} >
      COMP
         <
          C1: NPVARrec1.person = 2
          A1: @
         >
      DECOMP
         <
          C1: NPVARrec1.person = 2
          A1: @
         >
(*
   Removed, at least temporarily, JO.
   SUBRULE jou
      < >
      <T1:REFLPRO(KEY_joureflprokey){REFLPROrec1} >
         
           no COMP part 

      DECOMP
         <
          C1: NPVARrec1 =
                { number : singular
                  person : 2
                }
          A1: @
         >
*)

   SUBRULE (*zich*)
      < >
      < T1: REFLPRO(KEY_zichreflprokey){REFLPROrec1} >
      COMP
         <
          C1: (NPVARrec1.person = 3) OR (NPVARrec1.person = 4 )
          A1: @
         >
      DECOMP
         <
          C1: (NPVARrec1.person = 3) OR (NPVARrec1.person = 4 )
          A1: @
         >

   SUBRULE (*ons*)
      < >
      < T1: REFLPRO(KEY_onsreflprokey){REFLPROrec1} >
      COMP
         <
          C1: NPVARrec1 =
