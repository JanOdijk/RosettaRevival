\begin{verbatim}
%
% RULE RActive
< m1:I1::VERBPPROP{VERBPPROPrec1}
                           [ mu1,
                             predrel/VERBP{VERBPrec1}
                                    [ mu2,
                                      head/I2::VERB{VERBrec1}[ mu3 ]
                                    ],
                             mu4 
                           ]
>

< m :I3::VERBPPROP{VERBPPROPrec2}
                           [ mu1,
                             predrel/VERBP{VERBPrec1}
                                    [ mu2,
                                      head/I2::VERB{VERBrec1}[ mu3 ]
                                    ],
                             mu4 
                           ]
>


   MATCHCONDITIONS
      <
       I1: VERBPPROPrec1.voice = omegavoice
       I2: VERBrec1.modus = Omegamodus
       I3: VERBPPROPrec2.voice = active
      >

   PARAMETERS
      <
       voicepar:voicetype
      >

      COMP
         <
          C1: ((voicepar = active) AND (active IN VERBrec1.possvoices))   OR
              ((voicepar = passive) AND NOT(passive IN VERBrec1.possvoices))
          A1: VERBPPROPrec2 := VERBPPROPrec1;
              VERBPPROPrec2.voice :=  active;
         >
      DECOMP
         <
          C1: (active IN VERBrec1.possvoices)
          A1: VERBPPROPrec1 := VERBPPROPrec2;
              VERBPPROPrec1.voice := omegavoice;
              voicepar := active 
         >


&

\end{verbatim}

