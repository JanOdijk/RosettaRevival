% RULE DETPformation4
< m1: DEMADJ{DEMADJrec1} 
>

< m : DETP{DETPrec1}[head/DEMADJ{DEMADJrec1}]
>

      COMP
         <
          C1: true
            C2: (DEMADJrec1.key = KEY_ditDEMADJkey) OR 
                (DEMADJrec1.key = KEY_datDEMADJkey)
            A2: DETPrec1.possnumbers := [singular];
                DETPrec1.eORenForm := Noform;
                DETPrec1.possgenders := [neutgender];
            C2: (DEMADJrec1.key = KEY_dieDEMADJkey) OR 
                (DEMADJrec1.key = KEY_dezeDEMADJkey)
            A2: DETPrec1.possnumbers := [singular,plural];
                DETPrec1.eORenForm := eForm;
                DETPrec1.possgenders := [mascgender, femgender];
          A1: DETPrec1.definite := def;
              DETPrec1.posspred := true;
              DETPrec1.posscomas := [mass, count];

              (* .possnietnp and .mood have default value *)
         >

      DECOMP
         <
          C1: (DETPrec1.definite = def) AND
              (DETPrec1.posspred = true) AND
              (DETPrec1.possnietnp = false) AND
              (DETPrec1.posscomas = [mass, count]) AND
              (DETPrec1.mood = declxpmood) 
            C2: ((DEMADJrec1.key = KEY_ditDEMADJkey) OR 
                 (DEMADJrec1.key = KEY_datDEMADJkey)) AND
                (DETPrec1.possnumbers = [singular]) AND
                (DETPrec1.eORenForm = Noform) AND
                DETPrec1.possgenders = [neutgender] 
            A2:  @
            C2: ((DEMADJrec1.key = KEY_dieDEMADJkey) OR 
                 (DEMADJrec1.key = KEY_dezeDEMADJkey)) AND
                (DETPrec1.possnumbers = [singular,plural]) AND
                (DETPrec1.eORenForm = eForm) AND
                (DETPrec1.possgenders = [mascgender, femgender])
            A2:  @
