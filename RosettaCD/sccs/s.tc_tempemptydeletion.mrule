\begin{verbatim}
%
% FILTER Frefdeletion
< m: SENTENCE{SENTENCErec1}
              I1: [I2: mu1,
                   tempadvrel/ADVP{ADVPrec1}
                             [head/ADV{ADVrec1}
                                  [head/SUBADV{SUBADVrec1}
                                       [head/T1]
                                  ] 
                              ],
                    I3: mu2 
                   ]
>

   MATCHCONDITIONS
      <
       I1: RIGHTTOLEFT
       I2: (ALL(mu1, I4: rel1/..)) AND
           (QUOTE_temprefnotfound(mu1))
       I3: (NOT EXIST (mu2, [I4: rel1/..])) AND
           (QUOTE_temprefnotfound(mu2))
       I4: rel1 IN [leftdislocrel, shiftrel, subjrel, hetclrel, firstclrel, 
secondclrel, reflrel, erposrel, erqrel, defRadvrel, preadvrel, conjrel]
      >

<
   SUBRULE (* presrefempty *)
      < T1: BADV(AUX_presrefemptykey){BADVrec1} >

         <
          C1: (ADVPrec1 = AUX_advpONprsrfempty) AND
              (ADVrec1 = AUX_advONprsrfempty) AND
              (SUBADVrec1 = COPYT_badvtosubadv(BADVrec1)) AND
              (SUBADVrec1.lastaffix = noaffix) AND
              (SENTENCErec1.deixis = presentdeixis)
          A1: @
         >

   SUBRULE (* pastrefempty *)
      < T1: BADV(AUX_pastrefemptykey){BADVrec1} >

         <
          C1: (ADVPrec1 = AUX_advpONpstrfempty) AND
              (ADVrec1 = AUX_advONpstrfempty) AND
              (SUBADVrec1 = COPYT_badvtosubadv(BADVrec1)) AND
              (SUBADVrec1.lastaffix = noaffix) AND
              (SENTENCErec1.deixis = pastdeixis)
          A1: @
         >

>
&

\end{verbatim}
