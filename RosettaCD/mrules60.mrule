% TRANSFORMATION TParticle1
< m1:I1::SUBVERB{SUBVERBrec1}[mu1]
>

< m :SUBVERB{SUBVERBrec2}
                [ partrel/PART(SUBVERBrec1.particle){PARTrec1},
                  head/I1::SUBVERB{SUBVERBrec1}[mu1]
                ]
>

   MATCHCONDITIONS
      <
       I1: SUBVERBrec1.particle <> 0
      >

      COMP
         <
          C1: true
          A1: SUBVERBrec2 := SUBVERBrec1;
              SUBVERBrec2.lastaffix := partaffix;
         >
      DECOMP
         <
          C1: SUBVERBrec2 = SUBVERBrec1 EXCEPT FOR [lastaffix] AND
              SUBVERBrec2.lastaffix = partaffix AND
              PARTrec1.key = SUBVERBrec1.particle
          A1: @
         >

&

