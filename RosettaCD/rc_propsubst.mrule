              [ mu1,
                predrel/VERBP{VERBPrec1}
                       [ mu2,
                         complrel/OPENADJPPROP{OPENADJPPROPrec1}[mu4],
                         mu3 
                       ],
                mu8
              ] 
>

   MATCHCONDITIONS
      <
       m: NOT EXIST(mu3,[I11:../T1]) AND
          NOT EXIST(mu8,[I11:../T1])
          I11: T1.CAT IN AUX_propvarcats
      >


   PARAMETERS
      <
       LEVEL
      >



      COMP
         <
          C1:  ADJPPROPVARrec1.index = LEVEL 
	       C11: ADJPPROPVARrec1 = 
                    COPYT_ADJPPROPtoVAR(OPENADJPPROPrec1) (* !! *)
	       A11: @
          A1: @
         >
      DECOMP
         <
          C1: true
	       C11: true
	       A11: ADJPPROPVARrec1 := 
                    COPYT_ADJPPROPtoVAR(OPENADJPPROPrec1) 
          A1: ADJPPROPVARrec1.index := LEVEL
         >

&

\end{verbatim}

