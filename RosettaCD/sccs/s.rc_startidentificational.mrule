\begin{verbatim}
% RULE Ridentformation1
< m1: I3::NPVAR{NPVARrec1}
  m2: I1::NP{NPrec1}[mu1] 
>

< m :NPPROP{NPPROPrec1}
           [ subjrel/I2::NP{NPrec2}[mu1],
             idrel/I3::NPVAR{NPVARrec1} 
           ]
>

   MATCHCONDITIONS
      <
       I1: NPrec1.NPhead IN [hetNP, datNP, ditNP]
       I2: NPrec2.NPhead IN [hetNP, datNP, ditNP]
       I3: NOT (NPVARrec1.NPhead IN [watwhproNP, persproNP] ) 
      >

      COMP
         <
          C1: NPrec1.number = singular AND
              NPrec1.superdeixis = presentdeixis AND
              NPrec1.generic = nogeneric
          A1: NPPROPrec1.aktionsarts := [stative];
              (* all other attr. of NPPROP 
               retain their default values thetavp=omegathetanp *)
              NPrec2 := NPrec1;
              NPrec2.number := NPVARrec1.number;
              NPrec2.superdeixis := omegadeixis;
              NPrec2.generic := omegageneric;
         >
      DECOMP
         <
          C1: (NPPROPrec1 = NPPROPrecdef EXCEPT FOR [aktionsarts]) AND
               NPPROPrec1.aktionsarts = [stative] AND
              (NPrec2.number = NPVARrec1.number ) AND
              NPrec2.superdeixis = omegadeixis AND
              NPrec2.generic = omegageneric
          A1: NPrec1 := NPrec2;
              NPrec1.number := singular;
              NPrec1.superdeixis := presentdeixis;
              NPrec1.generic := nogeneric;
         >

&

\end{verbatim}

