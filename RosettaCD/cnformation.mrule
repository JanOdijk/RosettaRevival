          A1: NPrec1 := NPrec2;
              NPrec1.superdeixis := CNrec4.superdeixis;
              NPrec1.cases := [nominative];
              NPrec1.generic := nogeneric;
              CNrec1 := CNrec4;
              CNrec2 := CNrec3;
              CNrec2.cases := [nominative];
         >


&

