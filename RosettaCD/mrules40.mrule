% TRANSFORMATION TADJCONTROL1    (* subject antecedent *)
                                    (* x1 om te verzuipen bang *)
< m1: ADJPFORMULA{ADJPFORMULArec1}
                 [I4:subjrel/T1,
                  predrel/ADJP{ADJPrec1}
                          [mu2,
                           complrel/I1::SENTENCE{SENTENCErec1}
                                       [I2:sig1,
                                        I6:sig2,
                                        I3:subjrel/T2,
                                        I5:mu3
                                       ],
                           head/ADJ{ADJrec1}[mu4],
                           mu5
                          ],
                  mu7
                 ]
>

< m : ADJPFORMULA{ADJPFORMULArec1}
                 [I4:subjrel/T1,
                  predrel/ADJP{ADJPrec1}
                          [mu2,
                           complrel/I1::SENTENCE{SENTENCErec1}
                                       [I2:sig1,
                                        I6:sig2,
                                        I5:mu3
                                       ],
                           head/ADJ{ADJrec1}[mu4],
                           mu5
                          ],
                  mu7
                 ]
>     

   MATCHCONDITIONS
      <I1: SENTENCErec1.prosubject = true
       I2: ALL(sig1, shiftrel/..)
       I3: T2.cat IN [NPVAR, CNVAR]
       I4: T1.cat IN [NPVAR, CNVAR, EMPTYVAR]
       I5: NOT EXIST(mu3, [shiftrel/..]) AND
           NOT EXIST(mu3, [conjrel/..])
       I6: ALL(sig2, conjrel/..)

      >

<
   SUBRULE  (* 1 *)
      <T1:NPVAR{NPVARrec1}
       T2:NPVAR{NPVARrec2}
      >
      <T1:NPVAR{NPVARrec1}>

      COMP
         <
          C1: (NPVARrec1 = NPVARrec2 EXCEPT FOR [cases]) AND
              (NPVARrec2.cases = [Nominative])
          A1: @
         >
      DECOMP
         <
          C1: true
          A1: NPVARrec2 := NPVARrec1;
              NPVARrec2.cases := [Nominative]
         >

   SUBRULE (* 2 *)
      <T1:CNVAR{CNVARrec1}
       T2:CNVAR{CNVARrec2}
      >        
      <T1:CNVAR{CNVARrec1}>

      COMP
         <
          C1: (CNVARrec1 = CNVARrec2 EXCEPT FOR [cases]) AND
              (CNVARrec2.cases = [Nominative])
          A1: @
         >
      DECOMP
         <
          C1: true
          A1: CNVARrec2 := CNVARrec1;
              CNVARrec2.cases := [Nominative]
         >
>
&

\end{verbatim}
