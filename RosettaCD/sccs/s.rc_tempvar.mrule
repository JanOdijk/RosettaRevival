% RULE Rtempadvsubstitution1           (* temporal ADVP or
                                           frequential NP under S *)
< m1:CLAUSE{CLAUSErec1}
           [ mu1,
             I1: rel1/T2,
             mu2,
             predrel/VERBP{VERBPrec1}[mu3],
             mu4
           ] 
SUBST:
  m2: T1
>

< m :CLAUSE{CLAUSErec2}
           [ mu1,
             I2: rel1/T3,
             mu2,
             predrel/VERBP{VERBPrec1}[mu3],
             mu4
           ] 
>

   MATCHCONDITIONS
      <
       m1 : ALL
       m2 : ALL
            QUOTE_Gsubstordercondition(mu2, mu3, mu4, T1) AND
            T1.CAT IN [ADVP, NP]
       m  : ALL
            QUOTE_substordercondition(mu2, mu3, mu4) 
       I1 : rel1 IN [tempadvrel, shiftrel] AND
            (T2.CAT IN [ADVPVAR, NPVAR] WITH T2.REC.index = LEVEL )
       I2 : rel1 IN [tempadvrel, shiftrel] AND
            T3.CAT IN [ADVP, NP]
      >

   PARAMETERS
      <
       LEVEL
      >

<
   SUBRULE (* 1 ADVP *)
      <T1: ADVP{ADVPrec1}[mu5]
       T2: ADVPVAR{ADVPVARrec1}[] >
      <T3: ADVP{ADVPrec2}[mu5] >

      COMP
         <
          C1: (tempadv IN ADVPrec1.actsubcefs) AND
              ((ADVPrec1.class = reference) OR
               (ADVPrec1.class = frequential) OR
               ((ADVPrec1.class = duration) AND
                ((ADVPrec1.aspect = perfective) AND
                 (CLAUSErec1.aktionsarts * [stative, activity] <> [])
                ) OR
                ((ADVPrec1.aspect = imperfective) AND
                 (CLAUSErec1.aktionsarts * [accomplishment, achievement] <> [])
                )
               )
              ) AND
              QUOTE_deixismax(CLAUSErec1.deixis,
                          CLAUSErec1.superdeixis, ADVPrec1.superdeixis)
              C11: rel1 = shiftrel AND
                   ADVPVARrec1 = COPYT_advptovar(ADVPrec1) 
                                  EXCEPT FOR [mood] AND
                   ADVPrec1.mood = declxpmood AND
                   ADVPVARrec1.mood = topicxpmood
              A11: @
              C12: rel1 <> shiftrel AND
                   ADVPVARrec1 = COPYT_advptovar(ADVPrec1) AND
                   ADVPrec1.mood = declxpmood
              A12: @
              C13: ADVPrec1.mood = wh AND
                   ADVPVARrec1 = COPYT_advptovar(ADVPrec1) 
              A13: @       
          A1: ADVPrec2 := ADVPrec1;
              ADVPrec2.superdeixis := omegadeixis;
              CLAUSErec2 := CLAUSErec1;

         >
      DECOMP
         <
          C1: (tempadv IN ADVPrec2.actsubcefs) 
              C11:((ADVPrec2.class = reference) OR
                    (ADVPrec2.class = frequential)) 
                  C111: rel1 = shiftrel AND
                        ADVPrec2.mood = declxpmood
                  A111: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
                        ADVPVARrec1.mood := topicxpmood; 
                  C112: rel1 <> shiftrel AND
                        ADVPrec2.mood = declxpmood
                  A112: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
                  C113: ADVPrec2.mood = wh
                  A113: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
              A11:CLAUSErec1 := CLAUSErec2;
                  ADVPrec1 := ADVPrec2;
                  QUOTE_assdeixismax(CLAUSErec1.deixis,
                          CLAUSErec1.superdeixis, ADVPrec1.superdeixis);

              C12: (ADVPrec2.class = duration) 
                  C121: (ADVPrec2.aspect = perfective) 
                        C1211: stative IN CLAUSErec2.aktionsarts
			       C12111: rel1 = shiftrel AND
				       ADVPrec2.mood = declxpmood
			       A12111: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
				       ADVPVARrec1.mood := topicxpmood; 
			       C12112: rel1 <> shiftrel AND
				       ADVPrec2.mood = declxpmood
			       A12112: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
			       C12113: ADVPrec2.mood = wh
			       A12113: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
                        A1211: CLAUSErec1:= CLAUSErec2;
                               CLAUSErec1.aktionsarts := [stative];
                               ADVPrec1 := ADVPrec2;
                               QUOTE_assdeixismax(CLAUSErec1.deixis,
                                  CLAUSErec1.superdeixis, ADVPrec1.superdeixis);

                        C1212: activity IN CLAUSErec2.aktionsarts
			       C12121: rel1 = shiftrel AND
				       ADVPrec2.mood = declxpmood
			       A12121: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
				       ADVPVARrec1.mood := topicxpmood; 
			       C12122: rel1 <> shiftrel AND
				       ADVPrec2.mood = declxpmood
			       A12122: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
			       C12123: ADVPrec2.mood = wh
			       A12123: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
                        A1212: CLAUSErec1:= CLAUSErec2;
                               CLAUSErec1.aktionsarts := [activity];
                               ADVPrec1 := ADVPrec2;
                               QUOTE_assdeixismax(CLAUSErec1.deixis,
                                 CLAUSErec1.superdeixis, ADVPrec1.superdeixis);
                   A121: @

                   C122: (ADVPrec2.aspect = imperfective) 
                        C1221: accomplishment IN CLAUSErec2.aktionsarts
			       C12211: rel1 = shiftrel AND
				       ADVPrec2.mood = declxpmood
			       A12211: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
				       ADVPVARrec1.mood := topicxpmood; 
			       C12212: rel1 <> shiftrel AND
				       ADVPrec2.mood = declxpmood
			       A12212: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
			       C12213: ADVPrec2.mood = wh
			       A12213: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
                        A1221: CLAUSErec1:= CLAUSErec2;
                               CLAUSErec1.aktionsarts := [accomplishment];
                               ADVPrec1 := ADVPrec2;
                               QUOTE_assdeixismax(CLAUSErec1.deixis,
                                 CLAUSErec1.superdeixis, ADVPrec1.superdeixis);

                        C1222: achievement IN CLAUSErec2.aktionsarts
			       C12221: rel1 = shiftrel AND
				       ADVPrec2.mood = declxpmood
			       A12221: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
				       ADVPVARrec1.mood := topicxpmood; 
			       C12222: rel1 <> shiftrel AND
				       ADVPrec2.mood = declxpmood
			       A12222: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
			       C12223: ADVPrec2.mood = wh
			       A12223: ADVPVARrec1 := COPYT_advptovar(ADVPrec2);
                        A1222: CLAUSErec1:= CLAUSErec2;
                               CLAUSErec1.aktionsarts := [achievement];
                               ADVPrec1 := ADVPrec2;
                               QUOTE_assdeixismax(CLAUSErec1.deixis,
                                 CLAUSErec1.superdeixis, ADVPrec1.superdeixis);
                   A122: @
              A12: @ 

          A1: ADVPVARrec1.index := LEVEL;
         >

   SUBRULE (* 2 frequential NP *)
      <T1: NP{NPrec1}[mu5]
       T2: NPVAR{NPVARrec1}[] >
      <T3: NP{NPrec2}[mu6] >

      COMP
         <
          C1: (unitnoun IN NPrec1.actsubcs) AND
              (NPVARrec1 = COPYT_Nptovar(NPrec1)) AND
              (NPrec1.class = frequential) AND
              QUOTE_deixismax(CLAUSErec1.deixis,
                    CLAUSErec1.superdeixis, NPrec1.superdeixis)
              C11: NPVARrec1 = COPYT_nptovar(NPrec1) EXCEPT FOR [cases, mood] AND
                   (rel1 = shiftrel AND 
                    NPVARrec1.mood = topicxpmood AND
                    NPrec1.mood = declxpmood
                   )
              A11: QUOTE_assigncase(NPrec1,mu5,NPrec2,mu6,NPVARrec1.cases); 
              C12: NPVARrec1 = COPYT_nptovar(NPrec1) EXCEPT FOR [cases] AND
                   NPrec1.mood = declxpmood AND
                   rel1 <> shiftrel
              A12: QUOTE_assigncase(NPrec1,mu5,NPrec2,mu6,NPVARrec1.cases); 
              C13: NPVARrec1 = COPYT_nptovar(NPrec1) EXCEPT FOR [cases] AND
                   NPrec1.mood = wh 
              A13: QUOTE_assigncase(NPrec1,mu5,NPrec2,mu6,NPVARrec1.cases); 
          A1: NPrec2.superdeixis := omegadeixis;
              CLAUSErec2 := CLAUSErec1;
         >
      DECOMP
         <
          C1: (unitnoun IN NPrec2.actsubcs) AND
               (NPrec2.class = frequential) 
              C11: rel1 = shiftrel AND NPrec2.mood = declxpmood
              A11: QUOTE_assigncase(NPrec2,mu6,NPrec1,mu5,[Nominative]);
                   NPVARrec1 := COPYT_nptovar(NPrec2);
                   NPVARrec1.mood := topicxpmood;
                   NPrec1.generic := nogeneric;
                   NPVARrec1.generic := nogeneric;
              C12: NPrec2.mood = declxpmood AND
                   rel1 <> shiftrel
              A12: QUOTE_assigncase(NPrec2,mu6,NPrec1,mu5,[Nominative]); 
                   NPVARrec1 := COPYT_nptovar(NPrec2);
                   NPrec1.generic := nogeneric;
                   NPVARrec1.generic := nogeneric;
              C13: NPrec2.mood = wh
              A13: QUOTE_assigncase(NPrec2,mu6,NPrec1,mu5,[Nominative]); 
                   NPVARrec1 := COPYT_nptovar(NPrec2);
                   NPrec1.generic := nogeneric;
                   NPVARrec1.generic := nogeneric;
          A1: QUOTE_assdeixismax(CLAUSErec1.deixis,
                    CLAUSErec1.superdeixis, NPrec1.superdeixis);
              NPVARrec1.index := LEVEL;
              CLAUSErec1 := CLAUSErec2;

         >
