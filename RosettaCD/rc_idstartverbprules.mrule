RULE RIDSTARTVPPROP1VAR2ID0

<m1: I1::SUBVERB{SUBVERBREC1}[head/BVERB(KEY1){BVERBREC1}]
 m2: T1
>

<m: I4::VERBPPROP{VERBPPROPREC1}
       [ I2:subjrel/T1, 
         predrel/VERBP{VERBPREC1}
              [ objrel/NP{NPREC1}[detrel/DETP{DETPREC1}
                                       [head/ART(KEY4){ARTREC1}],
                                  head/CN{CNREC1}
                                       [head/NOUN{NOUNREC1}
                                           [head/SUBNOUN{SUBNOUNREC1}
                                               [head/BNOUN(KEY2){BNOUNREC1}]
                                           ]
                                        ]
                                  ],
                head/I5::VERB{VERBREC1}
                    [ head/SUBVERB{SUBVERBREC1}[head/BVERB(KEY3){BVERBREC2}]
                      ]
              ]
       ]
>

   MATCHCONDITIONS
      <
       I1: SUBVERBrec1.thetavp = vp120 
       I2: T1.CAT IN AUX_VARCATSET 
       m2: T1.CAT IN AUX_VARCATSET 
       I4: VERBPPROPrec1 =
		  { 
		   thetavp     : vp120
		   req         : [ pospol, negpol, omegapol]
		   env         : [ pospol, negpol, omegapol]
		   superdeixis : Omegadeixis
		   PROsubject  : false
		   voice       : omegavoice
		   synvpefs    : [synNP]
                   aktionsarts : []
		  }

       I5: VERBrec1 =
		  { 
                   thetavp      : vp120
		   number       : omeganumber   
		   persons      : []   
		   eORenForm    : NoForm   
		   modus        : omegamodus  
		   tense        : omegatense  
		  } AND
           VERBrec1.status IN [bareV, partV]
      >

COMP
 <
   C1: COMPINIDDICT(KEY1, VPID2)
      C11: SUBVERBrec1.lastaffix <> partaffix 
      A11: SUBVERBREC1 := COPYT_bverbtosubverb(BVERBREC2);
           VERBREC1 := COPYT_subverbtoverb(SUBVERBREC1);
           SUBNOUNREC1 := COPYT_bnountosubnoun(BNOUNREC1);
           NOUNREC1 := COPYT_subnountonoun(SUBNOUNREC1);
           CNREC1 := COPYT_nountocn(NOUNREC1);
           NPREC1 := COPYT_cntonp(CNREC1);
           NPREC1.definite:=def;
           NPREC1.superdeixis := omegadeixis;
           NPREC1.generic := omegageneric;
           NPREC1.specQ := yesspec;
           NPREC1.posspred := true;
           CNREC1.superdeixis := omegadeixis;           
           NOUNREC1.number := singular;
           VERBPPROPREC1.synvpefs := [synNP];
      C12: SUBVERBrec1.lastaffix = partaffix
      A12: SUBVERBREC1 := COPYT_bverbtosubverb(BVERBREC2);
           VERBREC1 := COPYT_subverbtoverb(SUBVERBREC1);
           SUBNOUNREC1 := COPYT_bnountosubnoun(BNOUNREC1);
           NOUNREC1 := COPYT_subnountonoun(SUBNOUNREC1);
           CNREC1 := COPYT_nountocn(NOUNREC1);
           NPREC1 := COPYT_cntonp(CNREC1);
           NPREC1.definite:=def;
           NPREC1.superdeixis := omegadeixis;
           NPREC1.generic := omegageneric;
           NPREC1.specQ := yesspec;
           NPREC1.posspred := true;
           CNREC1.superdeixis := omegadeixis;           
           VERBrec1.status := partV; 
           NOUNREC1.number := singular;
           VERBPPROPREC1.synvpefs := [synNP];
   A1: <KEY3,KEY4,KEY2> := COMPGETIDDICT(KEY1, VPID2);
       VERBPPROPrec1.thetavp := vp120;
 >
DECOMP
 <
   C1: DECOMPINIDDICT(<KEY3,KEY4,KEY2>, VPID2 )
      C11: SUBVERBREC1 = COPYT_bverbtosubverb(BVERBREC2) AND
           VERBREC1 = COPYT_subverbtoverb(SUBVERBREC1)   AND
           SUBNOUNREC1 = COPYT_bnountosubnoun(BNOUNREC1) AND
           NOUNREC1 = COPYT_subnountonoun(SUBNOUNREC1)   AND
           CNREC1 = COPYT_nountocn(NOUNREC1)             AND
           NPREC1 = COPYT_cntonp(CNREC1) 
                    EXCEPT FOR [specQ, NPhead, syntquant, possRform, generic,
                                mood, possnietnp, posspred, actcomas, number,
                                person, definite]        AND
           NPREC1.superdeixis = omegadeixis              AND
           NPREC1.generic = omegageneric                 AND
           NPREC1.specQ = yesspec                        AND
           NPREC1.posspred = true                        AND
           CNREC1.superdeixis = omegadeixis              AND
           NOUNREC1.number = singular                    AND
           VERBPPROPREC1.synvpefs = [synNP]              AND
           SUBVERBrec1.lastaffix = partaffix             AND
           VERBrec1.status = partV 
      A11: @
      C12: SUBVERBREC1 = COPYT_bverbtosubverb(BVERBREC2) AND
           VERBREC1 = COPYT_subverbtoverb(SUBVERBREC1)   AND
           SUBNOUNREC1 = COPYT_bnountosubnoun(BNOUNREC1) AND
           NOUNREC1 = COPYT_subnountonoun(SUBNOUNREC1)   AND
           CNREC1 = COPYT_nountocn(NOUNREC1)             AND
           NPREC1 = COPYT_cntonp(CNREC1) 
                    EXCEPT FOR [specQ, NPhead, syntquant, possRform, generic,
                                mood, possnietnp, posspred, actcomas, number,
                                person, definite]        AND
           NPREC1.superdeixis = omegadeixis              AND
           NPREC1.generic = omegageneric                 AND
           NPREC1.specQ = yesspec                        AND
           NPREC1.posspred = true                        AND
           CNREC1.superdeixis = omegadeixis              AND
           NOUNREC1.number = singular                    AND
           VERBPPROPREC1.synvpefs = [synNP]              AND
           SUBVERBrec1.lastaffix <> partaffix            AND
           VERBrec1.status = bareV 
      A12: @
   A1: WBONUS +5;
       KEY1 := DECOMPGETIDDICT(<KEY3,KEY4,KEY2>, VPID2) 
 >

&
\end{verbatim}

