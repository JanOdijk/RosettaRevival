\begin{verbatim}
% RULE RNPPsubst1
< m1: NPPROP{NPPROPrec1}
                 [mu1,
                  I1:rel1/I2::NPVAR{NPVARrec1},
                  mu2
                 ] 
SUBST:
  m2: T1=NP{NPrec1}[mu5]
>

< m : NPPROP{NPPROPrec1}
                 [mu1,
                  I1:rel1/T2=NP{NPrec2}[mu6],
                  mu2
                 ] 

>

   MATCHCONDITIONS
      <
       I2: NPVARrec1.index = LEVEL
       I1: rel1 IN [subjrel]
       
      >
   PARAMETERS
      <
       LEVEL
      >

      COMP
         <
          C1:NPPROPrec1.PROsubject =false AND
             NPVARrec1 = COPYT_nptovar(NPrec1) EXCEPT FOR [cases] AND
             NPrec1.cases = [Nominative] AND
             NPrec1.generic = nogeneric AND
             NPrec1.superdeixis = NPPROPrec1.superdeixis
          A1:QUOTE_assigncase(NPrec1,mu5,NPrec2,mu6,NPVARrec1.cases); 
             NPrec2.generic := omegageneric;
             NPrec2.superdeixis := omegadeixis;

         >
      DECOMP
         <
          C1: NPPROPrec1.PROsubject =false AND
              NPrec2.generic = omegageneric AND
              NPrec2.superdeixis = omegadeixis
          A1: QUOTE_assigncase(NPrec2,mu6,NPrec1,mu5,[Nominative]); 
              NPVARrec1 := COPYT_nptovar(NPrec2);
              NPVARrec1.index := LEVEL;
              NPrec1.generic := nogeneric;
              NPrec1.superdeixis := NPPROPrec1.superdeixis;
         >



&

\end{verbatim}
