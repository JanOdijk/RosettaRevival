          C1: NPrec2.actsubcs  * [unitnoun, plurunitnoun] <> [] AND
              NPrec2.generic = omegageneric AND
              NPrec2.superdeixis = omegadeixis AND
              accusative IN NPrec2.cases
          A1: QUOTE_Assigncase(NPrec2, mu4,NPrec1, mu3,[nominative]);
              NPrec1.superdeixis := PREPPrec1.superdeixis;
              NPrec1.generic := nogeneric;
         >


&

