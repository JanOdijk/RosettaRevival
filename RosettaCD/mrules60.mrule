\newpage
\begin{verbatim}
% RULE Rnoundim
< m1:CN{CNrec1}[mu2,head/NOUN{NOUNrec1}[head/SUBNOUN{SUBNOUNrec1}[mu1] ],mu3 ] 
>

< m :CN{CNrec2}[mu2,
                head/NOUN{NOUNrec2}
                 [head/SUBNOUN{SUBNOUNrec2}
                   [complrel/SUBNOUN{SUBNOUNrec1}[mu1], 
                    head/BNOUNSUFF(KEY_dimBNOUNSUFFkey){BNOUNSUFFrec1}
                   ] 
                 ],
                mu3
               ] 
>


      COMP
         <
          C1: SUBNOUNrec1.lastaffix <> dimaffix             AND
              SUBNOUNrec1.dimforms <> [noDim]               AND
              count IN SUBNOUNrec1.posscomas                AND
              NOUNrec1 = COPYT_subnounTOnoun(SUBNOUNrec1) 
                           EXCEPT FOR [number,geni]         AND
              CNrec1 = 
                  {                     
                   genders:           SUBNOUNrec1.genders
                   posscomas:         SUBNOUNrec1.posscomas
                  }
             C11: [unitnoun] * CNrec1.actsubcs = []
             A11: CNrec2 := CNrec1;
                  CNrec2 :=
                     {                     
                       genders:           BNOUNSUFFrec1.genders
                       posscomas:         [count]
                     };
             C12: [unitnoun] * CNrec1.actsubcs <> []
                  (* ons; cf. drie ons vs. drie onsjes; 
                              drie uur vs. drie uurtjes; FJ 18-10-90 *)
             A12: CNrec2 := CNrec1;
                  CNrec2 :=
                    {                     
                     genders:           BNOUNSUFFrec1.genders
                     posscomas:         [count]
                    };
                  CNrec2.actsubcs := 
                         CNrec1.actsubcs - [unitnoun] + [plurunitnoun];

          A1: SUBNOUNrec2 := SUBNOUNrec1;
              SUBNOUNrec2 :=
                  { 
                    lastaffix  :  dimaffix
                    genders    :  BNOUNSUFFrec1.genders
                    pluralforms:  BNOUNSUFFrec1.pluralforms
                    dimforms   :  BNOUNSUFFrec1.dimforms
                  };
              NOUNrec2 := COPYT_SUBNOUNtonoun(SUBNOUNrec2);
              NOUNrec2 :=
                  {
                   number:            NOUNrec1.number
                   geni:              NOUNrec1.geni
                  };
         >

      DECOMP
         <
          C1: SUBNOUNrec1.lastaffix <> dimaffix             AND
              SUBNOUNrec1.dimforms <> [noDim]               AND
              count IN SUBNOUNrec1.posscomas                AND
              NOUNrec2 = COPYT_subnounTOnoun(SUBNOUNrec2)  
                          EXCEPT FOR [number,geni]          AND
              SUBNOUNrec2 = 
                  { 
                    lastaffix  :  dimaffix
                    genders    :  BNOUNSUFFrec1.genders
                    pluralforms:  BNOUNSUFFrec1.pluralforms
                    dimforms   :  BNOUNSUFFrec1.dimforms
                  }                                        AND
              CNrec2 =
                  {                     
                   genders    :       BNOUNSUFFrec1.genders
                   posscomas  NI      count
                  }
          A1: NOUNrec1 := COPYT_subnounTOnoun(SUBNOUNrec1); 
              NOUNrec1 :=
                  {
                   number:            NOUNrec2.number
                   geni:              NOUNrec2.geni
                  };
              CNrec1 := CNrec2;
              CNrec1 :=
                  {                     
                   genders:           SUBNOUNrec1.genders
                   posscomas:         SUBNOUNrec1.posscomas
                  };
         >

&

\end{verbatim}
