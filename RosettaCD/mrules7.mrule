          I1:[ I2:mu1,
               conjrel/I5::VERB{VERBrec1}[mu5],
               I3:mu2,
               predrel/VERBP{VERBPrec1}
                   I8:[ I4:mu3,
                        I6:mu6 
                      ],
               mu7
             ]
>

   MATCHCONDITIONS
      <
       I1 : RIGHTTOLEFT
       I2 : ALL(mu1, I12:rel4/..)
       I3 : NOT EXIST(mu2, [conjrel/..])
       I4 : ALL(mu3, I14:rel3/..) AND
            NOT EXIST(mu3, [partrel/..])
       I5 : VERBrec1.status = bareV 
       I6 : ALL(mu6, I16:rel1/..)
       I7 : SENTENCErec1.finiteness = finite
       I8 : RIGHTTOLEFT
       I12: rel4 IN [leftdislocrel, shiftrel]
       I14: rel3 IN AUX_preVERBrels  
       I16: rel1 IN AUX_postverbrels
        
      >

      COMP
         <
          C1: true
          A1: @
         >
      DECOMP
         <
          C1: true
          A1: @
         >

&

