\begin{verbatim}
% RULE RdatumNP
< m1:PROPERNOUN{PROPERNOUNrec1}[mu1] 
>

< m :NP{NPrec1}[head/PROPERNOUN{PROPERNOUNrec1}[mu1]]
>



   PARAMETERS
      <
       superpar :deixistype
      >


      COMP
         <
          C1: PROPERNOUNrec1.subc = monthnoun
          A1: NPrec1 :=
               {
                   superdeixis:       superpar

                   class:             PROPERNOUNrec1.class
                   deixis:            PROPERNOUNrec1.deixis
                   aspect:            PROPERNOUNrec1.aspect
                   retro:             PROPERNOUNrec1.retro

                   definite:          def
                   person:            3
                   number:            singular
                   cases:             [nominative]
                   genders:           PROPERNOUNrec1.genders
                   sexes:             PROPERNOUNrec1.sexes
                   actsubcs:          [monthnoun]
                   temporal:          true
                   possgeni:          PROPERNOUNrec1.possgeni
                   animate:           PROPERNOUNrec1.animate
                   human:             PROPERNOUNrec1.human
                   actcomas:          [count]
                   thetanp:           omegathetanp
                   nounpatternefs:    []
                   posspred:          true
                   possnietnp:        false
                   mood:              declxpmood 
                   generic:           nogeneric
                   possRform:         false
                   syntquant:         omegaquant
                   NPhead:            otherNP
                   specQ:             yesspec 
                   coord:             nocoord    
               };
         >
      DECOMP
         <
          C1: PROPERNOUNrec1.subc = monthnoun AND
              NPrec1 =
               {

                   class:             PROPERNOUNrec1.class
                   deixis:            PROPERNOUNrec1.deixis
                   aspect:            PROPERNOUNrec1.aspect
                   retro:             PROPERNOUNrec1.retro

                   definite:          def
                   person:            3
                   number:            singular
                   cases:             [nominative]
                   genders:           PROPERNOUNrec1.genders
                   sexes:             PROPERNOUNrec1.sexes
                   actsubcs:          [monthnoun]
                   temporal:          true
                   possgeni:          PROPERNOUNrec1.possgeni
                   animate:           PROPERNOUNrec1.animate
                   human:             PROPERNOUNrec1.human
                   actcomas:          [count]
                   thetanp:           omegathetanp
                   nounpatternefs:    []
                   posspred:          true
                   possnietnp:        false
                   mood:              declxpmood 
                   generic:           nogeneric
                   possRform:         false
                   syntquant:         omegaquant
                   NPhead:            otherNP
                   specQ:             yesspec 
                   coord:             nocoord    
               }
          A1: superpar := NPrec1.superdeixis;
         >

&

\end{verbatim}
