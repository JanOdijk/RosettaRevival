\newpage
\begin{verbatim}
% RULE Rdatumyear
< m1:NP{NPrec1}[mu1,head/PROPERNOUN{PROPERNOUNrec1}[mu2]] 
  m2:NP{NPrec2}[mu3] 
>

< m :NP{NPrec1}
       [ mu1,
         head/PROPERNOUN{PROPERNOUNrec1}[mu2],
         postmodrel/NP{NPrec3}[mu3]
       ]
>


      COMP
         <
          C1: PROPERNOUNrec1.subc = monthnoun          AND
              NPrec2.cases = [nominative]              AND
              NPrec2.generic = nogeneric               AND
              NPrec2.superdeixis = NPrec1.superdeixis 
          A1: NPrec3 := NPrec2;
              NPrec3.cases := [accusative];
              NPrec3.generic := omegageneric;
              NPrec3.superdeixis := omegadeixis;
         >
      DECOMP
         <
          C1: PROPERNOUNrec1.subc = monthnoun          AND
              accusative IN NPrec3.cases               AND
              NPrec3.generic = omegageneric            AND
              NPrec3.superdeixis = omegadeixis 
          A1: NPrec2 := NPrec3;
              NPrec2.cases := [nominative];
              NPrec2.generic := nogeneric;
              NPrec2.superdeixis := NPrec1.superdeixis;
         >

&

\end{verbatim}
