% RULE RuttNp
< m1:T1=NP{NPrec1}[mu1]
>

< m :UTT{UTTrec1}[head/NP{NPrec2}[mu2]]
>

   PARAMETERS
      <
       casespar: caseSETtype
      >

      COMP
         <
          C1: NPrec1.generic = nogeneric AND
              NPrec1.cases = [nominative] AND
              (casespar = [nominative] OR
               casespar = [dative] OR
               casespar = [accusative])    AND
              (NOT QUOTE_varsintree(T1))
          A1: QUOTE_AssignCase(NPrec1, mu1, NPrec2, mu2, casespar);
              NPrec2.generic := omegageneric;
         >
      DECOMP
         <
          C1: NPrec2.generic = omegageneric
            C11: nominative IN NPrec2.cases
            A11: casespar := [nominative]
            C12: accusative IN NPrec2.cases AND
                 NPrec2.cases * [nominative] = []
            A12: casespar := [accusative]
            C13: [dative] = NPrec2.cases 
            A13: casespar := [dative]
          A1: QUOTE_AssignCase(NPrec2, mu2, NPrec1, mu1, [nominative]);
              NPrec1.generic := nogeneric;
              NPrec1.superdeixis := presentdeixis
         >
&

\end{verbatim}
