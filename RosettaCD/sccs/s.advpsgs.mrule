% RULE RADVPdegreemod1

< m1: I3::ADVP{ADVPrec1}
                    [head/ADV{ADVrec1}[mu2],
                     mu3
                    ]

  m2: T1
>

< m: I4::ADVP{ADVPrec2}
                   [I1:degreemodrel/T2,
                    head/ADV{ADVrec1}[mu2],
                    mu3
                    ]

>

   MATCHCONDITIONS
      <I1: T2.cat IN [QP, ADVP, NP]
       m2: T1.cat IN [QP, ADVP, NP]
       I3: ADVPrec1.temporal = true
       I4: ADVPrec2.temporal = true
      >

<

   SUBRULE (* 1 *) (* iets, (veel) minder, het meest, te weinig, 
                      (hoeveel) meer *)

      <T1: QP{QPrec1}[I2:mu7]>
      <T2: QP{QPrec2}[I2:mu7]>

   MATCHCONDITIONS
      <
       I2: ALL(mu7, I12:rel1/..)
       I12: rel1 IN [head, degreemodrel]
      >

      COMP
         <
          C1: QPrec1.hop <> true AND
              QPrec1.superdeixis = ADVPrec1.superdeixis
          A1: QPrec2 := QPrec1;
              QPrec2.superdeixis := omegadeixis;
              ADVPrec2 := ADVPrec1;
              ADVPrec2.mood := QPrec2.mood  (* m.h.o. op een WH-QP *)

         >
      DECOMP
         <
          C1: QPrec2.hop <> true AND
              QPrec2.superdeixis = omegadeixis AND
              ADVPrec2.mood = QPrec2.mood
          A1: QPrec1 := QPrec2;
              QPrec1.superdeixis := ADVPrec2.superdeixis;
              ADVPrec1 := ADVPrec2;
              ADVPrec1.mood := declxpmood          
>

   SUBRULE  (* 2 *) (* te, zeer, (heel) erg, voor mij te, 
                       drie uur te, hoe *)

      <T1: ADVP{ADVPrec3}[I2:mu6]>
      <T2: ADVP{ADVPrec4}[I2:mu6]>

   MATCHCONDITIONS
      <
       I2: ALL(mu6, I12:rel1/..)
       I12: rel1 IN [head, degreemodrel]
      >

      COMP
         <
          C1: degreeadv IN ADVPrec3.actsubcefs AND
              ADVPrec1.superdeixis = ADVPrec3.superdeixis
          A1: ADVPrec2 := ADVPrec1;
              ADVPrec4 := ADVPrec3;
              ADVPrec4.superdeixis := omegadeixis;
              ADVPrec2.mood := ADVPrec4.mood;  (* m.h.o. op een WH-ADV *)
         >
      DECOMP
         <
          C1: degreeadv IN ADVPrec4.actsubcefs  AND
              ADVPrec4.superdeixis = omegadeixis AND
              ADVPrec2.mood = ADVPrec4.mood  
          A1: ADVPrec1 := ADVPrec2;
              ADVPrec3 := ADVPrec4;
              ADVPrec3.superdeixis := ADVPrec1.superdeixis;
              ADVPrec1.mood := declxpmood          
         >
>

&

\end{verbatim}
