% TRANSFORMATION TADJvoorobjmovement1
< m1: I2::ADJPFORMULA{ADJPFORMULArec1}
                  [mu1,
                   predrel/ADJP{ADJPrec1}
                           [mu2,
                            mu3,
                            head/T1,
                            mu4,
                            I3:voorobjrel/T2,
                            mu5
                           ]
                   ]
>
< m: I2::ADJPFORMULA{ADJPFORMULArec1}
                  [mu1,
                   predrel/ADJP{ADJPrec1}
                           [mu2,
                            I1:erposrel/T2,
                            I4:mu3,
                            head/T1,
                            mu4,
                            mu5
                           ]
                   ]
>


   MATCHCONDITIONS
      <
       I1: (T2.CAT IN [PREPP, VARPREPP] WITH T2.REC.headkey =
           KEY_voorioprepkey)
       I2: voorsubjectiveadj IN ADJPFORMULArec1.actsubcefs 
       I3: T2.CAT IN [PREPP, VARPREPP] 
       I4: ALL(mu3, I14:rel1/..)
       I14: rel1 IN [degreemodrel, negrel,
                     tempadvrel, locadvrel, causadvrel]
       (* volgorde scherper beregelen  *)
      >

      COMP
         <
          C1: true
          A1: @
         >

      DECOMP
         <
          C1: true
          A1: @
         >

&

