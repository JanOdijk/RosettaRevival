% RULE Rqpomtemod (* modificatie van genoeg door om-te-zin met BIGPRO-object *)

< m1: QP{QPrec1}[mu1,
                 head/Q{Qrec1}
                ]
  m2: I2::SENTENCE{SENTENCErec1}[mu2]
>

< m : I3::QP{QPrec2}[mu1,
                     head/Q{Qrec1},
                     omcomplrel/I2::SENTENCE{SENTENCErec2}[mu2]
                    ]
>


   MATCHCONDITIONS
      <m1: (QPrec1.thanas = omcompl) AND
           (QPrec1.thanascompl = false)
       I3: (QPrec2.thanascompl = true) AND
           (QPrec2.thanas = omcompl)

       I2: SENTENCErec2 = 
                   { prosubject :  true
                     mood       :  whmod
                     infsort    :  omteinf
                     finiteness :  infinite
                   }
      >


      COMP
         <
          C1: SENTENCErec1.superdeixis = QPrec1.superdeixis
          A1: QPrec2 := QPrec1;
              QPrec2.thanascompl := true;
              SENTENCErec2 := SENTENCErec1;
              SENTENCErec2.superdeixis := omegadeixis;
         >
      DECOMP
         <
          C1: SENTENCErec2.superdeixis = omegadeixis
          A1: QPrec1 := QPrec2;
              QPrec1.thanas := omcompl;
              QPrec1.thanascompl := false;
              SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.superdeixis := QPrec1.superdeixis;
         >

&

