%
% TRANSFORMATION Targreflspelling11
< m1:CLAUSE{CLAUSErec1}
        I1:[ mu1,
             subjrel/NPVAR{NPVARrec1},
             mu2,
             predrel/VERBP{VERBPrec1}
                    [ mu3,
                      rel2/VARPREPP{VARPREPPrec1}
                                 [ mu9,
                                   objrel/NPVAR{NPVARrec2},
                                   mu10
                                 ],
                      mu4
                    ],
             mu5
           ]
>

< m :CLAUSE{CLAUSErec1}
        I1:[ mu1,
             subjrel/NPVAR{NPVARrec1},
             mu2,
             predrel/VERBP{VERBPrec1}
                 I2:[ mu3,
                      rel2/PREPP{PREPPrec1}
                                 [ mu9,
                                   objrel/T1,
                                   mu10
                                 ],
                      mu4
                    ],
             mu5
           ]
>

   MATCHCONDITIONS
      <
       m1 : NPVARrec1 = NPVARrec2 EXCEPT FOR [cases] AND
            NOT EXIST (mu3, [../I13::NPVAR{NPVARrec3}]) AND
            NOT EXIST (mu4, [../I14::NPVAR{NPVARrec4}]) AND
            NOT EXIST (mu4, [../VARPREPP{}
                                  [mu7,../I15::NPVAR{NPVARrec5},mu8]]) AND
            rel2 IN [dirargrel,locargrel,predrel]                      AND
            EXIST(mu3,[objrel/..])
       I1 : RIGHTTOLEFT
       I2 : RIGHTTOLEFT
       I13: NPVARrec3 = NPVARrec1 EXCEPT FOR [cases] 
       I14: NPVARrec4 = NPVARrec1 EXCEPT FOR [cases] 
       I15: NPVARrec5 = NPVARrec1 EXCEPT FOR [cases]
       m  : NOT EXIST (mu3, [../I16::NP{NPrec2}[mu6]]) AND
            EXIST(mu3,[objrel/..])
       I16: NPrec2.NPhead = zichzelfNP
      >
<
   SUBRULE   (* me *)
      < >
      <T1:REFLPRO(AUX_meREFLPROkey){REFLPROrec1} >

      COMP
         <
          C1: NPVARrec1 =  {
                             person : 1 
                             number : singular
                           }
          A1: PREPPrec1 := VARPREPPrec1;
         >
      DECOMP
         <
          C1: NPVARrec1 =  {
                             person : 1 
                             number : singular
                           }                    
          A1:  NPVARrec2 := NPVARrec1;
               NPVARrec2.cases := [accusative];
               VARPREPPrec1 := PREPPrec1;
         >

   SUBRULE   (* mij *)
      < >
      <T1: REFLPRO(AUX_mijREFLPROkey){REFLPROrec1}  >

(* NO COMP part *)
      DECOMP
         <
          C1:  NPVARrec1 = {
                             person : 1
                             number : singular
                           }                    
          A1:  NPVARrec2 := NPVARrec1;
               NPVARrec2.cases := [accusative];
               VARPREPPrec1 := PREPPrec1;
         > 

   SUBRULE   (* je *)
      < >
      <T1:REFLPRO(AUX_jeREFLPROkey){REFLPROrec1} >

      COMP
         <
          C1: NPVARrec1.person = 2 
          A1: PREPPrec1 := VARPREPPrec1;
         >
      DECOMP
         <
          C1: NPVARrec1.person = 2 
          A1: NPVARrec2 := NPVARrec1;
              NPVARrec2.cases := [accusative];
              VARPREPPrec1 := PREPPrec1;
         >

   SUBRULE  (* zich *)
      < >
      <T1:REFLPRO(AUX_zichREFLPROkey){REFLPROrec1} >

      COMP
         <
          C1: NPVARrec1.person IN [3,4]
          A1: PREPPrec1 := VARPREPPrec1;
         >
      DECOMP
         <
          C1: NPVARrec1.person IN [3,4] 
          A1: NPVARrec2 := NPVARrec1;
              NPVARrec2.cases := [accusative];
              VARPREPPrec1 := PREPPrec1;
         >

   SUBRULE  (* ons *)
      < >
      <T1:REFLPRO(AUX_onsREFLPROkey){REFLPROrec1} >

      COMP
         <
          C1: NPVARrec1 =  {
                             person : 1 
                             number : plural
                           }
          A1: PREPPrec1 := VARPREPPrec1;
         >
      DECOMP
         <
          C1: NPVARrec1 =  {
                             person : 1 
                             number : plural
                           }                     
          A1:  NPVARrec2 := NPVARrec1;
               NPVARrec2.cases := [accusative];
               VARPREPPrec1 := PREPPrec1;
         >

   SUBRULE       (* u *)
      < >
      <T1:REFLPRO(AUX_uREFLPROkey){REFLPROrec1} >

      COMP
         <
          C1: NPVARrec1 =  {
                             person : 5 
                             number : singular
                           }
          A1: PREPPrec1 := VARPREPPrec1;
         >
      DECOMP
         <
          C1: NPVARrec1.person IN [4,5]
          A1: NPVARrec2 := NPVARrec1;
              NPVARrec2.cases := [accusative];
              VARPREPPrec1 := PREPPrec1;
         >

>


&

