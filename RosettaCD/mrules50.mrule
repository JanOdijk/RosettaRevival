   MATCHCONDITIONS
      <
       m1 : NOT EXIST (mu3, [../I13::NPVAR{NPVARrec3}]) AND
            NOT EXIST (mu4, [../I14::NPVAR{NPVARrec4}]) AND
            NOT EXIST (mu4, [../VARPREPP{}[mu7,../I15::NPVAR{NPVARrec5},mu8]]) 
       I1 : RIGHTTOLEFT
       I2 : RIGHTTOLEFT
       I3 : NPrec1 = AUX_NPONElkaar
       I4: ( NPVARrec1.number = plural ) OR
           ( NPVARrec1.NPhead = menNP )    OR
           ( NPVARrec1.NPhead = jegenNP)
       I13: NPVARrec3 = NPVARrec1 EXCEPT FOR [cases] 
       I14: NPVARrec4 = NPVARrec1 EXCEPT FOR [cases] 
       I15: NPVARrec5 = NPVARrec1 EXCEPT FOR [cases]
       m  : NOT EXIST (mu3, [../I16::NP{NPrec2}[mu6]])
       I16: NPrec1 = AUX_NPONElkaar
      >

      COMP
         <
          C1: true
          A1: NPrec1  := AUX_NPONElkaar
         >
      DECOMP
         <
          C1: true
          A1: @
         >
&
