       I1: SENTENCErec1.VRcompl = true 
       I2: SENTENCErec2.VRcompl = true 
       I3: rel1 IN [objrel, indobjrel, argrel] AND
           T1.CAT IN [NP, NPVAR, CNVAR]
       I4: T1.CAT IN [NP, NPVAR, CNVAR]
       >


<
   SUBRULE
      <mu3: [mu6]>
      <mu3: [mu6]>

   MATCHCONDITIONS
      <
       mu3: ALL(mu6, I31:rel2/..) AND NOT EXIST(mu6,[objrel/..])
       I31: QUOTE_VPrelprec(objrel, rel2)
      >

      COMP
         <
          C1: rel1 = objrel AND
              SENTENCErec1.synvpefs * (AUX_objvps+[synzijn]) <> []  
          A1: SENTENCErec2 := SENTENCErec1;
         >

      DECOMP
         <
          C1: SENTENCErec2.synvpefs * (AUX_objvps+[synzijn]) <> []
          A1: SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.synvpefs := SENTENCErec2.synvpefs * 
                          (AUX_objvps + [synzijn]);
              rel1 := objrel;
         >


   SUBRULE
      <mu3: [mu6]>
      <mu3: [mu6]>

   MATCHCONDITIONS
      <
       mu3: ALL(mu6, I31:rel2/..) AND NOT EXIST(mu6,[indobjrel/..])
       I31: QUOTE_VPrelprec(indobjrel, rel2)
      >

      COMP
         <
          C1: rel1 = indobjrel AND
              (T1.CAT IN [NP, NPVAR, CNVAR] WITH
              Dative IN T1.REC.cases)
              C11: SENTENCErec1.synvpefs * AUX_indobjvps <> [] 
              A11: SENTENCErec2 := SENTENCErec1;
              C12: SENTENCErec1.adjuncts * [benfactNP] <> []
              A12: SENTENCErec2 := SENTENCErec1;
          A1: @
         >
      DECOMP
         <
          C1: 
              (T1.CAT IN [NP, NPVAR, CNVAR] WITH
              Dative IN T1.REC.cases)
              C11: SENTENCErec2.synvpefs * AUX_indobjvps <> [] 
              A11: SENTENCErec1 := SENTENCErec2;
                   SENTENCErec1.synvpefs := 
                           SENTENCErec2.synvpefs * AUX_indobjvps;
              C12: SENTENCErec2.adjuncts * [benfactNP] <> []
              A12: SENTENCErec1 := SENTENCErec2;
          A1: rel1 := indobjrel;
         >


   SUBRULE
      <mu3: [mu6]>
      <mu3: [mu6]>

   MATCHCONDITIONS
      <
       mu3: ALL(mu6, I31:rel2/..)
       I31: QUOTE_VPrelprec(argrel, rel2) OR rel2 IN 
             [aanobjrel, voorobjrel]
      >

      COMP
         <
          C1: rel1 = argrel AND
              SENTENCErec1.synvpefs * AUX_VRvps <> []  
          A1: SENTENCErec2 := SENTENCErec1;
         >
      DECOMP
         <
          C1: SENTENCErec2.synvpefs * AUX_VRvps <> []
          A1: SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.synvpefs := SENTENCErec2.synvpefs * AUX_VRvps;
              rel1 := argrel;
         >





>
&

