% RULE RCNmodADJP                  
< SUBST:
  m1: CN{CNrec1}[mu4,
                 I1:head/T1,
                 mu8
                ]

  m2: OPENADJPPROP{OPENADJPPROPrec1}
                            [subjrel/CNVAR{CNVARrec1},
                             predrel/ADJP{ADJPrec1}[mu2,
                                                    head/ADJ{ADJrec1}[mu3]
                                                   ] 
                            ]
>

< m : CN{CNrec2}[modrel/ADJP{ADJPrec2}[mu2,
                                       head/ADJ{ADJrec1}[mu3]
                                      ],
                 mu4,
                 I1:head/T1,
                 mu8
                ]
>


   MATCHCONDITIONS
      <
       I1: T1.CAT  IN [NOUN, EN]
      >

   PARAMETERS
      <
       LEVEL
      >

<
   SUBRULE
      <T1: NOUN{NOUNrec1}[mu1] >
      <T1: NOUN{NOUNrec1}[mu1] >


      COMP
         <
          C1: (attributive IN ADJPrec1.actuseefs) AND
              ADJPrec1.eorenform = noform AND
              (OPENADJPPROPrec1.superdeixis = CNrec1.superdeixis) AND
              (OPENADJPPROPrec1 = COPYT_ADJPtoADJPPROP(ADJPrec1)) AND
              (OPENADJPPROPrec1.PROsubject = true) AND 
              (OPENADJPPROPrec1.aktionsarts = [stative]) AND
              (CNVARrec1 = COPYT_CNtoVAR(CNrec1)) AND
              (CNVARrec1.index = LEVEL)
               C11: ADJrec1.temporal = true AND
                    CNrec1.temporal = true AND
                    CNrec1.deixis = NOUNrec1.deixis AND
                    CNrec1.aspect = NOUNrec1.aspect AND
                    CNrec1.retro = NOUNrec1.retro AND
                    CNrec1.class = NOUNrec1.class

                    C111: CNrec1.actsubcs * [unitnoun] = []
                    A111: CNrec2 := CNrec1;
                    C112: CNrec1.actsubcs * [unitnoun] <>  []
                    A112: CNrec2 := CNrec1;
                          CNrec2.actsubcs := 
                                 CNrec2.actsubcs - [unitnoun] + [plurunitnoun];

               A11: CNrec2.deixis := ADJrec1.deixis;
                    CNrec2.aspect := ADJrec1.aspect;
                    CNrec2.retro := ADJrec1.retro;
                    CNrec2.class := ADJrec1.class;
               C12: true
                  (* equivalent with
                    ADJrec1.temporal = false OR
                    CNrec1.temporal = false OR
                    (ADJrec1.temporal = true AND
                     CNrec1.temporal = true AND
                     CNrec1.deixis = NOUNrec1.deixis AND
                     CNrec1.aspect = NOUNrec1.aspect AND
                     CNrec1.retro = NOUNrec1.retro AND
                     CNrec1.class = NOUNrec1.class
                    )
                   Only the last ADJP should give its temporal features to CN
                   *)
                    C121: CNrec1.actsubcs * [unitnoun] = []
                    A121: CNrec2 := CNrec1;
                    C122: CNrec1.actsubcs * [unitnoun] <>  []
                    A122: CNrec2 := CNrec1;
                          CNrec2.actsubcs := 
                                 CNrec2.actsubcs - [unitnoun] + [plurunitnoun];
               A12: @
          A1:  ADJPrec2 := ADJPrec1;
               ADJPrec2.actuseefs := [attributive];
         >

      DECOMP
         <
          C1: attributive IN ADJPrec2.actuseefs AND
              ADJPrec2.eorenform = noform AND
              NOT (unitnoun IN CNrec2.actsubcs) (* provisional *)
              C11: CNrec2.deixis = ADJrec1.deixis AND
                   CNrec2.aspect = ADJrec1.aspect AND
                   CNrec2.retro = ADJrec1.retro AND
                   CNrec2.class = ADJrec1.class AND
                   CNrec2.temporal = true AND
                   ADJrec1.temporal = true
              A11: CNrec1 := CNrec2;
                   CNrec1.deixis := NOUNrec1.deixis;
                   CNrec1.aspect := NOUNrec1.aspect;
                   CNrec1.retro := NOUNrec1.retro;
                   CNrec1.class := NOUNrec1.class;
              C12: (CNrec2.deixis = NOUNrec1.deixis AND
                    CNrec2.aspect = NOUNrec1.aspect AND
                    CNrec2.retro = NOUNrec1.retro AND
                    CNrec2.class = NOUNrec1.class AND
                    CNrec2.temporal = true AND
                    ADJrec1.temporal = true
                   ) OR
                   CNrec2.temporal = false OR
                   ADJrec1.temporal = false
              A12: CNrec1 := CNrec2;
          A1: ADJPrec1 := ADJPrec2;
              ADJPrec1.actuseefs := [attributive, nominalised];
              OPENADJPPROPrec1 := COPYT_ADJPtoADJPPROP(ADJPrec1);
              OPENADJPPROPrec1.superdeixis := CNrec1.superdeixis;
              OPENADJPPROPrec1.PROsubject := true;
              OPENADJPPROPrec1.aktionsarts := [stative];
              CNVARrec1 := COPYT_CNtoVAR(CNrec1);
              CNVARrec1.index := LEVEL;

         >

   SUBRULE
      <T1: EN{ENrec1}[mu1] >
      <T1: EN{ENrec1}[mu1] >


      COMP
         <
          C1: (attributive IN ADJPrec1.actuseefs) AND
              ADJPrec1.eorenform = noform AND
              (OPENADJPPROPrec1.superdeixis = CNrec1.superdeixis) AND
              (OPENADJPPROPrec1 = COPYT_ADJPtoADJPPROP(ADJPrec1)) AND
              (OPENADJPPROPrec1.PROsubject = true) AND 
              (OPENADJPPROPrec1.aktionsarts = [stative]) AND
              (CNVARrec1 = COPYT_CNtoVAR(CNrec1)) AND
              (CNVARrec1.index = LEVEL)
          A1:  ADJPrec2 := ADJPrec1;
               ADJPrec2.actuseefs := [attributive];
               CNrec2 := CNrec1;
         >

      DECOMP
         <
          C1: attributive IN ADJPrec2.actuseefs AND
              ADJPrec2.eorenform = noform
          A1: ADJPrec1 := ADJPrec2;
              ADJPrec1.actuseefs := [attributive, nominalised];
              OPENADJPPROPrec1 := COPYT_ADJPtoADJPPROP(ADJPrec1);
              OPENADJPPROPrec1.superdeixis := CNrec2.superdeixis;
              OPENADJPPROPrec1.PROsubject := true;
              OPENADJPPROPrec1.aktionsarts := [stative];
              CNVARrec1 := COPYT_CNtoVAR(CNrec2);
              CNVARrec1.index := LEVEL;
              CNrec1 := CNrec2

         >
>
&

