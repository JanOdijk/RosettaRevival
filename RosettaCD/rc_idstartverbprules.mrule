% RULE RIDNPFORMATION3
< m1: CN{CNREC1}[mu1]
  m2: T1
>

< m : NP{NPREC1}[I2:detrel/T2,
                 head/CN{CNrec1}[mu1]
                ]
>
   MATCHCONDITIONS
      <
       m2: T1.CAT IN [NPVAR, CNVAR]
       I2: T2.CAT IN [NPVAR, CNVAR]
      >

<
   SUBRULE (* 1 *)
      < T1: NPVAR{NPVARrec1} >
      < T2: NPVAR{NPVARrec2} >

COMP
 <
   C1: TRUE
       C11: NPVARrec1.mood <> wh
       A11: NPREC1 := COPYT_CNtoNP(CNREC1);
            NPREC1.mood := NPVARrec1.mood;
            NPREC1.definite := NPVARrec1.definite;
       C12: NPVARrec1.mood = wh
       A12: NPREC1 := COPYT_CNtoNP(CNREC1);
            NPREC1.mood := declxpmood;
            NPREC1.definite := def;
   A1: NPVARrec2 := NPVARrec1;
       NPVARrec2.cases := [genitive];
       NPREC1.actcomas := CNREC1.posscomas;
       NPREC1.posspred := true;
       NPREC1.generic := nogeneric;
       NPREC1.cases := [nominative];
       NPREC1.specQ := yesspec;
 >

DECOMP
 <
   C1: NPREC1 = COPYT_CNtoNP(CNREC1)
         EXCEPT FOR [specQ, NPhead, syntquant, possRform, generic,
                     mood, possnietnp, posspred, definite, actcomas]
                                            AND
       NPREC1.actcomas = CNREC1.posscomas   AND
       NPREC1.posspred = true               AND
       NPREC1.generic = nogeneric           AND
       NPREC1.cases = [nominative]          AND
       NPREC1.specQ = yesspec               AND
       ((genitive  IN NPVARrec2.cases)            AND
        (((NPVARrec2.mood = NPREC1.mood)  AND
          (NPVARrec2.mood <> wh)          AND
          (NPREC1.definite = NPVARrec2.definite)
         )                                    OR
         ((NPrec1.mood = declxpmood)      AND
          (NPVARrec2.mood = wh)           AND
          (NPREC1.definite = def)
         )
        )         
       )
   A1: NPVARrec1 := NPVARrec2;
 >

   SUBRULE (* 2 *)
      < T1: CNVAR{CNVARrec1} >
      < T2: CNVAR{CNVARrec2} >

COMP
 <
   C2: CNVARrec1.cases = [nominative]
   A2: CNVARrec2 := CNVARrec1;
       CNVARrec2.cases := [genitive];
       NPREC1 := COPYT_CNtoNP(CNREC1);
       NPREC1.actcomas := CNREC1.posscomas;
       NPREC1.definite := def;
       NPREC1.posspred := true;
       NPREC1.generic := nogeneric;
       NPREC1.cases := [nominative];
       NPREC1.specQ := yesspec;
       NPREC1.mood := declxpmood;
 >

DECOMP
 <
   C2: NPREC1 = COPYT_CNtoNP(CNREC1)
         EXCEPT FOR [specQ, NPhead, syntquant, possRform, generic,
                     mood, possnietnp, posspred, definite, actcomas]
                                            AND
       NPREC1.actcomas = CNREC1.posscomas   AND
       NPREC1.definite = def                AND
       NPREC1.posspred = true               AND
       NPREC1.generic = nogeneric           AND
       NPREC1.cases = [nominative]          AND
       NPREC1.specQ = yesspec               AND
       ((genitive  IN CNVARrec2.cases)            AND
        (NPREC1.mood = declxpmood)
       )                                    AND
       genitive IN CNVARrec2.cases  
   A2: CNVARrec1 := CNVARrec2;
       CNVARrec1.cases := [nominative];
 >
>
&
