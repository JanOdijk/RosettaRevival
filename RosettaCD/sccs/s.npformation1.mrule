                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                         };
         >

      DECOMP
         <
          C1: POSSADJrec1.antecedent = noant AND
              POSSADJrec1.eORenForm  = noForm AND
              POSSADJrec1.mood = wh AND
              POSSADJrec1.geni = false
              (* .reduced doesn't have to be tested in analysis since
                 both forms are accepted *) AND
               NPrec2 = { posspred          : true
                          definite          : indef
                          possnietnp        : false
                          mood              : wh

                          number            : NOUNrec3.number
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant 
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                         }  
          A1:   
              WHPROrec1 := COPYT_BWHPROTOWHPRO(BWHPROrec1);
              NPrec1.possRform := WHPROrec1.possRform ;
              NPrec1.sexes := WHPROrec1.sexes;
              NPrec1.number := WHPROrec1.number; 
              NPrec1.animate := WHPROrec1.animate;
              NPrec1.human := yeshuman;
              NPrec1.NPhead := otherNP;   (* altijd 'wie' *)
              
              NPrec1.posspred := true; 
              NPrec1.syntquant := omegaquant;(* 29-11-89; FJ*)
                                    (* because of JO's modifications 
                                       in RNPformation6 *)
              NPrec1.definite  := indef;
              NPrec1.mood      := wh;
              NPrec1.specQ     := yesspec;

              (* rest of attributes, including superdeixis and
                 generic, is default: *)
              NPrec1 := { req           : [pospol,negpol,omegapol]
                          env           : [pospol,negpol,omegapol]
                          class         : omegaTimeAdvClass
                          superdeixis   : omegadeixis
                          deixis        : omegadeixis
                          aspect        : omegaAspect
                          retro         : false
                          person        : 3
                          cases         : [nominative]
                          genders       : [omegagender] 
                          actsubcs      : [othernoun]
                          temporal      : false
                          possgeni      : false
                          actcomas      : [count]
                          thetanp       : omegathetanp
                          nounpatternefs: []
                          possnietnp    : false
