% RULE CNformation1
< m1: NOUN{NOUNrec1}[mu1]
>

< m : CN{CNrec1}[head/NOUN{NOUNrec1}[mu1]
                ]
>

      COMP
         <
          C1: true
          A1: CNrec1 := COPYT_NOUNtoCN(NOUNrec1)
              (* .req hasn't been determined yet;
                 .superdeixis/.definite have default value *)     
         >

      DECOMP
         <
          C1: CNrec1 = COPYT_NOUNtoCN(NOUNrec1)
          A1:  @
         >
&


