       m1: EXIST(mu1, [extraposrel/..]) AND 
           (CLAUSErec1.synvpefs * (AUX_extraposvps + [synzijn])
                                             - AUX_oblVRvps <> [])
       m : EXIST(mu1, [extraposrel/..]) AND 
           (CLAUSErec2.synvpefs * (AUX_extraposvps + [synzijn]) 
                                             - AUX_oblVRvps <> [])

      >



      COMP
         <
          C1: true
          A1: CLAUSErec2 := CLAUSErec1;
         >
      DECOMP
         <
          C1: true
          A1: CLAUSErec1 := CLAUSErec2;
              CLAUSErec1.synvpefs := 
                 CLAUSErec2.synvpefs * (AUX_extraposvps+ [synzijn]) 
                                  - AUX_oblVRvps;
         >

&

