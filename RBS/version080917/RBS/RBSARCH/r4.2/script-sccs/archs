case $# in
0) echo "Usage: archs componentname [\"pattern\"] "; exit 1;;
1) ;;
2) ;;
3) ;;
*) echo "Usage: archs componentname [\"pattern\"] "; exit 1;;
esac
savedir=`pwd`
cd $RBS_ARCHIVE$RBS_RELEASE
trap 'cd $savedir; exit 1' 0
if ( test "$1" = actions )
then
     cd actions
     ls  2> /dev/null
else
     cd $1
     ls $3 $2 2> /dev/null
fi
cd $savedir
trap 0
