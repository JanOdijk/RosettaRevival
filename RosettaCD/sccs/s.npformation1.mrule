% RULE RNPformation10

< m1: I2::CN{CNrec1}[I1:mu1]
>

< m : NP{NPrec1}[head/I2::CN{CNrec1}[I1:mu1]
                ]
>


   MATCHCONDITIONS             (* meant to block the formation of NP out of EN 
                                  plus an numerical modifier or a postmodifier
                                  while there is no ADJP or SENTENCE
                                *)
      < 
        I1:(EXIST(mu1, [head/NOUN{NOUNrec1}[mu8]]) OR
           EXIST(mu1, [I11:modrel/T1])) AND
           NOT EXIST(mu1, [posrel/NP{NPrec2}[mu9]])
        I11: T1.cat IN  [ADJP, SENTENCE]
        I2: ([unitnoun] *  CNrec1.actsubcs = []) AND
            ((mass IN CNrec1.posscomas) OR (plural IN CNrec1.numbers)) AND
              (* to exclude 'koning' and 'gele boek';
                 NB. 'hij is koning' is formed IN the NPPROP-subgrammar *)
            CNrec1.definite <> def
              (* in order to prevent the definite CN 'drie boeken'
                 to become an NP *)

      >

      COMP
         <
          C1: true
              C11: singular IN CNrec1.numbers AND
                 mass IN CNrec1.posscomas 
                 
            A11: (* NPrec1 := COPYT_CNtoNP(CNrec1); *)
               NPrec1 := { 

                          number            : singular    (*NOUNrec.number *)
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : [mass]
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
        		  superdeixis: omegadeixis
                          NPhead     : otherNP
                         }  
            C12: plural IN CNrec1.numbers AND
                 count IN CNrec1.posscomas 
            A12: (* NPrec1 := COPYT_CNtoNP(CNrec1); *)
               NPrec1 := { 

                          number            : plural 
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : [count]
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          NPhead     : otherNP
        		  superdeixis: omegadeixis
                         }  
          A1: NPrec1.definite := indef;
              NPrec1.posspred := true;
              NPrec1.possnietnp := true; (* 'geen (gele) jassen' / 
                                                      'geen (lekkere) kaas' *)

         >

      DECOMP
         <
          C1: (NPrec1.definite = indef) AND
              (NPrec1.posspred =  true) AND
              (NPrec1.possnietnp = true) AND
              (NPrec1.cases = [nominative])
              C11: mass IN NPrec1.actcomas AND
                   NPrec1 = { 
                          number            : singular (*NOUN.number *)
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          human             : CNrec1.human
