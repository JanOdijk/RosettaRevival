% RULE RADJDegreemod1  
            
< m1: ADJPPROP{ADJPPROPrec1}[mu1,
                             predrel/ADJP{ADJPrec1}
                                     [I2:mu2,
                                      I3:mu3,
                                      head/ADJ{ADJrec1}[mu4],
                                      mu5 
                                     ]
                            ]  
  m2: T2
>

< m : ADJPPROP{ADJPPROPrec1}[mu1,
                             predrel/ADJP{ADJPrec2}
                                     [I2:mu2,
                                      I1:degreemodrel/T2,
                                      I3:mu3,
                                      head/ADJ{ADJrec1}[mu4],
                                      mu5
                                     ]
                            ]
>

   MATCHCONDITIONS
      <I1: T2.cat IN [QP, ADVP, NP]
       m2: T2.cat IN [QP, ADVP, NP]
       I2: NOT EXIST (mu2, [I12:rel1/..])
       I12: rel1 IN [prepobjrel, aanobjrel, degreemodrel] 
       I3: (NOT EXIST (mu3, [I13:rel2/..])) 
       I13: rel2 IN [objrel, indobjrel, causadvrel, 
                     locadvrel, tempadvrel, degreemodrel]
      >

<
   SUBRULE (* 1 *) (* iets, (veel) minder (dan Jan), het meest (van ons), 
                      te weinig, hoeveel meer *)
                                 
                   (* NB. `te weinig' is a paraphrase of `niet genoeg', and it
                           is to be translated into English as: not enough;
                           possibly this requires an idiomatic treatment *)

      <T2: QP{QPrec1}[mu7, head/Q{Qrec1}, I4:mu8]>
      <T2: QP{QPrec1}[mu7, head/Q{Qrec1}, I4:mu8]>

   MATCHCONDITIONS
      < I4: NOT EXIST(mu8, [omcomplrel/..]) (* in that case RADJdegreemod2a
                                              applies*) 
        T2: ((Qrec1.key = KEY_meestqkey) -> 
             NOT EXIST(mu7, [artrel/..])) AND
                             (* in that case the next subrule applies *)
             (Qrec1.key <> KEY_meest2qkey  )
                             (* in that case only a DETP-path is possible *)  
     >

(* nog niet geregeld: als QPrec1.hop = true en premodrel in mu7, dan mu3 leeg
*)

      COMP
         <
          C1: true
          A1: ADJPrec2 := ADJPrec1;
              ADJPrec2.mood := QPrec1.mood;  (* m.h.o. op een WH-QP *)
         >
      DECOMP
         <
          C1: ADJPrec2.mood = QPrec1.mood
          A1: ADJPrec1 := ADJPrec2;
              ADJPrec1.mood := declxpmood;
         >

(* The relevance of a subclassification of QPs is still to be investigated.
   Therefore no more restrictions have been stated yet. *)

   SUBRULE (* 1a *)
      <T2: QP{QPrec1}[artrel/ART(KEY_hetartkey){ARTrec1}, 
                      head/Q(KEY_meestqkey){Qrec1}, 
                      mu8
                     ]
      >
      <T2: QP{QPrec1}[artrel/ART(KEY_hetartkey){ARTrec1}, 
                      head/Q(KEY_meestqkey){Qrec1}, 
                      mu8
                     ]
      >

      COMP
         <
          C1: true
          A1: ADJPrec2 := ADJPrec1;
         >
      DECOMP
         <
          C1: true
          A1: ADJPrec1 := ADJPrec2;
         >


   SUBRULE  (* 2 *) (* te, zeer, voor mij te, drie meter te, hoe, even 
                       als vroeger, zo als mogelijk*)
      <T2: ADVP{ADVPrec1}[I3:mu6]>
      <T2: ADVP{ADVPrec1}[I3:mu6]>

   MATCHCONDITIONS
      <I3: NOT EXIST(mu6, [omcomplrel/..])  (* in that case RADJDegreemod2b
                                               applies *)
      >

      COMP
         <
          C1: degreeadv IN ADVPrec1.actsubcefs 
          A1: ADJPrec2 := ADJPrec1;
              ADJPrec2.mood := ADVPrec1.mood  (* m.h.o. op een WH-ADV *)
         >
      DECOMP
         <
          C1: degreeadv IN ADVPrec1.actsubcefs 
          A1: ADJPrec1 := ADJPrec2;
              ADJPrec1.mood := declxpmood          
         >

   SUBRULE  (* 3 *)  (* (bijna) drie meter lang, hoeveel meter lang *)
                     (* NB. Phrases such as `drie uur lang' 
                            should only be analysed as ADVPs.
                         *)  

      <T2: NP{NPrec1}[mu6] >
      <T2: NP{NPrec1}[mu6] >

      COMP
         <
          C1: (NPrec1.actsubcs * [unitnoun, plurunitnoun] <> []) AND
              (ADJrec1.temporal = false)
          A1: ADJPrec2 := ADJPrec1;
              ADJPrec2.mood := NPrec1.mood  (* m.h.o. op een WH-NP *)
         >
      DECOMP
         <
          C1: (NPrec1.actsubcs * [unitnoun, plurunitnoun] <> []) AND
              (ADJrec1.temporal = false)
          A1: ADJPrec1 := ADJPrec2;
              ADJPrec1.mood := declxpmood          

         >
>
&

\end{verbatim}
