% RULE RNPpartitiveformation2    (* DETP + () + NOUN + posrel --> 
                                    DETP van POSSADJ + [() + NOUN] *)
< m1: CN{CNrec1}[mu1,
                 head/NOUN{NOUNrec3}[mu6],
                 mu7,
                 posrel/NP{NPrec1}[mu2, 
                                   head/T1
                                   ]
                ]
  m2: DETP{DETPrec1}[mu9]
>

< m : NP{NPrec2}[detrel/DETP{DETPrec3}[modrel/DETP{DETPrec2}[mu9],
                                       partrel/PREP(KEY_vanprepkey){PREPrec1},
                                       head/T2
                                      ],
                 mu1,
                 head/CN{CNrec1}[mu1,
                                 head/NOUN{NOUNrec3}[mu6],
                                 mu7
                                 ]
                ]
>


<
   (* 1 *)      (* twee + boeken mijn vader --> twee van mijn vaders boeken *)
   SUBRULE
      < 
        T1:CN{CNrec2}[mu3,head/NOUN{NOUNrec1}[mu4]] 
      >

      < 
        T2:NP{NPrec1}[mu2,head/CN{CNrec2}[mu3,head/NOUN{NOUNrec2}[mu4]]] 
      >

      COMP
         <
          C1: CNrec2.possgeni = true AND
              DETPrec1.superdeixis = CNrec1.superdeixis
          A1: NOUNrec2 := NOUNrec1;
              NOUNrec2.geni := true;
              DETPrec3 := 
                         { possnietnp  :  DETPrec1.possnietnp
                           definite    :  adef
                           posspred    :  DETPrec1.posspred
                           possnumbers :  [singular, plural]
                                            (* om een van mijn aan te kunnen 
                                               is extra atribuut nodig *)
                           posscomas   :  DETPrec1.posscomas
                           mood        :  DETPrec1.mood
                           eORenform   :  noform
                           superdeixis : omegadeixis
                           };
              NPrec2 := { posspred          : DETPrec3.posspred
                          definite          : adef
                          possnietnp        : DETPrec3.possnietnp
                          mood              : DETPrec3.mood

                          number            : NOUNrec3.number
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                          
                         } ;
               DETPrec2 := DETprec1;
               DETPrec2.superdeixis := omegadeixis;
         >

      DECOMP
         <
           C1: (CNrec2.possgeni = true) AND
               (NOUNrec2.geni = true) AND
               DETPrec3 = 
                         { possnietnp  :  DETPrec2.possnietnp
                           definite    :  adef
                           posspred    :  DETPrec2.posspred
                           possnumbers :  [singular, plural]
                                            (* om een van mijn aan te kunnen 
                                               is extra atribuut nodig *)
                           posscomas   :  DETPrec2.posscomas
                           mood        :  DETPrec1.mood
                           eORenform   :  noform
                           superdeixis : omegadeixis
                           }
              AND
              NPrec2 = { posspred          : DETPrec3.posspred
                          definite          : adef
                          possnietnp        : DETPrec3.possnietnp
                          mood              : DETPrec3.mood

                          number            : NOUNrec3.number
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                         }  AND
               DETPrec2.superdeixis = omegadeixis

          A1:  NOUNrec1 := NOUNrec2;
               NOUNrec1.geni := false;
               DETPrec1 := DETPrec2;
               DETPrec1.superdeixis := CNrec1.superdeixis;
         >


   (* 2 *)      (* twee  +  boeken ik --> twee van mijn boeken *)
   SUBRULE
      < 
        T1:PERSPRO{PERSPROrec1}[head/BPERSPRO{BPERSPROrec1}]
        mu2:[] 
      >

      < 
        T2:POSSADJ{POSSADJrec1}[head/BPERSPRO{BPERSPROrec1}] 
      >


      COMP
         <
          C1: NPrec1.actcomas * [count] <> [] AND
              NPrec1 = 
		  {
		  req           : [pospol,negpol,omegapol]
		  env           : [pospol,negpol,omegapol]
		  class         : omegaTimeAdvClass
		  superdeixis   : omegadeixis
		  deixis        : omegadeixis
		  aspect        : omegaAspect
		  retro         : false
		  definite      : def
		  person        : PERSPROrec1.person
		  number        : PERSPROrec1.number
		  cases         : [nominative]
		  genders       : [PERSPROrec1.gender] 
		  sexes         : []
		  actsubcs      : [othernoun]
		  temporal      : false
		  possgeni      : false
		  animate       : PERSPROrec1.animate
		  human         : Omegahuman
		  thetanp       : omegathetanp
		  nounpatternefs: []
		  posspred      : true
		  possnietnp    : false
		  mood          : declxpmood
		  generic       : omegageneric
		  possRform     : PERSPROrec1.possRform
		  syntquant     : omegaquant
		  NPhead        : PERSPROrec1.NPhead
		  specQ         : yesspec
		  } AND
              PERSPROrec1 = COPYT_BPERSPROTOPERSPRO(BPERSPROrec1) AND
              (DETPrec1.superdeixis = CNrec1.superdeixis)

              C11: BPERSPROrec1.key <> KEY_wijBPERSPROkey
              A11: POSSADJrec1.eORenForm  := noForm 
              C12: BPERSPROrec1.key = KEY_wijBPERSPROkey      (* onze *)
              A12: POSSADJrec1.eORenForm := eForm 

          A1: DETPrec3 := 
                         { possnietnp  :  DETPrec1.possnietnp
                           definite    :  adef
                           posspred    :  DETPrec1.posspred
                           possnumbers :  [singular, plural]
                                            (* om een van mijn aan te kunnen 
                                               is extra atribuut nodig *)
                           posscomas   :  DETPrec1.posscomas
                           mood        :  DETPrec1.mood
                           eORenform   :  noform
                           superdeixis :  omegadeixis
                           }; 
              NPrec2 :=  { posspred          : DETPrec3.posspred
                          definite          : adef
                          possnietnp        : DETPrec3.possnietnp
                          mood              : DETPrec3.mood

                          number            : NOUNrec3.number
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                         } ;
 
                 DETPrec2 := DETPrec1;
                 DETPrec2.superdeixis := omegadeixis;
         >

      DECOMP
         <
          C1: POSSADJrec1.antecedent = noant AND
              POSSADJrec1.mood = declxpmood AND
              POSSADJrec1.geni = false
              (* .reduced doesn't have to be tested in analysis since
                 both forms are accepted *)
               AND 
               DETPrec3 = 
                         { possnietnp  :  DETPrec2.possnietnp
                           definite    :  adef
                           posspred    :  DETPrec2.posspred
                           possnumbers :  [singular, plural]
                                            (* om een van mijn aan te kunnen 
                                               is extra atribuut nodig *)
                           posscomas   :  DETPrec2.posscomas
                           mood        :  DETPrec2.mood
                           eORenform   :  noform
                           } AND
               NPrec2 =  { posspred          : DETPrec3.posspred
                          definite          : adef
                          possnietnp        : DETPrec3.possnietnp
                          mood              : DETPrec3.mood

                          number            : NOUNrec3.number
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                         }  AND
              DETPrec2.superdeixis = omegadeixis
              C11: POSSADJrec1.eORenForm  = noForm AND
                   BPERSPROrec1.key <> KEY_wijBPERSPROkey
              A11: @
              C12: POSSADJrec1.eORenForm  = eForm AND        (* onze *)
                   BPERSPROrec1.key = KEY_wijBPERSPROkey 
              A12:@
          A1: PERSPROrec1 := COPYT_BPERSPROTOPERSPRO(BPERSPROrec1);
              NPrec1 := 
		  {
		  req           : [pospol,negpol,omegapol]
		  env           : [pospol,negpol,omegapol]
		  class         : omegaTimeAdvClass
		  superdeixis   : omegadeixis
		  deixis        : omegadeixis
		  aspect        : omegaAspect
		  retro         : false
		  definite      : def
		  person        : PERSPROrec1.person
		  number        : PERSPROrec1.number
		  cases         : [nominative]
		  genders       : [PERSPROrec1.gender] 
		  sexes         : []
		  actsubcs      : [othernoun]
		  temporal      : false
		  possgeni      : false
		  animate       : PERSPROrec1.animate
		  human         : Omegahuman
		  actcomas      : [count]
		  thetanp       : omegathetanp
		  nounpatternefs: []
		  posspred      : true
		  possnietnp    : false
		  mood          : declxpmood
		  generic       : omegageneric
		  possRform     : PERSPROrec1.possRform
		  syntquant     : omegaquant
		  NPhead        : PERSPROrec1.NPhead
		  specQ         : yesspec
		  };
           DETPrec1 := DETPrec2;
           DETPrec1.superdeixis := CNrec1.superdeixis;
         >

   (* 3 *)      (* twee + boeken Jan --> twee van Jans boeken *) 
   SUBRULE
      < 
        T1:PROPERNOUN{PROPERNOUNrec1}[mu8]
      >

      < 
        T2:NP{NPrec1}[mu2, head/PROPERNOUN{PROPERNOUNrec2}[mu8]] 
      >

      COMP
         <
          C1: PROPERNOUNrec1.possgeni = true AND
              DETPrec1.superdeixis = CNrec1.superdeixis
          A1: PROPERNOUNrec2 := PROPERNOUNrec1;
              PROPERNOUNrec2.geni := true;
              NPrec1 := { definite          : def
                          number            : PROPERNOUNrec2.number
                          possgeni          : PROPERNOUNrec2.possgeni
                          animate           : PROPERNOUNrec2.animate
                          human             : PROPERNOUNrec2.human
                         };
               DETPrec3 := 
                         { possnietnp  :  DETPrec1.possnietnp
                           definite    :  adef
                           posspred    :  DETPrec1.posspred
                           possnumbers :  [singular, plural]
                                            (* om een van mijn aan te kunnen 
                                               is extra atribuut nodig *)
                           posscomas   :  DETPrec1.posscomas
                           mood        :  DETPrec1.mood
                           eORenform   :  noform
                           superdeixis :  omegadeixis
                           }; 
              NPrec2 :=  { posspred          : DETPrec3.posspred
                          definite          : adef
                          possnietnp        : DETPrec3.possnietnp
                          mood              : DETPrec3.mood

                          number            : NOUNrec3.number       
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : NPrec1.syntquant 
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                         } ;
               DETPrec2 := DETPrec1;
               DETPrec2.superdeixis := omegadeixis;
         >

      DECOMP
         <
          C1: (PROPERNOUNrec2.possgeni = true) AND
              (PROPERNOUNrec2.geni = true) AND
              NPrec1  = { definite          : def
                          number            : PROPERNOUNrec2.number
                          possgeni          : PROPERNOUNrec2.possgeni
                          animate           : PROPERNOUNrec2.animate
                          human             : PROPERNOUNrec2.human
                        } AND
              DETPrec3 = 
                         { possnietnp  :  DETPrec2.possnietnp
                           definite    :  adef
                           posspred    :  DETPrec2.posspred
                           possnumbers :  [singular, plural]
                                            (* om een van mijn aan te kunnen 
                                               is extra atribuut nodig *)
                           posscomas   :  DETPrec2.posscomas
                           mood        :  DETPrec2.mood
                           eORenform   :  noform
                           } AND 
              NPrec2 =  { posspred          : DETPrec3.posspred
                          definite          : adef
                          possnietnp        : DETPrec3.possnietnp
                          mood              : DETPrec3.mood

                          number            : NOUNrec3.number         
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : NPrec1.syntquant 
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                         }  AND
               DETPrec2.superdeixis = omegadeixis
          A1:  PROPERNOUNrec1 := PROPERNOUNrec2;
               DETPrec1 := DETPrec2;
               DETPrec1.superdeixis := CNrec1.superdeixis;
         >
>

&

