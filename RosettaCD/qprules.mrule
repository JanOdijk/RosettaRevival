\begin{verbatim}
% RULE RQPCOMPARATIVEcomplmod

< m1: QP{QPrec1}[head/Q{Qrec1}]
  m2: T3
>


< m : QP{QPrec1}[head/Q{Qrec1},
                     thanascomplrel/THANP{THANPrec1}
                         [head/THANAS(KEY_danthanaskey){THANASrec1},
                          I2:complrel/T4
                         ]
                    ]

>


   MATCHCONDITIONS
      <m1: QPrec1.thanas = dancompl
       m: QPrec1.thanas = dancompl 

       I2: (T4.cat IN [NPVAR, CNVAR] WITH T4.REC.cases = [Nominative])
           OR (T4.cat IN [ADVP, ADJP, PREPP, QP, DETP] )

       m2: T3.cat IN [NPVAR, CNVAR, ADVP, OPENADJPPROP, PREPP, QP, DETP]

           (* ?? CNVAR: ?*de jongen dan wie ik groter ben
                      vs. de jongen die kleiner is dan ik
           *)
      >

<
   SUBRULE (* 1 *)
      <T3: NPVAR{NPVARrec1}>
      <T4: NPVAR{NPVARrec2}>
 
      COMP
         <
          C1: NPVARrec1.superdeixis = QPrec1.superdeixis
          A1: NPVARrec2 := NPVARrec1;
              NPVARrec2.superdeixis := omegadeixis;
  
         >
      DECOMP
         <
          C1: NPVARrec2.superdeixis = omegadeixis
          A1: NPVARrec1 := NPVARrec2;
              NPVARrec1.generic := nogeneric;
              NPVARrec1.superdeixis := QPrec1.superdeixis;
          >

   SUBRULE (* 2 *)
      <T3: CNVAR{CNVARrec1}>
      <T4: CNVAR{CNVARrec2}>
 
      COMP
         <
          C1: CNVARrec1.superdeixis = QPrec1.superdeixis
          A1: CNVARrec2 := CNVARrec1;
              CNVARrec2.superdeixis := omegadeixis;

         >
      DECOMP
         <
          C1: CNVARrec2.superdeixis = omegadeixis 
          A1: CNVARrec1 := CNVARrec2;
              CNVARrec1.superdeixis := QPrec1.superdeixis;
          >


   SUBRULE (* 3 *)
      <T3: ADVP{ADVPrec1}[mu9]>
      <T4: ADVP{ADVPrec2}[mu9]>
 
      COMP
         <
          C1: ADVPrec1.superdeixis = QPrec1.superdeixis
          A1: ADVPrec2 := ADVPrec1;
              ADVPrec2.superdeixis := omegadeixis;

         >
      DECOMP
         <
          C1: ADVPrec2.superdeixis = omegadeixis
          A1: ADVPrec1 := ADVPrec2;
              ADVPrec1.superdeixis := QPrec1.superdeixis;
         >


   SUBRULE (* 4 *) (* meer dan mogelijk/wenselijk *)

      <T3: OPENADJPPROP{OPENADJPPROPrec3}
                  [subjrel/EMPTY(KEY_zeroemptykey){EMPTYrec1},
                   predrel/ADJP{ADJPrec3}[mu2] 
                  ]
      >

      <T4: ADJP{ADJPrec3}[mu2] >

 


     COMP
         <
          C1: OPENADJPPROPrec3.superdeixis = QPrec1.superdeixis
          A1: @

         >
      DECOMP
         <
          C1: true
          A1: OPENADJPPROPrec3 := COPYT_ADJPtoADJPPROP(ADJPrec3);
              OPENADJPPROPrec3.aktionsarts := [stative];
              OPENADJPPROPrec3.prosubject := true;
              OPENADJPPROPrec3.superdeixis := QPrec1.superdeixis;
         >

   SUBRULE (* 5 *)

      <T3: PREPP{PREPPrec1}[mu9]>
      <T4: PREPP{PREPPrec2}[mu9]>
 
      COMP
         <
          C1: PREPPrec1.superdeixis = QPrec1.superdeixis
          A1: PREPPrec2 := PREPPrec1;
              PREPPrec2.superdeixis := omegadeixis;
         >
      DECOMP
         <
          C1: PREPPrec2.superdeixis = omegadeixis
          A1: PREPPrec1 := PREPPrec2;
              PREPPrec1.superdeixis := QPrec1.superdeixis;
         >

SUBRULE (* 6 *) (* minder dan drie; provisional account to be replaced by
                   a rule that modifies a NUM that is the head of DETP (within
                   subgrammar DETPformation)  *)
      <T3: DETP{DETPrec1}[mu9]>
      <T4: DETP{DETPrec2}[mu9]>
 
      COMP
         <
          C1: DETPrec1.superdeixis = QPrec1.superdeixis AND
              DETPrec1.definite = indef
          A1: DETPrec2 := DETPrec1;
              DETPrec2.superdeixis := omegadeixis;
         >
      DECOMP
         <
          C1: DETPrec2.superdeixis = omegadeixis AND
              DETPrec2.definite = indef
          A1: DETPrec1 := DETPrec2;
              DETPrec1.superdeixis := QPrec1.superdeixis;
         >
>


&

\end{verbatim}
