% TRANSFORMATION TADJCONTROL4 (* subject antecedent for subject of 
                                 QP/ADVP-complement  *)
                          (* Jan is slim genoeg om op tijd te komen 
                             Jan is te slim om te laat te komen *)

                          (* aparte regels nodig voor: 
                             het is te laat om te zwemmen;
                             het boek is te zwaar om te dragen (proobj);
                             de reis is te kort om te kunnen slapen 
                             (IOEMPTY moet worden aangenomen anders kan de reis 
                              niet worden uitgesloten als subject van kunnen 
                              slapen (TADJcontrol4));
                           *)

     
< m1: ADJPFORMULA{ADJPFORMULArec1}
                 [I4:subjrel/T3,                   
                  predrel/ADJP{ADJPrec1}
                          [I9:mu2,
                           I8:rel1/N1[I1:mu1],
                           I10:mu4,
                           postadjrel/I7::SENTENCE{SENTENCErec1}
                                                     [I2:sig1,
                                                      I5:sig2,
                                                      I6:subjrel/T2,
                                                      I3:mu3
                                                     ],
                           mu6
                          ]
                 ]
> 
< m : ADJPFORMULA{ADJPFORMULArec1}
                 [I4:subjrel/T3,          
                  predrel/ADJP{ADJPrec1}
                          [I9:mu2,
                           I8:rel1/N1[I1:mu1],
                           I10:mu4,
                           postadjrel/I7::SENTENCE{SENTENCErec1}
                                                     [I2:sig1,
                                                      I5:sig2,
                                                      I3:mu3
                                                     ],
                           mu6
                          ]
                 ]
> 
    


   MATCHCONDITIONS
      <I1: EXIST(mu1, [I11:head/T1]) 
       I11: T1.cat IN [Q,ADV] 
       I2: ALL(sig1, shiftrel/..)
       I3: NOT EXIST(mu3, [shiftrel/..]) AND
           NOT EXIST(mu3, [conjrel/..])
       I4: T3.cat IN [NPVAR, CNVAR, EMPTYVAR]
       I5: ALL(sig2, conjrel/..)
       I6: T2.cat IN [NPVAR, CNVAR, NP]
       I7: SENTENCErec1 = 
                          {
                           prosubject: true
                           finiteness: infinite
                           infsort   : omteinf
                           mood      : declarative
                          }
       I8: rel1 IN [hoprel, degreemodrel]
       I9: NOT EXIST(mu2, [indobjrel/..]) AND
           NOT EXIST(mu2, [I19:rel2/T9])
      I10: NOT EXIST(mu4, [I19:rel2/T9])
      I19: (rel2 IN [voorobjrel]) AND 
           (T9.CAT IN [VARPREPP] )
      >

<
   SUBRULE  (* 0, for testing *)
      <T3:EMPTYVAR{EMPTYVARrec1}
       T2:NP{NPrec5}[head/BIGPRO{BIGPROrec1}
                          [head/BBIGPRO(KEY_zegenBIGPROkey){BBIGPROrec1}]]
      >

      <T3:EMPTYVAR{EMPTYVARrec1}>

      COMP
         <
          C1: (NPrec5 = AUX_NPONzegenBGPR) AND
              BIGPROrec1.antecedent = EMPTYVARrec1.index AND
              BIGPROrec1 = COPYT_BtoBIGPRO(BBIGPROrec1)
          A1: @
         >
      DECOMP
         <
          C1: true
          A1: NPrec5 := AUX_NPONzegenBGPR;
              BIGPROrec1 := COPYT_BtoBIGPRO(BBIGPROrec1);
              BIGPROrec1.antecedent := EMPTYVARrec1.index;
         >

   SUBRULE  (* 1 *)
      <T3:NPVAR{NPVARrec1}
       T2:NPVAR{NPVARrec2}
      >
      <T3:NPVAR{NPVARrec1}>

      COMP
         <
          C1: (NPVARrec1 = NPVARrec2 EXCEPT FOR [cases]) AND
              (NPVARrec2.cases = [Nominative])
          A1: @
         >
      DECOMP
         <
          C1: true
          A1: NPVARrec2 := NPVARrec1;
              NPVARrec2.cases := [Nominative]
         >

   SUBRULE (* 2 *)
      <T3:CNVAR{CNVARrec1}
       T2:CNVAR{CNVARrec2}
      >        
      <T3:CNVAR{CNVARrec1}>

      COMP
         <
          C1: (CNVARrec1 = CNVARrec2 EXCEPT FOR [cases]) AND
              (CNVARrec2.cases = [Nominative])
          A1: @
         >
      DECOMP
         <
          C1: true
          A1: CNVARrec2 := CNVARrec1;
              CNVARrec2.cases := [Nominative]
         >
>
