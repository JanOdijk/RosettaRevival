% RULE RDETPsuperdeixis
< m1:DETP{DETPrec1}[mu1] 
>

< m :DETP{DETPrec2}[mu1] 
>

   PARAMETERS
      <
       super : deixistype
      >

      COMP
         <
          C1: DETPrec1.superdeixis = omegadeixis AND
              super IN [presentdeixis, pastdeixis]
          A1: DETPrec2 := DETPrec1;
              DETPrec2.superdeixis := super;
         >
      DECOMP
         <
          C1: DETPrec2.superdeixis IN [presentdeixis, pastdeixis]
          A1: DETPrec1 := DETPrec2;
              DETPrec1.superdeixis := omegadeixis;
              super := DETPrec2.superdeixis;
         >

&

