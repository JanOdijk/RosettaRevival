% RULE RADJomtemod1 (* leuk om te zien, handig om mee te nemen *)

< m1: ADJPPROP{ADJPPROPrec1}[mu1,
                             predrel/ADJP{ADJPrec1}
                                     [mu2,
                                      head/I1::ADJ{ADJrec1}[mu3],
                                      mu4
                                     ]
                            ] 

 m2: SENTENCE{SENTENCErec1}[I2:mu5]
>

< m : ADJPPROP{ADJPPROPrec2}[mu1,
                             predrel/ADJP{ADJPrec2}
                                     [mu2,
                                      head/I1::ADJ{ADJrec1}[mu3],
                                      mu4,
                                      omtemodrel/SENTENCE{SENTENCErec2}[I2:mu5]
                                     ]
                            ] 
>
   MATCHCONDITIONS
      <
       I1: subjectiveadj IN ADJrec1.subcs 
       I2: EXIST(mu5, [shiftrel/..]) AND
           SENTENCErec1 = 
                          {mood     :  proobjmod
                           prosubject: true
                           finiteness: infinite
                           infsort: omteinf
                          }                           
                           
      >

      COMP
         <
          C1: SENTENCErec1.superdeixis = omegadeixis
          A1: ADJPrec2 := ADJPrec1;
              ADJPPROPrec2 := ADJPPROPrec1;
              ADJPrec2.actsubcefs := [subjectiveadj];
              ADJPPROPrec2.actsubcefs := [subjectiveadj];
              SENTENCErec2 := SENTENCErec1;
              SENTENCErec2.superdeixis := ADJPPROPrec2.superdeixis;

         >
      DECOMP
         <
          C1: SENTENCErec2.superdeixis = ADJPPROPrec2.superdeixis AND
              ADJPrec2.actsubcefs * [subjectiveadj] <> [] AND
              ADJPPROPrec2.actsubcefs * [subjectiveadj] <> []
          A1: ADJPrec1 := ADJPrec2;
              SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.superdeixis := omegadeixis;          
              ADJPPROPrec1 := ADJPPROPrec2;
              ADJPrec1.actsubcefs := ADJrec1.subcs;
              ADJPPROPrec1.actsubcefs := ADJrec1.subcs;
              (* this assignment is provisional;
                 reduction of .actsubcefs to [] not yet realised 
                 in M-grammar; cf also the exception statements 
                 in mrules32
              *)
         >
&

\end{verbatim}
