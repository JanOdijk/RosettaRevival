%
% RULE Rlatenmood
< m1:I6::CLAUSE{CLAUSErec1}
           [ subjrel/NP{NPrec1}[mu6],
             mu1,
             predrel/VERBP{VERBPrec1}
                    [ mu2,
                      head/T1,
                      I2:mu3
                    ],
             mu4
           ] 
>

< m :I7::SENTENCE{SENTENCErec1}
             [ subjrel/NP{NPrec1}[mu6],
               mu1,
               predrel/VERBP{VERBPrec1}
                      [ mu2,
                        head/VERB{VERBrec3}
                            [ head/VERB{VERBrec5}
                                  [head/SUBVERB{SUBVERBrec5}
                                   [head/BVERB(KEY_latenbverbkey){BVERBrec5}] 
                                  ],
                              adjoinrel/T2
                            ],
                        I2:mu3
                      ],
               mu4
             ] 
>

   MATCHCONDITIONS
      <
       I2 : ALL(mu3, I12:rel2/..)
       I6 : CLAUSErec1.finiteness = finite  AND
            CLAUSErec1.deixis = presentdeixis AND
            CLAUSErec1.modus  = imperative
       I7 : SENTENCErec1.deixis = presentdeixis AND
            SENTENCErec1 =
		   {
                    modus      : indicative
                    finiteness : finite
 		    mood       : latenmood
		    senttype   : mainclause
                    conjkey    : 0
                    adverbial  : false
                    temporal   : false
		    conjclass  : omegaTimeAdvClass
		    conjdeixis : omegadeixis
		    conjaspect : omegaAspect
		    conjretro  : false
		   }


       I12: rel2 IN AUX_postverbrels 
      >

<
   SUBRULE
      < T1: I1::VERB{VERBrec1}[mu5]>
      < T2: I3::VERB{VERBrec2}[mu5]>

   MATCHCONDITIONS
      <
       I1: VERBrec1.status IN [bareV, partV, prepV]
       I3: VERBrec2.status IN [bareV, partV, prepV]
      >


      COMP
         <
          C1: VERBrec1.modus = omegamodus AND
              1 IN VERBrec1.persons       AND
              VERBrec1.number = plural    AND
              NPrec1.person = 1           AND
              NPrec1.number = plural      
          A1: VERBrec2 := VERBrec1;
              VERBrec2.modus := infinitive;
              VERBrec2.persons := [];
              VERBrec2.number := omeganumber;
              SUBVERBrec5 := COPYT_bverbtosubverb(BVERBrec5);
              SUBVERBrec5.lastaffix := noaffix;
              VERBrec5 := COPYT_subverbtoverb(SUBVERBrec5);
              VERBrec5 :=
		  {
		  number    : plural
		  persons   : [1]
		  eORenForm : NoForm
		  modus     : indicative
		  tense     : presenttense
		  };
              VERBrec3 := VERBrec5;
              VERBrec3.status := VV;
              SENTENCErec1 := COPYT_clausetosentence(CLAUSErec1);
              SENTENCErec1 :=
		   {
 		    mood     : latenmood
		    senttype : mainclause
                    modus    : indicative
		   }
            (* other attributes retain default values *)
         >

      DECOMP
         <
          C1: VERBrec2.modus   = infinitive  AND
              VERBrec2.persons = []          AND
              VERBrec2.number  = omeganumber AND
              NPrec1.person    = 1           AND
              NPrec1.number    = plural      AND 
              SUBVERBrec5 = COPYT_bverbtosubverb(BVERBrec5) AND
              SUBVERBrec5.lastaffix = noaffix               AND
              VERBrec5 = COPYT_subverbtoverb(SUBVERBrec5)   AND
              VERBrec5 =
		  {
		  number    : plural
		  persons   NI 1
		  eORenForm : NoForm
		  modus     : indicative
		  tense     : presenttense
		  }                                          AND
              VERBrec3 = VERBrec5 EXCEPT FOR [status, foundauxes, synvpefs2] AND
              VERBrec3.status = VV
          A1: VERBrec1         := VERBrec2;
              VERBrec1.modus   := omegamodus;
              VERBrec1.persons := [1];
              VERBrec1.number  := plural;
              CLAUSErec1 := COPYT_sentencetoclause(sentencerec1);
              CLAUSErec1.modus := imperative;
         >

   SUBRULE
      < T1: I1::VERB{VERBrec1}[head/VERB{VERBrec4}[mu7], mu5]>
      < T2: I3::VERB{VERBrec2}[head/VERB{VERBrec6}[mu8], mu5]>

   MATCHCONDITIONS
      <
       I1: VERBrec1.status IN [VV]
       I3: VERBrec2.status IN [VV]
      >


      COMP
         <
          C1: VERBrec1.modus = omegamodus AND
              1 IN VERBrec1.persons       AND
              VERBrec1.number = plural    AND
              NPrec1.person = 1           AND
              NPrec1.number = plural      AND
              VERBrec4.status = bareV     AND
              VERBrec4.modus = omegamodus
          A1: VERBrec2 := VERBrec1;
              VERBrec2.modus := infinitive;
              VERBrec2.persons := [];
              VERBrec2.number := omeganumber;
              VERBrec6 := VERBrec4;
              VERBrec6.modus := infinitive;
              SUBVERBrec5 := COPYT_bverbtosubverb(BVERBrec5);
              SUBVERBrec5.lastaffix := noaffix;
              VERBrec5 := COPYT_subverbtoverb(SUBVERBrec5);
              VERBrec5 :=
		  {
		  number    : plural
		  persons   : [1]
		  eORenForm : NoForm
		  modus     : indicative
		  tense     : presenttense
		  };
              VERBrec3 := VERBrec5;
              VERBrec3.status := VV;
              SENTENCErec1 := COPYT_clausetosentence(CLAUSErec1);
              SENTENCErec1 :=
		   {
 		    mood     : latenmood
		    senttype : mainclause
                    modus    : indicative
		   }
            (* other attributes retain default values *)
         >

      DECOMP
         <
          C1: VERBrec2.modus   = infinitive  AND
              VERBrec2.persons = []          AND
              VERBrec2.number  = omeganumber AND
              VERBrec6.modus   = infinitive  AND
              VERBrec6.status  = VV          AND
              NPrec1.person    = 1           AND
              NPrec1.number    = plural      AND 
              SUBVERBrec5 = COPYT_bverbtosubverb(BVERBrec5) 
                   EXCEPT FOR [lastaffix]                   AND
              SUBVERBrec5.lastaffix = noaffix               AND
              VERBrec5 = COPYT_subverbtoverb(SUBVERBrec5)   AND
              VERBrec5 =
		  {
		  number    : plural
		  persons   : [1]
		  eORenForm : NoForm
		  modus     : indicative
		  tense     : presenttense
		  }                                          AND
              VERBrec3 = VERBrec5 EXCEPT FOR [status, foundauxes, synvpefs2] AND
              VERBrec3.status = VV
          A1: VERBrec1         := VERBrec2;
              VERBrec1.modus   := omegamodus;
              VERBrec1.persons := [1];
              VERBrec1.number  := plural;
              CLAUSErec1 := COPYT_sentencetoclause(sentencerec1);
              CLAUSErec1.modus := imperative;
              VERBrec4 := VERBrec6;
              VERBrec4.modus := omegamodus;
         >



>
&

\end{verbatim}
