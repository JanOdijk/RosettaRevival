              EMPTYVARrec2 = EMPTYVARrec1 AND
              QUOTE_deixismax(CLAUSErec1.deixis,CLAUSErec1.superdeixis,
                    OPENADVPPROPrec1.superdeixis)
          A1: @
         >
      DECOMP
         <
          C1: true
          A1: WBONUS -1
              CLAUSEVARrec1 := COPYT_CLAUSEtovar(CLAUSErec1);
              CLAUSEVARrec1.index := LEVEL;
              OPENADVPPROPrec1 := COPYT_advptoprop (ADVPrec1);
              OPENADVPPROPrec1 :=
                     { 
                       aktionsarts   : [stative]
                       PROsubject   : true
                     };
              EMPTYVARrec2 := EMPTYVARrec1 ;
              QUOTE_assdeixismax(CLAUSErec1.deixis,CLAUSErec1.superdeixis,
                    OPENADVPPROPrec1.superdeixis);
         >
>


