\newpage
\begin{verbatim}
% TRANSFORMATION Tconjdel1
< m1:SENTENCE{SENTENCErec1}
             [ shiftrel/T1,
               conjrel/CONJ(KEY_ofCONJkey){CONJrec1},
               I1:mu1
             ]

>

< m : SENTENCE{SENTENCErec2}
             [ shiftrel/T1,
               I2:sig1,
               I1:mu1
             ]

>

   MATCHCONDITIONS
      <
       I1: NOT EXIST(mu1, [conjrel/..])
       I2: ALL(sig1, conjrel/CONJ(KEY_ofCONJkey){CONJrec2})
      >

<
   SUBRULE
      < >
      <sig1:[] >

      COMP
         <
          C1: SENTENCErec1.mood =whinterrogative AND
              SENTENCErec1.finiteness = finite AND
              SENTENCErec1.conjkey = KEY_ofCONJkey AND
              SENTENCErec1.senttype = subordinateclause
          A1: SENTENCERec2 := SENTENCErec1;
              SENTENCErec2.conjkey := 0;
      
         >
      DECOMP
         <
          C1: SENTENCErec2.mood =whinterrogative  AND
              SENTENCErec2.finiteness = finite    AND
              SENTENCERec2.conjkey = 0            AND
              SENTENCErec2.senttype = subordinateclause
          A1: SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.conjkey := KEY_ofCONJkey
         >

   SUBRULE
      < >
      <sig1:[conjrel/CONJ(KEY_ofCONJkey){CONJrec3}] >

(* no COMP part *)

      DECOMP
         <
          C1: SENTENCErec2.mood =whinterrogative AND
              SENTENCErec2.finiteness = finite
          A1: SENTENCErec1 := SENTENCErec2
         >

>

&

\end{verbatim}
