% RULE RUtt1
< m1:T1
>

< m :UTT{UTTrec1}[head/T1]
>

<
   SUBRULE
      <T1:SENTENCE{SENTENCErec1}[mu1] >
      <T1:SENTENCE{SENTENCErec1}[mu1] >


      COMP
         <
          C1: SENTENCErec1.senttype = mainclause  AND
              (SENTENCErec1.finiteness = finite  OR
               SENTENCErec1.mood = WHinterrogative OR
               SENTENCErec1.mood = imperativemood
              )                                         AND
              QUOTE_polarityok(T1)                      AND
              (NOT QUOTE_varsintree(T1))                AND
              (NOT QUOTE_accmenzeintree(T1))
          A1: @
           >
      DECOMP
         <
          C1: SENTENCErec1.senttype = mainclause AND
              (SENTENCErec1.finiteness = finite  OR
               SENTENCErec1.mood = WHinterrogative  OR
               SENTENCErec1.mood = imperativemood
              ) AND
              QUOTE_polarityok(T1)                     AND
              (NOT QUOTE_accmenzeintree(T1))
          A1: @
         >

>
&

\end{verbatim}

