              SENTENCErec5.superdeixis = omegadeixis AND
              SENTENCErec6.superdeixis = omegadeixis AND
              SENTENCErec3.coord = simplecoord       AND
              SENTENCErec5.coord = nocoord           AND
              SENTENCErec6.coord = nocoord           AND 
              SENTENCErec5 =
