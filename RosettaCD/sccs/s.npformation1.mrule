% RULE RNPformation5      (*  .. + NOUN + posrel --> detrel + [..  + NOUN] *)
< m1: CN{CNrec1}[mu1,
                 head/NOUN{NOUNrec3}[mu6],
                 mu7,
                 posrel/T1
                ]
>

< m : NP{NPrec2}[detrel/T2,
                 mu1,
                 head/CN{CNrec1}[mu1,
                                 head/NOUN{NOUNrec3}[mu6],
                                 mu7
                                 ]
                ]
>


<
   (* 1 *)      (* boek mijn vader --> mijn vaders boek *)
   SUBRULE
      < 
        T1:NP{NPrec1}[mu2, 
                      head/CN{CNrec2}[mu3,head/NOUN{NOUNrec1}[mu4]]]
      >

      < 
        T2:NP{NPrec3}[mu2,
                      head/CN{CNrec3}[mu3,head/NOUN{NOUNrec2}[mu4]]] 
      >


      COMP
         <
          C1: CNrec2.possgeni = true AND
              NPrec1.cases = [nominative] AND
              CNrec2.cases = [nominative] AND
              NOUNrec1.geni = false
          A1: NPrec3 := NPrec1;
              NPrec3.cases := [genitive];
              CNrec3 := CNrec2;
              CNrec3.cases := [genitive];
              NOUNrec2 := NOUNrec1;
              NOUNrec2.geni := true;

              NPrec2 := { posspred          : true
                          definite          : def
                          possnietnp        : false
                          mood              : NPrec3.mood 
                          number            : NOUNrec3.number
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                         }  
             (* The other attributes have default value *)
         >

      DECOMP
         <
           C1: CNrec3.possgeni = true AND
               NOUNrec2.geni = true AND
               NPrec3.cases = [genitive] AND
               CNrec3.cases = [genitive] AND
               
               NPrec2 = { posspred          : true
                          definite          : def 
                          possnietnp        : false
                          mood              : NPrec3.mood 

                          number            : NOUNrec3.number
                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          actcomas          : CNrec1.posscomas
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP
                          cases      : [nominative]
                         }  

          A1:  NOUNrec1 := NOUNrec2;
               NOUNrec1.geni := false;
               CNrec2 := CNrec3;
               CNrec2.cases := [Nominative];
               NPrec1 := NPrec3;
