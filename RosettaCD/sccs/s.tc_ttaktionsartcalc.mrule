% TRANSFORMATION taktaccomplishment1
< m1:VERBPPROP{VERBPPROPrec1}
              [mu1,
               predrel/VERBP{VERBPrec1}
                      [mu2,
                       I1: rel1/T1,
                       mu3,
                       head/VERB{VERBrec1}[mu4] 
                      ] 
              ]
>

< m :VERBPPROP{VERBPPROPrec2}
              [mu1,
               predrel/VERBP{VERBPrec1}
                      [mu2,
                       I1: rel1/T1,
                       mu3,
                       head/VERB{VERBrec1}[mu4] 
                      ] 
              ]

>
   MATCHCONDITIONS
      < I1: (rel1 IN [objrel, prepobjrel]) AND (T1.CAT IN [NPVAR(* , VARPREPP*)])
      >
      COMP
         <
          C1: (durativeclass IN VERBrec1.classes) AND
              (VERBrec1.thetavp IN [vp000, vp100, vp120, vp123]) AND
              (T1.REC.specq = yesspec) AND
              (VERBPPROPrec1.aktionsarts = [])
          A1: VERBPPROPrec2 := VERBPPROPrec1;
              VERBPPROPrec2.aktionsarts := [accomplishment];
         >
      DECOMP
         <
          C1: (durativeclass IN VERBrec1.classes) AND
              (VERBrec1.thetavp IN [vp000, vp100, vp120, vp123]) AND
              (T1.REC.specq = yesspec) AND
              (accomplishment IN VERBPPROPrec2.aktionsarts)
          A1: VERBPPROPrec1 := VERBPPROPrec2;
              VERBPPROPrec1.aktionsarts := [];

         >

&
\end{verbatim}
