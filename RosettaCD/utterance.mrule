\newpage
\begin{verbatim}
% RULE Ruttadvpprop
< m1: CLOSEDADVPPROP{CLOSEDADVPPROPrec1}
              [subjrel/I1::EMPTYVAR{EMPTYVARrec1}, predrel/T1]
SUBST:
  m2: EMPTY(KEY_zeroemptykey){EMPTYrec1}
>

< m :UTT{UTTrec1}[head/T2]
>


   MATCHCONDITIONS
      <
       I1: EMPTYVARrec1.index = LEVEL
      >


   PARAMETERS
      <
       LEVEL
      >

<
   SUBRULE
      < T1: ADVP{ADVPrec1}[mu1] >
      < T2: ADVP{ADVPrec1}[mu1] >

      COMP
         <
          C1: ADVPrec1.actsubcefs - [locadv, tempadv] <> [] AND
              QUOTE_polarityOK(T1)                 AND
              (NOT QUOTE_varsintree(T1))           AND
              CLOSEDADVPPROPrec1 = COPYT_ADVPtoPROP(ADVPrec1) EXCEPT FOR
                     [ aktionsarts , superdeixis , PROsubject ]   AND
              CLOSEDADVPPROPrec1 = 
	      {
	      aktionsarts NI stative
	      superdeixis : presentdeixis
	      PROsubject  : false
	      } AND
              EMPTYVARrec1 = 
	      {
	      person : 2
	      number : singular
	      }
          A1: @
         >
      DECOMP
         <
          C1: ADVPrec1.actsubcefs - [locadv, tempadv] <> [] AND
              QUOTE_polarityOK(T2)
          A1: CLOSEDADVPPROPrec1 := COPYT_ADVPtoPROP(ADVPrec1);
              CLOSEDADVPPROPrec1 := 
	      {
	      aktionsarts : [stative]
	      superdeixis : presentdeixis
	      PROsubject  : false
	      };
              EMPTYVARrec1 :=
	      {
	      person : 2
	      number : singular
	      };
              EMPTYVARrec1.index  := LEVEL;
                       >

>
&

\end{verbatim}
