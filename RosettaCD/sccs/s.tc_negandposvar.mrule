% RULE Rnegvar3 (* meltnegrel under S *)
< m1:CLAUSE{CLAUSErec1}
        I1:[ I3:mu1,
             I7:rel1/T2,
             I2:mu2,
             predrel/VERBP{VERBPrec1}[I5:mu4],
             I6:mu5 
           ] 
>

< m :CLAUSE{CLAUSErec1}
        I1:[ I3:mu1,
             meltnegrel/NEGVAR{NEGVARrec1},
             I4:rel1/T2,
             I2:mu2,
             predrel/VERBP{VERBPrec1}[I5:mu4],
             I6:mu5 
           ] 
>

   MATCHCONDITIONS
      <
       I1 : RIGHTTOLEFT
       I2 : NOT EXIST(mu2,[I13:rel2/..]) AND NOT EXIST(mu2,[sentadvrel/..])
       I3 : NOT EXIST(mu1,[I13:rel2/..])
       I4 : T2.CAT IN [RADVP,NPVAR,ADVPVAR,VARPREPP] AND QUOTE_possnietxp(T2) 
       I5 : NOT EXIST(mu4, [meltnegrel/..])
       I6 : NOT EXIST(mu5, [meltnegrel/..])
       I7 : relpar = rel1 AND
            T2.CAT IN [RADVP,NPVAR,ADVPVAR,VARPREPP] AND QUOTE_possnietxp(T2) 
       I13: rel2 IN [posrel, negrel]
      >

   PARAMETERS
      <
       relpar: synrel
      >

      COMP
         <
          C1: rel1 IN [tempadvrel, preadvrel, postsubjrel, locadvrel, 
                         erposrel, subjrel, shiftrel]
          A1: @
         >
      DECOMP
         <
          C1: rel1 IN [tempadvrel, preadvrel, postsubjrel, locadvrel, 
                         erposrel, subjrel, shiftrel]
          A1: relpar := rel1;
         >


&

