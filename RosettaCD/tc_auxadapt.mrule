           [ mu1,
             predrel/VERBP{VERBPrec1}
                    [ I4:mu2,
                      head/I3::VERB{VERBrec1}
                          [ head/SUBVERB{SUBVERBrec1}
                                [ partrel/PART{PARTrec1},
                                  head/SUBVERB{SUBVERBrec2}[mu3] 
                                ]
                          ],
                      auxrel/I2::VERB{VERBrec2}[mu4],
                      mu5 
                    ],
             mu6 
           ]
>

< m :CLAUSE{CLAUSErec1}
           [ mu1,
             predrel/VERBP{VERBPrec1}
                    [ I4:mu2,
                      partrel/PART{PARTrec1},
                      head/I1::VERB{VERBrec3}
                             [ auxrel/I2::VERB{VERBrec2}[mu4],
                               head/I5::VERB{VERBrec4}
                                      [ head/SUBVERB{SUBVERBrec2}[mu3]]
                             ],
                      mu5 
                    ],
             mu6 
           ]
>

   MATCHCONDITIONS
      <
       I1: VERBrec3.status IN [auxV, teAuxV]
       I3: VERBrec1.modus = pastpart AND VERBrec1.status = partV
       I2: VERBrec2.subc IN [hebbenaux, zijnaux, wordenaux] AND
           VERBrec2.status IN [bareV, teV]
       I5: VERBrec4.modus = pastpart AND VERBrec4.status = bareV
       I4: NOT EXIST(mu2, [partrel/PART{}])
      >

     (* no COMP part *)

      DECOMP
         <
          C1: VERBrec3 =
		   {
		   conjclasses   : VERBrec2.conjclasses
		   particle      : VERBrec4.particle
		   number        : VERBrec2.number        
		   persons       : VERBrec2.persons       
		   eORenForm     : VERBrec2.eORenForm     
		   modus         : VERBrec2.modus         
		   tense         : VERBrec2.tense         
		   possvoices    : VERBrec4.possvoices    
		   reflexivity   : VERBrec4.reflexivity   
		   synvps        : VERBrec4.synvps        
		   thetavp       : VERBrec4.thetavp       
		   CaseAssigner  : VERBrec4.CaseAssigner  
		   subc          : VERBrec4.subc          
		   perfauxs      : VERBrec4.perfauxs      
		   prepkey1      : VERBrec4.prepkey1      
		   prepkey2      : VERBrec4.prepkey2      
		   controller    : VERBrec4.controller    
		   verbraiser    : VERBrec4.verbraiser    
		   IPP           : VERBrec4.IPP           
                   classes       : VERBrec4.classes
		   }
              C11: VERBrec2.status = bareV AND
                   VERBrec3.status = AuxV
              A11: @
              C12: VERBrec2.status = teV AND
                   VERBrec3.status = teAuxV
              A12: @
          A1: SUBVERBrec1 := SUBVERBrec2;
              SUBVERBrec1.lastaffix := partaffix;
              VERBrec1 := VERBrec4;
              VERBrec1.status := partV;
         >



&

\end{verbatim}
