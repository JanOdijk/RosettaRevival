% RULE RStartexistential1
< m1: NPVAR{NPVARrec1} 
>

< m : NPPROP{NPPROPrec1}[ objrel/NPVAR{NPVARrec1} ] 
>

      COMP
         <
          C1: NPVARrec1.definite = indef
          A1: NPPROPrec1.aktionsarts := [stative]; 
              (* NPPROPrec retains its default values *)
         >
      DECOMP
         <
          C1: (NPVARrec1.definite = indef) AND
              (NPPROPrec1 = NPPROPrecdef EXCEPT FOR [aktionsarts]) AND
              (NPPROPrec1.aktionsarts = [stative])
          A1: @
         >

&

