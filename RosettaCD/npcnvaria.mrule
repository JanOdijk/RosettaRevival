% RULE RNOUNargmod1          (* provisional: varinsertie (alleen NPVAR) 
                                             + caseassignment *)
< m1: CN{CNrec1}[  
                 head/I1::NOUN{NOUNrec1}[mu2]
                ] 
  m2: NPVAR{NPVARrec1}
>

< m : CN{CNrec1}[
                head/I1::NOUN{NOUNrec1}[mu2],
                prepobjrel/VARPREPP{VARPREPPrec1}
                         [head/PREP(NOUNrec1.prepkey){PREPrec1},
                          objrel/NPVAR{NPVARrec2}
                         ]
               ]
>

   MATCHCONDITIONS
      <
       I1: synPREPNP IN NOUNrec1.nounpatterns AND
           NOUNrec1.possgeni = false
      >

     
      COMP
         <
          C1: NPVARrec1.cases = [Nominative]
          A1: NPVARrec2 := NPVARrec1;
              NPVARrec2.cases := [dative, accusative];
              VARPREPPrec1 := VARPREPPRECDEF;
              VARPREPPrec1.headkey := PREPrec1.key;
              VARPREPPrec1.synppefs := [synNP];
              VARPREPPrec1.thetapp := PREPrec1.thetapp;
              VARPREPPrec1.actsubcefs := PREPrec1.subcs;
              VARPREPPrec1.specQ := NPVARrec2.specQ;
         >
      DECOMP
         <
          C1: [dative, accusative] * NPVARrec2.cases <> [] AND
              VARPREPPrec1 = VARPREPPRECDEF EXCEPT FOR [headkey, synppefs,
              thetapp, actsubcefs, specQ] AND
              synNP IN VARPREPPrec1.synppefs AND
              VARPREPPrec1.headkey = PREPrec1.key AND
              VARPREPPrec1.thetapp = PREPrec1.thetapp AND
              VARPREPPrec1.actsubcefs* PREPrec1.subcs <> [] AND
              VARPREPPrec1.specQ = NPVARrec2.specQ
          A1: NPVARrec1 := NPVARrec2;
              NPVARrec1.cases := [Nominative];
         >

&

\end{verbatim}
