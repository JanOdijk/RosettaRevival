case $# in
0) echo "Usage: inspect componentname filename"; exit 1;;
1) echo "Usage: inspect componentname filename"; exit 1;;
2) ;;
*) echo "Usage: inspect componentname filename"; exit 1;;
esac
savedir=`pwd`
cd $RBS_USER_ROOT$RBS_USER$RBS_RELEASE
# 
if ( test -s  $RBS_ARCHIVE$RBS_RELEASE/$1/$2 )
then cp $RBS_ARCHIVE$RBS_RELEASE/$1/$2 $2$$.rbs
     ($RBS_EDITOR $2$$.rbs ; rm -f  $2$$.rbs ) &
else echo "INSPECT: File does not exist in archive directory!"
fi
#
cd $savedir
