% RULE Rmoodprespart
< m1:CLAUSE{CLAUSErec1}
           [ I1:subjrel/T1,
             mu1,
             predrel/VERBP{VERBPrec1}
                    [ mu2,
                      head/I2::VERB{VERBrec1}[mu3] 
                    ] 
           ] 
>

< m :SENTENCE{SENTENCErec1}
           [ I1:subjrel/T1,
             mu1,
             predrel/VERBP{VERBPrec1}
                    [ mu2,
                      head/I3::VERB{VERBrec2}[mu3] 
                    ] 
           ] 
>

   MATCHCONDITIONS
      <
       m1: CLAUSErec1.aspect = imperfective
       m:  SENTENCErec1.aspect = imperfective
       I1: T1.CAT IN [NPVAR, CNVAR]
       I2: VERBrec1.status IN [bareV, partV, prepV]
       I3: VERBrec2.status IN [bareV, partV, prepV]
      >

      COMP
         <
          C1: CLAUSErec1.PROsubject = true AND
              VERBrec1.modus = omegamodus AND
              CLAUSErec1.modus = omegamodus AND
              CLAUSErec1.VRcompl =false
          A1: SENTENCErec1 := COPYT_clauseTOsentence(CLAUSErec1);
              SENTENCErec1 :=
		   {
 		    mood       : anterelative
		    senttype   : subordinateclause
                    modus      : prespart
                    finiteness : infinite
                    infsort    : omegainf
                    adverbial  : false
                    temporal   : false
                    conjclass  : omegaTimeadvClass
                    conjdeixis : omegadeixis
                    conjaspect : omegaAspect
                    conjretro  : false
		   };
              VERBrec2 := VERBrec1;
              VERBrec2.modus := prespart;

         >
      DECOMP
         <
          C1: SENTENCErec1 =
		   {
                    VRcompl    : false
                    PROsubject : true
 		    mood       : anterelative
		    senttype   : subordinateclause
                    modus      : prespart
                    finiteness : infinite
                    infsort    : omegainf
                    adverbial  : false
                    temporal   : false
                    conjclass  : omegaTimeadvClass
                    conjdeixis : omegadeixis
                    conjaspect : omegaAspect
                    conjretro  : false
		   }                                         AND

              VERBrec2.modus = prespart
          A1: CLAUSErec1 := COPYT_sentenceTOclause(SENTENCErec1);
              CLAUSErec1.modus := omegamodus;
              VERBrec1 := VERBrec2;
              VERBrec1.modus := omegamodus;
         >


&

\end{verbatim}
