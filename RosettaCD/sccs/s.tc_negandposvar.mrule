% RULE RPOSVAR (* wel *)
< m1: CLAUSE{CLAUSErec1}
         I1:[ I2:mu1,
              I7:mu2,
              predrel/VERBP{VERBPrec1}[I5:mu3],
              mu4  
            ]
>

< m :CLAUSE{CLAUSErec1}
            [ I4:mu1,
              posrel/POSVAR{POSVARrec1},
              I3:mu2,
              predrel/VERBP{VERBPrec1}[I5:mu3],
              I6:mu4  
            ]
>

   MATCHCONDITIONS
      <
       I1 : RIGHTTOLEFT
       I2 : ALL(mu1, I12:rel1/..) AND NOT EXIST(mu1, [I13:rel2/..])
       I3 : NOT EXIST(mu2, [I14:rel2/..]) 
       I4 : NOT EXIST(mu1, [I13:rel2/..])
       I5 : NOT EXIST(mu3, [I14:rel2/..])
       I6 : NOT EXIST(mu4, [I14:rel2/..])
       I7 : NOT EXIST(mu2, [I14:rel2/..])  AND
            NOT EXIST(mu2, [I12:rel1/..])
       I12: rel1 IN [ leftdislocrel, shiftrel, subjrel, erqrel, erposrel, 
                      preadvrel, sentadvrel, defRadvrel, hetclrel, firstclrel,
                      secondclrel, reflrel, tempadvrel]
       I13: rel2 IN [posrel, negrel]  
       I14: rel2 IN [posrel, negrel, meltnegrel]
      >


      COMP
         <
          C1: true
          A1: @
         >
      DECOMP
         <
          C1: true
          A1: @
         >



&

\end{verbatim}

