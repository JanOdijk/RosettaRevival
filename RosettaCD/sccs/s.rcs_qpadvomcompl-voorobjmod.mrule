\begin{verbatim}
 RULE RADVomtemod

< m1: ADVP{ADVPrec1}[mu1,
                     head/ADV{ADVrec1}[mu2]
                    ]
  m2: I2::SENTENCE{SENTENCErec1}[mu3]
>

< m : I1::ADVP{ADVPrec2}
              [mu1,
               head/ADV{ADVrec1}[mu2],
               omcomplrel/I2::SENTENCE{SENTENCErec2}[mu3]
              ]
>


   MATCHCONDITIONS
      <
       m1: (ADVPrec1.thanascompl = false) AND
           (ADVPrec1.thanas = omcompl) 
       I1: (ADVPrec2.thanas = omcompl) AND
           (ADVPrec2.thanascompl = true)
       I2: SENTENCErec2 = 
                   { prosubject :  true
                     mood       :  whmod
                     infsort    :  omteinf
                     finiteness :  infinite
                   }
      >

      COMP
         <
          C1: SENTENCErec1.superdeixis = ADVPrec1.superdeixis
          A1: ADVPrec2 := ADVPrec1;
              ADVPrec2.thanascompl := true;
              SENTENCErec2 := SENTENCErec1;
              SENTENCErec2.superdeixis := omegadeixis;
         >
      DECOMP
         <
          C1: SENTENCErec2.superdeixis = omegadeixis
          A1: ADVPrec1 := ADVPrec2;
              ADVPrec1.thanas := omcompl;
              ADVPrec1.thanascompl := false;
              SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.superdeixis := ADVPrec1.superdeixis;
         >


&

\end{verbatim}
