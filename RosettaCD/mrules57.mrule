\begin{verbatim}
%
%TRANSFORMATION TPOSSADJSPELLING1

< m1: VERBPPROP{VERBPPROPREC1}[mu10,
                               I1:subjrel/T1,
                               mu1,
                               predrel/VERBP{VERBPrec1}
                                 [mu2,
                                  objrel/NP{NPrec1}
                                     [I2:detrel/T2,
                                      mu4
                                     ],
                                  mu5,
                                  head/VERB{VERBrec1}[mu3]
                                 ],
                               mu11
                              ]
>

< m : VERBPPROP{VERBPPROPREC1}[mu10,
                               I1:subjrel/T1,
                               mu1,
                               predrel/VERBP{VERBPrec1}
                                 [mu2,
                                  objrel/NP{NPrec1}
                                     [detrel/POSSADJ{POSSADJrec1}
                                           [head/BPERSPRO(KEY1){BPERSPROrec1} 
                                           ],
                                      mu4
                                     ],
                                  mu5,
                                  head/VERB{VERBrec1}[mu3]
                                 ],
                               mu11
                              ]
>

  MATCHCONDITIONS
   <
     I1: T1.CAT IN [NPVAR,CNVAR]
     I2: T2.CAT IN [NPVAR,CNVAR]
   >
<
   SUBRULE
      < T1:NPVAR{NPVARrec1}
        T2:NPVAR{NPVARrec2}
      >
      < T1:NPVAR{NPVARrec1}
      >


      COMP
         <
          C1:  NPVARREC1 = NPVARREC2 EXCEPT FOR [cases] AND
               NPVARREC2.cases = [genitive]           
               C11: NPVARREC1.person = 3               AND
                    NPVARREC1.number = singular        AND
                    (mascgender IN NPVARREC1.genders    OR
                      (neutgender IN NPVARREC1.genders   AND
                       masculine IN NPVARrec1.sexes)
                    )
               A11: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_hijBPERSPROkey;
               C12: NPVARREC1.person = 3               AND 
                    (* het kind => zijn *)
                    NPVARREC1.number = singular        AND
                    neutgender IN NPVARrec1.genders    AND
                    NPVARrec1.sexes = []
               A12: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_hijBPERSPROkey;
               C13: NPVARREC1.person = 1               AND
                    NPVARREC1.number = singular        AND
                    NPVARREC1.genders = [omegagender]  AND
                    NPVARREC1.animate = yesAnimate 
               A13: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_ikBPERSPROkey;
               C14: NPVARREC1.person = 2               AND (*je,jullie,jegen*)
                    NPVARREC1.genders = [omegagender]  AND
                    NPVARREC1.animate = yesAnimate
               A14: POSSADJREC1.mood := declxpmood;
                    POSSADJREC1.reduced := true;
                    KEY1 := KEY_jijBPERSPROkey;
               C16: NPVARREC1.person = 1               AND
                    NPVARREC1.number = plural          AND
                    NPVARREC1.genders = [omegagender]  AND
                    NPVARREC1.animate = yesAnimate 
                    C161: (mascgender IN NPrec1.genders OR
                           femgender IN NPrec1.genders)
                    A161: POSSADJREC1.EOrEnForm := eForm
               A16: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_wijBPERSPROkey;
               C17: NPVARREC1.person = 3               AND
                    NPVARREC1.number = singular        AND
                    (femgender IN NPVARREC1.genders    OR
                      (neutgender IN NPVARREC1.genders   AND
                       feminine IN NPVARrec1.sexes)
                    )
               A17: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_zijBPERSPROkey1;
               C18: NPVARREC1.person = 3               AND
                    NPVARREC1.number = plural
               A18: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_zijBPERSPROkey2;
               C19: NPVARREC1.person = 4               AND
                    NPVARREC1.number = singular
               A19: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_uBPERSPROkey;
               C20: NPVARREC1.person = 5               AND
                    NPVARREC1.number = singular
               A20: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_uBPERSPROkey;
               C21: NPVARREC1.mood = wh                AND
                    NPVARREC1.genders = [omegagender]  AND
                    NPVARREC1.sexes = [masculine]      AND
                    NPVARREC1.number = singular
               A21: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_hijBPERSPROkey;
               C22: NPVARREC1.mood = wh                AND
                    NPVARREC1.genders = [omegagender]  AND
                    NPVARREC1.sexes = [feminine]       AND
                    NPVARREC1.number = singular
               A22: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_zijBPERSPROkey1;
               C23: NPVARREC1.mood = wh                AND
                    NPVARREC1.genders = [mascgender]   AND
                    NPVARREC1.sexes = []               AND
                    NPVARREC1.number = singular
               A23: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_hijBPERSPROkey;
          A1:  @
         >

      DECOMP
         <
          C1: POSSADJREC1.mood = declxpmood
               C11: NPVARREC1.person = 3               AND
                    NPVARREC1.number = singular        AND
                    (mascgender IN NPVARREC1.genders    OR
                      (neutgender IN NPVARREC1.genders   AND
                       masculine IN NPVARrec1.sexes)
                    )                                  AND
                    KEY1 = KEY_hijBPERSPROkey
               A11: @ 
               C12: NPVARREC1.person = 3               AND
                    NPVARREC1.number = singular        AND
                    neutgender IN NPVARrec1.genders    AND
                    NPVARrec1.sexes = []               AND
                    KEY1 = KEY_hijBPERSPROkey
               A12: @
               C13: NPVARREC1.person = 1               AND
                    NPVARREC1.number = singular        AND
                    NPVARREC1.genders = [omegagender]  AND
                    NPVARREC1.animate = yesAnimate     AND
                    KEY1 = KEY_ikBPERSPROkey
               A13: @
               C14: NPVARREC1.person = 2               AND (*je,jullie,jegen*)
                    NPVARREC1.genders = [omegagender]  AND
                    NPVARREC1.animate = yesAnimate     AND
                    POSSADJREC1.reduced = true         AND
                    KEY1 = KEY_jijBPERSPROkey
               A14: @
               C16: NPVARREC1.person = 1               AND
                    NPVARREC1.number = plural          AND
                    NPVARREC1.genders = [omegagender]  AND
                    NPVARREC1.animate = yesAnimate     AND
                    KEY1 = KEY_wijBPERSPROkey
                    C161: (mascgender IN NPrec1.genders OR
                           femgender IN NPrec1.genders) AND
                          POSSADJREC1.EOrEnForm = eForm
                    A161: @
               A16: @
               C17: NPVARREC1.person = 3               AND
                    NPVARREC1.number = singular        AND
                    (femgender IN NPVARREC1.genders    OR
                      (neutgender IN NPVARREC1.genders   AND
                       feminine IN NPVARrec1.sexes)
                    )                                  AND
                    KEY1 = KEY_zijBPERSPROkey1
               A17: @
               C18: NPVARREC1.person = 3               AND
                    NPVARREC1.number = plural          AND
                    KEY1 = KEY_zijBPERSPROkey2
               A18: @
               C19: NPVARREC1.person = 4               AND
                    NPVARREC1.number = singular        AND
                    KEY1 = KEY_uBPERSPROkey
               A19: @
               C20: NPVARREC1.person = 5               AND
                    NPVARREC1.number = singular        AND
                    KEY1 = KEY_uBPERSPROkey
               A20: @
               C21: NPVARREC1.mood = wh                AND
                    NPVARREC1.genders = [omegagender]  AND
                    NPVARREC1.sexes = [masculine]      AND
                    NPVARREC1.number = singular        AND
                    KEY1 = KEY_hijBPERSPROkey
               A21: @
               C22: NPVARREC1.mood = wh                AND
                    NPVARREC1.genders = [omegagender]  AND
                    NPVARREC1.sexes = [feminine]       AND
                    NPVARREC1.number = singular        AND
                    KEY1 = KEY_zijBPERSPROkey1
               A22: @
               C23: NPVARREC1.mood = wh                AND
                    NPVARREC1.genders = [mascgender]   AND
                    NPVARREC1.sexes = []               AND
                    NPVARREC1.number = singular        AND
                    KEY1 = KEY_hijBPERSPROkey
               A23: @
         A1: NPVARREC2 := NPVARREC1;
             NPVARREC2.cases := [genitive];
         >

   SUBRULE
      < T1:CNVAR{CNVARrec1}
        T2:CNVAR{CNVARrec2}
      >
      < T1:CNVAR{CNVARrec1}
      >
      COMP
         <
          C1:  CNVARREC1 = CNVARREC2 EXCEPT FOR [cases] AND
               CNVARREC2.cases = [genitive]
               C11: singular IN CNVARREC1.numbers      AND
                    (mascgender IN CNVARREC1.genders    OR
                      (neutgender IN CNVARREC1.genders   AND
                       masculine IN CNVARrec1.sexes)
                    )
               A11: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_hijBPERSPROkey;
               C12: singular IN CNVARREC1.numbers      AND
                    neutgender IN CNVARrec1.genders    AND
                    CNVARrec1.sexes = []
               A12: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_hijBPERSPROkey;
               C13: singular IN CNVARREC1.numbers      AND
                    CNVARREC1.genders = [omegagender]  AND
                    CNVARREC1.animate = yesAnimate 
               A13: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_ikBPERSPROkey;
               C14: singular IN CNVARREC1.numbers      AND
                    CNVARREC1.genders = [omegagender]  AND
                    CNVARREC1.animate = yesAnimate 
               A14: POSSADJREC1.mood := declxpmood;
                    POSSADJREC1.reduced := true;
                    KEY1 := KEY_jijBPERSPROkey;
               C15: singular IN CNVARREC1.numbers      AND
                    CNVARREC1.genders = [omegagender]  AND
                    CNVARREC1.animate = yesAnimate 
               A15: POSSADJREC1.mood := declxpmood;
                    POSSADJREC1.reduced := true;
                    KEY1 := KEY_jijBPERSPROkey;
               C16: plural IN CNVARREC1.numbers      AND
                    CNVARREC1.genders = [omegagender]  AND
                    CNVARREC1.animate = yesAnimate 
                    C161: (mascgender IN NPrec1.genders OR
                           femgender IN NPrec1.genders)
                    A161: POSSADJREC1.EOrEnForm := eForm
               A16: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_wijBPERSPROkey;
               C17: singular IN CNVARREC1.numbers      AND
                    (femgender IN CNVARREC1.genders    OR
                      (neutgender IN CNVARREC1.genders   AND
                       feminine IN CNVARrec1.sexes)
                    )
               A17: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_zijBPERSPROkey1;
               C18: plural IN CNVARREC1.numbers 
               A18: POSSADJREC1.mood := declxpmood;
                    KEY1 := KEY_zijBPERSPROkey2;
          A1:  @
         >

      DECOMP
         <
          C1: POSSADJREC1.mood = declxpmood
               C11: singular IN CNVARREC1.numbers      AND
                    (mascgender IN CNVARREC1.genders    OR
                      (neutgender IN CNVARREC1.genders   AND
                       masculine IN CNVARrec1.sexes)
                    )                                  AND
                    KEY1 = KEY_hijBPERSPROkey
               A11: @ 
               C12: singular IN CNVARREC1.numbers      AND
                    neutgender IN CNVARrec1.genders    AND
                    CNVARrec1.sexes = []               AND
                    KEY1 = KEY_hijBPERSPROkey
               A12: @
               C13: singular IN CNVARREC1.numbers      AND
                    CNVARREC1.genders = [omegagender]  AND
                    CNVARREC1.animate = yesAnimate     AND
                    KEY1 = KEY_ikBPERSPROkey
               A13: @
               C14: singular IN CNVARREC1.numbers      AND
                    CNVARREC1.genders = [omegagender]  AND
                    CNVARREC1.animate = yesAnimate     AND
                    POSSADJREC1.reduced = true         AND
                    KEY1 = KEY_jijBPERSPROkey
               A14: @
               C15: plural IN CNVARREC1.numbers      AND
                    CNVARREC1.genders = [omegagender]  AND
                    CNVARREC1.animate = yesAnimate     AND
                    POSSADJREC1.reduced = true         AND
                    KEY1 = KEY_jijBPERSPROkey
               A15: @
               C16: plural IN CNVARREC1.numbers      AND
                    CNVARREC1.genders = [omegagender]  AND
                    CNVARREC1.animate = yesAnimate     AND
                    KEY1 = KEY_wijBPERSPROkey
                    C161: (mascgender IN NPrec1.genders OR
                           femgender IN NPrec1.genders) AND
                          POSSADJREC1.EOrEnForm = eForm
                    A161: @
               A16: @
               C17: singular IN CNVARREC1.numbers      AND
                    (femgender IN CNVARREC1.genders    OR
                      (neutgender IN CNVARREC1.genders   AND
                       feminine IN CNVARrec1.sexes)
                    )                                  AND
                    KEY1 = KEY_zijBPERSPROkey1
               A17: @
               C18: plural IN CNVARREC1.numbers      AND
                    KEY1 = KEY_zijBPERSPROkey2
               A18: @
         A1: CNVARREC2 := CNVARREC1;
             CNVARREC2.cases := [genitive];
         >

>

&
\end{verbatim}

