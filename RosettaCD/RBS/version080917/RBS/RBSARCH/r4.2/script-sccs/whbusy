( ls -l $RBS_ARCHIVE$RBS_RELEASE/build*.rbs 2> /dev/null ) | awk '{printf "user %s is building!\n", $3}'
( ls -l $RBS_ARCHIVE$RBS_RELEASE/integrate*.rbs 2> /dev/null ) | awk '{printf "user %s is integrating!\n", $3}'
( ls -l $RBS_ARCHIVE$RBS_RELEASE/cons.rbs 2> /dev/null ) | awk '{printf "user %s is consolidating!\n", $3}'
if ( test -f $RBS_USER_ROOT$RBS_USER$RBS_RELEASE/build.rbs )
then echo "You are building!"
fi
if ( test -f $RBS_USER_ROOT$RBS_USER$RBS_RELEASE/grab.rbs )
then echo "You are grabbing!"
fi
if ( test -f $RBS_USER_ROOT$RBS_USER$RBS_RELEASE/integrate.rbs )
then echo "You are integrating!"
fi
if ( test -f $RBS_USER_ROOT$RBS_USER$RBS_RELEASE/free.rbs )
then echo "You are freeing!"
fi
mods=`(ls $RBS_USER_ROOT$RBS_USER$RBS_RELEASE/modify*.rbs 2> /dev/null) | head -1`
if test -z "$mods"
then :
else echo "You are modifying"
fi
