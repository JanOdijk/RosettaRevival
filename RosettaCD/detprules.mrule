% RULE RDETPpartitiveformation (* this rule must be orderd after 
                                  RDETPsuperdeixis! *)
< m1: DETP{DETPrec1}[I1:head/T1]
  m2: DETP{DETPrec2}[mu2] 
>

< m : DETP{DETPrec3}[modrel/DETP{DETPrec4}[I1:head/T1],
                     partrel/PREP(KEY_vanprepkey){PREPrec1},
                     head/DETP{DETPrec5}[mu2]
                    ]
>
   MATCHCONDITIONS
      <
       I1: T1.CAT IN [NUM, DET]
      >

      COMP
         <
          C1: DETPrec2.definite = def AND
              DETPrec1.superdeixis = DETPrec2.superdeixis
          A1: DETPrec3 := 
                 { possnietnp  :  DETPrec1.possnietnp
                   definite    :  adef
                   posspred    :  DETPrec1.posspred
                   possnumbers :  DETPrec2.possnumbers 
                                           (* om een van de aan
                                              kunnen is extra atribuut 
                                              nodig *)
                   posscomas   :  DETPrec1.posscomas
                   mood        :  DETPrec1.mood
                   eORenform   :  DETPrec2.eorenform
                   possgenders :  DETPrec2.possgenders
                   superdeixis :  DETPrec1.superdeixis
                 };
               DETPrec4 := DETPrec1;
               DETPrec4.superdeixis := omegadeixis;
               DETPrec5 := DETPrec2;
               DETPrec5.superdeixis := omegadeixis;
         >

      DECOMP
         <
          C1: DETPrec5.definite = def AND
              DETPrec3 =
                 { possnietnp  :  DETPrec4.possnietnp
                   definite    :  adef
                   posspred    :  DETPrec4.posspred
                   possnumbers :  DETPrec5.possnumbers 
                   posscomas   :  DETPrec4.posscomas
                   mood        :  DETPrec4.mood
                   eORenform   :  Noform  (* moet zijn: kopie van DETPrec5;
                                             NB: rol DEMADJ in surfregels *)
                   possgenders :  [omegagender] 
                                          (* moet zijn: kopie van DETPrec5;
                                             NB: rol DEMADJ in surfregels *)


                 } AND
               DETPrec4.superdeixis = omegadeixis AND
               DETPrec5.superdeixis = omegadeixis
          A1:  DETPrec1 := DETPrec4;
               DETPrec1.superdeixis := DETPrec3.superdeixis;
               DETPrec2 := DETPrec5;
               DETPrec2.superdeixis := DETPrec3.superdeixis;
         >
&


\end{verbatim}
