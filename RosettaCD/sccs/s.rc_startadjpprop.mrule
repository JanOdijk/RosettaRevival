\begin{verbatim}
% RULE RSTARTADJPPROPFORMATION000      (* het is koud/regenachtig *)
< m1:  I1::SUBADJ{SUBADJrec1}[mu1]
>

< m :  I2::ADJPPROP{ADJPPROPrec1}
                 [subjrel/NP{NPrec1}
                         [head/PERSPRO{PERSPROrec1}
                             [head/BPERSPRO(AUX_hetbpersprokey){BPERSPROrec1}]
                         ],
                  predrel/I3::ADJP{ADJPrec1}
                          [head/I4::ADJ{ADJrec1}
                                   [head/SUBADJ{SUBADJrec1}[mu1] 
                                   ] 
                          ] 
                  ]
>

   MATCHCONDITIONS
      <
       I1: SUBADJrec1.thetaadj = adjp000 
       I2: ADJPPROPrec1 = ADJPPROPRECDEF EXCEPT FOR [thetaadj, actsubcefs,
                          temporal, class, deixis, aspect, retro] AND
           ADJPPROPrec1.thetaadj =adjp000
       I3: ADJPrec1 = ADJPRECDEF EXCEPT FOR [thetaadj, actsubcefs, eorenform,
                                 eformation, enominalised, reflexivity,
                                 temporal, class, deixis, aspect, retro] AND
           ADJPrec1.thetaadj =adjp000
       I4: ADJrec1 = {form      : positive
                      eORenform : noform
                     }
      >

      COMP
         <
          C1: true
          A1: PERSPROrec1 := COPYT_BPERSPROtoPERSPRO(BPERSPROrec1);
              NPrec1 := AUX_NPONHetBprspr;
              ADJrec1 := COPYT_subadjtoadj(SUBADJrec1);
              ADJPrec1 := COPYT_adjtoadjp(ADJrec1);
              ADJPrec1.actsubcefs := ADJrec1.subcs;
              ADJPPROPrec1 := COPYT_adjptoadjpprop(ADJPrec1) 
          >
      DECOMP
         <
          C1: (PERSPROrec1 = COPYT_BPERSPROtoPERSPRO(BPERSPROrec1)) AND
              (nominative IN PERSPROrec1.persprocases) AND
              (NPrec1 = AUX_NPONHetBprspr) AND 
              (ADJrec1 = COPYT_subadjtoadj(SUBADJrec1)) AND
              (ADJPrec1 = COPYT_adjtoadjp(ADJrec1)) AND
              (ADJPrec1.actsubcefs = ADJrec1.subcs) AND
              (ADJPPROPrec1 = COPYT_adjptoadjpprop(ADJPrec1)) 
          A1: @
         >

&

\end{verbatim}
