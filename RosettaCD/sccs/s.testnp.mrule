% RULE Rbnounsg
< m1:BNOUN{BNOUNrec1} 
>

< m :NP{NPrec1}
       [ detrel/T1,
         head/CN{CNrec1}
             [ head/NOUN{NOUNrec1}
                   [ head/SUBNOUN{SUBNOUNrec1}[head/BNOUN{BNOUNrec1}] 
                   ] 
             ]
       ]
>


<
   SUBRULE  (* de os *)
      < >
      <T1:ART(AUX_deARTkey){ARTrec1} >

      COMP
         <
          C1: BNOUNrec1.pluralforms <> [onlyplural] AND
              BNOUNrec1.genders * [mascgender, femgender] <> []              
          A1: SUBNOUNrec1 := COPYT_bnounTOsubnoun(BNOUNrec1);
              NOUNrec1    := COPYT_subnounTOnoun(SUBNOUNrec1);
              NOUNrec1.number := singular;
              CNrec1 := COPYT_nounTOcn(NOUNrec1);
              CNrec1 :=
                {
		  superdeixis   : Omegadeixis
		  definite      : omegadef
		  numbers       : [singular]
		  cases         : [Nominative]
		  actsubcs      : [othernoun]
                  detlessnp     : false
                };
               NPrec1 := COPYT_cnTOnp(CNrec1);
               NPrec1 :=
		  {
		  definite       : def
		  person         : 3
		  number         : singular
		  actcomas       : [count]
		  posspred       : true
		  possnietnp     : false
		  mood           : declxpmood
		  generic        : nogeneric
		  possRform      : false
		  syntquant      : omegaquant
		  NPhead         : otherNP
		  specQ          : yesspec
		  }
               
         >
      DECOMP
         <
          C1: BNOUNrec1.pluralforms <> [onlyplural] AND
              BNOUNrec1.genders * [mascgender, femgender] <> [] AND
              SUBNOUNrec1 = COPYT_bnounTOsubnoun(BNOUNrec1) AND
              NOUNrec1    = COPYT_subnounTOnoun(SUBNOUNrec1) AND
              NOUNrec1.number = singular AND
              CNrec1 = COPYT_nounTOcn(NOUNrec1) AND
              CNrec1 =
                {
		  superdeixis   : Omegadeixis
		  definite      : omegadef
		  numbers       : [singular]
		  cases         : [Nominative]
		  actsubcs      : [othernoun]
                  detlessnp     : false
                } AND
               NPrec1 = COPYT_cnTOnp(CNrec1) AND
               NPrec1 =
		  {
		  definite       : def
		  person         : 3
		  number         : singular
		  actcomas       : [count]
		  posspred       : true
		  possnietnp     : false
		  mood           : declxpmood
		  generic        : nogeneric
		  possRform      : false
		  syntquant      : omegaquant
		  NPhead         : otherNP
		  specQ          : yesspec
		  }
               
          A1: @
         >


   SUBRULE  (* het huis *)
      < >
      <T1:ART(AUX_hetARTkey){ARTrec1} >

      COMP
         <
          C1: BNOUNrec1.pluralforms <> [onlyplural] AND
              BNOUNrec1.genders * [neutgender] <> []              
          A1: SUBNOUNrec1 := COPYT_bnounTOsubnoun(BNOUNrec1);
              NOUNrec1    := COPYT_subnounTOnoun(SUBNOUNrec1);
              NOUNrec1.number := singular;
              CNrec1 := COPYT_nounTOcn(NOUNrec1);
              CNrec1 :=
                {
		  superdeixis   : Omegadeixis
		  definite      : omegadef
		  numbers       : [singular]
		  cases         : [Nominative]
		  actsubcs      : [othernoun]
                  detlessnp     : false
                };
               NPrec1 := COPYT_cnTOnp(CNrec1);
               NPrec1 :=
		  {
		  definite       : def
		  person         : 3
		  number         : singular
		  actcomas       : [count]
		  posspred       : true
		  possnietnp     : false
		  mood           : declxpmood
		  generic        : nogeneric
		  possRform      : false
		  syntquant      : omegaquant
		  NPhead         : otherNP
		  specQ          : yesspec
		  }
               
         >
      DECOMP
         <
          C1: BNOUNrec1.pluralforms <> [onlyplural] AND
              BNOUNrec1.genders * [mascgender, femgender] <> [] AND
              SUBNOUNrec1 = COPYT_bnounTOsubnoun(BNOUNrec1) AND
              NOUNrec1    = COPYT_subnounTOnoun(SUBNOUNrec1) AND
              NOUNrec1.number = singular AND
              CNrec1 = COPYT_nounTOcn(NOUNrec1) AND
              CNrec1 =
                {
		  superdeixis   : Omegadeixis
		  definite      : omegadef
		  numbers       : [singular]
		  cases         : [Nominative]
		  actsubcs      : [othernoun]
                  detlessnp     : false
                } AND
               NPrec1 = COPYT_cnTOnp(CNrec1) AND
               NPrec1 =
		  {
		  definite       : def
