% RULE RsimpleopenADJPPcoord
< m1:COORD{COORDrec1}
  m2:OPENADJPPROP{OPENADJPPROPrec1}
         [ subjrel/T1,
           predrel/ADJP{ADJPREC1}[mu1]
         ]
  m3:OPENADJPPROP{OPENADJPPROPrec2}
         [ subjrel/T2,
           predrel/ADJP{ADJPREC2}[mu2]
         ]
>

< m :OPENADJPPROP{OPENADJPPROPrec3}
         [ subjrel/T3,
           predrel/ADJP{ADJPrec3}
	       [ argrel/ADJP{ADJPrec5}[mu1],
                 conjrel/COORD{COORDrec1},
                 argrel/ADJP{ADJPrec6}[mu2] 
               ]
         ]
>

      COMP
         <
          C1: QUOTE_checkindices(T1,T2)                       AND
              apc IN COORDrec1.patterns                       AND
              simple IN COORDrec1.kinds                       AND
              ADJPrec1.mood = ADJPrec2.mood                   AND
              ADJPrec1.eORenForm = NOForm                     AND
              ADJPrec2.eORenForm = NOForm                     AND
              ADJPrec1.coord = nocoord                        AND
              ADJPrec2.coord = nocoord                        AND
              OPENADJPPROPrec1 =
                {
                 class           : ADJPrec1.class
                 deixis          : ADJPrec1.deixis
                 aspect          : ADJPrec1.aspect
                 retro           : ADJPrec1.retro
                 aktionsarts     : OPENADJPPROPrec2.aktionsarts
                 superdeixis     : OPENADJPPROPrec2.superdeixis
                 temporal        : ADJPrec1.temporal
                 thetaadj        : ADJPrec1.thetaadj
                 ADJPatternefs   : ADJPrec1.ADJPatternefs
                 PROsubject      : true
                 mood            : OPENADJPPROPrec2.mood
                }                                             AND
              OPENADJPPROPrec2 =
                {
                 class           : ADJPrec2.class
                 deixis          : ADJPrec2.deixis
                 aspect          : ADJPrec2.aspect
                 retro           : ADJPrec2.retro
                 temporal        : ADJPrec2.temporal
                 thetaadj        : ADJPrec2.thetaadj
                 ADJPatternefs   : ADJPrec2.ADJPatternefs
                 PROsubject      : true
                }                                             AND
              OPENADJPPROPrec1.actsubcefs 
                   * ADJPrec1.actsubcefs <> []                AND
              OPENADJPPROPrec2.actsubcefs 
                   * ADJPrec2.actsubcefs <> []
          A1: QUOTE_copytreeifvar(T1,T3);
              ADJPrec5 := ADJPrec1;
              ADJPrec6 := ADJPrec2;
              ADJPrec3 :=
                  {
                   req:               [pospol, negpol, omegapol]
                   env:               [pospol, negpol, omegapol]

                   class:             omegatimeadvClass
                   aspect:            omegaaspect
                   retro:             false
                   deixis:            omegadeixis

                   actsubcefs:        [otheradj]
                   eFormation:        RegEformation
                   eNominalised:      RegEnominalised
                   form:              positive
                   eORenForm:         ADJPrec1.eORenForm
                   
                   temporal:          false
                   thetaadj:          omegathetaadjp   
                   ADJPatternefs:     []
                   reflexivity:       notreflexive
                   
                   mood:              ADJPrec1.mood
                   coord:             simplecoord 
                  };

              ADJprec3.actuseefs := ADJPrec1.actuseefs * ADJPrec2.actuseefs;
              OPENADJPPROPrec3 :=
                  {
                   class                : omegatimeadvclass
                   deixis               : omegadeixis
                   aspect               : omegaaspect
                   retro                : false

                   actsubcefs           : [otheradj]  
                   aktionsarts          : [stative]
                   superdeixis          : OPENADJPPROPrec1.superdeixis

                   temporal             : false
                   thetaadj             : omegathetaadjp
                   ADJPatternefs        : []
                   PROsubject           : true
                   mood                 : ADJPrec1.mood
                  };

         >

      DECOMP
         <
          C1: apc IN COORDrec1.patterns                       AND
              simple IN COORDrec1.kinds                       AND
              ADJPrec5.mood = ADJPrec6.mood                   AND
              ADJPrec5.eORenForm = NoForm                     AND
              ADJPrec6.eORenForm = NoForm                     AND
              ADJPrec5.coord = nocoord                        AND
              ADJPrec6.coord = nocoord                        AND
              ADJPrec3 =
                  {
                   req:               [pospol, negpol, omegapol]
                   env:               [pospol, negpol, omegapol]

                   class:             omegatimeadvClass
                   aspect:            omegaaspect
                   retro:             false
                   deixis:            omegadeixis

                   actsubcefs:        [otheradj]
                   eFormation:        RegEformation
                   eNominalised:      RegEnominalised
                   form:              positive
                   eORenForm:         ADJPrec5.eORenForm
                   
                   temporal:          false
                   thetaadj:          omegathetaadjp   
                   ADJPatternefs:     []
                   reflexivity:       notreflexive
                   
                   mood:              ADJPrec5.mood
                   coord:             simplecoord 
                  }                                                   AND
              ADJPrec3.actuseefs = ADJPrec5.actuseefs * 
                                              ADJPrec6.actuseefs      AND
              OPENADJPPROPrec3 =
                 {
                  class:               omegatimeadvclass
                  deixis:              omegadeixis
                  aspect:              omegaaspect
                  retro:               false

                  actsubcefs:          [otheradj]
                  aktionsarts:         [stative]
                  thetaadj:            omegathetaadjp
                  ADJPatternefs:       []
                  PROsubject:          true
                  mood:                ADJPrec5.mood
                 }
          A1: QUOTE_copytreeifvar(T3,T1);
              QUOTE_copytreeifvar(T3,T2);
              ADJPrec1 := ADJPrec5;
              ADJPrec2 := ADJPrec6;
              OPENADJPPROPrec1 :=
                {
                 class           : ADJPrec5.class
                 deixis          : ADJPrec5.deixis
                 aspect          : ADJPrec5.aspect
                 retro           : ADJPrec5.retro
                 aktionsarts     : OPENADJPPROPrec3.aktionsarts
                 superdeixis     : OPENADJPPROPrec3.superdeixis
                 temporal        : ADJPrec5.temporal
                 actsubcefs      : ADJPrec5.actsubcefs
                 thetaadj        : ADJPrec5.thetaadj
                 ADJPatternefs   : ADJPrec5.ADJPatternefs
                 PROsubject      : true
                 mood            : OPENADJPPROPrec3.mood
                };
              OPENADJPPROPrec2 :=
                {
                 class           : ADJPrec6.class
                 deixis          : ADJPrec6.deixis
                 aspect          : ADJPrec6.aspect
                 retro           : ADJPrec6.retro
                 aktionsarts     : OPENADJPPROPrec3.aktionsarts
                 superdeixis     : OPENADJPPROPrec3.superdeixis
                 actsubcefs      : ADJPrec6.actsubcefs
                 temporal        : ADJPrec6.temporal
                 thetaadj        : ADJPrec6.thetaadj
                 ADJPatternefs   : ADJPrec6.ADJPatternefs
                 PROsubject      : true
                 mood            : OPENADJPPROPrec3.mood
                };                                            

         >

&

