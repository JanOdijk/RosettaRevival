\begin{verbatim}
%
% TRANSFORMATION Taspectneutralization
< m1:SENTENCE{SENTENCErec1}
           [mu1, predrel/VERBP{VERBPrec1}[mu2], mu3] 
>

< m :SENTENCE{SENTENCErec2}
           [mu1, predrel/VERBP{VERBPrec1}[mu2], mu3]
>
<
   SUBRULE (* 1 imperfective *)
      <mu2: [I1: mu9] >
      <mu2: [I1: mu9] >

   MATCHCONDITIONS
      <
       I1:(NOT EXIST (mu9, [auxrel/I11:: VERB{VERBrec1}[mu4]]))   AND
          (NOT EXIST (mu9, [I12: rel1/VERB{VERBrec2}[I13: mu5]])) AND
          ((NOT EXIST (mu9, [head/I15::VERB{VERBrec4}[mu7]])) OR
           ((EXIST (mu9, [head/I15::VERB{VERBrec4}[mu7]])) AND
            (NOT EXIST (mu9, [auxrel/I14::VERB{VERBrec3}[mu6]]))
           )
          )
       I11: (VERBrec1.status IN [bareV, teV]) AND
            (VERBrec1.subc IN [hebbenaux, zijnaux]) 
       I12: (rel1 IN [head, auxrel, progauxrel]) AND 
            (VERBrec2.status = AuxV) 
       I13: (EXIST (mu5, [auxrel/I14::VERB{VERBrec3}[mu6]]))
       I14: (VERBrec3.subc IN [hebbenaux, zijnaux])
       I15: (VERBrec4.modus = pastpart)
      >


      COMP
         <
          C1: SENTENCErec1.aspect = imperfective AND
              QUOTE_tempdurnotfound(mu1)
          A1: SENTENCErec2 := SENTENCErec1;
              SENTENCErec2.aspect := omegaaspect;
         >

      DECOMP
         <
          C1: SENTENCErec2.aspect = omegaaspect AND
              QUOTE_tempdurnotfound(mu1)
                AND
              (SENTENCErec2.finiteness  = finite OR
               SENTENCErec2.modus = prespart OR
               (NOT QUOTE_tempretronotfound(mu1))
              ) AND
              (SENTENCErec2.senttype = mainclause ->
               [achievement] * SENTENCErec2.aktionsarts = []
              ) 
          A1: SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.aspect := imperfective;
         > 

   SUBRULE (* 2 habitual *)
      <mu2: [mu9] >
      <mu2: [mu9] >

      COMP
         <
          C1: (SENTENCErec1.aspect = habitual) AND
              (NOT QUOTE_tempfreqnotfound(mu1)) 
          A1: SENTENCErec2 := SENTENCErec1;
              SENTENCErec2.aspect := omegaaspect;
         >

      DECOMP
         <
          C1: (SENTENCErec2.aspect = omegaaspect) AND
              (NOT QUOTE_tempfreqnotfound(mu1))
          A1: SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.aspect := habitual;

         >

   SUBRULE (* 3 perfective *)
      <mu2: [mu9] >
      <mu2: [mu9] >

      COMP
         <
          C1: SENTENCErec1.aspect = perfective
          A1: SENTENCErec2 := SENTENCErec1;
              SENTENCErec2.aspect := omegaaspect;
         >

      DECOMP
         <
          C1: SENTENCErec2.aspect = omegaaspect
          A1: SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.aspect := perfective;

         >
>
&

\end{verbatim}
