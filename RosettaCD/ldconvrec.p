      LDCONVREC_GetAttrbs := LSCONVREC_xxkeyrecord( t^.LS^.SFCATfield^);
      END;
   PFCAT: BEGIN
      LDCONVREC_GetAttrbs := LSCONVREC_xxkeyrecord( t^.LS^.PFCATfield^);
      END;
   GLUE: BEGIN
      LDCONVREC_GetAttrbs := LSCONVREC_xxemptyrecord( t^.LS^.GLUEfield^);
      END;
   END;
END {LDCONVREC_GetAttrbs};

FUNCTION LDCONVREC_GetNode(t: LDCONVREC_pAttrStruct;
                                    VAR s: LISTREE_pStree): BOOLEAN;
VAR ls: LSSTREE_pStree;
    catstr: STR_STRING;
    catlen: STR_Range;
    correct: BOOLEAN;
    dummy: LSDOMAINT_LSrecord;
BEGIN
   ls := LoopHoles.Retype(s,LSSTREE_pStree);
   dummy := ls^.LS^;
   LDTYPETOSTR_Cat(catstr, catlen, s^.cat);
   correct := LSCONVATTR_AttrToRec(catstr, catlen, t, dummy);
   IF correct THEN ls^.LS^ := dummy;
   LDCONVREC_GetNode := correct;
END {LDCONVREC_GetNode};

