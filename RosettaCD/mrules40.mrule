   MATCHCONDITIONS
      <I1: SENTENCErec1.prosubject = true
       I2: ALL(sig1, shiftrel/..)
       I3: NOT EXIST(mu7, [shiftrel/..]) AND
           NOT EXIST(mu7, [conjrel/..])
       I4: T1.cat IN [NPVAR, CNVAR]
       I5: T2.cat IN [NPVAR, CNVAR]
       I6: ALL(sig1, conjrel/..)
