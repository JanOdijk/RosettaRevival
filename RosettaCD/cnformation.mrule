% RULE BNOUNtoSUBNOUN   
< m1: BNOUN{BNOUNrec1} 
>

< m : SUBNOUN{SUBNOUNrec1}[head/BNOUN{BNOUNrec1}]
>                       

      COMP
         <
          C1:  true
          A1:  SUBNOUNrec1 := COPYT_bnounTOsubnoun(BNOUNrec1)
               (* lastaffix receives the default value noaffix *)
         >

      DECOMP
         <
          C1:  SUBNOUNrec1 = COPYT_bnounTOsubnoun(BNOUNrec1) AND
               SUBNOUNrec1.lastaffix = noaffix
          A1:  @
         >
&

