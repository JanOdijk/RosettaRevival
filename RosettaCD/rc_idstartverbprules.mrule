% RULE RIdDeriv13
< m1: I1::SUBVERB{SUBVERBrec1}[head/BVERB(KEY1){BVERBrec1} ] 
  m2: T1
  m3: NPVAR{NPVARrec5}
>
< m:  IDFORMATION/RIDTOPICXPMARKING
        [XPPROPTOCLAUSE/RLOCOPENPREPPROPSUBSTITUTION{LEVEL:X1 
                                                    SUBST:2}
           [VERBPPROPFORMATION/RVERBPATTERN1
              [VERBPPROPFORMATION/RSTARTVPPROP120
                 [VERBDERIVATION/RBVERBTOSUB
                    [BLEX/BVERB(KEY2)
                    ],
                  BLEX/I2:T1,
                  BLEX/PREPPPROPVAR(X1){PREPPPROPVARrec1}
                 ]
              ],
            PREPPFORMATION/RPPPMOOD1
              [PREPPFORMATION/TPPPPROSUBJECT
                 [PREPPFORMATION/RPPPSUBSTITUTION1{LEVEL:X2
                                                   SUBST:2}
                    [PREPPPROPFORMATION/RPREPTOFORMULA
                       [PREPPPROPFORMATION/TPREPPATTERN1A
                          [PREPPPROPFORMATION/RSTARTPREPPPROP120
                             [PREPDERIVATION/RPREPTOSUB
                                [BLEX/PREP(KEY3)
                                ],
                              BLEX/T2,
                              BLEX/NPVAR(X2){NPVARrec1}
                             ]
                          ]
                       ],
                       IDFORMATION/RIDNPFORMATION2
                          [CNFORMATION/RCNMODPP{LEVEL:X3
                                                SUBST:1}
                             [CNFORMATION/RCNFORMATION1
                                [CNFORMATION/RSUBNOUNTONOUN1
                                   [NPDERIVATION/BNOUNTOSUBNOUN
                                      [BLEX/BNOUN(KEY4)
                                      ]
                                   ]
                                ],
                              IDFORMATION/RIDVARPPTOPP
                                [PREPPFORMATION/RPPPMOOD1
                                   [PREPPFORMATION/TPPPPROSUBJECT
                                      [PREPPFORMATION/FPPPPREPROSUBJECT
                                         [PREPPPROPFORMATION/TPPCA1
                                            [PREPPPROPFORMATION/RPREPTOFORMULA
                                               [PREPPPROPFORMATION/RPREPPPROPVOICE
                                                  [PREPPPROPFORMATION/TPREPPATTERN1A
                                                     [PREPPPROPFORMATION/RSTARTPREPPPROP120
                                                        [PREPDERIVATION/RPREPTOSUB
                                                           [BLEX/PREP(KEY5)
                                                           ],
                                                         BLEX/CNVAR(X3){CNVARrec3},
                                                         BLEX/NPVAR{NPVARrec6}
                                                        ]
                                                     ]
                                                  ]
                                               ]
                                            ]
                                         ]
                                      ]
                                   ]
                                ]
                             ]
                          
                       ]
                    ]
                 ]
              ]
           ]
        ]
>

   MATCHCONDITIONS
      <
       I1: SUBVERBrec1.thetavp = vp120
       I2: T1.CAT IN AUX_VARCATSET
       m2: T1.CAT IN AUX_VARCATSET
      >


<
   SUBRULE  (* 1 *)
      <T1:NPVAR{NPVARrec2}>
      <T1:NPVAR{NPVARrec2}
       T2:NPVAR{NPVARrec3}>

COMP
 <
   C1: COMPINIDDICT(KEY1, VPID13)
   A1: <KEY2, KEY3, KEY4, KEY5> := COMPGETIDDICT (KEY1, VPID13);
       NPVARrec3 := NPVARrec2;
       NPVARrec6 := NPVARrec5;
 >
DECOMP
 <
   C1: DECOMPINIDDICT(<KEY2, KEY3, KEY4, KEY5>, VPID13) AND
       NPVARREC3.index = NPVARREC2.index
   A1: WBONUS +5;
       KEY1 := DECOMPGETIDDICT(<KEY2, KEY3, KEY4, KEY5>, VPID13);
       SUBVERBrec1 := COPYT_bverbtosubverb(BVERBrec1);
       NPVARrec5 := NPVARrec6;
 >

   SUBRULE  (* 2 *)
      <T1:CNVAR{CNVARrec1}>
      <T1:CNVAR{CNVARrec1}
       T2:CNVAR{CNVARrec2}>

COMP
 <
   C1: COMPINIDDICT(KEY1, VPID13) 
   A1: <KEY2, KEY3, KEY4, KEY5> := COMPGETIDDICT (KEY1, VPID13);
       NPVARrec6 := NPVARrec5;
       CNVARrec2 := CNVARrec1;
 >
DECOMP
 <
   C1: DECOMPINIDDICT(<KEY2, KEY3, KEY4, KEY5>, VPID13) AND
       CNVARREC2.index = CNVARREC1.index
   A1: WBONUS +5;
       KEY1 := DECOMPGETIDDICT(<KEY2, KEY3, KEY4, KEY5>, VPID13);
       SUBVERBrec1 := COPYT_bverbtosubverb(BVERBrec1);
       NPVARrec5 := NPVARrec6;
 >
>
&

\end{verbatim}
