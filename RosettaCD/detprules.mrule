% RULE DETPformation2
< m1: NUM{NUMrec1} 
>

< m : DETP{DETPrec1}[head/NUM{NUMrec1}
                    ]
>

      COMP
         <
          C1: true
          A1: DETPrec1.possnietnp  := true;
              DETPrec1.definite    := indef;
              DETPrec1.posspred    := true;
              DETPrec1.mood        := declxpmood;
              DETPrec1.possnumbers := [NUMrec1.number];
              DETPrec1.possgenders := [omegagender];
              DETPrec1.posscomas   := [count];
              DETPrec1.syntquant   := omegaquant;    (* the distinction between 
                                                    omgequant and equant has 
                                                    become irrelevant; 26/5/88;
                                                  *)                    
                          
         >

      DECOMP
         <
          C1: (DETPrec1.definite    = indef) AND
              (DETPrec1.posspred    = true) AND
              (DETPrec1.possnumbers = [NUMrec1.number]) AND
              (DETPrec1.posscomas   = [count]) AND
              (DETPrec1.mood   = declxpmood) AND
              (DETPrec1.eORenForm   = noform) AND
              (DETPrec1.possgenders = [omegagender]) AND
              (DETPrec1.syntquant   = omegaquant)    (* see COMP *)

          A1:  @
         >
&

