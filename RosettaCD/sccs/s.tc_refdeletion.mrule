\begin{verbatim}
% RULE Rtempadvsubstitution1           (* temporal ADVP under S *)
< m1:CLAUSE{CLAUSErec1}
           [ mu1,
             tempadvrel/I1::ADVPVAR{ADVPVARrec1},
             mu2,
             predrel/VERBP{VERBPrec1}[mu3],
             mu4
           ] 

  m2: ADVP{ADVPrec1}[mu5]
>

< m :CLAUSE{CLAUSErec1}
           [ mu1,
             tempadvrel/ADVP{ADVPrec1}[mu5],
             mu2,
             predrel/VERBP{VERBPrec1}[mu3],
             mu4
           ] 
>

   MATCHCONDITIONS
      <
       m1 : (* substordercondition(mu2, mu3, mu4) *) true
       m  : (* substordercondition(mu2, mu3, mu4) *) true
       I1 : ADVPVARrec1.index = LEVEL
      >

   PARAMETERS
      <
        LEVEL
      >

      COMP
         <
          C1: (tempadv IN ADVPrec1.actsubcefs) AND
              (ADVPVARrec1 = COPYT_advptovar(ADVPrec1)) AND
              ((ADVPrec1.class = reference) OR
               ((ADVPrec1.class = duration) AND
                ((ADVPrec1.aspect = perfective) AND
                 (CLAUSErec1.aktionsarts * [stative, activity] <> [])
                ) OR
                ((ADVPrec1.aspect = imperfective) AND
                 (CLAUSErec1.aktionsarts * [accomplishment, achievement] <> [])
                )
               )
              )
          A1: @
         >
      DECOMP
         <
          C1: (tempadv IN ADVPrec1.actsubcefs) AND
              ((ADVPrec1.class = reference) OR
               ((ADVPrec1.class = duration) AND
                ((ADVPrec1.aspect = perfective) AND
                 (CLAUSErec1.aktionsarts * [stative, activity] <> [])
                ) OR
                ((ADVPrec1.aspect = imperfective) AND
                 (CLAUSErec1.aktionsarts * [accomplishment, achievement] <> [])
                )
               )
              )
          A1: ADVPVARrec1 := COPYT_advptovar(ADVPrec1);
              ADVPVARrec1.index := LEVEL;

         >


&

\end{verbatim}

