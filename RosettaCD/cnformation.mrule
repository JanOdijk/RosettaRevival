% RULE RSUBNOUNtoNOUN1    (* singular *)
                          (* NB. for Spanish and English the corresponding 
                             rule also deals with onlyplurals; so 'een 
                             schaar' is correctly mapped onto 'scissors' 
                             and 'tijeras'. For dutch no  example has been 
                             found yet that would motivate a similar
                             extension. *)

                                
<m1: SUBNOUN{SUBNOUNrec1}[mu1]
>

< m: NOUN{NOUNrec1}[head/SUBNOUN{SUBNOUNrec1}[mu1]]
>

     COMP
     < 
      C1: count IN SUBNOUNrec1.posscomas 
          (* dus wel: vent, maar niet: melk *) 
                   AND
          (NOT OnlyPlural IN SUBNOUNrec1.pluralforms
          )      (* dus niet: hersenen *)
       A1: NOUNrec1 := COPYT_SUBNOUNtoNOUN(SUBNOUNrec1);
           NOUNrec1.number := singular;
           NOUNrec1.geni := false;
           NOUNrec1.posscomas := [count];
           (* oneigenlijke reductie van inherente setwaarde;
              actcomas ipv posscomas lijkt voor NOUN verdedigbaar 
              reductie is alleen nodig als [count, mass] als inherente 
              waarde geaccepteerd wordt *)
     >

     DECOMP 
     < 
      C1: 
          NOUNrec1 = COPYT_SUBNOUNtoNOUN(SUBNOUNrec1) 
                    AND
          NOUNrec1.geni = false 
                    AND
          count IN SUBNOUNrec1.posscomas
                    AND
          (NOT OnlyPlural IN SUBNOUNrec1.pluralforms
          )         AND 
          (NOUNrec1.number = singular
          ) 

       A1: @
     >
&

\end{verbatim}
