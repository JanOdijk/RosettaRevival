% RULE Rindeppresentdeixis
< m1:CLAUSE{CLAUSErec1}
           [I1: mu1,
            tempadvrel/T1,
            I2: mu2,
            predrel/VERBP{VERBPrec1}
                   [mu3,
                    rel1/VERB{VERBrec1}[mu4]
                   ]
           ] 
>

< m :CLAUSE{CLAUSErec2}
           [I1: mu1,
            tempadvrel/T1,
            I2: mu2,
            predrel/VERBP{VERBPrec1}
                   [mu3,
                    rel1/VERB{VERBrec2}[mu4]
                   ]
           ] 

>
   MATCHCONDITIONS
      <
       I1: NOT EXIST (mu1, [I3: rel2/T2])
       I2: ALL(mu2, I3: rel2/T2)
       I3: rel2 IN [agvpadvrel, locadvrel,sentadvrel,subjvpadvrel,tempadvrel,
vpadvrel]
      >


<
   SUBRULE
      <T1: ADVPVAR{ADVPVARrec1}[ ]>
      <T1: ADVPVAR{ADVPVARrec1}[ ]>

      COMP
         <
          C1:((ADVPVARrec1.class = reference) AND
              ((ADVPVARrec1.deixis = presentdeixis) OR
               (ADVPVARrec1.deixis = omegadeixis)
              ) AND
              (CLAUSErec1.finiteness = finite) AND
              (CLAUSErec1.deixis = omegadeixis) AND
              (VERBrec1.tense = omegatense)
             )

             C11:(CLAUSErec1.superdeixis = omegadeixis)
             A11:CLAUSErec2 := CLAUSErec1;
             
             C12:(CLAUSErec1.superdeixis = presentdeixis) (* geval graag *)
             A12: CLAUSErec2 := CLAUSErec1;
                  CLAUSErec2.superdeixis := omegadeixis;

          A1: CLAUSErec2.deixis := presentdeixis;
              VERBrec2 := VERBrec1;
              VERBrec2.tense := presenttense;
         >
      DECOMP
         <
          C1:((ADVPVARrec1.class = reference) AND
              ((ADVPVARrec1.deixis = presentdeixis) OR
               (ADVPVARrec1.deixis = omegadeixis)
              ) AND
              (CLAUSErec2.finiteness = finite) AND
              ((CLAUSErec2.superdeixis = omegadeixis) OR
               (CLAUSErec2.superdeixis = presentdeixis)
              ) AND
              (CLAUSErec2.deixis = presentdeixis) AND
              (VERBrec2.tense = presenttense)
             )
E 2
