% RULE RPassive
< m1:I1::VERBPPROP{VERBPPROPrec1}
                      [ I6:sig1,
                        I5:subjrel/T1,
                        I7:mu1,
                        predrel/VERBP{VERBPrec1}
                               [ mu2,
                                 head/I2::VERB{VERBrec1}[mu3 ]
                               ],
                        mu4
                      ]
>

< m :I3::VERBPPROP{VERBPPROPrec2}
                      [ I6:sig1,
                        I7:mu1,
                        predrel/VERBP{VERBPrec1}
                               [ byobjrel/VARPREPP{VARPREPPrec1}
                                     [ head/PREP(KEY_doorbyprepkey){PREPrec1},
                                       I5:objrel/T1
                                     ],
                                 mu2,
                                 head/I4::VERB{VERBrec3}[mu3 ]
                               ],
                        mu4
                      ]
>

   MATCHCONDITIONS
      <
       I1: ( VERBPPROPrec1.voice = omegavoice) AND
           ( VERBPPROPrec1.thetavp IN [vp100, vp120, vp123] )  
       I2: VERBrec1.modus = omegamodus
       I3: ( VERBPPROPrec2.voice = passive) AND
           ( VERBPPROPrec2.thetavp IN [vp100, vp120, vp123] )
       I4: VERBrec3.modus = pastpart
       I5: T1.CAT IN [ NPVAR, CNVAR, EMPTYVAR ]
       I6: ALL(sig1, leftdislocrel/..)
       I7: NOT EXIST(mu1, [leftdislocrel/..])
      >

   PARAMETERS
      <
       voicepar:voicetype
      >

<
   SUBRULE
      <T1:NPVAR{NPVARrec1} >
      <T1:NPVAR{NPVARrec1} >


      COMP
         <
          C1: (( voicepar = passive) AND (passive IN VERBrec1.possvoices )) OR
              (( voicepar = active)  AND NOT(active IN VERBrec1.possvoices))
          A1: VERBrec3 := VERBrec1;
              VERBrec3.modus := pastpart;
              VERBPPROPrec2 := VERBPPROPrec1;
              VERBPPROPrec2.voice := passive;

              VARPREPPrec1 :=
              {
               actsubcefs  : [doorby]
               headkey     : KEY_doorbyprepkey
               thetapp    : pp120
               retro      : PREPrec1.retro
               aspect     : PREPrec1.aspect
               deixis     : PREPrec1.deixis
               class      : PREPrec1.class
               actsubcefs : PREPrec1.subcs
               specq      : NPVARrec1.specq
               mood       : NPVARrec1.mood
               synppefs   : [synNP]
              };

         >
      DECOMP
         <
          C1: (passive IN VERBrec3.possvoices) AND
              ( VARPREPPrec1 =
                      {
                       req         : [pospol, negpol, omegapol]
                       env         : [pospol, negpol, omegapol]
                       headkey     : KEY_doorbyprepkey
                       class       : PREPrec1.class
                       deixis      : PREPrec1.deixis
                       aspect      : PREPrec1.aspect
                       retro       : PREPrec1.retro
                       superdeixis : omegadeixis
                       actsubcefs NI doorby  
                       specq       : NPVARrec1.specq
                       mood        : NPVARrec1.mood
                       synppefs    NI synNP
                      }
              )  
          A1: VERBrec1 := VERBrec3;
              VERBrec1.modus := omegamodus;
              VERBPPROPrec1 := VERBPPROPrec2;
              VERBPPROPrec1.voice := omegavoice;
              voicepar := passive

         >


   SUBRULE
      <T1: T2  >
      <T1: T2 >

   MATCHCONDITIONS
      <
       T1: T2.CAT IN [EMPTYVAR, CNVAR]
      >

      COMP
         <
          C1: (( voicepar = passive) AND (passive IN VERBrec1.possvoices )) OR
              (( voicepar = active)  AND NOT(active IN VERBrec1.possvoices))
          A1: VERBrec3 := VERBrec1;
              VERBrec3.modus := pastpart;
              VERBPPROPrec2 := VERBPPROPrec1;
              VERBPPROPrec2.voice := passive;

              VARPREPPrec1 :=
              {
               actsubcefs  : [doorby]
               headkey     : KEY_doorbyprepkey
               thetapp    : pp120
               retro      : PREPrec1.retro
               aspect     : PREPrec1.aspect
               deixis     : PREPrec1.deixis
               class      : PREPrec1.class
               actsubcefs : PREPrec1.subcs
               specq      : yesspec
               mood       : declxpmood
               synppefs   : [synNP]
              };

         >
      DECOMP
         <
          C1: (passive IN VERBrec3.possvoices) AND
              ( VARPREPPrec1 =
                      {
                       req         : [pospol, negpol, omegapol]
                       env         : [pospol, negpol, omegapol]
                       headkey     : KEY_doorbyprepkey
                       class       : PREPrec1.class
                       deixis      : PREPrec1.deixis
                       aspect      : PREPrec1.aspect
                       retro       : PREPrec1.retro
                       superdeixis : omegadeixis
                       actsubcefs NI doorby  
                       specq       : yesspec
                       mood        : declxpmood
                       synppefs    NI synNP
                      }
              )  
          A1: VERBrec1 := VERBrec3;
              VERBrec1.modus := omegamodus;
              VERBPPROPrec1 := VERBPPROPrec2;
