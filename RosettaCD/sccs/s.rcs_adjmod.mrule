% RULE RADJDegreemod2a     (* genoeg ... om mee te nemen
                           te weinig ... om voor naar huis te komen *)
            
< m1: ADJPPROP{ADJPPROPrec1}[mu1,
                             predrel/ADJP{ADJPrec1}
                                     [I2:mu2,
                                      I3:mu3,
                                      head/T1,
                                      mu4 
                                     ]
                            ]  
  m2: I1::QP{QPrec1}[head/T3,
                 omcomplrel/I5::SENTENCE{SENTENCErec1}[mu5]
                ]
>

< m : ADJPPROP{ADJPPROPrec1}
           [mu1,
            predrel/ADJP{ADJPrec2}
                    [I2:mu2,
                     degreemodrel/I1::QP{QPrec1}[head/T3],
                     I3:mu3,
                     head/T1,
                     mu4,
                     postadjrel/I6::SENTENCE{SENTENCErec2}[mu5]
                     ]
            ]
>

   MATCHCONDITIONS
      <
       I1: QPrec1.thanas = omcompl
       I2: NOT EXIST (mu2, [I12:rel1/..])
       I12: rel1 IN [prepobjrel, aanobjrel, degreemodrel] 
       I3: NOT EXIST (mu3, [I13:rel2/..])
       I13: rel2 IN [objrel, indobjrel, causadvrel, 
                     locadvrel, tempadvrel, degreemodrel]
       I5: SENTENCErec1 = 
                          {
                           prosubject: true
                           finiteness: infinite
                           infsort: omteinf
                          }  AND
           SENTENCErec1.mood IN [declarative, proobjmod] 
       I6: SENTENCErec2 = 
                          {
                           prosubject: true
                           finiteness: infinite
                           infsort   : omteinf
                          } AND
           SENTENCErec2.mood IN [declarative, proobjmod] 

      >


      COMP
         <
          C1: SENTENCErec1.superdeixis = omegadeixis
          A1: ADJPrec2 := ADJPrec1;
              ADJPrec2.mood := QPrec1.mood;  (* m.h.o. op een WH-QP *)
              SENTENCErec2 := SENTENCErec1;
              SENTENCErec2.superdeixis := QPrec1.superdeixis;

         >
      DECOMP
         <
          C1: SENTENCErec2.superdeixis = QPrec1.superdeixis 
          A1: ADJPrec1 := ADJPrec2;
              ADJPrec1.mood := declxpmood;
              SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.superdeixis := omegadeixis;          

          >
&

\end{verbatim}
