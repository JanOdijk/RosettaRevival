% RULE RNPformation1                  (* NP uit DETP en CN met NOUN *)
< m1: CN{CNrec1}[I1:mu1]
E 2
I 1
 
I 2
  m2: DETP{DETPrec1}[mu2,
                     I2:head/T1
                    ]
>

< m : NP{NPrec1}[detrel/DETP{DETPrec2}[mu2,
                                       I2:head/T1],
                 head/CN{CNrec1}[I1:mu1]
                ]
>

   MATCHCONDITIONS
      <
        I1: NOT EXIST(mu1,[head/EN{ENrec1}]) AND
            NOT EXIST(mu1, [posrel/NP{NPrec2}[mu8]])
        I2: (T1.CAT IN [DET, DEMADJ, QP, DETP, CARD])
            OR 
            (T1.CAT IN [NUM] WITH T1.REC.key <> KEY_een1NUMkey) 
                             (* Er zijn twee NUMs `een': eentje met
                                en eentje zonder accent; de laatste
                                wordt hier uitgesloten en komt alleen
                                door de regel voor de partitieve DETP
                                `een van de'. `een' zonder accent is
                                overigens ook ART.
                             *)
      >

      COMP
         <
            C1: DETPrec1.posscomas * CNrec1.posscomas <> [] AND
                  DETPrec1.possnumbers * CNrec1.numbers <> [] AND
                 ((CNrec1.definite = DETPrec1.definite )
                 OR (CNrec1.definite = omegadef)
                 OR (CNrec1.definite = def AND DETPrec1.mood = wh)) AND
                 (DETPrec1.superdeixis = CNrec1.superdeixis)
    
                 C11: plural IN CNrec1.numbers AND
                      ((omegagender IN DETPrec1.possgenders ) OR
                       (DETPrec1.eORenform = eform))
                 A11: NPrec1.actcomas := [count];
                      NPrec1.number := plural  
                                   (* alle/veel/vele boeken;
                                      always 'noform'!! *)
                 C12: ((singular IN CNrec1.numbers) AND
                      (mass IN CNrec1.posscomas)) AND
                      ((DETPrec1.possgenders = [omegagender]) OR
                       (DETPrec1.possgenders * CNrec1.genders <> []))
                 A12: NPrec1.actcomas := [mass];
                      NPrec1.number := singular    (* alle suiker *)
                 C13: singular IN CNrec1.numbers  AND
                      count IN CNrec1.posscomas AND
                      ((omegagender IN DETPrec1.possgenders ) OR
                       (DETPrec1.possgenders * CNrec1.genders <> []))
                 A13: NPrec1.actcomas := [count];
                      NPrec1.number := singular        (* elk boek; elke man *)
         A1: NPrec1 := { posspred          : DETPrec1.posspred
                          definite          : DETPrec1.definite
                          possnietnp        : DETPrec1.possnietnp
                          mood              : DETPrec1.mood

                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro

                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP  
                         };
              DETPrec2 := DETPrec1;
              DETPrec2.superdeixis := omegadeixis;

                          
         >

      DECOMP
         <
          C1:  NPrec1 = { posspred          : DETPrec2.posspred
                          definite          : DETPrec2.definite
                          possnietnp        : DETPrec2.possnietnp
                          mood              : DETPrec2.mood

                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro

                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                          NPhead     : otherNP  
                         }  AND
         
              (NPrec1.cases = [nominative]) AND
              ((CNrec1.definite = DETPrec2.definite )
              OR (CNrec1.definite = omegadef)
              OR (CNrec1.definite = def AND DETPrec2.mood = wh))  AND
              DETPrec2.posscomas * CNrec1.posscomas <>[] AND
              DETPrec2.possnumbers * CNrec1.numbers <> [] AND
              (DETPrec2.superdeixis = omegadeixis)           
                                  (* probleem: een van de *)

                   C11:(plural IN CNrec1.numbers) AND
                       (NPrec1.actcomas * [count] <> []) AND
                       (NPrec1.number = plural) AND
                       ((DETPrec2.possgenders = [omegagender]) OR
                        (DETPrec2.eORenform = eform))
                   A11: @
                   C12: (singular IN CNrec1.numbers) AND
                        (mass IN CNrec1.posscomas) AND
                        (NPrec1.actcomas * [mass]<> []) AND
                        (NPrec1.number = singular) AND 
                      ((DETPrec2.possgenders = [omegagender]) OR
                       (DETPrec2.possgenders * CNrec1.genders <> []))
                   A12: @
                   C13: singular IN CNrec1.numbers AND
                        NPrec1.actcomas * [count] <> [] AND
                        NPrec1.number = singular AND
                      ((DETPrec2.possgenders = [omegagender]) OR
                       (DETPrec2.possgenders * CNrec1.genders <> []))
                  A13:  @
            A1:  DETPrec1 := DETPrec2;
                 DETPrec1.superdeixis := CNrec1.superdeixis;
         >



&


