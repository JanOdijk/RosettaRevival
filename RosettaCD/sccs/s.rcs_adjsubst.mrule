\begin{verbatim}
% RULE RADJSENTENCESUBSTITUTION2
< m1: I3::N1[ subjrel/SENTENCEVAR{SENTENCEVARrec1},
                                mu1,
                                predrel/I2::N2
                                       [ I1:mu2,
                                         head/T2
                                       ]
                              ]
SUBST:
 m2: I4::SENTENCE{SENTENCErec1}[mu3]   (*substituent*) 
>

< m : I3::N1[ subjrel/NP{NPrec1}
                         [head/PERSPRO{PERSPROrec1}
                               [head/BPERSPRO(KEY_hetbpersprokey){BPERSPROrec1}]   
                         ],
                 mu1,
                 predrel/I2::N2
                         [ I1:mu2,
                           complrel/I4::SENTENCE{SENTENCErec1}[mu3],
                           head/T2
                         ]
               ]
>

   MATCHCONDITIONS
      <
       I1: ( NOT EXIST(mu2,[complrel/T1]) ) AND
           ( NOT EXIST(mu2,[objrel/T1]) )
       I2: N2.CAT IN [ADJP]
       I3: N1.CAT IN [ADJPPROP]
       I4: SENTENCErec1.infsort <> omteinf AND
           (SENTENCErec1.finiteness = infinite 
                        -> SENTENCErec1.prosubject = true) AND
           SENTENCErec1.infsort <> inf
      >   

   PARAMETERS
      <
       LEVEL
      >


      COMP
         <
          C1: SENTENCEVARrec1.index = LEVEL
	      C11: SENTENCEVARrec1 =
                        COPYT_sentencetovar(SENTENCErec1)
	      A11: PERSPROrec1:=COPYT_BPERSPROtoPERSPRO(BPERSPROrec1) ;
                   PERSPROrec1.reduced := false;
                   (* persprocases retains the default value*)
                   NPrec1 := AUX_NPONhetBPRSPR;
          A1: @
         >
      DECOMP
         <
          C1: true
	      C11: ( PERSPROrec1 = 
		     {
		      number       : BPERSPROrec1.number
		      gender       : BPERSPROrec1.gender
		      person       : BPERSPROrec1.person
		      generalized  : BPERSPROrec1.generalized
		      persprocases : [nominative]
		     }
                   ) AND
           (* all are equal to the values of the attributes in BPERSPROrec1,
     except for  persprocases (default value) and reduced: any value is 
accepted: reduced and unreduced *)
                   ( NPrec1 = AUX_NPONhetBPRSPR)
	      A11: SENTENCEVARrec1 :=
                        COPYT_sentencetovar(SENTENCErec1);
                   SENTENCEVARrec1.index := LEVEL;
          A1: @
         >

&

\end{verbatim}
