\begin{verbatim}
%
% TRANSFORMATION TNOC1
                         (* nonsubject antecedent *)
< m1:CLAUSE{CLAUSErec1}
           [ mu1,
             predrel/VERBP{VERBPrec1}
                    [ mu2,
                      I1:rel1/T1,
                      mu3,
                      head/VERB{VERBrec1}[mu4],
                      mu5
                    ],
             mu6,
             extraposrel/I4::SENTENCE{SENTENCErec1}
                        [ I2:sig1,
                          I5:sig2,
                          subjrel/T2,
                          I3:mu7 
                        ],
             mu8
           ]
>

< m :CLAUSE{CLAUSErec1}
           [ mu1,
             predrel/VERBP{VERBPrec1}
                    [ mu2,
                      I1:rel1/T1,
                      mu3,
                      head/VERB{VERBrec1}[mu4],
                      mu5
                    ],
             mu6,
             extraposrel/I4::SENTENCE{SENTENCErec1}
                        [ I2:sig1,
                          I5:sig2,
                          I3:mu7 
                        ],
             mu8
           ]
>

   MATCHCONDITIONS
      <
       I1: rel1 IN [objrel, indobjrel]
       I2: ALL(sig1, shiftrel/..)
       I3: (NOT EXIST (mu7, [shiftrel/..])) AND
           (NOT EXIST (mu7, [conjrel/..] ))
       I4: SENTENCErec1.PROsubject = true
       I5: ALL(sig2, conjrel/..)
      >

<
   SUBRULE
      <
       T1:NPVAR{NPVARrec1}
       T2:NPVAR{NPVARrec2}
      >
      <T1:NPVAR{NPVARrec1}>

      COMP
         <
          C1:( ((rel1 = objrel) AND (VERBrec1.Controller = object)) OR
              ((rel1 = indobjrel) AND (VERBrec1.controller = indobj)) ) AND
              (NPVARrec1 = NPVARrec2 EXCEPT FOR [cases] ) AND
              (NPVARrec2.cases = [Nominative])
          A1: @
         >
      DECOMP
         <
          C1:( ((rel1 = objrel) AND (VERBrec1.Controller = object)) OR
              ((rel1 = indobjrel) AND (VERBrec1.controller = indobj)) ) 
          A1: NPVARrec2 := NPVARrec1;
              NPVARrec2.cases := [Nominative]
         >


   SUBRULE
      <
       T1:CNVAR{CNVARrec1}
       T2:CNVAR{CNVARrec2} 
      >
      <T1:CNVAR{CNVARrec1}>

      COMP
         <
          C1:( ((rel1 = objrel) AND (VERBrec1.Controller = object)) OR
              ((rel1 = indobjrel) AND (VERBrec1.controller = indobj)) ) AND
              (CNVARrec1 = CNVARrec2 EXCEPT FOR [cases] ) AND
              (CNVARrec2.cases = [Nominative])
          A1: @
         >
      DECOMP
         <
          C1:( ((rel1 = objrel) AND (VERBrec1.Controller = object)) OR
              ((rel1 = indobjrel) AND (VERBrec1.controller = indobj)) ) 
          A1: CNVARrec2 := CNVARrec1;
              CNVARrec2.cases := [Nominative]
         >

>
&

\end{verbatim}

