\begin{verbatim}
% RULE RSUBNOUNtoNOUN2    (* plural/mass *)

< m1: SUBNOUN{SUBNOUNrec1}[mu1]
>

< m : NOUN{NOUNrec1}[head/SUBNOUN{SUBNOUNrec1}[mu1]
                    ]
>

   PARAMETERS
      <
       numberpar:numbertype
      >


     COMP
     < 
      C1: true
        C11: numberpar = singular AND
             mass IN SUBNOUNrec1.posscomas  (* melk, schoeisel *) AND
             SUBNOUNrec1.pluralforms * [OnlyPlural] = [] 
        A11: NOUNrec1 := COPYT_SUBNOUNtoNOUN(SUBNOUNrec1);
             NOUNrec1.number := singular;
             NOUNrec1.posscomas := [mass]; 
             (* cf. remarks at A14  *)
        C12: numberpar = singular AND
             (SUBNOUNrec1.pluralforms * [OnlyPlural] <> [] )
                                                 (* hersenen *) OR
             (SUBNOUNrec1.posscomas  = [count])
        A12: NOUNrec1 := COPYT_SUBNOUNtoNOUN(SUBNOUNrec1);
             NOUNrec1.number := plural;
        C13: numberpar = plural AND
             count IN SUBNOUNrec1.posscomas (* boek *) AND
             SUBNOUNrec1.pluralforms * [NoPlural] = [] 
        A13: NOUNrec1 := COPYT_SUBNOUNtoNOUN(SUBNOUNrec1);
             NOUNrec1.number := plural;
        C14: numberpar = plural AND
             SUBNOUNrec1.posscomas  = [mass] 
        A14: NOUNrec1 := COPYT_SUBNOUNtoNOUN(SUBNOUNrec1);
             NOUNrec1.number := singular;
             NOUNrec1.posscomas := [mass]; 
             (*  oneigenlijke wijziging van inherent attribuut;
              actcomas ipv posscomas voor NOUN lijkt verdedigbaar;
              reductie is alleen nodig als [count, mass] als inherente 
              waarde geaccepteerd wordt *)
      A1: NOUNrec1.geni := false;
     >

     DECOMP
     < 
      C1: NOUNrec1.geni = false  AND
          NOUNrec1 = COPYT_SUBNOUNtoNOUN(SUBNOUNrec1)
        C11: NOUNrec1.number = singular AND
             mass IN SUBNOUNrec1.posscomas  (* melk, schoeisel *) 
        A11: numberpar := singular;
        C13: NOUNrec1.number = plural AND
             count IN SUBNOUNrec1.posscomas (* boek *) AND
             SUBNOUNrec1.pluralforms * [NoPlural] = [] 
        A13: numberpar := plural;
      A1: @
     >

&
\end{verbatim}
