%
% RULE RNPPROPformation
< m1: NP{NPrec1}[mu1] 
  m2: T1
>


< m :  NPPROP{NPPROPrec1}[I1:subjrel/T1,
                          predrel/NP{NPrec2}[mu1]
                         ]
>

   MATCHCONDITIONS
      <
       m2: T1.CAT IN AUX_VARCATSET 
       I1: T1.CAT IN AUX_VARCATSET 
      >


      COMP
         <
          C1: NPrec1.posspred = true AND
              NPrec1.generic = nogeneric
          A1: NPPROPrec1 := { superdeixis        : NPrec1.superdeixis   
                              aktionsarts        : [stative]
                              thetanp            : NPrec1.thetanp    
                              nounpatternefs     : NPrec1.nounpatternefs 
                            };
              NPrec2 := NPrec1;
              NPrec2.superdeixis := omegadeixis;
              NPrec2.generic := omegageneric;
         >
      DECOMP
         <
          C1: NPrec2.posspred = true  AND
              NPrec2.superdeixis = omegadeixis AND
              NPrec2.generic = omegageneric AND
              NPPROPrec1 = NPPROPrecdef EXCEPT FOR [superdeixis, 
                   aktionsarts, thetanp, nounpatternefs]  AND
              NPPROPrec1 = { aktionsarts        : [stative]
                             thetanp            : NPrec2.thetanp    
                             nounpatternefs     : NPrec2.nounpatternefs
                           }
          A1: NPrec1 := NPrec2;
              NPrec1.superdeixis := NPPROPrec1.superdeixis;
              NPrec1.generic := nogeneric;
         >
&

\end{verbatim}
