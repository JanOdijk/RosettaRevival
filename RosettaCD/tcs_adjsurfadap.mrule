TRANSFORMATION TADJNPOVERADV      (* uitgezet: Niet AF  *)   
                                    (* direct and indirect object movement *)
< m1: ADJPFORMULA{ADJPFORMULArec1}
                  [mu1,
                   predrel/ADJP{ADJPrec1}
                           [I1:mu2, 
                            I2:mu3,
                            I3:rel1/T1,
                            head/T2,
                            mu4
                           ],
                   mu5
                  ]
>

< m1: ADJPFORMULA{ADJPFORMULArec1}
                  [mu1,
                   predrel/ADJP{ADJPrec1}
                           [I1:mu2, 
                            I3:rel1/T1,
                            I2:mu3,
                            head/T2,
                            mu4
                           ],
                   mu5
                  ]
>
  
   MATCHCONDITIONS
      <I1: ALL (mu2, I11:rel3/..)
       I2: NOT EXIST (mu3, [I11:rel3/..])
           (* AND  ALL(mu3, I12:rel2/..)
              I12: rel2 IN [prepobjrel, objrel, aanobjrel, degreemodrel 
           *)  
       I3: rel1 IN [objrel, indobjrel]  AND
           T1.cat IN [NPVAR, NP]
       I11: rel3 IN AUX_preobjrels    (* moet nog gechecked;
                                         is nu [indobjrel, voorobjrel, aanobjrel
                                         byobjrel, agvpadvrel, subjvpadvrel, 
                                         vpadvrel] *) 
      >

      COMP
         <
        C1: true
          C11:rel1 = objrel
          A11:@
          C12:rel1 = indobjrel
          A12:@
        A1:@
         >
      DECOMP
         <
          C1:true
            C11:true
            A11:rel1 := objrel
            C12:true
            A12:rel1 := indobjrel
          A1: @
         >


&
