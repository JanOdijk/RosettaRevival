\newpage
\begin{verbatim}
% RULE Rdatumdayname
< m1:NP{NPrec1}[I2:mu1] 
  m2:NP{NPrec2}[mu2]
>

< m :NP{NPrec1}[modrel/NP{NPrec4}[mu2], I2:mu1]
>

   MATCHCONDITIONS
      <
       I2: EXIST(mu1, [detrel/..])
      >


      COMP
         <
          C1: monthnoun IN NPrec1.actsubcs          AND
              daynoun IN NPrec2.actsubcs            AND
              NPrec2.superdeixis = NPrec1.superdeixis AND
              NPrec2.generic = nogeneric              AND
              NPrec2.cases = [nominative]
          A1: NPrec4 := NPrec2;
              NPrec4.superdeixis := omegadeixis;
              NPrec4.generic     := omegageneric; 
         >
      DECOMP
         <
          C1: NPrec4.superdeixis = omegadeixis       AND
              NPrec4.generic     = omegageneric      AND 
              monthnoun IN NPrec1.actsubcs           AND
              daynoun IN NPrec4.actsubcs             AND
              nominative IN NPrec4.cases
          A1: NPrec2 := NPrec4;
              NPrec2.generic := nogeneric;
              NPrec2.superdeixis := NPrec1.superdeixis;  
              NPrec2.cases := [nominative];
         >

&

\end{verbatim}
