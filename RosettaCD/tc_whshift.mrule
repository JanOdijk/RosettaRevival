\begin{verbatim}
%
% TRANSFORMATION TWhshift1a
< m1:CLAUSE{CLAUSErec1}[I1:sig1, I2:mu1 ]
>

< m :CLAUSE{CLAUSErec2}[I1:sig1, I3:mu11]
>

   MATCHCONDITIONS
      <
       I1: ALL(sig1, leftdislocrel/..)
       I2: NOT EXIST(mu1,[I21:rel1/..])
           I21: rel1 IN [leftdislocrel, shiftrel]
       I3: NOT EXIST(mu11, [leftdislocrel/..])  AND
           QUOTE_FIRSTrel(mu11, shiftrel)         (* not really necessary *)
      >

<

(* temporarily put off too many ambiguitiues!
   SUBRULE   preadvrel; it shifts the leftmost wh preadvrel 
      <mu1 :[I6:mu6, preadvrel/I8::NPVAR{NPVARrec1}, I7:mu7] >
      <mu11:[shiftrel/I8::NPVAR{NPVARrec1}, I6:mu6, I7:mu7]>

   MATCHCONDITIONS
      <
       I6: ALL(mu6, I61:rel6/..)
           I61: QUOTE_Srelprec(rel6, preadvrel) 
       I7: ALL(mu7, I71:rel7/..) AND NOT EXIST(mu7,[I72:rel2/..])
           I71: QUOTE_Srelprec(Preadvrel,rel7)
           I72: rel2 IN [reflrel, postsubjrel]
       I8: NPVARrec1.mood = wh
      >

      COMP
         <
          C1: NPVARrec1.cases * [Accusative, Dative] <> []  AND
              NPVARrec1.cases * [Rcase]   = []              AND
              ((CLAUSErec1.synvpefs *  AUX_objvps + AUX_indobjvps <> []) OR
               (CLAUSErec1.adjuncts * [benfactNP] <> [])
          A1: CLAUSErec2 := CLAUSErec1;
         >
      DECOMP
         <
          C1: NPVARrec1.cases * [Accusative, Dative] <> []  AND
              NPVARrec1.cases * [Rcase]   = []              
              C11: CLAUSErec2.synvpefs * AUX_objvps + AUX_indobjvps <> []
              A11: CLAUSErec1 := CLAUSErec2;
                   CLAUSErec1.synvpefs := 
                      CLAUSErec2.synvpefs * AUX_objvps + AUX_indobjvps;
              C12: CLAUSErec2.adjuncts * [benfactNP] <> []
              A12: CLAUSErec1 := CLAUSErec2;
          A1: @
         >
*)

   SUBRULE  (* subjrel *)
      <mu1 :[I6:mu6, I8:subjrel/T1, I7:mu7] >
      <mu11:[I8:shiftrel/T1, I6:mu6, I7:mu7] >

   MATCHCONDITIONS
      <
       I6: ALL(mu6, I61:rel6/..) AND NOT EXIST(mu6, [subjrel/..])
           I61: QUOTE_Srelprec(rel6, subjrel)
       I7: ALL(mu7, I71:rel7/..) AND NOT EXIST(mu7, [I72:rel2/..])
           I71: QUOTE_Srelprec(Subjrel, rel7)
           I72: rel2 IN [subjrel, postsubjrel]
       I8: (T1.CAT IN [NPVAR] WITH T1.REC.mood IN [wh, relativexpmood]) OR
           (T1.CAT IN [CNVAR])                                          OR
           (T1.CAT IN [NP] WITH
            ((T1.REC.mood = wh)   AND
             (QUOTE_VARinNP(T1)
             )
            )
           )
      >

      COMP
         <
          C1: CLAUSErec1.finiteness = finite AND
              (T1.CAT IN [NPVAR, CNVAR, NP] WITH 
                (T1.REC.cases * [Nominative, Accusative] <> []  AND
                 T1.REC.cases * [Rcase]   = []            
                )
              )
          A1: CLAUSErec2 := CLAUSErec1;
         >
      DECOMP
         <
          C1: CLAUSErec2.finiteness = finite AND
              (T1.CAT IN [NPVAR, CNVAR, NP] WITH (
               T1.REC.cases * [Nominative, Accusative] <> []  AND
               T1.REC.cases * [Rcase]   = []    )
              )
          A1: CLAUSErec1 := CLAUSErec2;
         >

   SUBRULE  (* postsubjrel *)
      <mu1 :[I6:mu6, I8:postsubjrel/T1, I7:mu7] >
      <mu11:[I8:shiftrel/T1, I6:mu6, I7:mu7] >

   MATCHCONDITIONS
      <
       I6: ALL(mu6, I61:rel6/..) AND NOT EXIST(mu6, [postsubjrel/..]) AND
           (NOT EXIST(mu6, [subjrel/..])           OR 
            EXIST(mu6, [subjrel/I62::NP{NPrec1}[mu10]])
           )
           I61: QUOTE_Srelprec(rel6, postsubjrel)
           I62: NPrec1.NPhead IN [ecNP] 
       I7: ALL(mu7, I71:rel7/..) AND NOT EXIST(mu7, [erposrel/..])
                AND NOT EXIST(mu7, [I72:rel2/..])
           I71: QUOTE_Srelprec(postsubjrel, rel7) 
           I72: rel2 IN [subjrel, postsubjrel,tempadvrel]
       I8: (T1.CAT IN [NPVAR] WITH T1.REC.mood = wh)                     OR
           (T1.CAT IN [NP] WITH
            ((T1.REC.mood = wh)   AND
             (QUOTE_VARinNP(T1)
             )
            )
           )
      >

      COMP
         <
          C1: (T1.CAT IN [NPVAR, CNVAR, NP] WITH
               (T1.REC.cases * [Nominative] <> []  AND
                T1.REC.cases * [Rcase]  = []       
               )
              )                                                 AND
              (CLAUSErec1.thetavp IN [vp100, vp120, vp123] OR
               CLAUSErec1.synvpefs * AUX_VRvps <> []) AND
              CLAUSErec1.PROsubject = false
          A1: CLAUSErec2 := CLAUSErec1;
         >
      DECOMP
         <
          C1: (T1.CAT IN [NPVAR, CNVAR, NP] WITH
               (T1.REC.cases * [Nominative] <> []  AND
                T1.REC.cases * [Rcase]  = []       
               )
              )                                                 AND
              (CLAUSErec2.thetavp IN [vp100, vp120, vp123] OR
               CLAUSErec2.synvpefs * AUX_VRvps <> []) AND
              CLAUSErec2.PROsubject = false
          A1: CLAUSErec1 := CLAUSErec2;
         >


   SUBRULE  (* tempadvrel *)
      <mu1 :[I6:mu6, tempadvrel/I8::ADVPVAR{ADVPVARrec1}, I7:mu7 ]>
      <mu11:[shiftrel/I8::ADVPVAR{ADVPVARrec1}, I6:mu6, I7:mu7 ]>

   MATCHCONDITIONS
      <
       I6: ALL(mu6, I61:rel6/..)
           I61: QUOTE_Srelprec(rel6, tempadvrel)
       I7: ALL(mu7, I71:rel7/..)
           I71: QUOTE_Srelprec(tempadvrel, rel7 )  AND rel7 <> reflrel
       I8: ADVPVARrec1.mood IN [wh,topicxpmood] AND 
           tempadv IN ADVPVARrec1.actsubcefs
      >

      COMP
         <
          C1: true
          A1: CLAUSErec2 := CLAUSErec1;
         >
      DECOMP
         <
          C1: true
          A1: CLAUSErec1 := CLAUSErec2;
         >


   SUBRULE  (* sentadvrel *)
      <mu1 :[I6:mu6, sentadvrel/I8::ADVPVAR{ADVPVARrec1}, I7:mu7 ]>
      <mu11:[shiftrel/I8::ADVPVAR{ADVPVARrec1}, I6:mu6, I7:mu7 ]>

   MATCHCONDITIONS
      <
       I6: ALL(mu6, I61:rel6/..)
           I61: QUOTE_Srelprec(rel6, sentadvrel)
