% RULE rstartprepp120   (* op het huis, op het huis af, het huis in *)
< m1:SUBPREP{SUBPREPrec1}[head/PREP{PREPrec1} ] 
  m2:T1
>

< m :PREPP{PREPPrec1}[mu1]
>

   MATCHCONDITIONS
      <
       m2: T1.CAT IN AUX_VARCATSET
      >

<
   SUBRULE  (* normalprep *)
      < >
      <mu1:[head/PREP{PREPrec1}, I1:argrel/T1] >

   MATCHCONDITIONS
      <
       I1: T1.CAT IN AUX_VARCATSET
      >


      COMP
         <
          C1: PREPrec1.thetapp = pp120 AND
              SUBPREPrec1 = PREPrec1   AND
              PREPrec1.preptype = normalprep AND
              PREPrec1.postform IN [ pre, both]
              C11: T1.CAT IN [NPVAR, PREPPVAR] 
              A11: PREPPrec1   := PREPPrecdef;
                   PREPPrec1.specq := T1.REC.specq;
                   PREPPrec1.mood  := T1.REC.mood;
              C12: T1.CAT IN [ CNVAR]
              A12: PREPPrec1   := PREPPrecdef;
                   PREPPrec1.specq := yesspec;
                   PREPPrec1.mood  := declxpmood;
              C13: T1.CAT IN [ADVPVAR]
              A13: PREPPrec1 := PREPPrecdef;
                   PREPPrec1.specq := yesspec;
                   PREPPrec1.mood  := T1.REC.mood;
          A1: PREPPrec1.thetapp := pp120;
              PREPPrec1.headkey := PREPrec1.key;
	      PREPPrec1.actsubcefs := PREPrec1.subcs;
	      PREPPrec1.class := PREPrec1.class;
	      PREPPrec1.deixis := PREPrec1.deixis;
	      PREPPrec1.aspect := PREPrec1.aspect;
	      PREPPrec1.retro := PREPrec1.retro;
         >
      DECOMP
         <
          C1: PREPrec1.thetapp = pp120                              AND
              PREPrec1.preptype = normalprep                        AND
              PREPrec1.postform IN [ pre, both]                     AND
              PREPPrec1   = PREPPrecdef                  
                   EXCEPT FOR [thetapp, headkey, actsubcefs, class,
                                deixis, aspect, retro, specq,mood]  AND
              PREPPrec1.thetapp = pp120                             AND
              PREPPrec1.headkey = PREPrec1.key                      AND
	      PREPPrec1.actsubcefs = PREPrec1.subcs                 AND
	      PREPPrec1.class = PREPrec1.class                      AND
	      PREPPrec1.deixis = PREPrec1.deixis                    AND
	      PREPPrec1.aspect = PREPrec1.aspect                    AND
	      PREPPrec1.retro = PREPrec1.retro                      
              C11: T1.CAT IN [NPVAR, PREPPVAR] WITH
                    ( T1.REC.specq = PREPPrec1.specq  AND
                      T1.REC.mood  = PREPPrec1.mood
                    )
              A11: @
              C12: T1.CAT IN [CNVAR] AND
                   PREPPrec1.specq = yesspec AND
                   PREPPrec1.mood = declxpmood
              A12: @
              C13: PREPPrec1.specq = yesspec AND
                   (T1.CAT IN [ADVPVAR] WITH T1.REC.mood = PREPPrec1.mood)
              A13: @
          A1: SUBPREPrec1 := PREPrec1;
         >

   SUBRULE  (* splitprep *)
      < >
      <mu1:[ head/PREP{PREPrec1}, 
             I1:argrel/T1, 
             partrel/PART(KEY1){PARTrec1}
           ]
      >

   MATCHCONDITIONS
      <
       I1: T1.CAT IN AUX_VARCATSET
      >


      COMP
         <
          C1: PREPrec1.thetapp = pp120 AND
              SUBPREPrec1 = PREPrec1   AND
              PREPrec1.preptype = splitprep
              C11: T1.CAT IN [NPVAR, PREPPVAR] 
              A11: PREPPrec1   := PREPPrecdef;
                   PREPPrec1.specq := T1.REC.specq;
                   PREPPrec1.mood  := T1.REC.mood;
              C12: T1.CAT IN [ CNVAR]
              A12: PREPPrec1   := PREPPrecdef;
                   PREPPrec1.specq := yesspec;
                   PREPPrec1.mood  := declxpmood;
              C13: T1.CAT IN [ADVPVAR]
              A13: PREPPrec1 := PREPPrecdef;
                   PREPPrec1.specq := yesspec;
                   PREPPrec1.mood  := T1.REC.mood;
          A1: PREPPrec1.thetapp := pp120;
              PREPPrec1.headkey := PREPrec1.key;
	      PREPPrec1.actsubcefs := PREPrec1.subcs;
	      PREPPrec1.class := PREPrec1.class;
	      PREPPrec1.deixis := PREPrec1.deixis;
	      PREPPrec1.aspect := PREPrec1.aspect;
	      PREPPrec1.retro := PREPrec1.retro;
              KEY1 := PREPrec1.postpkey;
         >
      DECOMP
         <
          C1: PREPrec1.thetapp = pp120                              AND
              PREPrec1.preptype = splitprep                         AND
              PARTrec1.key = PREPrec1.postpkey                      AND
              PREPPrec1   = PREPPrecdef                  
                   EXCEPT FOR [thetapp, headkey, actsubcefs, class,
                                deixis, aspect, retro, specq, mood] AND
              PREPPrec1.thetapp = pp120                             AND
              PREPPrec1.headkey = PREPrec1.key                      AND
	      PREPPrec1.actsubcefs = PREPrec1.subcs                 AND
	      PREPPrec1.class = PREPrec1.class                      AND
	      PREPPrec1.deixis = PREPrec1.deixis                    AND
	      PREPPrec1.aspect = PREPrec1.aspect                    AND
	      PREPPrec1.retro = PREPrec1.retro                      AND
              KEY1 = PREPrec1.postpkey
              C11: T1.CAT IN [NPVAR, PREPPVAR] WITH
                    ( T1.REC.specq = PREPPrec1.specq  AND
                      T1.REC.mood  = PREPPrec1.mood
                    )
              A11: @
              C12: T1.CAT IN [CNVAR] AND
                   PREPPrec1.specq = yesspec AND
                   PREPPrec1.mood = declxpmood
              A12: @
              C13: PREPPrec1.specq = yesspec AND
                   (T1.CAT IN [ADVPVAR] WITH T1.REC.mood = PREPPrec1.mood)
              A13: @
          A1: SUBPREPrec1 := PREPrec1;
         >
   SUBRULE  (* postprep *)
      < >
      <mu1:[ I1:argrel/T1, head/PREP{PREPrec1}] >

   MATCHCONDITIONS
      <
       I1: T1.CAT IN AUX_VARCATSET
      >


      COMP
         <
          C1: PREPrec1.thetapp = pp120 AND
              SUBPREPrec1 = PREPrec1   AND
              PREPrec1.preptype = postprep
              C11: T1.CAT IN [NPVAR, PREPPVAR] 
              A11: PREPPrec1   := PREPPrecdef;
                   PREPPrec1.specq := T1.REC.specq;
                   PREPPrec1.mood  := T1.REC.mood;
              C12: T1.CAT IN [ CNVAR]
              A12: PREPPrec1   := PREPPrecdef;
                   PREPPrec1.specq := yesspec;
                   PREPPrec1.mood  := declxpmood;
              C13: T1.CAT IN [ADVPVAR]
              A13: PREPPrec1 := PREPPrecdef;
                   PREPPrec1.specq := yesspec;
                   PREPPrec1.mood  := T1.REC.mood;
          A1: PREPPrec1.thetapp := pp120;
              PREPPrec1.headkey := PREPrec1.key;
	      PREPPrec1.actsubcefs := PREPrec1.subcs;
	      PREPPrec1.class := PREPrec1.class;
	      PREPPrec1.deixis := PREPrec1.deixis;
	      PREPPrec1.aspect := PREPrec1.aspect;
	      PREPPrec1.retro := PREPrec1.retro;
         >
      DECOMP
         <
          C1: PREPrec1.thetapp = pp120                              AND
              PREPrec1.preptype = postprep                          AND
              PREPPrec1   = PREPPrecdef                  
                   EXCEPT FOR [thetapp, headkey, actsubcefs, class,
                                deixis, aspect, retro, specq, mood] AND
              PREPPrec1.thetapp = pp120                             AND
              PREPPrec1.headkey = PREPrec1.key                      AND
	      PREPPrec1.actsubcefs = PREPrec1.subcs                 AND
	      PREPPrec1.class = PREPrec1.class                      AND
	      PREPPrec1.deixis = PREPrec1.deixis                    AND
	      PREPPrec1.aspect = PREPrec1.aspect                    AND
	      PREPPrec1.retro = PREPrec1.retro                      
              C11: T1.CAT IN [NPVAR, PREPPVAR] WITH
                    ( T1.REC.specq = PREPPrec1.specq  AND
                      T1.REC.mood  = PREPPrec1.mood
                    )
              A11: @
              C12: T1.CAT IN [CNVAR] AND
                   PREPPrec1.specq = yesspec AND
                   PREPPrec1.mood = declxpmood
              A12: @
              C13: PREPPrec1.specq = yesspec AND
                   (T1.CAT IN [ADVPVAR] WITH T1.REC.mood = PREPPrec1.mood)
              A13: @
          A1: SUBPREPrec1 := PREPrec1;
         >


>
&

