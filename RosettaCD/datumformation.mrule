          C1: PROPERNOUNrec1.subc = monthnoun AND
              DETPrec2.superdeixis = omegadeixis
          A1: DETPrec1 := DETPrec2;
              DETPrec1.superdeixis := NPrec1.superdeixis;
         >

&

\end{verbatim}
