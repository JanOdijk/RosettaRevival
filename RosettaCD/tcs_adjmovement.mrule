%
% TRANSFORMATION TADJPAcomplmovement
< m1: I2::ADJPFORMULA{ADJPFORMULArec1}
                  [mu1,
                   predrel/ADJP{ADJPrec1}
                           [mu2,
                            I3:mu3,
                            head/T1,
                            I4:paprepobjrel/T2,
                            mu5
                           ]
                   ]
>
< m: I2::ADJPFORMULA{ADJPFORMULArec2}
                  [mu1,
                   predrel/ADJP{ADJPrec2}
                           [mu2,
                            I1:rel1/T2,
                            I3:mu3,
                            head/T1,
                            mu4,
                            mu5
                           ]
                   ]
>


   MATCHCONDITIONS
      <
       I1: rel1 IN [erposrel] AND
           T2.CAT IN [PREPP, VARPREPP]
       I2: ADJPFORMULArec2.adjpatternefs * AUX_PAadjps <> [] 
       I3: ALL(mu3, I13:rel2/..)
       I13: rel2 IN [degreemodrel, negrel]
       I4: T2.CAT IN [PREPP, VARPREPP] 
       (* moet de volgorde nadrukkelijker beregeld ??*)       
      >

      COMP
         <
          C1: true
          A1: ADJPFORMULArec2 :=   ADJPFORMULArec1;
              ADJPrec2 := ADJPrec1;
         >

      DECOMP
         <
          C1: true
          A1: ADJPFORMULArec1 :=   ADJPFORMULArec2;
              ADJPrec1 := ADJPrec2;
              ADJPFORMULArec1.adjpatternefs := 
              ADJPFORMULArec2.adjpatternefs * AUX_PAadjps;
         >

&

\end{verbatim}
