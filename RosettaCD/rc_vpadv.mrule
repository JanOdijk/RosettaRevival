\begin{verbatim}
%
% RULE Rvpadv
< SUBST: 
 m1:CLAUSE{CLAUSErec1}                     (* substituent *)
              [ mu1,
                predrel/VERBP{VERBPrec1}[ mu2 ],
                mu3 
              ] 

  m2:I1::OPENADVPPROP{OPENADVPPROPrec1}
                 [ subjrel/I2::CLAUSEVAR{CLAUSEVARrec1},
                   predrel/ADVP{ADVPrec1}[mu4] 
                 ]
>

< m :CLAUSE{CLAUSErec1}                  
              [ mu1,
                predrel/VERBP{VERBPrec1}
                       [ vpadvrel/ADVP{ADVPrec1}[mu4],
                         mu2 
                       ],
                mu3 
              ] 
>

   MATCHCONDITIONS
      <
       I1: vpadv IN OPENADVPPROPrec1.actsubcefs
       I2: CLAUSEVARrec1.index = LEVEL                            
      >

   PARAMETERS
      <
       LEVEL
      >


      COMP
         <
          C1: OPENADVPPROPrec1.Qstatus = false                       AND
              CLAUSEVARrec1 = COPYT_CLAUSEtovar(CLAUSErec1)  AND
              OPENADVPPROPrec1 = COPYT_advptoprop (ADVPrec1) EXCEPT FOR
                      [superdeixis, PROsubject, aktionsarts]         AND
              OPENADVPPROPrec1 = 
		   {
		   aktionsarts: [stative]
		   PROsubject : true
		   }                                                 AND
              vpadv IN ADVPrec1.actsubcefs                           AND
              QUOTE_deixismax(CLAUSErec1.deixis,CLAUSErec1.superdeixis,
                    OPENADVPPROPrec1.superdeixis)
          A1: @
         >
      DECOMP
         <
          C1: ADVPrec1.Qstatus = false AND
              vpadv IN ADVPrec1.actsubcefs 
          A1: WBONUS -1
              CLAUSEVARrec1 := COPYT_CLAUSEtovar(CLAUSErec1); 
              CLAUSEVARrec1.index := LEVEL;
              OPENADVPPROPrec1 := COPYT_advptoprop (ADVPrec1);
              OPENADVPPROPrec1 := 
		   {
		   aktionsarts: [stative]
		   PROsubject : true
		   };
              QUOTE_assdeixismax(CLAUSErec1.deixis,CLAUSErec1.superdeixis,
                    OPENADVPPROPrec1.superdeixis);

         >

&

\end{verbatim}

