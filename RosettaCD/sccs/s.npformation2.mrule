% RULE RNPINDEFmodposs

< m1: NP{NPrec1}[head/INDEFPRO{INDEFPROrec1}[mu10]]

  m2: NP{NPrec2}[mu1, 
                 head/T1,
                 mu2
                ]
>

< m : NP{NPrec1} [head/INDEFPRO{INDEFPROrec1}[mu10],
                  posrel/PREPP{PREPPrec1}[head/PREP(AUX_vanprepkey){PREPrec1},
                                          objrel/NP{NPrec3}[mu1,
                                                            head/T2,
                                                            mu2
                                                            ]
                                          ]     
                 ]
>

<
(* 1 *) 
SUBRULE          (*  iets van de buren, iemand van de firma *)

<T1: CN{CNrec1}[mu3]>
<T2: CN{CNrec2}[mu3]>

      COMP
         <
          C1:  NPrec1.superdeixis = NPrec2.superdeixis AND
               NPrec2.cases = [nominative] AND
               CNrec1.cases = [nominative] AND
               NPrec2.generic = nogeneric 
          A1:  NPrec3 := NPrec2;
               NPrec3.superdeixis := omegadeixis;
               NPrec3.generic := omegageneric;
               NPrec3.cases := [accusative];
               CNrec2 := CNrec1;
               CNrec2.cases := [accusative]
         >

      DECOMP
         <
          C1:  NPrec3.superdeixis = omegadeixis AND
               accusative IN NPrec3.cases AND
               accusative IN CNrec2.cases AND
               NPrec3.generic = omegageneric
          A1:  NPrec2 := NPrec3;
               NPrec2.superdeixis := NPrec1.superdeixis;
               NPrec2.cases := [nominative];
               NPrec2.generic := nogeneric;
               CNrec1 := CNrec2;
               CNrec1.cases := [nominative];
         >
 
(* 2 *) 
SUBRULE            (* iets van ATW *)

<T1: PROPERNOUN{PROPERNOUNrec1}[mu4]>
<T2: PROPERNOUN{PROPERNOUNrec1}[mu4]>

      COMP
         <
          C1:  NPrec1.superdeixis = NPrec2.superdeixis AND
               NPrec2.cases = [nominative] AND
               NPrec2.generic = nogeneric
          A1:  NPrec3 := NPrec2;
               NPrec3.superdeixis := omegadeixis;
               NPrec3.generic := omegageneric;
               NPrec3.cases := [accusative]
         >

      DECOMP
         <
          C1:  NPrec3.superdeixis = omegadeixis AND
               accusative IN NPrec3.cases AND
               NPrec3.generic = omegageneric
          A1:  NPrec2 := NPrec3;
               NPrec2.superdeixis := NPrec1.superdeixis;
               NPrec2.cases := [nominative];
               NPrec2.generic := nogeneric
         >

(* 3 *) 
SUBRULE            (* iets van ons *)

<T1: PERSPRO{PERSPROrec1}[mu5]>
<T2: PERSPRO{PERSPROrec2}[mu5]>

      COMP
         <
          C1:  NPrec1.superdeixis = NPrec2.superdeixis AND
               NPrec2.cases = [nominative] AND
               NPrec2.generic = nogeneric AND
               PERSPROrec1.persprocases = [nominative]
          A1:  PERSPROrec2 := PERSPROrec1;
               PERSPROrec2.persprocases := [accusative];
               NPrec3 :=  NPrec2;
               NPrec3.superdeixis := omegadeixis;
               NPrec3.generic := omegageneric;
               NPrec3.cases := [accusative]
         >

      DECOMP
         <
          C1:  PERSPROrec2.persprocases * 
                         [accusative, dative] <> [] AND
               NPrec3.superdeixis = omegadeixis AND
               accusative IN NPrec3.cases AND
               NPrec3.generic = omegageneric
          A1:  NPrec2 := NPrec3;
               NPrec2.superdeixis := NPrec1.superdeixis;
               NPrec2.cases := [nominative];
               NPrec2.generic := nogeneric;
               PERSPROrec1 := PERSPROrec2;
               PERSPROrec1.persprocases := [nominative]
         >
>
&


\end{verbatim}
