% RULE detpformation1
< m1: DET{DETrec1}[mu1] 
>

< m : DETP{DETPrec1}[head/DET{DETrec1}[mu1]
                    ]
>

      COMP
         <
          C1: true
             C11: DETrec1.eformation <> noformation AND
                  DETrec1.eorenform = noform
             A11: DETPrec1.possgenders := [neutgender]
             C12: DETrec1.eformation <> noformation AND
                  DETrec1.eorenform = eform
             A12: DETPrec1.possgenders := [mascgender, femgender]
             C13: DETrec1.eformation = noformation 
             A13: DETPrec1.possgenders := [omegagender]
          A1: DETPrec1 := 
                 { possnietnp  :  DETrec1.possnietnp
                   definite    :  DETrec1.definite
                   posspred    :  DETrec1.posspred
                   possnumbers :  DETrec1.possnumbers 
                   posscomas   :  DETrec1.posscomas
                   mood        :  DETrec1.mood
                   eORenform   :  DETrec1.eORenform
           (*        syntquant   :  DETrec1.syntquant *)
                 }
         >

      DECOMP
         <
          C1: DETPrec1 = 
                 { possnietnp  :  DETrec1.possnietnp
                   definite    :  DETrec1.definite
                   posspred    :  DETrec1.posspred
                   possnumbers :  DETrec1.possnumbers 
                   posscomas   :  DETrec1.posscomas
                   mood        :  DETrec1.mood
                   eORenform   :  DETrec1.eORenform
           (*        syntquant   :  DETrec1.syntquant  *)
                 }
             C11: DETrec1.eformation <> noformation AND
                  DETrec1.eorenform = noform AND
                  DETPrec1.possgenders * [neutgender] <> []
             A11: @
             C12: DETrec1.eformation <> noformation AND
                  DETrec1.eorenform = eform AND
                  DETPrec1.possgenders * [mascgender, femgender] <> []
             A12: @
             C13: DETrec1.eformation = noformation AND
                  DETPrec1.possgenders = [omegagender]
             A13: @
          A1:  @
         >
&

\end{verbatim}
