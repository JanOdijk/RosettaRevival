\begin{verbatim}
% RULE Rsentenceomtecompl
< m1:I4::N1
               [ mu1,
                 predrel/N2
                       [ mu2,
                         complrel/SENTENCEVAR{SENTENCEVARrec1},
                         mu3 
                       ],
                 mu5
               ] 
SUBST:
  m2:SENTENCE{SENTENCErec1}
             [ I1:sig1, 
               conjrel/CONJ(AUX_omCONJkey){CONJrec1},
               I3:mu4
             ]        (* substituent *) 
>

< m :I4::N1
               [ mu1,
                 predrel/N2
                       [ mu2,
                         complrel/SENTENCE{SENTENCErec2}
				      [ I1:sig1, 
					I2:sig2,
					I3:mu4
				      ],        
                         mu3 
                       ],
                 I5:mu5
               ] 
>


   MATCHCONDITIONS
      <
       I1:  ALL(sig1, shiftrel/..)
       I2:  ALL(sig2, conjrel/CONJ(AUX_omCONJkey){CONJrec2})
       I3:  NOT EXIST (mu4, [I31:rel1/..])
            I31: rel1 IN [shiftrel, conjrel]
       I4: N1.CAT IN [ADJPPROP, VERBPPROP]
       I5: NOT EXIST(mu5,[I51:../T1])
           I51: T1.CAT IN AUX_propvarcats
      >




   PARAMETERS
      <
       LEVEL
      >


<
   SUBRULE
      < >
      <sig2:[] >

      COMP
         <
          C1:  SENTENCEVARrec1.index = LEVEL AND
               SENTENCErec1.infsort = omteinf AND
               SENTENCEVARRec1.infsort = omteinf AND
	       SENTENCEVARrec1 = 
                    COPYT_SENTENCEtoVAR(SENTENCErec1) (* !! *)
	  
          A1: SENTENCErec2 := SENTENCErec1;
              SENTENCErec2.infsort := teinf;
         >
      DECOMP
         <
          C1: SENTENCErec2.infsort = teinf
	  A1: SENTENCEVARrec1 := 
                    COPYT_SENTENCEtoVAR(SENTENCErec2);
              SENTENCEVARrec1.infsort := omteinf; 
              SENTENCEVARrec1.index := LEVEL;
              SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.infsort := omteinf; 
         >

   SUBRULE
      < >
      <sig2:[conjrel/CONJ(AUX_omCONJkey){CONJrec3}] >

    (* no  COMP part *)

      DECOMP
         <
          C1: true
          A1: SENTENCEVARrec1 := COPYT_sentenceTOvar(SENTENCErec2);
              SENTENCEVARrec1.index := LEVEL;
              SENTENCErec1 := SENTENCErec2;
         >

>
&

\end{verbatim}
