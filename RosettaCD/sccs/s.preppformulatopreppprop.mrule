% RULE rpppsubstitution2 
< m1:PREPPFORMULA{PREPPFORMULArec1}
         [ mu1,
           predrel/PREPP{PREPPrec1}
                  [ mu2,
                    errel/I2::NPVAR{NPVARrec1},
                    head/T3,
                    mu3 
                  ],
           mu4 
         ]  
  SUBST:
  m2: NP{NPrec1}[head/T1]
>

< m :PREPPFORMULA{PREPPFORMULArec1}
         [ mu1,
           predrel/PREPP{PREPPrec2}
                  [ mu2,
                    errel/T2,
                    I1:sig1,
                    head/T3,
                    mu3 
                  ],
           mu4 
         ]  
>





   MATCHCONDITIONS
      <
       I1: ALL(sig1, gluerel/..)
       I2: NPVARrec1.index = LEVEL
      >

   PARAMETERS
      <
       LEVEL
      >



<
   SUBRULE           (* in het --> erin *)
      <T1:PERSPRO{PERSPROrec1}
                [head/BPERSPRO(KEY_hetbpersprokey){BPERSPROrec1}] >
      <T2: RADVP{RADVPrec1}[head/RADV(KEY_erRADVkey){RADVrec1} ]
       sig1:[ gluerel/GLUE{GLUErec1}]
      >

      COMP
         <
          C1: NPrec1 = AUX_NPonhetbprspr  EXCEPT FOR 
                           [cases, generic, superdeixis]           AND
              PERSPROrec1 = COPYT_bpersprotoperspro(BPERSPROrec1)  AND
              NPVARrec1 = COPYT_nptovar(NPrec1) EXCEPT FOR [cases] AND
              NPrec1.cases = [Nominative]                          AND
              NPVARrec1.cases = [Rcase]                            AND
              PERSPROrec1 =
                 { 
                  reduced      : false
                  persprocases : [Nominative]
                 }                                                 AND
              NPrec1.generic = nogeneric                           AND
              NPrec1.superdeixis = PREPPFORMULArec1.superdeixis    AND
              PREPPrec1.radvkey = 0
          A1: RADVPrec1 :=
                {
                  mood     : RADVrec1.mood
                  radvkey  : RADVrec1.key
                  kindefs    : [Pk]
                };
              PREPPrec2 := PREPPrec1;
              PREPPrec2.radvkey := KEY_erRADVkey;
         >
      DECOMP
         <
          C1: RADVPrec1 =
                {
                  mood     : RADVrec1.mood
                  radvkey  : RADVrec1.key
                }                                     AND
              Pk IN RADVPrec1.kindefs                 AND
              PREPPrec2.radvkey = KEY_erRADVkey
          A1: NPrec1 := AUX_NPonhetbprspr  ;
              PERSPROrec1 := COPYT_bpersprotoperspro(BPERSPROrec1);
              NPVARrec1 := COPYT_nptovar(NPrec1);
              NPVARrec1.index := LEVEL;
              NPrec1.cases := [Nominative];
              NPVARrec1.cases := [Rcase];
              PERSPROrec1 :=
                 { 
                  reduced      : false
                  persprocases : [Nominative]
                 };
              NPrec1.generic := nogeneric;
              NPrec1.superdeixis := PREPPFORMULArec1.superdeixis;
              PREPPrec1 := PREPPrec2;
              PREPPrec1.radvkey := 0
         >


   SUBRULE           (* in dit --> hierin *)
      <T1: DEMPRO(KEY_ditdemprokey){DEMPROrec1} >
      <T2: RADVP{RADVPrec1}[head/RADV(KEY_hierRADVkey){RADVrec1} ]
       sig1:[ gluerel/GLUE{GLUErec1}]
      >

      COMP
         <
          C1: NPrec1 = AUX_NPonditdmpr  EXCEPT FOR 
                        [cases, superdeixis, generic]              AND
              NPVARrec1 = COPYT_nptovar(NPrec1) EXCEPT FOR [cases] AND
              NPrec1.cases = [Nominative]                          AND
              NPVARrec1.cases = [Rcase]                            AND
              NPrec1.generic = nogeneric                           AND
              NPrec1.superdeixis = PREPPFORMULArec1.superdeixis    AND
              PREPPrec1.radvkey = 0
          A1: RADVPrec1 :=
                {
                  mood     : RADVrec1.mood
                  radvkey  : RADVrec1.key
                  kindefs    : [Pk]
                };
              PREPPrec2 := PREPPrec1;
              PREPPrec2.radvkey := KEY_hierRADVkey;

         >
      DECOMP
         <
          C1: RADVPrec1 =
                {
                  mood     : RADVrec1.mood
                  radvkey  : RADVrec1.key
                }                                     AND
              Pk IN RADVPrec1.kindefs                 AND
              PREPPrec2.radvkey = KEY_hierRADVkey
