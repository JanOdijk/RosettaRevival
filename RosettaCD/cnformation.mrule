% RULE RCNmodPP                   (* CN is substituent *)
< SUBST:
  m1: CN{CNrec1}[mu1,
                 I1:head/N1[mu2],
                 I3:mu3]

  m2: OPENPREPPPROP{OPENPREPPPROPrec1}
                              [subjrel/CNVAR{CNVARrec1},
                               predrel/PREPP{PREPPrec1}[mu4]
                              ]
>

< m : CN{CNrec2}[mu1,
                 I1:head/N1[mu2],
                 I3:mu3,
                 postmodrel/PREPP{PREPPrec1}[mu4]
                ]
>

   MATCHCONDITIONS
      <
       I1: N1.CAT  IN [NOUN, EN]
       I3: NOT EXIST(mu3,[postmodrel/SENTENCE{SENTENCErec1}[mu9] ]) 
      >

   PARAMETERS
      <
       LEVEL
      >

      COMP
         <
          C1: (OPENPREPPPROPrec1 = COPYT_PREPPtoPREPPPROP(PREPPrec1)
                      EXCEPT FOR [superdeixis, specQ, actsubcefs, 
                                  PROsubject, aktionsarts]) AND
              (OPENPREPPPROPrec1.specq = PREPPrec1.specQ) AND
              (OPENPREPPPROPrec1.actsubcefs = PREPPrec1.actsubcefs) AND
              (OPENPREPPPROPrec1.PROsubject = true) AND
              (OPENPREPPPROPrec1.aktionsarts = [stative]) AND
              (OPENPREPPPROPrec1.superdeixis = CNrec1.superdeixis) AND
              (CNVARrec1 = COPYT_CNtoVAR(CNrec1)) AND
              (CNVARrec1.index = LEVEL)
          A1: CNrec2 := CNrec1;
              CNrec2.possgeni := false; 
              CNrec2.actsubcs := CNrec1.actsubcs - [plurunitnoun, unitnoun];
         >

      DECOMP
         <
          C1: true (* CNrec2.possgeni = false; via TCNnounposs *)
          A1: WBONUS -1;
              OPENPREPPPROPrec1 := COPYT_PREPPtoPREPPPROP(PREPPrec1);
              OPENPREPPPROPrec1.PROsubject := true;
              OPENPREPPPROPrec1.aktionsarts := [stative];
              OPENPREPPPROPrec1.superdeixis := CNrec2.superdeixis;
              CNVARrec1 := COPYT_CNtoVAR(CNrec1);
              CNVARrec1.index := LEVEL;
              CNrec1 := CNrec2;
         >
&


\end{verbatim}
