   SUBRULE  (* subjvpadvrel graag, analysis only*)
      <mu1 :[I6:mu6, subjvpadvrel/I8::ADVP{ADVPrec1}[mu10], I7:mu7 ]>
      <mu11:[shiftrel/I8::ADVP{ADVPrec1}[mu10], I6:mu6, I7:mu7 ]>

   MATCHCONDITIONS
      <
       I6: ALL(mu6, I61:rel6/..)
           I61: QUOTE_Srelprec(rel6, subjvpadvrel)  
