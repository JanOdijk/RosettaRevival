< m :ADJPFORMULA{ADJPFORMULArec1}
        I1:[ rel1/I4::NPVAR{NPVARrec1},
             predrel/ADJP{ADJPrec1}
                 I2:[ mu3,
                      rel3/I3::NP{NPrec1}
                                 [head/RECIPRO(AUX_elkaarkey){RECIPROrec1}],
                      mu4
                    ]
           ]
