% TRANSFORMATION TADJPREPPExtrapos1 (* met x1 bezig --> bezig met x1
                                     aan x1 bekend -->  bekend aan x1 
                                   *)
< m1: ADJPFORMULA{ADJPFORMULArec1}
                  [mu1,
                   predrel/ADJP{ADJPrec1}
                           [mu2,
                            I1:rel1/N1[mu5,
                                       head/PREP{PREPrec1},
                                       I6:objrel/T1
                                      ],
                            mu3,
                            head/ADJ{ADJrec1}[mu6],
                            I3:sig1,
                            mu4
                           ]
                  ]
>
< m : ADJPFORMULA{ADJPFORMULArec1}
                  [mu1,
                   predrel/ADJP{ADJPrec1}
                           [I5:mu2,
                            I4:mu3,
                            head/ADJ{ADJrec1}[mu6],
                            I3:sig1,
                            I2:paprepobjrel/N1[mu5,
                                             head/PREP{PREPrec1},
                                             I6:objrel/T1
                                            ],
                            mu4
                           ]
                  ]
>

   MATCHCONDITIONS
      <I4: NOT EXIST(mu3, [I14:rel2/..])
       I5: NOT EXIST(mu2, [I15:rel3/..])
       I1: (rel1 IN [prepobjrel, aanobjrel]) AND
           (N1.cat IN [PREPP, VARPREPP])
       I2: N1.cat IN [PREPP, VARPREPP]
       I6: T1.cat IN [CNVAR, NPVAR, NP]
       I3: ALL (sig1, hoprel/..)
       I14: rel2 IN [causadvrel, tempadvrel, locadvrel, indobjrel, objrel, 
                     locargrel, dirargrel, erposrel]
       I15: rel3 IN [prepobjrel, aanobjrel]

      >

      COMP
         <
          C1:true
             C11: PREPrec1.key = KEY_aanioprepkey AND
                  ADJPrec1.adjpatternefs * AUX_aanobjadjps <> [] AND
                  rel1 = aanobjrel
             A11: @
             C13: PREPrec1.key = ADJrec1.prepkey AND
                  ADJPrec1.adjpatternefs * AUX_prepobjadjps <> [] AND
                  rel1 = prepobjrel
             A13: @
          A1:@
         >
      DECOMP
         <
          C1:true
             C11: PREPrec1.key = KEY_aanioprepkey AND
                  ADJPrec1.adjpatternefs * AUX_aanobjadjps <> []
             A11: rel1 := aanobjrel
             C13: PREPrec1.key = ADJrec1.prepkey AND
                  ADJPrec1.adjpatternefs * AUX_prepobjadjps <> []  AND
                  ADJPrec1.adjpatternefs * AUX_PAadjps = []   (* cf.remarks *)
             A13: rel1 := prepobjrel
          A1:@
         >

&

\end{verbatim}
