\begin{verbatim}
%
% TRANSFORMATION Thetclplacement1
< m1:CLAUSE{CLAUSErec1}
           [ I1:mu1,
             I2:mu2,
             predrel/VERBP{VERBPrec1}
                    [ I3:mu3,
                      objrel/NP{NPrec1}[head/PERSPRO{PERSPROrec1}[mu5]],
                      I4:mu4
                    ],
             mu6 
           ]
>

< m :CLAUSE{CLAUSErec2}
           [ I1:mu1,
             hetclrel/PERSPRO{PERSPROrec1}[mu5],
             I2:mu2,
             predrel/VERBP{VERBPrec1}
                    [ I3:mu3,
                      I4:mu4
                    ],
             mu6 
           ]
>

   MATCHCONDITIONS
      <
       I1: ALL(mu1, I11:rel1/..)
           I11: QUOTE_Srelprec(rel1, hetclrel)
       I2: ALL(mu2, I21:rel2/..)
           I21: QUOTE_Srelprec(hetclrel, rel2)
       I3: ALL(mu3, I31:rel3/..)
           I31: QUOTE_VPrelprec(rel3, objrel)
       I4: ALL(mu4, I41:rel4/..)
           I41: QUOTE_VPrelprec(objrel, rel4)
      >

      COMP
         <
          C1: NPrec1 = AUX_NPonhetBPRSPR EXCEPT FOR [cases] AND
              accusative IN NPrec1.cases AND
              CLAUSErec1.synvpefs * AUX_objandhetvps <> []               
          A1: CLAUSErec2 := CLAUSErec1;
              CLAUSErec2.synvpefs := CLAUSErec1.synvpefs * AUX_objandhetvps;
         >
      DECOMP
         <
          C1: CLAUSErec2.synvpefs * AUX_objandhetvps <> []
          A1: NPrec1 := AUX_NPonhetBPRSPR;
              NPrec1.cases := [accusative];
              CLAUSErec1 := CLAUSErec2;
              CLAUSErec1.synvpefs := CLAUSErec2.synvpefs * AUX_objandhetvps;
         >


&

