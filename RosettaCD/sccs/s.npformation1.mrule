   (* 2 *)      (* boek ik --> mijn boek *)
   SUBRULE
      < 
        T1:NP{NPrec1}[head/PERSPRO{PERSPROrec1}[head/BPERSPRO{BPERSPROrec1}]]
      >

      < 
        T2:POSSADJ{POSSADJrec1}[head/BPERSPRO{BPERSPROrec1}] 
      >


      COMP
         <
          C1: NPrec1 = { req           : [pospol,negpol,omegapol]
                         env           : [pospol,negpol,omegapol]
                         class         : omegaTimeAdvClass
                         superdeixis   : omegadeixis
                         deixis        : omegadeixis
                         aspect        : omegaAspect
                         retro         : false
                         definite      : def
                         person        : PERSPROrec1.person
                         number        : PERSPROrec1.number
                         cases         : [nominative]
                         genders       : [PERSPROrec1.gender] 
                         actsubcs      : [othernoun]
                         sexes         : []
                         temporal      : false
                         possgeni      : false
                         animate       : PERSPROrec1.animate
                         human         : Omegahuman
                         thetanp       : omegathetanp
                         nounpatternefs: []
