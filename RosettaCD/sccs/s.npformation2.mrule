\begin{verbatim}
% TRANSFORMATION TNPnoTOomega2
< m1:NP{NPrec1}[ mu1,
                 head/CN{CNrec1}
                   [mu2, 
                    I1:modrel/SENTENCE{SENTENCErec1}
                            [ mu5,
                              predrel/VERBP{VERBPrec1}
                                [mu7, rel1/I2::VERB{VERBrec1}[mu8] ],
                              mu6
                            ],
                    mu3
                   ],
                 mu4 
               ]
>

< m :NP{NPrec1}[ mu1,
                 head/CN{CNrec1}
                   [mu2, 
                    I1:modrel/SENTENCE{SENTENCErec1}
                            [ mu5,
                              predrel/VERBP{VERBPrec1}
                                [mu7, rel1/I3::VERB{VERBrec2}[mu8] ],
                              mu6
                            ],
                    mu3
                   ],
                 mu4 
               ]
>


   MATCHCONDITIONS
      <
       I1: SENTENCErec1.mood = anterelative AND
           SENTENCErec1.modus IN [prespart,pastpart]
       I2: VERBrec1.eORenForm = NoForm
       I3: VERBrec2.eORenForm = OmegaForm
      >


      COMP
         <
          C1: true
          A1: VERBrec2 := VERBrec1;
              VERBrec2.eORenForm := OmegaForm;
         >
      DECOMP
         <
          C1: true
          A1: VERBrec1 := VERBrec2;
              VERBrec1.eORenForm := NoForm;
         >


&

