           ) OR
           (((T2.cat IN [NP] WITH T2.REC.actsubcs * [unitnoun, plurunitnoun] <> []) 
                                                   (* amountnp *) OR
            (T2.cat IN [NP] WITH T2.REC.actcomas * [mass] <> []) 
            ) AND
            (rel1 IN [premodrel])
           )
       I3: NOT EXIST(mu1, [premodrel/..]) AND
           NOT EXIST(mu1, [degreemodrel/..])
      >


< 
   SUBRULE (* 1 *)    (* te veel *)
      <T1:ADVP{ADVPrec1}[mu9] >
      <T2:ADVP{ADVPrec2}[mu9] >

