                            [mu1,
                             predrel/I2::ADJP{ADJPrec1}
                                     [mu2,
                                      head/ADJ{ADJrec1}[mu5],
                                      mu4 
                                     ]
                            ]  
  m2: T2

>

< m : I4::ADJPPROP{ADJPPROPrec1}
              [mu1,
               predrel/I2::ADJP{ADJPrec1}
                       [mu2,
                        head/ADJ{ADJrec1}[mu5],
                        voorobjrel/VARPREPP{VARPREPPrec1}
                                      [head/PREP(AUX_voorioprepkey){PREPrec1},
                                       I1:objrel/T2
                                      ],
                        mu4
                       ]
              ]
