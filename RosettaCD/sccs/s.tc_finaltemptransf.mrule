< m1: SENTENCE{SENTENCErec1}
            [mu1, predrel/VERBP{VERBPrec1}[mu2 ], mu3] 
>

< m : SENTENCE{SENTENCErec2}
            [mu1, predrel/VERBP{VERBPrec1}[mu2 ], mu3]

>
   MATCHCONDITIONS
      <
       m1: (EXIST (mu2, [auxrel/I11:: VERB{VERBrec1}[mu4]]))   OR
           (EXIST (mu2, [I12: rel1/VERB{VERBrec2}[I13: mu5]])) OR
           (NOT QUOTE_TempRetroNotFound(mu1)) OR 
           (SENTENCErec1.conjretro = true) OR
           ((EXIST (mu2, [head/I15::VERB{VERBrec4}[mu7]])) AND
            (NOT EXIST (mu2, [auxrel/I16::VERB{VERBrec5}[mu8]]))
           )
       m : (EXIST (mu2, [auxrel/I11:: VERB{VERBrec1}[mu4]]))   OR
           (EXIST (mu2, [I12: rel1/VERB{VERBrec2}[I13: mu5]])) OR
           (NOT QUOTE_TempRetroNotFound(mu1)) OR
           (SENTENCErec2.conjretro = true) OR
           ((EXIST (mu2, [head/I15::VERB{VERBrec4}[mu7]])) AND
            (NOT EXIST (mu2, [auxrel/I16::VERB{VERBrec5}[mu8]]))
           )
       I11: (VERBrec1.status IN [bareV, teV]) AND
            (VERBrec1.subc IN [hebbenaux, zijnaux]) 
       I12: (rel1 IN [head, auxrel, progauxrel]) AND 
            (VERBrec2.status IN [AuxV, teAuxV]) 
       I13: (EXIST (mu5, [auxrel/I14::VERB{VERBrec3}[mu6]]))
       I14: (VERBrec3.subc IN [hebbenaux, zijnaux])
       I15: (VERBrec4.modus = pastpart)
       I16: (VERBrec5.subc IN [hebbenaux, zijnaux, wordenaux]) OR
            (VERBrec5.status IN [teVV, VV, AuxV, teAuxV])
      >

     COMP
        <
          C1: SENTENCErec1.retro = true
          A1: SENTENCErec2 := SENTENCErec1;
              SENTENCErec2.retro := false;
         >
      DECOMP
         <
          C1: SENTENCErec2.retro = false
          A1: SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.retro := true;
         >


&
\end{verbatim}
