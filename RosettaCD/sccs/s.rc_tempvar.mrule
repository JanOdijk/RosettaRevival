%
% RULE Rrefvarinsertion
< m1: VERBPPROP{VERBPPROPrec1}
            [I1: mu1,
             predrel/VERBP{VERBPrec1}[mu3],
             mu2
            ] 

  m2: T1 
>

< m : VERBPPROP{VERBPPROPrec1}
            [I1: mu1,
             tempadvrel/T1,
             predrel/VERBP{VERBPrec1}[mu3],
             mu2
            ] 
>
   MATCHCONDITIONS
      <
       I1: (QUOTE_temprefnotfound (mu1)) AND
           (QUOTE_tempretronotfound(mu1)) AND
           (QUOTE_tempdurnotfound(mu1))
       m2: T1.CAT IN [ADVPVAR, PREPPVAR, SENTENCEVAR]
       m : NOT EXIST(mu2, [I21:rel2/..])
           I21: rel2 IN [postsentadvrel, posttemprel]
      >
<
   SUBRULE
      < T1: ADVPVAR{ADVPVARrec1} >
      < T1: ADVPVAR{ADVPVARrec1} >

      COMP
         <
          C1: (ADVPVARrec1.class = reference) AND
              (ADVPVARrec1.retro = false)
          A1: @
         >
      DECOMP
         <
          C1: (ADVPVARrec1.class = reference) AND
              (ADVPVARrec1.retro = false)
          A1: @
         >

   SUBRULE
      < T1: PREPPVAR{PREPPVARrec1} >
      < T1: PREPPVAR{PREPPVARrec1} >

      COMP
         <
          C1: (PREPPVARrec1.class = reference) AND
              (PREPPVARrec1.retro = false)
          A1: @
         >
      DECOMP
         <
          C1: (PREPPVARrec1.class = reference) AND
              (PREPPVARrec1.retro = false)
          A1: @
         >

   SUBRULE
      < T1: SENTENCEVAR{SENTENCEVARrec1} >
      < T1: SENTENCEVAR{SENTENCEVARrec1} >

      COMP
         <

          C1: (SENTENCEVARrec1.conjclass = reference) AND
              (SENTENCEVARrec1.conjretro = false)
          A1: @
         >
      DECOMP
         <
          C1: (SENTENCEVARrec1.conjclass = reference) AND
              (SENTENCEVARrec1.conjretro = false)
          A1: @
         >
>
&

\end{verbatim}
