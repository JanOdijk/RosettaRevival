\begin{verbatim}
%
% FILTER FpostunitNP (* block the further generation of unitNPs with 
                        singular plural NOUN that do not contain a 
                        numeral modifier or determiner, or that do contain a 
                        non-numerical modifier  *)

< m : NP{NPrec1}[mu1,
                 head/CN{CNrec1}
                               [ mu2,
                                 head/NOUN{NOUNrec1}[mu3],
                                 mu4
                                ]
                ]
>

   MATCHCONDITIONS
      <
       m:( (* unitnoun IN CNrec1.actsubcs AND *)
          CNrec1.numbers = [plural] AND
          NOUNrec1.number = singular AND
          ((NOT  EXIST (mu1, [detrel/I11::DETP{DETPrec1}[mu5]])) AND
           (NOT  EXIST (mu2, [modrel/I12::DETP{DETPrec2}[mu6]]))) OR
           (EXIST(mu2, [I13:rel3/T3])) OR
           (EXIST(mu4, [I14:rel4/T4]))
         )
      I11: DETPrec1.definite = indef AND
           plural IN DETPrec1.possnumbers (* extra attributen of key-set
                                              nodig om `vele' en `weinige'
                                              uit te sluiten van I11 en I12 *)
      I12: DETPrec2.definite = indef AND
           plural IN DETPrec1.possnumbers 
      I13: (rel3 IN [modrel]) AND (T3.CAT IN [ADJP, SENTENCE])
      I14: (rel4 IN [modrel, postmodrel]) AND 
           (T4.CAT IN [SENTENCE, ADJP, PREPP, ADVP])
      >


      COMP
         <
          C1: true
          A1: @
         >

      (* No DECOMP *)
&
\end{verbatim}
