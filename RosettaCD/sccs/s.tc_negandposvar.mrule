% RULE Rnegvar2                 (* meltnegvar in VP *)
< m1:CLAUSE{CLAUSErec1}
           [ I3:mu1,
             predrel/VERBP{VERBPrec1}
                 I1:[ mu2,
                      I2:rel1/T2,
                      mu4 
                    ],
             mu5 
           ] 
>

< m :CLAUSE{CLAUSErec1}
           [ I3:mu1,
             predrel/VERBP{VERBPrec1}
                 I1:[ mu2,
                      meltnegrel/NEGVAR{NEGVARrec1},
                      I4:rel1/T2,
                      mu4 
                    ],
             mu5 
           ] 
>

   MATCHCONDITIONS
      <
       m1 : NOT EXIST(mu4, [meltnegrel/..])  AND 
            NOT EXIST(mu5, [meltnegrel/..])
       m  : NOT EXIST(mu4, [meltnegrel/..])  AND 
            NOT EXIST(mu5, [meltnegrel/..])
       I1 : RIGHTTOLEFT
       I3 : NOT EXIST(mu1,[I13:rel2/..])
       I2 : rel1 = relpar AND
            T2.CAT IN [NPVAR,ADVPVAR,VARPREPP] AND QUOTE_possnietxp(t2)
       I4 : T2.CAT IN [NPVAR,ADVPVAR,VARPREPP] AND QUOTE_possnietxp(t2)
       I13: rel2 IN [posrel, negrel]
      >


   PARAMETERS
      <
       relpar : synrel
      >



      COMP
         <
          C1: rel1 IN [objrel, indobjrel, prepobjrel, predrel, locargrel,
                        dirargrel, vpadvrel, agvpadvrel, byobjrel]
          A1: @
         >
      DECOMP
         <
          C1: rel1 IN [objrel, indobjrel, prepobjrel, predrel, locargrel,
                        dirargrel, vpadvrel, agvpadvrel, byobjrel]
          A1: relpar := rel1;
         >


&

\end{verbatim}

