       I4: CNrec2.definite <> def AND
           ((mass IN CNrec2.posscomas) OR
            (plural IN CNrec2.numbers)) 
       I5: CNrec3.definite <> def AND
           ((mass IN CNrec3.posscomas) OR
            (plural IN CNrec3.numbers)) 
      m2: NPrec1.definite = indef
      >

