\begin{verbatim}
% RULE Radjunctvar2
< m1:VERBPPROP{VERBPPROPrec1}
       [ mu1,
         predrel/VERBP{VERBPrec1}
           [ I2:mu2,
             I6:mu6,
             head/VERB{VERBrec1}[mu3],
             mu4 
           ],
         mu5
       ] 
m2: T1
>

< m :VERBPPROP{VERBPPROPrec2}
       [ mu1,
         predrel/VERBP{VERBPrec1}
           [ I2:mu2,
             rel1/T1,
             I6:mu6,
             head/VERB{VERBrec1}[mu3],
             mu4 
           ],
         mu5
       ] 

>

   MATCHCONDITIONS
      <
       I2: ALL(mu2,I21:rel2/..)
           I21: QUOTE_VPrelprec(rel2,indobjrel) AND 
                [rel2] * [indobjrel,aanobjrel, voorobjrel] = []
       I6: ALL(mu6,I61:rel6/..)
           I61: QUOTE_VPrelprec(indobjrel,rel6) AND 
                [rel6] * [indobjrel,aanobjrel, voorobjrel] = []
      >

   PARAMETERS
      <
       adjunctpar:adjuncttype
      >

      COMP
         <
          C1: benfactNP IN VERBrec1.adjuncts            AND
              [benfactNP] * VERBPPROPrec1.adjuncts = [] AND
              T1.CAT IN [NPVAR,CNVAR]                   AND
              adjunctpar = benfactNP
          A1: VERBPPROPrec2 := VERBPPROPrec1;
              VERBPPROPrec2.adjuncts := VERBPPROPrec1.adjuncts + [benfactNP];
              rel1 := indobjrel;
         >
      DECOMP
         <
          C1: benfactNP IN VERBrec1.adjuncts      AND
              benfactNP IN VERBPPROPrec2.adjuncts AND
              T1.CAT IN [NPVAR,CNVAR]             AND
              rel1 = indobjrel
          A1: adjunctpar := benfactNP;
              VERBPPROPrec1 := VERBPPROPrec2;
              VERBPPROPrec1.adjuncts := VERBPPROPrec2.adjuncts - [benfactNP];
         >



&

\end{verbatim}
