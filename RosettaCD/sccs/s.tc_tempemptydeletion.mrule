\begin{verbatim}
% FILTER Fretrodeletion
< m: SENTENCE{SENTENCErec1}
              I1: [I2: mu1,
                   tempadvrel/ADVP{ADVPrec1}
                             [head/ADV{ADVrec1}
                                  [head/SUBADV{SUBADVrec1}
                                       [head/BADV(AUX_retrorefemptykey){BADVrec1}]
                                  ] 
                              ],
                    I3: mu2 
                   ]
>

   MATCHCONDITIONS
      <
       I1: RIGHTTOLEFT
       I2: (ALL(mu1, I4: rel1/..)) AND
           (QUOTE_tempdurnotfound(mu1)) AND
           (QUOTE_tempretronotfound(mu1))
       I3: (NOT EXIST (mu2, [I5: rel2/..])) AND
           (QUOTE_temprefnotfound(mu2)) AND
           (QUOTE_tempretronotfound(mu2))
       I4: rel1 IN [leftdislocrel, shiftrel, subjrel, hetclrel, firstclrel, 
                    secondclrel, tempadvrel,reflrel, erposrel, erqrel, 
                    defRadvrel, preadvrel, conjrel]
       I5: rel2 IN [leftdislocrel, shiftrel, subjrel, hetclrel, firstclrel, 
                    secondclrel,reflrel, erposrel, erqrel, defRadvrel,
                    preadvrel, conjrel]
      >


         <
          C1: (ADVPrec1 = AUX_advpONrtrrfempty) AND
              (ADVrec1 = AUX_advONrtrrfempty) AND
              (SUBADVrec1 = COPYT_badvtosubadv(BADVrec1)) AND
              (SUBADVrec1.lastaffix = noaffix) AND
              (SENTENCErec1.retro= true) AND
              (SENTENCErec1.aspect = perfective OR
               (SENTENCErec1.aspect = imperfective AND
                SENTENCErec1.conjretro = true)
              )
          A1: @
         >

&

\end{verbatim}
