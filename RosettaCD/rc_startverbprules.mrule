%RULE RSIDSTARTVPPROP120

<m1: I1::SUBVERB{SUBVERBREC1}[head/BVERB(KEY1){BVERBREC1}]
 m2: T1
 m3: T2
>

<m: I4::VERBPPROP{VERBPPROPREC1}
             [ I2:subjrel/T1, 
               predrel/VERBP{VERBPREC1}
                      [ argrel/T2,
                        head/I5::VERB{VERBREC1}
                         [head/SUBVERB{SUBVERBREC1}[head/BVERB(KEY3){BVERBREC1}]
                         ]
                      ]
             ]
>

   MATCHCONDITIONS
      <
       I1: SUBVERBREC1.thetavp = vp120 
       I2: T1.CAT IN AUX_VARCATSET 
       m2: T1.CAT IN AUX_VARCATSET 
       I4: VERBPPROPREC1 =
		  { 
		   thetavp     : vp120
		   req         : [ pospol, negpol, omegapol]
		   env         : [ pospol, negpol, omegapol]
		   superdeixis : Omegadeixis
		   PROsubject  : false
		   voice       : omegavoice
		   synvpefs    : []
                   aktionsarts : []
                   adjuncts    : []
		  }



       I5: VERBREC1 =
		  { 
                   thetavp      : vp120
		   number       : omeganumber   
		   persons      : []   
		   eORenForm    : NoForm   
		   modus        : omegamodus  
		   tense        : omegatense  
		  } AND
           VERBREC1.status IN [bareV, partV]
      >

<
   SUBRULE (* 1 *)
     <T2: NPVAR{NPVARREC1}^NP{NPREC1}
                                [mu1,
                                 head/CN{CNREC1}
                                   [mu3,
                                    head/NOUN{NOUNREC1}
                                        [head/SUBNOUN{SUBNOUNREC1}
                                           [head/BNOUN(KEY2){BNOUNREC1}]
                                        ],
                                    mu4
                                   ],
                                 mu2
                                ]
      >
     <T2: NPVAR{NPVARREC1}^NP{NPREC1}
                                [mu1,
                                 head/CN{CNREC1}
                                   [mu3,
                                    head/NOUN{NOUNREC1}
                                        [head/SUBNOUN{SUBNOUNREC1}
                                           [head/BNOUN(KEY2){BNOUNREC1}]
                                        ],
                                    mu4
                                   ],
                                 mu2
                                ]
      >
COMP
 <
   C1: COMPINSIDDICT(KEY1, KEY2, 2)
      C11: SUBVERBREC1.lastaffix <> partaffix
      A11: KEY3 := COMPGETSIDDICT (KEY1, KEY2, 2);
           VERBREC1 := COPYT_subverbtoverb(SUBVERBREC1);
      C12: SUBVERBREC1.lastaffix = partaffix
      A12: KEY3 := COMPGETSIDDICT (KEY1, KEY2, 2);
           VERBREC1 := COPYT_subverbtoverb(SUBVERBREC1);
           VERBREC1.status := partV; 
   A1: VERBPPROPREC1.thetavp := vp120;
 >
DECOMP
 <
   C1: DECOMPINSIDDICT(KEY3, KEY2, 2)
      C11: VERBREC1 = COPYT_subverbtoverb(SUBVERBREC1) AND
           SUBVERBREC1.lastaffix <> partaffix           AND
           VERBREC1.status = bareV 
      A11: KEY1 := DECOMPGETSIDDICT(KEY3, KEY2, 2);
      C12: VERBREC1 = COPYT_subverbtoverb(SUBVERBREC1)  AND
           VERBREC1.status = partV                      AND
           SUBVERBREC1.lastaffix = partaffix
      A12: KEY1 := DECOMPGETSIDDICT(KEY3, KEY2, 2);
   A1: WBONUS +5;
       @
 >

  SUBRULE (* 2 *)
     <T2: CNVAR{CNVARREC1}^CN{CNREC1}
                                   [mu3,
                                    head/NOUN{NOUNREC1}
                                        [head/SUBNOUN{SUBNOUNREC1}
                                           [head/BNOUN(KEY2){BNOUNREC1}]
                                        ],
                                    mu4
                                   ]
      >
     <T2: CNVAR{CNVARREC1}^CN{CNREC1}
                                   [mu3,
                                    head/NOUN{NOUNREC1}
                                        [head/SUBNOUN{SUBNOUNREC1}
                                           [head/BNOUN(KEY2){BNOUNREC1}]
                                        ],
                                    mu4
                                   ]
      >

COMP
 <
   C1: COMPINSIDDICT(KEY1, KEY2, 2)
      C11: SUBVERBREC1.lastaffix <> partaffix
      A11: KEY3 := COMPGETSIDDICT (KEY1, KEY2, 2);
           VERBREC1 := COPYT_subverbtoverb(SUBVERBREC1);
      C12: SUBVERBREC1.lastaffix = partaffix
      A12: KEY3 := COMPGETSIDDICT (KEY1, KEY2, 2);
           VERBREC1 := COPYT_subverbtoverb(SUBVERBREC1);
           VERBREC1.status := partV; 
   A1: VERBPPROPREC1.thetavp := vp120;
 >
DECOMP
 <
   C1: DECOMPINSIDDICT(KEY3, KEY2, 2)
      C11: VERBREC1 = COPYT_subverbtoverb(SUBVERBREC1)  AND
           SUBVERBREC1.lastaffix <> partaffix           AND
           VERBREC1.status = bareV 
      A11: KEY1 := DECOMPGETSIDDICT(KEY3, KEY2, 2);
           SUBVERBREC1 := COPYT_bverbtosubverb(BVERBREC1);
      C12: VERBREC1 = COPYT_subverbtoverb(SUBVERBREC1)  AND
           VERBREC1.status = partV                      AND
           SUBVERBREC1.lastaffix = partaffix
      A12: KEY1 := DECOMPGETSIDDICT(KEY3, KEY2, 2);
           SUBVERBREC1 := COPYT_bverbtosubverb(BVERBREC1);
   A1: WBONUS +5;
       @
 >
>
