          C1: (ADJrec1.adjpatterns = [synnoadjpargs]) AND
              (ADJPrec1.adjpatternefs = [])  AND
              (ADJPPROPrec1.adjpatternefs = []) 
          A1: ADJPrec2 := ADJPrec1;
