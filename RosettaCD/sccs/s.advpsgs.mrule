              ADVPrec1.actsubcefs * ADVrec1.subcs <> [] AND
              ADVPPROPrec1 = COPYT_ADVPtoPROP(ADVPrec1) AND
              ADVPPROPrec1 = 
		  {
		  aktionsarts : [stative]
		  PROsubject  : false
		  }
          A1: @
         >

&

