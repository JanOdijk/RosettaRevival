                 }                                                 AND
              NPrec1.generic = nogeneric                           AND
              NPrec1.superdeixis = PREPPrec3.superdeixis    AND
              PREPPrec1.radvkey = 0
          A1: RADVPrec1 :=
                {
                  mood     : RADVrec1.mood
                  radvkey  : RADVrec1.key
                  kindefs    : [Pk]
                };
              PREPPrec2 := PREPPrec1;
              PREPPrec2.radvkey := KEY_overalRADVkey;
         >
      DECOMP
         <
          C1: RADVPrec1 =
                {
                  mood     : RADVrec1.mood
                  radvkey  : RADVrec1.key
                }                                     AND
              Pk IN RADVPrec1.kindefs                 AND
              PREPPrec2.radvkey = KEY_overalRADVkey
          A1: NPrec1 := AUX_NPonallesBNDFpr  ;
              INDEFPROrec1 := COPYT_BINDEFPROtoINDEFPRO(BINDEFPROrec1);
              NPVARrec1 := COPYT_nptovar(NPrec1);
              NPVARrec1.index := LEVEL;
              NPrec1.cases := [Nominative];
              NPVARrec1.cases := [Rcase];
              INDEFPROrec1 :=
                 { 
                  geni         : false
                 };
              NPrec1.generic := nogeneric;
              NPrec1.superdeixis := PREPPrec3.superdeixis;
              PREPPrec1 := PREPPrec2;
              PREPPrec1.radvkey := 0
         >

>
&

\end{verbatim}
