      COMP
         <
          C1: ( VERBPPROPrec1.synvpefs = [] ) AND
              ( SENTENCEVARrec1 =
		       { 
			  senttype   : subordinateclause
			}
              )
               
              C11: ( synHETOPENTESENT IN VERBrec1.synvps ) AND
                   ( SENTENCEVARrec1 =
			{ 
			  PROsubject : true
			  finiteness : infinite
			  infsort    : teinf
			  mood       : declarative
			}
		   )
                   
	      A11: VERBPPROPrec2 := VERBPPROPrec1;
                   VERBPPROPrec2.synvpefs := [synHETOPENTESENT]

	      C12: ( synHETOPENOMTESENT IN VERBrec1.synvps ) AND
                   ( SENTENCEVARrec1 =
			{ 
			  PROsubject : true
			  finiteness : infinite
			  infsort    : omteinf
			  mood       : declarative
			}
		   )
	      A12: VERBPPROPrec2 := VERBPPROPrec1;
                   VERBPPROPrec2.synvpefs := [synHETOPENOMTESENT]

	      C13: ( synHETTHATSENT IN VERBrec1.synvps ) AND
                   ( SENTENCEVARrec1 =
			{ 
			  finiteness : finite
			  mood       : declarative
			}
		   )
	      A13: VERBPPROPrec2 := VERBPPROPrec1;
                   VERBPPROPrec2.synvpefs := [synHETTHATSENT]

	      C14: ( synHETASIFCLOSEDADJPPROP IN VERBrec1.synvps ) AND
                   ( SENTENCEVARrec1 =
			{ 
                          conjkey    : KEY_alsofCONJkey
			  finiteness : finite
			  mood       : declarative
			}
		   )
	      A14: VERBPPROPrec2 := VERBPPROPrec1;
                   VERBPPROPrec2.synvpefs := [synHETASIFCLOSEDADJPPROP ]

	      C15: ( synHETQSENT IN VERBrec1.synvps ) AND
                   ( SENTENCEVARrec1 =
			{ 
			  finiteness : finite
			}
		   )                                  AND
                   SENTENCEVARrec1.mood IN [yesnointerrogative, whinterrogative]
	      A15: VERBPPROPrec2 := VERBPPROPrec1;
                   VERBPPROPrec2.synvpefs := [synHETQSENT]

	      C16: ( synHETIFSENT IN VERBrec1.synvps ) AND
                   ( SENTENCEVARrec1 =
			{ 
                          conjkey    : KEY_alsCONJkey
			  finiteness : finite
			  mood       : declarative
			}
		   )
	      A16: VERBPPROPrec2 := VERBPPROPrec1;
                   VERBPPROPrec2.synvpefs := [synHETIFSENT]

          A1: PERSPROrec1 := COPYT_BPERSPROtoPERSPRO(BPERSPROREC1);
	      PERSPROrec1.reduced := false;
	      (* persprocases retains the default value *)
	      NPrec1 := AUX_NPonhetBPRSPR ;
         >
      DECOMP
         <
          C1: ( PERSPROrec1 = COPYT_BPERSPROtoPERSPRO(BPERSPROREC1) ) AND
	      (* both the reduced AND the unreduced form are accepted *)
	      ( NPrec1 = AUX_NPonhetBPRSPR  ) AND
              ( SENTENCEVARrec1 =
		       { 
			  senttype   : subordinateclause
			}
              )
	      C11: ( synHETOPENTESENT IN VERBrec1.synvps )        AND
	           ( synHETOPENTESENT IN VERBPPROPrec2.synvpefs ) AND 
		   ( SENTENCEVARrec1 =
			{ 
			  PROsubject : true
			  finiteness : infinite
			  infsort    : teinf
			  mood       : declarative
			}
		   )                                              

	      A11: @

	      C12: ( synHETOPENOMTESENT IN VERBrec1.synvps )        AND
                   ( synHETOPENOMTESENT IN VERBPPROPrec2.synvpefs ) AND 
                   ( SENTENCEVARrec1 =
			{ 
			  PROsubject : true
			  finiteness : infinite
			  infsort    : omteinf
			  mood       : declarative
			}
		   )
	      A12: @

	      C13: ( synHETTHATSENT IN VERBrec1.synvps )        AND
                    ( synHETTHATSENT IN VERBPPROPrec2.synvpefs ) AND 
                   ( SENTENCEVARrec1 =
			{ 
			  finiteness : finite
			  mood       : declarative
			}
		   )
	      A13: @

	      C14: ( synHETASIFCLOSEDADJPPROP IN VERBrec1.synvps )        AND
                    ( synHETASIFCLOSEDADJPPROP IN VERBPPROPrec2.synvpefs ) AND 
                   ( SENTENCEVARrec1 =
			{ 
                          conjkey    : KEY_alsofconjkey
			  finiteness : finite
			  mood       : declarative
			}
		   )
	      A14: @

	      C15: ( synHETQSENT IN VERBrec1.synvps )        AND
