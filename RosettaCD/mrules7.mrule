                   I8:[ I4:mu3,
                        head/I5::VERB{VERBrec1}
                                [head/SUBVERB{SUBVERBrec1}
                                    [ partrel/PART{PARTrec1},
                                      head/SUBVERB{SUBVERBrec2}[mu5]
                                    ]
                                ],
                        I6:mu6 
                      ],
               mu7
             ]
>

< m :I7::SENTENCE{SENTENCErec1}
          I1:[ I2:mu1,
               conjrel/I51::VERB{VERBrec2}
                          [ head/SUBVERB{SUBVERBrec2}[mu5]],
               I3:mu2,
               predrel/VERBP{VERBPrec1}
                   I8:[ I4:mu3,
                        partrel/PART{PARTrec1},
                        I6:mu6 
                      ],
               mu7
             ]
>

   MATCHCONDITIONS
      <
       I1 : RIGHTTOLEFT
       I2 : ALL(mu1, I12:rel4/..)
       I3 : NOT EXIST(mu2, [conjrel/..])
       I4 : ALL(mu3, I14:rel3/..)
       I5 : VERBrec1.status = partV
       I51: VERBrec2.status = bareV
       I6 : ALL(mu6, I16:rel1/..)
       I7 : SENTENCErec1.finiteness = finite
       I8 : RIGHTTOLEFT
       I12: rel4 IN [leftdislocrel, shiftrel]
       I14: rel3 IN  AUX_preVERBrels  
       I16: rel1 IN AUX_postverbrels
        
      >

      COMP
         <
          C1: SUBVERBrec1 = SUBVERBrec2 EXCEPT FOR [lastaffix] AND
              SUBVERBrec1.lastaffix = partaffix
          A1: VERBrec2 := VERBrec1;
              VERBrec2.status :=  bareV;
         >
      DECOMP
         <
          C1: true

          A1: SUBVERBrec1 := SUBVERBrec2;
              SUBVERBrec1.lastaffix := partaffix;
              VERBrec1 := VERBrec2;
              VERBrec1.status := partV;
         >

&



