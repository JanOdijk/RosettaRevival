% TRANSFORMATION TADJObjectOk2
                               (* Insertion of ECNP in case the ergative
                                 object does not move into subjectposition *)
< m1:I4::ADJPFORMULA{ADJPFORMULArec1}
           [ I1:mu1,
             predrel/ADJP{ADJPrec1}
                    [ mu2,
                      indobjrel/T1,
                      mu3,
                      I3:objrel/T2,
                      mu4,
                      head/ADJ{ADJrec1}[mu5],
                      mu6 
                    ]
           ]
>

< m :I4::ADJPFORMULA{ADJPFORMULArec1}
           [ I1:mu1,
             subjrel/NP{NPrec1}[head/EC(AUX_eckey){ECrec1} ],
             predrel/ADJP{ADJPrec1}
                    [ mu2,
                      indobjrel/T1,
                      mu3,
                      I3:objrel/T2,
                      mu4,
                      head/ADJ{ADJrec1}[mu5],
                      mu6 
                    ]
           ]
>


   MATCHCONDITIONS
      <
       I1:  NOT EXIST (mu1, [subjrel/..]) AND
            NOT EXIST (mu1, [postsubjrel/..]) (* probably superfluous*)
       I3:  T2.CAT IN [NPVAR, NP, CNVAR]
       I4:  ADJPFORMULArec1.PROsubject = false AND
            ADJPFORMULArec1.adjpatternefs * AUX_objandhetvps <> [] AND
            ADJPFORMULArec1.thetaadj IN [adjp012]
      >


<
   SUBRULE
      <T2:NPVAR{NPVARrec1} >
      <T2:NPVAR{NPVARrec1} >

   MATCHCONDITIONS
      <
       T2: [NPVARrec1.NPhead] * [persproNp, reducedpersproNP, hetNP] = []
      >

      COMP
         <
          C1: true
          A1: NPrec1 := AUX_NPonEC;
              NPrec1.number := NPVARrec1.number;
         >
      DECOMP
         <
          C1:
             ( NPrec1 = AUX_NPonEC EXCEPT FOR [number])         AND
             ( NPrec1.number = NPVARrec1.number)
          A1: @
         >

   SUBRULE
      <T2: NP{NPrec2}[mu9] >
      <T2: NP{NPrec2}[mu9] >

   MATCHCONDITIONS
      <
       T2: [NPrec2.NPhead] * [persproNp, reducedpersproNP,hetNP] = []
      >


      COMP
         <
          C1: true
          A1: NPrec1 := AUX_NPonEC;
              NPrec1.number := NPrec2.number;
         >
      DECOMP
         <
          C1: ( NPrec1 = AUX_NPonEC EXCEPT FOR [number])         AND
              ( NPrec1.number = NPrec2.number)
          A1: @
         >

   SUBRULE
      <T2: CNVAR{CNVARrec1} >
      <T2: CNVAR{CNVARrec1} >

      COMP
         <
          C1: true
              C11: singular IN CNVARrec1.numbers
              A11: NPrec1 := AUX_NPonEC;
                   NPrec1.number := singular;
              C12: plural IN CNVARrec1.numbers
              A12: NPrec1 := AUX_NPonEC;
                   NPrec1.number := plural;
          A1: @ 
         >

      DECOMP
         <
          C1: true
              C11: singular IN CNVARrec1.numbers             AND
                   NPrec1 = AUX_NPonEC EXCEPT FOR [number]    AND
                   NPrec1.number = singular         
              A11: @
              C12: plural IN CNVARrec1.numbers               AND
                   NPrec1 = AUX_NPonEC EXCEPT FOR [number]    AND
                   NPrec1.number = plural
              A12: @
          A1: @
         >

>
&

\end{verbatim}

