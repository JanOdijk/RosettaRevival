% RULE Radjsentenceomtecompl (* omteinf-complement *)
< m1: ADJPPROP{ADJPPROPrec1}
               [ mu1,
                 predrel/I5::ADJP{ADJPrec1}
                       [ mu2,
                         I4:rel1/SENTENCEVAR{SENTENCEVARrec1},
                         mu3 
                       ]
               ] 
SUBST:
  m2:SENTENCE{SENTENCErec1}
             [ I1:sig1, 
               conjrel/CONJ(KEY_omCONJkey){CONJrec1},
               I3:mu4
             ]        
>

< m : ADJPPROP{ADJPPROPrec1}
               [ mu1,
                 predrel/I5::ADJP{ADJPrec1}
                       [ mu2,
                         I4:rel1/SENTENCE{SENTENCErec2}
				      [ I1:sig1, 
					I2:sig2,
					I3:mu4
				      ],        
                         mu3 
                       ]
               ] 
>


   MATCHCONDITIONS
      <
       I4: rel1 IN [complrel] 
       I5: ADJPrec1.adjpatternefs * AUX_omteinfcompladjps <> []

       I1:  ALL(sig1, shiftrel/..)
       I2:  ALL(sig2, conjrel/CONJ(KEY_omCONJkey){CONJrec2})
       I3:  NOT EXIST (mu4, [I31:rel2/..])
            I31: rel2 IN [shiftrel, conjrel]
      >




   PARAMETERS
      <
       LEVEL
      >


<
   SUBRULE
      < >
      <sig2:[] >

      COMP
         <
          C1:  SENTENCEVARrec1.index = LEVEL AND
	       SENTENCEVARrec1 = 
                    COPYT_SENTENCEtoVAR(SENTENCErec1) (* !! *)  AND
               ADJPrec1.adjpatternefs * AUX_omteinfcompladjps <> [] AND
               SENTENCEVARrec1.infsort = omteinf AND
               SENTENCErec1.infsort = omteinf 
          A1: SENTENCErec2 := SENTENCErec1;
              SENTENCErec2.infsort := teinf;
         >

      DECOMP   
         <
          C1: SENTENCErec2.infsort = teinf AND
              ADJPrec1.adjpatternefs * AUX_omteinfcompladjps <> []
          A1: SENTENCEVARrec1 := 
                      COPYT_SENTENCEtoVAR(SENTENCErec2);
              SENTENCEVARrec1.index := LEVEL;
              SENTENCEVARrec1.infsort := omteinf; 
              SENTENCErec1 := SENTENCErec2;
              SENTENCErec1.infsort := omteinf; 
         >

   SUBRULE
      < >
      <sig2:[conjrel/CONJ(KEY_omCONJkey){CONJrec3}] >


      DECOMP
         <
          C1: SENTENCErec2.infsort = omteinf 
              
          A1: SENTENCEVARrec1 := COPYT_sentenceTOvar(SENTENCErec2);
              SENTENCEVARrec1.index := LEVEL;
              SENTENCErec1 := SENTENCErec2;
         >

>

&

\end{verbatim}
