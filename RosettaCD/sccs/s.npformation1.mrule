                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                         } ;

              NPrec1.actcomas :=  CNrec1.posscomas * DETPrec1.posscomas;
              DETPrec2 := DETPrec1;
              DETPrec2.superdeixis := omegadeixis;
          >

      DECOMP
         <
          C1: DETPrec2.posscomas * CNrec1.posscomas <> [] AND
              DETPrec2.possnumbers * CNrec1.numbers <> [] AND
              ((mass IN CNrec1.posscomas) -> (DETPrec2.definite = def))
               AND
              NPrec1 = { posspred          : DETPrec2.posspred
                          definite          : DETPrec2.definite
                          possnietnp        : DETPrec2.possnietnp
                          mood              : DETPrec2.mood

                          nounpatternefs    : CNrec1.nounpatternefs
                          thetanp           : CNrec1.thetanp
                          human             : CNrec1.human
                          animate           : CNrec1.animate
                          possgeni          : CNrec1.possgeni
                          temporal          : CNrec1.temporal
                          actsubcs          : CNrec1.actsubcs
                          sexes             : CNrec1.sexes
                          genders           : CNrec1.genders
                          class             : CNrec1.class 
                          deixis            : CNrec1.deixis
                          aspect            : CNrec1.aspect
                          retro             : CNrec1.retro
                          
                          NPhead     : enokNP
                          specQ      : yesspec (* auxiliary *)
                          syntquant  : omegaquant (* auxiliary *)
                          generic    : nogeneric
                          superdeixis: omegadeixis
                         }  AND

               (NPrec1.actcomas * 
                (CNrec1.posscomas * DETPrec2.posscomas) <> []) AND
               (NPrec1.cases = [nominative]) 
                C11: plural  IN CNrec1.numbers  AND   (*twee gele *)
                     NPrec1.number = plural 
                A11: @
                C12: singular  IN CNrec1.numbers AND
                     NPrec1.number = singular  (* een gele, die mooie *) 
