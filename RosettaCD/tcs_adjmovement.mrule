% TRANSFORMATION TADJcomplmovement
< m1: I5::ADJPFORMULA{ADJPFORMULArec1}
                  [mu1,
                   predrel/ADJP{ADJPrec1}
                           [mu2,
                            mu4,
                            I4:rel1/T2,
                            head/ADJ{ADJrec1}[mu3],
                            mu5
                           ]
                   ]
>
< m: I5::ADJPFORMULA{ADJPFORMULArec1}
                  [mu1,
                   predrel/ADJP{ADJPrec1}
                           [mu2,
                            I2:erposrel/T2,
                            I3:mu4,
                            head/ADJ{ADJrec1}[mu3],
                            mu5
                           ]
                   ]
>


   MATCHCONDITIONS
      <
       I2: T2.CAT IN [VARPREPP, PREPP] 
       I3: EXIST (mu4, [rel13/..]) AND ALL (mu4, rel13/..)
       I13: rel13 IN [degreemodrel, tempadvrel, locadvrel, causadvrel, negrel]
       I4: rel1 IN [prepobjrel, aanobjrel, locargrel] AND
           T2.CAT IN [VARPREPP, PREPP]
       I5: ADJPFORMULArec1.adjpatternefs * AUX_PAadjps = [] 
      >
<
   SUBRULE
      <T2: VARPREPP{VARPREPPrec1}[mu6] >
      <T2: VARPREPP{VARPREPPrec1}[mu6] >

      COMP
         <
          C1: true
             C11: VARPREPPrec1.headkey = KEY_aanioprepkey AND
                  ADJPrec1.adjpatternefs * AUX_aanobjadjps <> [] AND
                  rel1 = aanobjrel
             A11: @ 
             C12: VARPREPPrec1.headkey = ADJrec1.prepkey AND
                  ADJPrec1.adjpatternefs * AUX_prepobjadjps <> []  AND
                  rel1 = prepobjrel
             A12: @
             C13: synLOCPREPP IN ADJPrec1.adjpatternefs AND
                  rel1 = locargrel
             A13: @
          A1: @
         >

      DECOMP
          <
          C1: true
             C11: VARPREPPrec1.headkey = KEY_aanioprepkey AND
                  ADJPrec1.adjpatternefs * AUX_aanobjadjps <> [] 
             A11: rel1 := aanobjrel
             C12: VARPREPPrec1.headkey = ADJrec1.prepkey AND
                  ADJPrec1.adjpatternefs * AUX_prepobjadjps <> [] AND  
                  ADJPrec1.adjpatternefs * AUX_PAadjps = []   (* cf.remarks *)
             A12: rel1 := prepobjrel
             C13: synLOCPREPP IN ADJPrec1.adjpatternefs 
             A13: rel1 := locargrel
          A1: @
         >

   SUBRULE
      <T2: PREPP{PREPPrec1}[mu6] >
      <T2: PREPP{PREPPrec1}[mu6] >

      COMP
         <
          C1: true
             C11: PREPPrec1.headkey = KEY_aanioprepkey AND
                  ADJPrec1.adjpatternefs * AUX_aanobjadjps <> [] AND
                  rel1 = aanobjrel
             A11: @ 
             C12: PREPPrec1.headkey = ADJrec1.prepkey AND
                  ADJPrec1.adjpatternefs * AUX_prepobjadjps <> []  AND
                  rel1 = prepobjrel
             A12: @
             C13: synLOCPREPP IN ADJPrec1.adjpatternefs AND
                  rel1 = locargrel
             A13: @
          A1: @
         >

      DECOMP
          <
          C1: true
             C11: PREPPrec1.headkey = KEY_aanioprepkey AND
                  ADJPrec1.adjpatternefs * AUX_aanobjadjps <> [] 
             A11: rel1 := aanobjrel
             C12: PREPPrec1.headkey = ADJrec1.prepkey AND
                  ADJPrec1.adjpatternefs * AUX_prepobjadjps <> [] AND  
                  ADJPrec1.adjpatternefs * AUX_PAadjps = []   (* cf.remarks *)
             A12: rel1 := prepobjrel
             C13: synLOCPREPP IN ADJPrec1.adjpatternefs 
             A13: rel1 := locargrel
          A1: @
         >

>
