\begin{verbatim}
%
% RULE RPropsubst14             (* toevallig- happen*)
< m1: I1::ADVPPROP{ADVPPROPrec1}
              [ I4:mu18,
                I3:mu1,
                predrel/ADVP{ADVPrec1}
                       [ mu2,
                         head/ADV{ADVrec1}[mu3],
                         complrel/SENTENCEVAR{SENTENCEVARrec1},
                         mu4
                       ]
              ]
SUBST:
  m2: SENTENCE{SENTENCErec1}
                   [ subjrel/T1,
                     mu5,
                     predrel/VERBP{VERBPrec1}[mu6, rel2/VERB{VERBrec1}[mu7]]
                   ]
>
<
  m : VERBPPROP{VERBPPROPrec2}
               [ I4:mu18,
                 subjrel/T1,
                 I3:mu1,
                 rel1/I2::ADVP{ADVPrec1}
                          [ mu2,
                            head/ADV{ADVrec1}[mu3],
                            mu4
                          ],
                 mu5,
                 predrel/VERBP{VERBPrec1}[mu6, rel2/VERB{VERBrec2}[mu7 ]] 
               ]
>
   MATCHCONDITIONS
      <
       I1: ADVPPROPrec1.advpatternefs = [synCLOSEDINFSENT] 
       I2: synCLOSEDINFSENT IN ADVPrec1.advpatternefs
       I3: NOT EXIST(mu1, [I31:rel3/T2] )
           I31: rel3 IN [subjrel, leftdislocrel]
       I4: ALL(mu18, leftdislocrel/..)
      >

   PARAMETERS
      <
       LEVEL
      >


(*  *)

      COMP
         <
          C1: (SENTENCEVARrec1.index = LEVEL) AND
              (SENTENCEVARrec1 = COPYT_SENTENCEtovar(SENTENCErec1)) 
                                                                           AND
              ( ADVPPROPrec1 = COPYT_ADVPtoPROP(ADVPrec1) EXCEPT FOR 
                  [ aktionsarts, superdeixis, PROsubject, Radvb ] ) AND
              ( ADVPPROPrec1 =
		  {
		   aktionsarts  : [stative] 
		   Radvb         : false
		  } 
              )                                AND

              VERBrec1.status IN [bareV, partV] AND
              VERBrec1.modus = infinitive AND
              ( SENTENCErec1 = 
		  {
		  req          : [pospol,negpol,omegapol]
		  env          : [pospol,negpol,omegapol]
		  mood         : declarative
		  senttype     : subordinateclause
		  PROsubject   : false
		  modus        : infinitive
		  finiteness   : infinite
		  infsort      : inf
		  VRcompl      : false
		  conjkey      : 0
		  adverbial    : false
		  temporal     : false
		  conjclass    : omegaTimeAdvClass
		  conjdeixis   : omegadeixis
		  conjaspect   : omegaAspect
		  conjretro    : false
		  }
              )                                


              C11: sentadv IN ADVPrec1.actsubcefs 
              A11: rel1 := sentadvrel
             (* are there other subcases ? tempadvps, e.g. zojuist ? *)
          A1: VERBPPROPrec2 :=
		     { 
		      superdeixis : SENTENCErec1.superdeixis
		      PROsubject  : false
		      voice       : SENTENCErec1.voice 
		      synvpefs    : SENTENCErec1.synvpefs
		      thetavp     : SENTENCErec1.thetavp
		      aktionsarts : SENTENCErec1.aktionsarts
		     };
              VERBrec2 := VERBrec1;
              VERBrec2.modus := omegamodus;

 
         >
      DECOMP
         <
          C1: VERBPPROPrec2.superdeixis <> omegadeixis AND
              VERBrec2.modus = omegamodus AND
              VERBrec2.status IN [bareV, partV] 

              C11: (sentadv IN ADVPrec1.actsubcefs) AND
                   ( rel1 = sentadvrel )
              A11: @
 
          A1: SENTENCErec1 :=
		  {
		  voice        : VERBPPROPrec2.voice
		  synvpefs     : VERBPPROPrec2.synvpefs
		  thetavp      : VERBPPROPrec2.thetavp
                  adjuncts     : VERBPPROPrec2.adjuncts
		  aktionsarts  : VERBPPROPrec2.aktionsarts
		  req          : [pospol,negpol,omegapol]
		  env          : [pospol,negpol,omegapol]
		  mood         : declarative
		  senttype     : subordinateclause
		  PROsubject   : false
		  modus        : infinitive
		  finiteness   : infinite
		  infsort      : inf
		  VRcompl      : false
		  conjkey      : 0
		  adverbial    : false
		  temporal     : false
		  conjclass    : omegaTimeAdvClass
		  conjdeixis   : omegadeixis
		  conjaspect   : omegaAspect
		  conjretro    : false
                  superdeixis  : VERBPPROPrec2.superdeixis
		  };
              VERBrec1 := VERBrec2;
              VERBrec1.modus := infinitive;              
              SENTENCEVARrec1 := COPYT_SENTENCEtovar(SENTENCErec1);
              SENTENCEVARrec1.index := LEVEL;
              ADVPPROPrec1 := COPYT_ADVPtoPROP(ADVPrec1);
              ADVPPROPrec1 :=
                 {
		   aktionsarts  : [stative] 
                   RAdvb        : false
                   PROsubject   : false
                   superdeixis  : omegadeixis
                 }
                
         >


&

\end{verbatim}


